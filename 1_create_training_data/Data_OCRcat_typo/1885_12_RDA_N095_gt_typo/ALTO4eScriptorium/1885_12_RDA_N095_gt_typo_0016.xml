<TextLine BASELINE="241 1091 1302 1091" HEIGHT="27" HPOS="241" ID="tl_29" VPOS="1064" WIDTH="1061">
<String CONTENT="Très-curieuse lettre. Il connaît le livre de Buanorotti et a lu derniè¬" HEIGHT="27" HPOS="241" ID="string_tl_29" VPOS="1064" WIDTH="1061"/>
</TextLine>
<TextLine BASELINE="213 1129 1300 1129" HEIGHT="34" HPOS="213" ID="tl_30" VPOS="1095" WIDTH="1087">
<String CONTENT="rement l'&lt;i&gt;Histoire de la conspiration de Babeuf&lt;/i&gt;, par un nommé Fleury," HEIGHT="34" HPOS="213" ID="string_tl_30" VPOS="1095" WIDTH="1087"/>
</TextLine>
<TextLine BASELINE="211 1160 1301 1160" HEIGHT="34" HPOS="211" ID="tl_31" VPOS="1126" WIDTH="1090">
<String CONTENT="jeune écrivain de la réaction. Cette histoire n’a fait qu’ajouter à l’opinion" HEIGHT="34" HPOS="211" ID="string_tl_31" VPOS="1126" WIDTH="1090"/>
</TextLine>
<TextLine BASELINE="212 1191 1303 1191" HEIGHT="32" HPOS="212" ID="tl_32" VPOS="1159" WIDTH="1091">
<String CONTENT="qu’il avait conçue d’abord, du courage, de la probité et du grand carac¬" HEIGHT="32" HPOS="212" ID="string_tl_32" VPOS="1159" WIDTH="1091"/>
</TextLine>
<TextLine BASELINE="213 1223 1302 1223" HEIGHT="34" HPOS="213" ID="tl_33" VPOS="1189" WIDTH="1089">
<String CONTENT="tère de Babeuf et de ses amis. Il croit ne plus rien avoir à apprendre à" HEIGHT="34" HPOS="213" ID="string_tl_33" VPOS="1189" WIDTH="1089"/>
</TextLine>
<TextLine BASELINE="213 1255 1303 1255" HEIGHT="35" HPOS="213" ID="tl_34" VPOS="1220" WIDTH="1090">
<String CONTENT="ce point de vue, Il n’en est pas de même pour ce qui concerne le" HEIGHT="35" HPOS="213" ID="string_tl_34" VPOS="1220" WIDTH="1090"/>
</TextLine>
<TextLine BASELINE="214 1286 1303 1286" HEIGHT="32" HPOS="214" ID="tl_35" VPOS="1254" WIDTH="1089">
<String CONTENT="système des égaux, et la politique suivie par le célèbre conspirateur" HEIGHT="32" HPOS="214" ID="string_tl_35" VPOS="1254" WIDTH="1089"/>
</TextLine>
<TextLine BASELINE="212 1318 1302 1318" HEIGHT="34" HPOS="212" ID="tl_36" VPOS="1284" WIDTH="1090">
<String CONTENT="dans son &lt;i&gt;Orateur du Peuple&lt;/i&gt;. A ce propos, il établit une comparaison" HEIGHT="34" HPOS="212" ID="string_tl_36" VPOS="1284" WIDTH="1090"/>
</TextLine>
<TextLine BASELINE="214 1343 1303 1343" HEIGHT="27" HPOS="214" ID="tl_37" VPOS="1316" WIDTH="1089">
<String CONTENT="entre la tentative malheureuse de Babeuf et les événements de 1848 d’où" HEIGHT="27" HPOS="214" ID="string_tl_37" VPOS="1316" WIDTH="1089"/>
</TextLine>
<TextLine BASELINE="217 1380 1304 1380" HEIGHT="33" HPOS="217" ID="tl_38" VPOS="1347" WIDTH="1087">
<String CONTENT="il tire cette conclusion: Que la révolution sociale manquée en 1848 ne" HEIGHT="33" HPOS="217" ID="string_tl_38" VPOS="1347" WIDTH="1087"/>
</TextLine>
<TextLine BASELINE="215 1412 1304 1412" HEIGHT="33" HPOS="215" ID="tl_39" VPOS="1379" WIDTH="1089">
<String CONTENT="reviendra pas par la résurrection des hommes politiques qui l'ont" HEIGHT="33" HPOS="215" ID="string_tl_39" VPOS="1379" WIDTH="1089"/>
</TextLine>
<TextLine BASELINE="215 1443 1304 1443" HEIGHT="33" HPOS="215" ID="tl_40" VPOS="1410" WIDTH="1089">
<String CONTENT="manquée. «Il faut la faire surgir à nouveau du fond de la situation" HEIGHT="33" HPOS="215" ID="string_tl_40" VPOS="1410" WIDTH="1089"/>
</TextLine>
<TextLine BASELINE="215 1474 1305 1474" HEIGHT="33" HPOS="215" ID="tl_41" VPOS="1441" WIDTH="1090">
<String CONTENT="présente. Si ce n’est pas une loi absolue de l’histoire, c’est au moins" HEIGHT="33" HPOS="215" ID="string_tl_41" VPOS="1441" WIDTH="1090"/>
</TextLine>
<TextLine BASELINE="214 1506 808 1506" HEIGHT="31" HPOS="214" ID="tl_42" VPOS="1475" WIDTH="594">
<String CONTENT="une de ses plus grandes probabilités.»" HEIGHT="31" HPOS="214" ID="string_tl_42" VPOS="1475" WIDTH="594"/>
</TextLine>
