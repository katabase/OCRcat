<TextLine BASELINE="249 595 2705 595" HEIGHT="106" HPOS="249" ID="tl_3" VPOS="489" WIDTH="2456">
<String CONTENT="άναφλογίζειν. Comburere et cremare, consumer et brûler :" HEIGHT="106" HPOS="249" ID="string_tl_3" VPOS="489" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="251 715 2707 715" HEIGHT="100" HPOS="251" ID="tl_4" VPOS="615" WIDTH="2456">
<String CONTENT="comburere, comme χατακαίειν, sur des charbons ardents" HEIGHT="100" HPOS="251" ID="string_tl_4" VPOS="615" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="247 843 2709 843" HEIGHT="106" HPOS="247" ID="tl_5" VPOS="737" WIDTH="2462">
<String CONTENT="(c’est le causatif d’ardere) ; cremare, comme πιμπράναι, par" HEIGHT="106" HPOS="247" ID="string_tl_5" VPOS="737" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="253 965 2713 965" HEIGHT="106" HPOS="253" ID="tl_6" VPOS="859" WIDTH="2460">
<String CONTENT="flammes vives (c’est le causatif de flagrare). On brûle les" HEIGHT="106" HPOS="253" ID="string_tl_6" VPOS="859" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="251 1085 2713 1085" HEIGHT="102" HPOS="251" ID="tl_7" VPOS="983" WIDTH="2462">
<String CONTENT="morts, mortui cremantur, sur un bûcher flamboyant; on" HEIGHT="102" HPOS="251" ID="string_tl_7" VPOS="983" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="253 1209 2717 1209" HEIGHT="106" HPOS="253" ID="tl_8" VPOS="1103" WIDTH="2464">
<String CONTENT="brûle les vivants à petit feu, vivi comburuntur, et cette ma¬" HEIGHT="106" HPOS="253" ID="string_tl_8" VPOS="1103" WIDTH="2464"/>
</TextLine>
<TextLine BASELINE="255 1333 2713 1333" HEIGHT="106" HPOS="255" ID="tl_9" VPOS="1227" WIDTH="2458">
<String CONTENT="nière de parler rend plus frappante l'horreur de la mort" HEIGHT="106" HPOS="255" ID="string_tl_9" VPOS="1227" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="255 1455 1331 1455" HEIGHT="102" HPOS="255" ID="tl_10" VPOS="1353" WIDTH="1076">
<String CONTENT="par ce genre de supplice." HEIGHT="102" HPOS="255" ID="string_tl_10" VPOS="1353" WIDTH="1076"/>
</TextLine>
