<TextLine BASELINE="357 1803 2713 1803" HEIGHT="82" HPOS="357" ID="tl_12" VPOS="1721" WIDTH="2356">
<String CONTENT="Accidere. Evenire. Contingere. Obvenire. Obtingere." HEIGHT="82" HPOS="357" ID="string_tl_12" VPOS="1721" WIDTH="2356"/>
</TextLine>
<TextLine BASELINE="261 1949 2721 1949" HEIGHT="106" HPOS="261" ID="tl_13" VPOS="1843" WIDTH="2460">
<String CONTENT="Accidere, evenire et contingere marquent des événements" HEIGHT="106" HPOS="261" ID="string_tl_13" VPOS="1843" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="265 2071 2727 2071" HEIGHT="104" HPOS="265" ID="tl_14" VPOS="1967" WIDTH="2462">
<String CONTENT="favorables ou défavorables, le premier, lorsqu’ils sont" HEIGHT="104" HPOS="265" ID="string_tl_14" VPOS="1967" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="263 2191 2729 2191" HEIGHT="102" HPOS="263" ID="tl_15" VPOS="2089" WIDTH="2466">
<String CONTENT="inattendus,qu’ils surprennent; le second, lorsqu’ils sont" HEIGHT="102" HPOS="263" ID="string_tl_15" VPOS="2089" WIDTH="2466"/>
</TextLine>
<TextLine BASELINE="265 2315 2727 2315" HEIGHT="104" HPOS="265" ID="tl_16" VPOS="2211" WIDTH="2462">
<String CONTENT="attendus, pressentis ; le troisième, lorsqu’on les a prépa¬" HEIGHT="104" HPOS="265" ID="string_tl_16" VPOS="2211" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="269 2437 2731 2437" HEIGHT="102" HPOS="269" ID="tl_17" VPOS="2335" WIDTH="2462">
<String CONTENT="rés, amenés; obvenire et obtingere ne se disent que d’évé¬" HEIGHT="102" HPOS="269" ID="string_tl_17" VPOS="2335" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="271 2551 2723 2551" HEIGHT="94" HPOS="271" ID="tl_18" VPOS="2457" WIDTH="2452">
<String CONTENT="nements heureux. Les accidentia sont l’œuvre du hasard," HEIGHT="94" HPOS="271" ID="string_tl_18" VPOS="2457" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="269 2681 2725 2681" HEIGHT="102" HPOS="269" ID="tl_19" VPOS="2579" WIDTH="2456">
<String CONTENT="les evenientia sont des conséquences de nos actions ou" HEIGHT="102" HPOS="269" ID="string_tl_19" VPOS="2579" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="273 2807 2731 2807" HEIGHT="106" HPOS="273" ID="tl_20" VPOS="2701" WIDTH="2458">
<String CONTENT="des circonstances; les contingentia, des effets de nos" HEIGHT="106" HPOS="273" ID="string_tl_20" VPOS="2701" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="279 2927 2737 2927" HEIGHT="102" HPOS="279" ID="tl_21" VPOS="2825" WIDTH="2458">
<String CONTENT="efforts, de nos vœux, de nos fautes; les obtingentia et les" HEIGHT="102" HPOS="279" ID="string_tl_21" VPOS="2825" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="275 3041 2735 3041" HEIGHT="96" HPOS="275" ID="tl_22" VPOS="2945" WIDTH="2460">
<String CONTENT="obvenientia, des faveurs du sort. Cic. Fam. VI, 21. Time¬" HEIGHT="96" HPOS="275" ID="string_tl_22" VPOS="2945" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="283 3169 2739 3169" HEIGHT="100" HPOS="283" ID="tl_23" VPOS="3069" WIDTH="2456">
<String CONTENT="bam ne evenirent quæ acciderunt. Je craignais de voir ces" HEIGHT="100" HPOS="283" ID="string_tl_23" VPOS="3069" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="277 3291 2737 3291" HEIGHT="100" HPOS="277" ID="tl_24" VPOS="3191" WIDTH="2460">
<String CONTENT="hasards se réaliser. Le premier des deux verbes, eveni¬" HEIGHT="100" HPOS="277" ID="string_tl_24" VPOS="3191" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="279 3415 2743 3415" HEIGHT="104" HPOS="279" ID="tl_25" VPOS="3311" WIDTH="2464">
<String CONTENT="rent, se rapporte à Cicéron lui-même, à ses pressenti¬" HEIGHT="104" HPOS="279" ID="string_tl_25" VPOS="3311" WIDTH="2464"/>
</TextLine>
<TextLine BASELINE="285 3541 2743 3541" HEIGHT="106" HPOS="285" ID="tl_26" VPOS="3435" WIDTH="2458">
<String CONTENT="ments; le second, acciderunt, regarde les personnes qui" HEIGHT="106" HPOS="285" ID="string_tl_26" VPOS="3435" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="283 3659 2739 3659" HEIGHT="100" HPOS="283" ID="tl_27" VPOS="3559" WIDTH="2456">
<String CONTENT="se montrent surprises à l’heure de l’événement. Sén." HEIGHT="100" HPOS="283" ID="string_tl_27" VPOS="3559" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="285 3785 2739 3785" HEIGHT="104" HPOS="285" ID="tl_28" VPOS="3681" WIDTH="2454">
<String CONTENT="Ep. 110. Scies plura mala confiner* nobis quam accidere," HEIGHT="104" HPOS="285" ID="string_tl_28" VPOS="3681" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="285 3907 2751 3907" HEIGHT="104" HPOS="285" ID="tl_29" VPOS="3803" WIDTH="2466">
<String CONTENT="c’est-à-dire que nos souffrances sont plus souvent les suites" HEIGHT="104" HPOS="285" ID="string_tl_29" VPOS="3803" WIDTH="2466"/>
</TextLine>
<TextLine BASELINE="287 4033 2587 4033" HEIGHT="102" HPOS="287" ID="tl_30" VPOS="3931" WIDTH="2300">
<String CONTENT="de nos propres vœux que l’effet d’un hasard aveugle." HEIGHT="102" HPOS="287" ID="string_tl_30" VPOS="3931" WIDTH="2300"/>
</TextLine>
