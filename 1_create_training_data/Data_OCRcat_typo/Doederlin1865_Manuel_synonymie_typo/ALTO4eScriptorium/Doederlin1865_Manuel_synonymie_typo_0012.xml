</TextLine>
<TextLine BASELINE="209 3425 2657 3425" HEIGHT="104" HPOS="209" ID="tl_24" VPOS="3321" WIDTH="2448">
<String CONTENT="Meministi, quum quemdam affirmares esse in tua potes¬" HEIGHT="104" HPOS="209" ID="string_tl_24" VPOS="3321" WIDTH="2448"/>
</TextLine>
<TextLine BASELINE="205 3545 2657 3545" HEIGHT="102" HPOS="205" ID="tl_25" VPOS="3443" WIDTH="2452">
<String CONTENT="tate, dixisse me volaticum esse ac levem, et te non pedem" HEIGHT="102" HPOS="205" ID="string_tl_25" VPOS="3443" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="207 3671 2663 3671" HEIGHT="106" HPOS="207" ID="tl_26" VPOS="3565" WIDTH="2456">
<String CONTENT="ejus tenere, sed &lt;i&gt;pennam;&lt;/i&gt; mentitus sum, &lt;i&gt;pluma&lt;/i&gt; tenebatur" HEIGHT="106" HPOS="207" ID="string_tl_26" VPOS="3565" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="205 3793 2659 3793" HEIGHT="106" HPOS="205" ID="tl_27" VPOS="3687" WIDTH="2454">
<String CONTENT="quam remisit et fugit. Un jour, tu dois t’en souvenir, tu" HEIGHT="106" HPOS="205" ID="string_tl_27" VPOS="3687" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="201 3917 2657 3917" HEIGHT="108" HPOS="201" ID="tl_28" VPOS="3809" WIDTH="2456">
<String CONTENT="prétendais avoir une personne en ton pouvoir, et je te ré¬" HEIGHT="108" HPOS="201" ID="string_tl_28" VPOS="3809" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="197 4037 2653 4037" HEIGHT="106" HPOS="197" ID="tl_29" VPOS="3931" WIDTH="2456">
<String CONTENT="pondais qu’elle était volage et légère, que tu ne la tenais" HEIGHT="106" HPOS="197" ID="string_tl_29" VPOS="3931" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="199 4159 2653 4159" HEIGHT="104" HPOS="199" ID="tl_30" VPOS="4055" WIDTH="2454">
<String CONTENT="point par le pied, mais par une plume. Eh bien, ce n’était" HEIGHT="104" HPOS="199" ID="string_tl_30" VPOS="4055" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="195 4281 2647 4281" HEIGHT="104" HPOS="195" ID="tl_31" VPOS="4177" WIDTH="2452">
<String CONTENT="pas vrai: tu ne la retenais que par une petite plume de" HEIGHT="104" HPOS="195" ID="string_tl_31" VPOS="4177" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="199 4399 1973 4399" HEIGHT="102" HPOS="199" ID="tl_32" VPOS="4297" WIDTH="1774">
<String CONTENT="duvet qu’elle t’a laissée, et la voilà partie." HEIGHT="102" HPOS="199" ID="string_tl_32" VPOS="4297" WIDTH="1774"/>
</TextLine>
