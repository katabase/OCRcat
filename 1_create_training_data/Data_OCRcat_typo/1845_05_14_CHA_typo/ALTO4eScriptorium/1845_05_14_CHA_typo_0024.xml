<TextBlock HEIGHT="1285" HPOS="1342" ID="r_1_15" VPOS="4318" WIDTH="3551">
<Shape>
<Polygon POINTS="1342 4318 4893 4318 4893 5603 1342 5603"/>
</Shape>
<TextLine BASELINE="1447 4433 4891 4433" HEIGHT="114" HPOS="1447" ID="tl_29" VPOS="4319" WIDTH="3444">
<String CONTENT="Il lui donne avis qu’un secrétaire de l’ambassadeur d’Espagne, qui s’est" HEIGHT="114" HPOS="1447" ID="string_tl_29" VPOS="4319" WIDTH="3444"/>
</TextLine>
<TextLine BASELINE="1343 4531 4887 4531" HEIGHT="112" HPOS="1343" ID="tl_30" VPOS="4419" WIDTH="3544">
<String CONTENT="sauvé d’auprès de son maître, vient en France pour dire au roi des choses" HEIGHT="112" HPOS="1343" ID="string_tl_30" VPOS="4419" WIDTH="3544"/>
</TextLine>
<TextLine BASELINE="1351 4643 4873 4643" HEIGHT="124" HPOS="1351" ID="tl_31" VPOS="4519" WIDTH="3522">
<String CONTENT="importantes; il a vu cet homme à son passage et l’a beaucoup entretenu;" HEIGHT="124" HPOS="1351" ID="string_tl_31" VPOS="4519" WIDTH="3522"/>
</TextLine>
<TextLine BASELINE="1355 4731 4883 4731" HEIGHT="116" HPOS="1355" ID="tl_32" VPOS="4615" WIDTH="3528">
<String CONTENT="mais il y a des articles dont cet homme n’a pas voulu parler, disant qu’il" HEIGHT="116" HPOS="1355" ID="string_tl_32" VPOS="4615" WIDTH="3528"/>
</TextLine>
<TextLine BASELINE="1356 4847 4885 4847" HEIGHT="130" HPOS="1356" ID="tl_33" VPOS="4717" WIDTH="3529">
<String CONTENT="ne peut les dire qu’au roi. Il lui a donné de quoi continuer sa route jusqu’à" HEIGHT="130" HPOS="1356" ID="string_tl_33" VPOS="4717" WIDTH="3529"/>
</TextLine>
<TextLine BASELINE="1352 4947 4878 4947" HEIGHT="134" HPOS="1352" ID="tl_34" VPOS="4813" WIDTH="3526">
<String CONTENT="Paris, où il ne doit pas tarder à arriver. Cet homme, qui est Napolitain," HEIGHT="134" HPOS="1352" ID="string_tl_34" VPOS="4813" WIDTH="3526"/>
</TextLine>
<TextLine BASELINE="1354 5039 4888 5039" HEIGHT="126" HPOS="1354" ID="tl_35" VPOS="4913" WIDTH="3534">
<String CONTENT="lui a conté fort naturellement les raisons qui l’ont obligé à sortir de Na¬" HEIGHT="126" HPOS="1354" ID="string_tl_35" VPOS="4913" WIDTH="3534"/>
</TextLine>
<TextLine BASELINE="1344 5125 4892 5125" HEIGHT="114" HPOS="1344" ID="tl_36" VPOS="5011" WIDTH="3548">
<String CONTENT="ples, entre autres, qu’il a mis dans un four une fille dont il n’était pas con¬" HEIGHT="114" HPOS="1344" ID="string_tl_36" VPOS="5011" WIDTH="3548"/>
</TextLine>
<TextLine BASELINE="1355 5225 4892 5225" HEIGHT="116" HPOS="1355" ID="tl_37" VPOS="5109" WIDTH="3537">
<String CONTENT="tent, et l’a fait brûler de peur qu’on ne trouvât son corps s’il l'eût fait" HEIGHT="116" HPOS="1355" ID="string_tl_37" VPOS="5109" WIDTH="3537"/>
</TextLine>
<TextLine BASELINE="1351 5325 4891 5325" HEIGHT="114" HPOS="1351" ID="tl_38" VPOS="5211" WIDTH="3540">
<String CONTENT="mourir d’une autre mort.... Il doit juger par là que c’est un méchant" HEIGHT="114" HPOS="1351" ID="string_tl_38" VPOS="5211" WIDTH="3540"/>
</TextLine>
<TextLine BASELINE="1353 5437 4889 5437" HEIGHT="126" HPOS="1353" ID="tl_39" VPOS="5311" WIDTH="3536">
<String CONTENT="homme, qu’on ne doit pas laisser approcher du roi, mais que, s’il a de" HEIGHT="126" HPOS="1353" ID="string_tl_39" VPOS="5311" WIDTH="3536"/>
</TextLine>
<TextLine BASELINE="1353 5523 4883 5523" HEIGHT="114" HPOS="1353" ID="tl_40" VPOS="5409" WIDTH="3530">
<String CONTENT="bons avis a donner, on peut les tirer de lui, et lui faire dire tout ce qu’il" HEIGHT="114" HPOS="1353" ID="string_tl_40" VPOS="5409" WIDTH="3530"/>
</TextLine>
<TextLine BASELINE="1351 5602 2003 5602" HEIGHT="93" HPOS="1351" ID="tl_41" VPOS="5509" WIDTH="652">
<String CONTENT="sait, etc., etc." HEIGHT="93" HPOS="1351" ID="string_tl_41" VPOS="5509" WIDTH="652"/>
</TextLine>
</TextBlock>
