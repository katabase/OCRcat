</Shape>
<TextLine BASELINE="1428 2271 4876 2271" HEIGHT="130" HPOS="1428" ID="tl_11" VPOS="2141" WIDTH="3448">
<String CONTENT="On y remarque le passage suivant, écrit entièrement de la main de l’ami¬" HEIGHT="130" HPOS="1428" ID="string_tl_11" VPOS="2141" WIDTH="3448"/>
</TextLine>
<TextLine BASELINE="1336 2353 4878 2353" HEIGHT="116" HPOS="1336" ID="tl_12" VPOS="2237" WIDTH="3542">
<String CONTENT="ral, et signé (le commencement de la lettre étant d’un secrétaire). « Il me" HEIGHT="116" HPOS="1336" ID="string_tl_12" VPOS="2237" WIDTH="3542"/>
</TextLine>
<TextLine BASELINE="1330 2451 4876 2451" HEIGHT="112" HPOS="1330" ID="tl_13" VPOS="2339" WIDTH="3546">
<String CONTENT="« samble que vous ne scauries mieulx faire que de vous acheminer à Or¬" HEIGHT="112" HPOS="1330" ID="string_tl_13" VPOS="2339" WIDTH="3546"/>
</TextLine>
<TextLine BASELINE="1329 2551 4884 2551" HEIGHT="112" HPOS="1329" ID="tl_14" VPOS="2439" WIDTH="3555">
<String CONTENT="« leans, ou iespere que iaure le moien de vous veoir, et ce pendant asseures" HEIGHT="112" HPOS="1329" ID="string_tl_14" VPOS="2439" WIDTH="3555"/>
</TextLine>
<TextLine BASELINE="1333 2647 4873 2647" HEIGHT="110" HPOS="1333" ID="tl_15" VPOS="2537" WIDTH="3540">
<String CONTENT="« vous quil ne tiendra point en moy que nous nayons une paix, mais si on" HEIGHT="110" HPOS="1333" ID="string_tl_15" VPOS="2537" WIDTH="3540"/>
</TextLine>
<TextLine BASELINE="1333 2763 4883 2763" HEIGHT="128" HPOS="1333" ID="tl_16" VPOS="2635" WIDTH="3550">
<String CONTENT="« la peult faire aveques les articles que iay veus, l’on ne peut esperer que" HEIGHT="128" HPOS="1333" ID="string_tl_16" VPOS="2635" WIDTH="3550"/>
</TextLine>
<TextLine BASELINE="1331 2849 4883 2849" HEIGHT="116" HPOS="1331" ID="tl_17" VPOS="2733" WIDTH="3552">
<String CONTENT="« plus grans troubles en ce royaulme que jamais; car c’est trop grand pitie" HEIGHT="116" HPOS="1331" ID="string_tl_17" VPOS="2733" WIDTH="3552"/>
</TextLine>
<TextLine BASELINE="1330 2945 4881 2945" HEIGHT="112" HPOS="1330" ID="tl_18" VPOS="2833" WIDTH="3551">
<String CONTENT="« que de limiter ainssy certains lieux pour servir a Dieu, comme sil ne le" HEIGHT="112" HPOS="1330" ID="string_tl_18" VPOS="2833" WIDTH="3551"/>
</TextLine>
<TextLine BASELINE="1336 3019 2969 3019" HEIGHT="86" HPOS="1336" ID="tl_19" VPOS="2933" WIDTH="1633">
<String CONTENT="« vouloit estre eu tous endroicts. »" HEIGHT="86" HPOS="1336" ID="string_tl_19" VPOS="2933" WIDTH="1633"/>
</TextLine>
</TextBlock>
