<TextLine BASELINE="1457 6349 4897 6349" HEIGHT="116" HPOS="1457" ID="tl_46" VPOS="6233" WIDTH="3440">
<String CONTENT="Il le complimente sur sa sortie de prison. « .... Comme je prends plus de" HEIGHT="116" HPOS="1457" ID="string_tl_46" VPOS="6233" WIDTH="3440"/>
</TextLine>
<TextLine BASELINE="1355 6453 4903 6453" HEIGHT="116" HPOS="1355" ID="tl_47" VPOS="6337" WIDTH="3548">
<String CONTENT="« part que personne du monde à toutes les choses qui vous touchent, aussy" HEIGHT="116" HPOS="1355" ID="string_tl_47" VPOS="6337" WIDTH="3548"/>
</TextLine>
<TextLine BASELINE="1356 6545 4203 6545" HEIGHT="110" HPOS="1356" ID="tl_48" VPOS="6435" WIDTH="2847">
