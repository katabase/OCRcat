<String CONTENT="« .... Nous faisons toujours la guerre des Chouans; les Anglais viennent" HEIGHT="117" HPOS="1490" ID="string_tl_31" VPOS="4587" WIDTH="3472"/>
</TextLine>
<TextLine BASELINE="1376 4804 4964 4804" HEIGHT="119" HPOS="1376" ID="tl_32" VPOS="4685" WIDTH="3588">
