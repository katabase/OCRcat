<TextLine BASELINE="1491 2290 4967 2290" HEIGHT="119" HPOS="1491" ID="tl_11" VPOS="2171" WIDTH="3476">
<String CONTENT="« Je désire bien votre retour; je pourrai alors partir de Paris sans le" HEIGHT="119" HPOS="1491" ID="string_tl_11" VPOS="2171" WIDTH="3476"/>
</TextLine>
<TextLine BASELINE="1390 2392 4965 2392" HEIGHT="121" HPOS="1390" ID="tl_12" VPOS="2271" WIDTH="3575">
<String CONTENT="« chagrin de laisser ma sœur sans personne qui lui donne des soins parti¬" HEIGHT="121" HPOS="1390" ID="string_tl_12" VPOS="2271" WIDTH="3575"/>
</TextLine>
<TextLine BASELINE="1386 2476 4970 2476" HEIGHT="105" HPOS="1386" ID="tl_13" VPOS="2371" WIDTH="3584">
<String CONTENT="« culiers.... Berthier a donné hier une belle fête, mais les ambassadeurs" HEIGHT="105" HPOS="1386" ID="string_tl_13" VPOS="2371" WIDTH="3584"/>
</TextLine>
<TextLine BASELINE="1388 2590 4962 2590" HEIGHT="117" HPOS="1388" ID="tl_14" VPOS="2473" WIDTH="3574">
<String CONTENT="« trouvent bien étrange qu’un gouvernement si puissant ait si peu de repré¬" HEIGHT="117" HPOS="1388" ID="string_tl_14" VPOS="2473" WIDTH="3574"/>
</TextLine>
<TextLine BASELINE="1387 2690 4966 2690" HEIGHT="119" HPOS="1387" ID="tl_15" VPOS="2571" WIDTH="3579">
<String CONTENT="« sentation. Ils sont obligés d’aller passer leurs soirées chez les banquiers" HEIGHT="119" HPOS="1387" ID="string_tl_15" VPOS="2571" WIDTH="3579"/>
</TextLine>
<TextLine BASELINE="1387 2790 4965 2790" HEIGHT="117" HPOS="1387" ID="tl_16" VPOS="2673" WIDTH="3578">
<String CONTENT="« ou chez les ci-devant, qui sont les ennemis du gouvernement. Pas un" HEIGHT="117" HPOS="1387" ID="string_tl_16" VPOS="2673" WIDTH="3578"/>
</TextLine>
<TextLine BASELINE="1387 2890 4969 2890" HEIGHT="119" HPOS="1387" ID="tl_17" VPOS="2771" WIDTH="3582">
<String CONTENT="« ministre ne reçoit. Le public s’aperçoit bien du vide que vous faites. Pas" HEIGHT="119" HPOS="1387" ID="string_tl_17" VPOS="2771" WIDTH="3582"/>
</TextLine>
<TextLine BASELINE="1384 2990 4967 2990" HEIGHT="121" HPOS="1384" ID="tl_18" VPOS="2869" WIDTH="3583">
<String CONTENT="« un feu d’artifice pour égayer le peuple; aussi point d’élans, point de" HEIGHT="121" HPOS="1384" ID="string_tl_18" VPOS="2869" WIDTH="3583"/>
</TextLine>
<TextLine BASELINE="1388 3075 1866 3075" HEIGHT="102" HPOS="1388" ID="tl_19" VPOS="2973" WIDTH="478">
<String CONTENT="« joie.... »" HEIGHT="102" HPOS="1388" ID="string_tl_19" VPOS="2973" WIDTH="478"/>
</TextLine>
