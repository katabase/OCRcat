<TextLine BASELINE="687 1325 3317 1325" HEIGHT="97" HPOS="687" ID="tl_5" VPOS="1228" WIDTH="2630">
<String CONTENT="Il désire bien le voir revenir à Rome, il serait si heureux de" HEIGHT="97" HPOS="687" ID="string_tl_5" VPOS="1228" WIDTH="2630"/>
</TextLine>
<TextLine BASELINE="587 1427 3316 1427" HEIGHT="105" HPOS="587" ID="tl_6" VPOS="1322" WIDTH="2729">
<String CONTENT="passer une partie de sa vie avec lui.... «Il faut ioindre à cela," HEIGHT="105" HPOS="587" ID="string_tl_6" VPOS="1322" WIDTH="2729"/>
</TextLine>
<TextLine BASELINE="592 1523 3318 1523" HEIGHT="105" HPOS="592" ID="tl_7" VPOS="1418" WIDTH="2726">
<String CONTENT="«Monsieur, la nécessité dans laquelle on est d’auoir icy a la teste" HEIGHT="105" HPOS="592" ID="string_tl_7" VPOS="1418" WIDTH="2726"/>
</TextLine>
<TextLine BASELINE="588 1619 3322 1619" HEIGHT="105" HPOS="588" ID="tl_8" VPOS="1514" WIDTH="2734">
<String CONTENT="«du sacré collège un cardinal de uostre rang et de uostre autho¬" HEIGHT="105" HPOS="588" ID="string_tl_8" VPOS="1514" WIDTH="2734"/>
</TextLine>
<TextLine BASELINE="588 1717 3317 1717" HEIGHT="109" HPOS="588" ID="tl_9" VPOS="1608" WIDTH="2729">
<String CONTENT="«rité, i'ose uous dire que uous ne recognoistriés plus cette cour," HEIGHT="109" HPOS="588" ID="string_tl_9" VPOS="1608" WIDTH="2729"/>
</TextLine>
<TextLine BASELINE="589 1811 3317 1811" HEIGHT="107" HPOS="589" ID="tl_10" VPOS="1704" WIDTH="2728">
<String CONTENT="«iamais on n'a vu une telle dégaisne, si uous me permettés de me" HEIGHT="107" HPOS="589" ID="string_tl_10" VPOS="1704" WIDTH="2728"/>
</TextLine>
<TextLine BASELINE="593 1905 3315 1905" HEIGHT="105" HPOS="593" ID="tl_11" VPOS="1800" WIDTH="2722">
<String CONTENT="«seruir de ce terme, uous plaindriés, Monsieur, ceux qui y sont," HEIGHT="105" HPOS="593" ID="string_tl_11" VPOS="1800" WIDTH="2722"/>
</TextLine>
<TextLine BASELINE="591 2001 3318 2001" HEIGHT="107" HPOS="591" ID="tl_12" VPOS="1894" WIDTH="2727">
<String CONTENT="«et qui ont à respondre de tout ce qui s’y passe. Il ne m'est pas" HEIGHT="107" HPOS="591" ID="string_tl_12" VPOS="1894" WIDTH="2727"/>
</TextLine>
<TextLine BASELINE="590 2097 3314 2097" HEIGHT="107" HPOS="590" ID="tl_13" VPOS="1990" WIDTH="2724">
<String CONTENT="«permis d’en dire dauantage sans chiffre, c’est pourtant tout ce" HEIGHT="107" HPOS="590" ID="string_tl_13" VPOS="1990" WIDTH="2724"/>
</TextLine>
<TextLine BASELINE="588 2191 2274 2191" HEIGHT="101" HPOS="588" ID="tl_14" VPOS="2090" WIDTH="1686">
<String CONTENT="«que ie peu faire que de me retenir...»" HEIGHT="101" HPOS="588" ID="string_tl_14" VPOS="2090" WIDTH="1686"/>
</TextLine>
