<TextLine BASELINE="184 859 1552 859" HEIGHT="38" HPOS="184" ID="tl_4" VPOS="821" WIDTH="1368">
<String CONTENT="En fait d’art, la jeunesse est une chose fort relative. Des peintres comme" HEIGHT="38" HPOS="184" ID="string_tl_4" VPOS="821" WIDTH="1368"/>
</TextLine>
<TextLine BASELINE="147 909 1552 909" HEIGHT="38" HPOS="147" ID="tl_5" VPOS="871" WIDTH="1405">
<String CONTENT="Raphaël, Van Gogh, Lévitann, qui sont morts jeunes, nous ont tout de même" HEIGHT="38" HPOS="147" ID="string_tl_5" VPOS="871" WIDTH="1405"/>
</TextLine>
<TextLine BASELINE="145 960 1553 960" HEIGHT="38" HPOS="145" ID="tl_6" VPOS="922" WIDTH="1408">
<String CONTENT="légué une œuvre riche et mûre, tandis que d’autres, qui vivent âgés, restent" HEIGHT="38" HPOS="145" ID="string_tl_6" VPOS="922" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="143 1010 665 1010" HEIGHT="36" HPOS="143" ID="tl_7" VPOS="974" WIDTH="522">
<String CONTENT="toute leur vie des apprentis." HEIGHT="36" HPOS="143" ID="string_tl_7" VPOS="974" WIDTH="522"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="545" HPOS="143" ID="r_3_2" VPOS="1021" WIDTH="1412">
<Shape>
<Polygon POINTS="143 1021 1555 1021 1555 1566 143 1566"/>
</Shape>
<TextLine BASELINE="183 1061 1553 1061" HEIGHT="39" HPOS="183" ID="tl_8" VPOS="1022" WIDTH="1370">
<String CONTENT="Il y a des jeunes qui laissent paraître dans leurs œuvres une fatigue de" HEIGHT="39" HPOS="183" ID="string_tl_8" VPOS="1022" WIDTH="1370"/>
</TextLine>
<TextLine BASELINE="145 1111 1552 1111" HEIGHT="38" HPOS="145" ID="tl_9" VPOS="1073" WIDTH="1407">
<String CONTENT="la vie et un manque de foi dans l’être humain qu’on ne rencontre pas même" HEIGHT="38" HPOS="145" ID="string_tl_9" VPOS="1073" WIDTH="1407"/>
</TextLine>
<TextLine BASELINE="144 1161 1553 1161" HEIGHT="39" HPOS="144" ID="tl_10" VPOS="1122" WIDTH="1409">
<String CONTENT="chez les vieux. Les grands maîtres ont travaillé jusqu’à leur dernier souffle" HEIGHT="39" HPOS="144" ID="string_tl_10" VPOS="1122" WIDTH="1409"/>
</TextLine>
<TextLine BASELINE="144 1212 1551 1212" HEIGHT="38" HPOS="144" ID="tl_11" VPOS="1174" WIDTH="1407">
<String CONTENT="avec l'entrain de la jeunesse et avec cet immense amour de l'homme, qui" HEIGHT="38" HPOS="144" ID="string_tl_11" VPOS="1174" WIDTH="1407"/>
</TextLine>
<TextLine BASELINE="146 1262 1554 1262" HEIGHT="38" HPOS="146" ID="tl_12" VPOS="1224" WIDTH="1408">
<String CONTENT="poussa, par exemple, un Daumier, presque aveugle, à continuer à se pencher" HEIGHT="38" HPOS="146" ID="string_tl_12" VPOS="1224" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="145 1313 1554 1313" HEIGHT="38" HPOS="145" ID="tl_13" VPOS="1275" WIDTH="1409">
<String CONTENT="sur la pierre lithographique; un Renoir, paralysé, à peindre en se faisant" HEIGHT="38" HPOS="145" ID="string_tl_13" VPOS="1275" WIDTH="1409"/>
</TextLine>
<TextLine BASELINE="144 1363 1552 1363" HEIGHT="37" HPOS="144" ID="tl_14" VPOS="1326" WIDTH="1408">
<String CONTENT="attacher un pinceau à sa main infirme. Des géants de la peinture, tels le Titien," HEIGHT="37" HPOS="144" ID="string_tl_14" VPOS="1326" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="146 1414 1554 1414" HEIGHT="37" HPOS="146" ID="tl_15" VPOS="1377" WIDTH="1408">
<String CONTENT="Rembrandt, Goya et tant d’autres, créèrent, au déclin de leur vie, justement" HEIGHT="37" HPOS="146" ID="string_tl_15" VPOS="1377" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="145 1465 1553 1465" HEIGHT="38" HPOS="145" ID="tl_16" VPOS="1427" WIDTH="1408">
<String CONTENT="leurs œuvres les plus jeunes, et j’entends par là les plus vivantes et les plus" HEIGHT="38" HPOS="145" ID="string_tl_16" VPOS="1427" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="146 1515 1554 1515" HEIGHT="37" HPOS="146" ID="tl_17" VPOS="1478" WIDTH="1408">
<String CONTENT="hardies, pleines d'esprit novateur, tant par leur technique qu'au point de vue" HEIGHT="37" HPOS="146" ID="string_tl_17" VPOS="1478" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="145 1565 1135 1565" HEIGHT="36" HPOS="145" ID="tl_18" VPOS="1529" WIDTH="990">
<String CONTENT="de leur conception et de leur résonance émotive." HEIGHT="36" HPOS="145" ID="string_tl_18" VPOS="1529" WIDTH="990"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="443" HPOS="140" ID="r_3_3" VPOS="1578" WIDTH="1414">
<Shape>
<Polygon POINTS="140 1578 1554 1578 1554 2021 140 2021"/>
</Shape>
<TextLine BASELINE="183 1616 1553 1616" HEIGHT="37" HPOS="183" ID="tl_19" VPOS="1579" WIDTH="1370">
<String CONTENT="Le problème de la jeunesse, en art, n’a donc rien à voir avec les données" HEIGHT="37" HPOS="183" ID="string_tl_19" VPOS="1579" WIDTH="1370"/>
</TextLine>
<TextLine BASELINE="145 1667 1553 1667" HEIGHT="37" HPOS="145" ID="tl_20" VPOS="1630" WIDTH="1408">
<String CONTENT="de l’état civil. Toutefois, en peinture comme pour le reste, les générations" HEIGHT="37" HPOS="145" ID="string_tl_20" VPOS="1630" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="144 1717 1552 1717" HEIGHT="38" HPOS="144" ID="tl_21" VPOS="1679" WIDTH="1408">
<String CONTENT="successives se développent, travaillent et laissent aux générations futures" HEIGHT="38" HPOS="144" ID="string_tl_21" VPOS="1679" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="145 1768 1552 1768" HEIGHT="37" HPOS="145" ID="tl_22" VPOS="1731" WIDTH="1407">
<String CONTENT="des œuvres pouvant leur servir d’exemple, amorçant parfois des problèmes" HEIGHT="37" HPOS="145" ID="string_tl_22" VPOS="1731" WIDTH="1407"/>
</TextLine>
<TextLine BASELINE="144 1817 1553 1817" HEIGHT="36" HPOS="144" ID="tl_23" VPOS="1781" WIDTH="1409">
<String CONTENT="que celles-ci auront à résoudre. Les arts bénéficient actuellement en Bulgarie" HEIGHT="36" HPOS="144" ID="string_tl_23" VPOS="1781" WIDTH="1409"/>
</TextLine>
<TextLine BASELINE="144 1869 1553 1869" HEIGHT="38" HPOS="144" ID="tl_24" VPOS="1831" WIDTH="1409">
<String CONTENT="de la présence d’une génération de maîtres mûrs, encore pleins de vie et de" HEIGHT="38" HPOS="144" ID="string_tl_24" VPOS="1831" WIDTH="1409"/>
</TextLine>
<TextLine BASELINE="141 1919 1553 1919" HEIGHT="37" HPOS="141" ID="tl_25" VPOS="1882" WIDTH="1412">
<String CONTENT="talent, qui font l'orgueil de la nation. En même temps, se développe et s'affirme" HEIGHT="37" HPOS="141" ID="string_tl_25" VPOS="1882" WIDTH="1412"/>
</TextLine>
<TextLine BASELINE="145 1969 1553 1969" HEIGHT="36" HPOS="145" ID="tl_26" VPOS="1933" WIDTH="1408">
<String CONTENT="une génération nouvelle, dont les quelques œuvres présentées ici ne sauraient" HEIGHT="36" HPOS="145" ID="string_tl_26" VPOS="1933" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="145 2020 1551 2020" HEIGHT="37" HPOS="145" ID="tl_27" VPOS="1983" WIDTH="1406">
<String CONTENT="que très incomplètement définir la physionomie. L’œuvre de cette génération" HEIGHT="37" HPOS="145" ID="string_tl_27" VPOS="1983" WIDTH="1406"/>
</TextLine>
</TextBlock>
