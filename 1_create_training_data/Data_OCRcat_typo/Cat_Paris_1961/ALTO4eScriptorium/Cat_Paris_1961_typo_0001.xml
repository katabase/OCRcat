<TextLine BASELINE="173 1452 1548 1452" HEIGHT="58" HPOS="173" ID="tl_16" VPOS="1394" WIDTH="1375">
<String CONTENT="L’attitude du dessinateur Van Thienen, par contre, est entièrement diffé¬" HEIGHT="58" HPOS="173" ID="string_tl_16" VPOS="1394" WIDTH="1375"/>
</TextLine>
<TextLine BASELINE="140 1504 1549 1504" HEIGHT="58" HPOS="140" ID="tl_17" VPOS="1446" WIDTH="1409">
<String CONTENT="rente : « Pour moi, l'art est un dialogue... Je ne puis concevoir l’artiste « œil »..." HEIGHT="58" HPOS="140" ID="string_tl_17" VPOS="1446" WIDTH="1409"/>
</TextLine>
<TextLine BASELINE="141 1555 1550 1555" HEIGHT="59" HPOS="141" ID="tl_18" VPOS="1496" WIDTH="1409">
<String CONTENT="Ouvert au monde, il y prend son sujet, pas pour l’anecdote, sans doute, mais" HEIGHT="59" HPOS="141" ID="string_tl_18" VPOS="1496" WIDTH="1409"/>
</TextLine>
<TextLine BASELINE="143 1610 1552 1610" HEIGHT="63" HPOS="143" ID="tl_19" VPOS="1547" WIDTH="1409">
<String CONTENT="un peu comme un point d'appui en dehors de lui-même... Le ton de ce dialogue" HEIGHT="63" HPOS="143" ID="string_tl_19" VPOS="1547" WIDTH="1409"/>
</TextLine>
<TextLine BASELINE="142 1655 1553 1655" HEIGHT="57" HPOS="142" ID="tl_20" VPOS="1598" WIDTH="1411">
<String CONTENT="est celui d’une conversation entre vieux amis, faite de longs cheminements" HEIGHT="57" HPOS="142" ID="string_tl_20" VPOS="1598" WIDTH="1411"/>
</TextLine>
<TextLine BASELINE="144 1709 1011 1709" HEIGHT="52" HPOS="144" ID="tl_21" VPOS="1657" WIDTH="867">
<String CONTENT="silencieux et jalonnés d’apparentes banalités. »" HEIGHT="52" HPOS="144" ID="string_tl_21" VPOS="1657" WIDTH="867"/>
</TextLine>
