<TextLine BASELINE="164 1052 1540 1052" HEIGHT="61" HPOS="164" ID="tl_8" VPOS="991" WIDTH="1376">
<String CONTENT="Jef Verheyen, représentant du groupe anversois Hessenhuis, qui s’impose" HEIGHT="61" HPOS="164" ID="string_tl_8" VPOS="991" WIDTH="1376"/>
</TextLine>
<TextLine BASELINE="133 1105 1540 1105" HEIGHT="64" HPOS="133" ID="tl_9" VPOS="1041" WIDTH="1407">
<String CONTENT="par une très grande activité, est un subjectif avoué : « L’action qu’exerce la" HEIGHT="64" HPOS="133" ID="string_tl_9" VPOS="1041" WIDTH="1407"/>
</TextLine>
<TextLine BASELINE="134 1156 1541 1156" HEIGHT="63" HPOS="134" ID="tl_10" VPOS="1093" WIDTH="1407">
<String CONTENT="peinture, écrit-il, se développe au plus intime de notre être. Echappant à toute" HEIGHT="63" HPOS="134" ID="string_tl_10" VPOS="1093" WIDTH="1407"/>
</TextLine>
<TextLine BASELINE="133 1206 1542 1206" HEIGHT="63" HPOS="133" ID="tl_11" VPOS="1143" WIDTH="1409">
<String CONTENT="approche raisonnée, elle prend racine dans la totalité des phénomènes..." HEIGHT="63" HPOS="133" ID="string_tl_11" VPOS="1143" WIDTH="1409"/>
</TextLine>
<TextLine BASELINE="136 1253 1544 1253" HEIGHT="59" HPOS="136" ID="tl_12" VPOS="1194" WIDTH="1408">
<String CONTENT="Ni l'histoire, ni le temps ne pourront un jour révéler la vérité picturale, puisque" HEIGHT="59" HPOS="136" ID="string_tl_12" VPOS="1194" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="135 1301 1546 1301" HEIGHT="56" HPOS="135" ID="tl_13" VPOS="1245" WIDTH="1411">
<String CONTENT="celle-ci existe dans l'instant... Il n’y aura jamais d’étalon qui puisse mesurer" HEIGHT="56" HPOS="135" ID="string_tl_13" VPOS="1245" WIDTH="1411"/>
</TextLine>
<TextLine BASELINE="137 1357 1546 1357" HEIGHT="63" HPOS="137" ID="tl_14" VPOS="1294" WIDTH="1409">
<String CONTENT="les pouvoirs (ou limites) de l'imagination. Quant à ce qu'on appelle l’évolution," HEIGHT="63" HPOS="137" ID="string_tl_14" VPOS="1294" WIDTH="1409"/>
</TextLine>
<TextLine BASELINE="137 1404 1170 1404" HEIGHT="52" HPOS="137" ID="tl_15" VPOS="1352" WIDTH="1033">
<String CONTENT="elle n'a de limites que chez ceux qui ne la vivent point. »" HEIGHT="52" HPOS="137" ID="string_tl_15" VPOS="1352" WIDTH="1033"/>
</TextLine>
