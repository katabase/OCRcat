<TextLine BASELINE="349 181 1701 181" HEIGHT="48" HPOS="349" ID="tl_2" VPOS="133" WIDTH="1352">
<String CONTENT="n'est certes pas exempte de faiblesses, qui, dans certaines toiles, sautent" HEIGHT="48" HPOS="349" ID="string_tl_2" VPOS="133" WIDTH="1352"/>
</TextLine>
<TextLine BASELINE="349 237 1700 237" HEIGHT="53" HPOS="349" ID="tl_3" VPOS="184" WIDTH="1351">
<String CONTENT="aux yeux, soit parce que leurs jeunes auteurs sont incapables de les éviter," HEIGHT="53" HPOS="349" ID="string_tl_3" VPOS="184" WIDTH="1351"/>
</TextLine>
<TextLine BASELINE="349 286 1701 286" HEIGHT="50" HPOS="349" ID="tl_4" VPOS="236" WIDTH="1352">
<String CONTENT="soit parce qu’ils ne possèdent pas l’astuce et la technique, d'ailleurs peu" HEIGHT="50" HPOS="349" ID="string_tl_4" VPOS="236" WIDTH="1352"/>
</TextLine>
<TextLine BASELINE="349 330 1049 330" HEIGHT="42" HPOS="349" ID="tl_5" VPOS="288" WIDTH="700">
<String CONTENT="recommandable, pour les camoufler." HEIGHT="42" HPOS="349" ID="string_tl_5" VPOS="288" WIDTH="700"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="1064" HPOS="341" ID="r_2_2" VPOS="338" WIDTH="1374">
