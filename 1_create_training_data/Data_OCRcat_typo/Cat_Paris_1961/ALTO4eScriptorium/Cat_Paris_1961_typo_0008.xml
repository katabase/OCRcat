</Shape>
<TextLine BASELINE="384 386 1703 386" HEIGHT="47" HPOS="384" ID="tl_6" VPOS="339" WIDTH="1319">
<String CONTENT="Il importe davantage, me semble-t-il, de relever que cette œuvre a des qualités" HEIGHT="47" HPOS="384" ID="string_tl_6" VPOS="339" WIDTH="1319"/>
</TextLine>
<TextLine BASELINE="348 438 1704 438" HEIGHT="47" HPOS="348" ID="tl_7" VPOS="391" WIDTH="1356">
<String CONTENT="évidentes, qui l'emportent sur ses défauts. La plupart des toiles, des sculptures" HEIGHT="47" HPOS="348" ID="string_tl_7" VPOS="391" WIDTH="1356"/>
</TextLine>
<TextLine BASELINE="348 486 1705 486" HEIGHT="44" HPOS="348" ID="tl_8" VPOS="442" WIDTH="1357">
<String CONTENT="et des gravures des jeunes artistes bulgares sont puisées à la source de la" HEIGHT="44" HPOS="348" ID="string_tl_8" VPOS="442" WIDTH="1357"/>
</TextLine>
<TextLine BASELINE="348 538 1705 538" HEIGHT="44" HPOS="348" ID="tl_9" VPOS="494" WIDTH="1357">
<String CONTENT="vie même et inspirées par celle-ci, et c'est là la plus importante de leurs" HEIGHT="44" HPOS="348" ID="string_tl_9" VPOS="494" WIDTH="1357"/>
</TextLine>
<TextLine BASELINE="349 589 1706 589" HEIGHT="45" HPOS="349" ID="tl_10" VPOS="544" WIDTH="1357">
<String CONTENT="qualités. La majorité des œuvres est consacrée à la contemporanéité, dévoilant" HEIGHT="45" HPOS="349" ID="string_tl_10" VPOS="544" WIDTH="1357"/>
</TextLine>
<TextLine BASELINE="348 638 1707 638" HEIGHT="43" HPOS="348" ID="tl_11" VPOS="595" WIDTH="1359">
<String CONTENT="les aspirations et les sentiments de l’homme moderne. La note caractéris¬" HEIGHT="43" HPOS="348" ID="string_tl_11" VPOS="595" WIDTH="1359"/>
</TextLine>
<TextLine BASELINE="345 691 1707 691" HEIGHT="45" HPOS="345" ID="tl_12" VPOS="646" WIDTH="1362">
<String CONTENT="tique et en même temps très positive de cette œuvre est la tendance de toute" HEIGHT="45" HPOS="345" ID="string_tl_12" VPOS="646" WIDTH="1362"/>
</TextLine>
<TextLine BASELINE="348 743 1708 743" HEIGHT="46" HPOS="348" ID="tl_13" VPOS="697" WIDTH="1360">
<String CONTENT="une série de jeunes artistes à arriver à un style individuel, personnel, à une" HEIGHT="46" HPOS="348" ID="string_tl_13" VPOS="697" WIDTH="1360"/>
</TextLine>
<TextLine BASELINE="346 794 1709 794" HEIGHT="47" HPOS="346" ID="tl_14" VPOS="747" WIDTH="1363">
<String CONTENT="originalité de langage plastique, sans laquelle ne saurait naître une véritable" HEIGHT="47" HPOS="346" ID="string_tl_14" VPOS="747" WIDTH="1363"/>
</TextLine>
<TextLine BASELINE="345 847 1710 847" HEIGHT="49" HPOS="345" ID="tl_15" VPOS="798" WIDTH="1365">
<String CONTENT="œuvre d'art. Point n'est besoin d’un long examen des dites œuvres pour" HEIGHT="49" HPOS="345" ID="string_tl_15" VPOS="798" WIDTH="1365"/>
</TextLine>
<TextLine BASELINE="347 896 1709 896" HEIGHT="47" HPOS="347" ID="tl_16" VPOS="849" WIDTH="1362">
<String CONTENT="remarquer la facture originale de la plupart d'entre elles, la variété de leur" HEIGHT="47" HPOS="347" ID="string_tl_16" VPOS="849" WIDTH="1362"/>
</TextLine>
<TextLine BASELINE="346 949 1709 949" HEIGHT="50" HPOS="346" ID="tl_17" VPOS="899" WIDTH="1363">
<String CONTENT="manière, depuis le souci du détail jusqu'au laconisme poussé à l'extrême," HEIGHT="50" HPOS="346" ID="string_tl_17" VPOS="899" WIDTH="1363"/>
</TextLine>
<TextLine BASELINE="345 1000 1710 1000" HEIGHT="52" HPOS="345" ID="tl_18" VPOS="948" WIDTH="1365">
<String CONTENT="depuis la palette éblouissante jusqu'à la sobriété voisine de la gravure, où" HEIGHT="52" HPOS="345" ID="string_tl_18" VPOS="948" WIDTH="1365"/>
</TextLine>
<TextLine BASELINE="344 1051 1711 1051" HEIGHT="52" HPOS="344" ID="tl_19" VPOS="999" WIDTH="1367">
<String CONTENT="l’espace et les volumes ne sont qu'ébauchés, depuis le coloris sonore et clair" HEIGHT="52" HPOS="344" ID="string_tl_19" VPOS="999" WIDTH="1367"/>
</TextLine>
<TextLine BASELINE="342 1102 1711 1102" HEIGHT="52" HPOS="342" ID="tl_20" VPOS="1050" WIDTH="1369">
<String CONTENT="jusqu'aux harmonies mineures, car le réalisme tel que nous le concevons" HEIGHT="52" HPOS="342" ID="string_tl_20" VPOS="1050" WIDTH="1369"/>
</TextLine>
<TextLine BASELINE="344 1154 1712 1154" HEIGHT="53" HPOS="344" ID="tl_21" VPOS="1101" WIDTH="1368">
<String CONTENT="est justement une méthode et non pas un cliché; car ce réalisme est étranger" HEIGHT="53" HPOS="344" ID="string_tl_21" VPOS="1101" WIDTH="1368"/>
</TextLine>
<TextLine BASELINE="342 1205 1713 1205" HEIGHT="53" HPOS="342" ID="tl_22" VPOS="1152" WIDTH="1371">
<String CONTENT="au piétinement sur place, ainsi que le disait Maïakovski : « Plus de poètes," HEIGHT="53" HPOS="342" ID="string_tl_22" VPOS="1152" WIDTH="1371"/>
</TextLine>
<TextLine BASELINE="343 1257 1713 1257" HEIGHT="56" HPOS="343" ID="tl_23" VPOS="1201" WIDTH="1370">
<String CONTENT="de bons, de divers... », ce qui pour nous pourrait se traduire par : « Plus de" HEIGHT="56" HPOS="343" ID="string_tl_23" VPOS="1201" WIDTH="1370"/>
</TextLine>
<TextLine BASELINE="343 1305 1714 1305" HEIGHT="52" HPOS="343" ID="tl_24" VPOS="1253" WIDTH="1371">
<String CONTENT="peintres, de bons, de divers... », afin que soient conservées la richesse et la" HEIGHT="52" HPOS="343" ID="string_tl_24" VPOS="1253" WIDTH="1371"/>
</TextLine>
<TextLine BASELINE="343 1358 1714 1358" HEIGHT="57" HPOS="343" ID="tl_25" VPOS="1301" WIDTH="1371">
<String CONTENT="diversité de l'expression plastique, sans laquelle disparaîtrait la richesse du" HEIGHT="57" HPOS="343" ID="string_tl_25" VPOS="1301" WIDTH="1371"/>
</TextLine>
<TextLine BASELINE="342 1401 1138 1401" HEIGHT="46" HPOS="342" ID="tl_26" VPOS="1355" WIDTH="796">
<String CONTENT="contenu et en fin de compte l'art lui-même." HEIGHT="46" HPOS="342" ID="string_tl_26" VPOS="1355" WIDTH="796"/>
</TextLine>
