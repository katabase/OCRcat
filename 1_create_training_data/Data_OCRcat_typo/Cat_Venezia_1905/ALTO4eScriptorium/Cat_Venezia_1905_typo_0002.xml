</TextLine>
<TextLine BASELINE="687 891 2065 891" HEIGHT="35" HPOS="687" ID="tl_10" VPOS="856" WIDTH="1378">
<String CONTENT="tichi mattoni da focolare che si trovano in una vecchia curio¬" HEIGHT="35" HPOS="687" ID="string_tl_10" VPOS="856" WIDTH="1378"/>
</TextLine>
<TextLine BASELINE="684 958 2065 958" HEIGHT="44" HPOS="684" ID="tl_11" VPOS="914" WIDTH="1381">
