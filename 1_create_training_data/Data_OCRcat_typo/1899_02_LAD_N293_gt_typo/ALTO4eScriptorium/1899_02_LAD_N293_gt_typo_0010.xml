<TextLine BASELINE="387 2562 3033 2562" HEIGHT="94" HPOS="387" ID="tl_20" VPOS="2468" WIDTH="2646">
<String CONTENT="Précieuse correspondance toute relative à la traduction française de son" HEIGHT="94" HPOS="387" ID="string_tl_20" VPOS="2468" WIDTH="2646"/>
</TextLine>
<TextLine BASELINE="331 2631 3035 2631" HEIGHT="96" HPOS="331" ID="tl_21" VPOS="2535" WIDTH="2704">
<String CONTENT="&lt;i&gt;Waldemar&lt;/i&gt;. Il entre dans les détails les plus minutieux pour la traduction," HEIGHT="96" HPOS="331" ID="string_tl_21" VPOS="2535" WIDTH="2704"/>
</TextLine>
<TextLine BASELINE="318 2699 3032 2699" HEIGHT="90" HPOS="318" ID="tl_22" VPOS="2609" WIDTH="2714">
<String CONTENT="l’impression et l'illustration de cet ouvrage dont il attend l'apparition avec" HEIGHT="90" HPOS="318" ID="string_tl_22" VPOS="2609" WIDTH="2714"/>
</TextLine>
<TextLine BASELINE="318 2767 3033 2767" HEIGHT="93" HPOS="318" ID="tl_23" VPOS="2674" WIDTH="2715">
<String CONTENT="une impatience d'enfant. Entre temps, il parle de l'invasion des Français en" HEIGHT="93" HPOS="318" ID="string_tl_23" VPOS="2674" WIDTH="2715"/>
</TextLine>
<TextLine BASELINE="320 2834 3032 2834" HEIGHT="87" HPOS="320" ID="tl_24" VPOS="2747" WIDTH="2712">
<String CONTENT="Allemagne; ils ont rasé à coup de boulets de canon sa maison de campa¬" HEIGHT="87" HPOS="320" ID="string_tl_24" VPOS="2747" WIDTH="2712"/>
</TextLine>
<TextLine BASELINE="318 2914 3034 2914" HEIGHT="99" HPOS="318" ID="tl_25" VPOS="2815" WIDTH="2716">
<String CONTENT="gnes située près Dusseldorf, mais l’état de guerre entre les deux pays" HEIGHT="99" HPOS="318" ID="string_tl_25" VPOS="2815" WIDTH="2716"/>
</TextLine>
<TextLine BASELINE="320 2970 3038 2970" HEIGHT="98" HPOS="320" ID="tl_26" VPOS="2872" WIDTH="2718">
<String CONTENT="n’empêcha pas l’impression de &lt;i&gt;Waldemar&lt;/i&gt; qui parut en 1796. Jacobi" HEIGHT="98" HPOS="320" ID="string_tl_26" VPOS="2872" WIDTH="2718"/>
</TextLine>
<TextLine BASELINE="319 3039 3038 3039" HEIGHT="89" HPOS="319" ID="tl_27" VPOS="2950" WIDTH="2719">
<String CONTENT="témoigne son mécontentement de se voir si mal traduit et déclare que pour" HEIGHT="89" HPOS="319" ID="string_tl_27" VPOS="2950" WIDTH="2719"/>
</TextLine>
<TextLine BASELINE="317 3105 3040 3105" HEIGHT="95" HPOS="317" ID="tl_28" VPOS="3010" WIDTH="2723">
<String CONTENT="cette raison il ne viendra pas à Paris. L'infidélité du traducteur le punit" HEIGHT="95" HPOS="317" ID="string_tl_28" VPOS="3010" WIDTH="2723"/>
</TextLine>
<TextLine BASELINE="318 3174 3034 3174" HEIGHT="95" HPOS="318" ID="tl_29" VPOS="3079" WIDTH="2716">
<String CONTENT="de son ambition qui l’a poussé à acquérir une renommée plus étendue," HEIGHT="95" HPOS="318" ID="string_tl_29" VPOS="3079" WIDTH="2716"/>
</TextLine>
<TextLine BASELINE="319 3244 3038 3244" HEIGHT="94" HPOS="319" ID="tl_30" VPOS="3150" WIDTH="2719">
<String CONTENT="«car je m'attends à toutes sortes de dégoûts qui me viendront de cette" HEIGHT="94" HPOS="319" ID="string_tl_30" VPOS="3150" WIDTH="2719"/>
</TextLine>
<TextLine BASELINE="316 3311 3039 3311" HEIGHT="98" HPOS="316" ID="tl_31" VPOS="3213" WIDTH="2723">
<String CONTENT="traduction que j'avais follement désirée, tout persuadé que j'étais qu'il" HEIGHT="98" HPOS="316" ID="string_tl_31" VPOS="3213" WIDTH="2723"/>
</TextLine>
<TextLine BASELINE="316 3382 3039 3382" HEIGHT="98" HPOS="316" ID="tl_32" VPOS="3284" WIDTH="2723">
<String CONTENT="était presque impossible d’en faire une supportable.» — Jacobi parle aussi" HEIGHT="98" HPOS="316" ID="string_tl_32" VPOS="3284" WIDTH="2723"/>
</TextLine>
<TextLine BASELINE="316 3447 3038 3447" HEIGHT="91" HPOS="316" ID="tl_33" VPOS="3356" WIDTH="2722">
<String CONTENT="de la traduction française du &lt;i&gt;Messie&lt;/i&gt; de Klopstock; il parle souvent de ce" HEIGHT="91" HPOS="316" ID="string_tl_33" VPOS="3356" WIDTH="2722"/>
</TextLine>
<TextLine BASELINE="314 3509 1444 3509" HEIGHT="75" HPOS="314" ID="tl_34" VPOS="3434" WIDTH="1130">
<String CONTENT="célèbre poète et de ses œuvres." HEIGHT="75" HPOS="314" ID="string_tl_34" VPOS="3434" WIDTH="1130"/>
</TextLine>
