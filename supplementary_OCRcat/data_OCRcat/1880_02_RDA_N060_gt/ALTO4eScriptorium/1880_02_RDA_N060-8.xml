<TextLine BASELINE="190 1042 1256 1042" HEIGHT="32" HPOS="190" ID="tl_25" VPOS="1010" WIDTH="1066">
<String CONTENT="Tres-curieuse lettre sur le voyage qu'il lit en Amérique, de 1824 à" HEIGHT="32" HPOS="190" ID="string_tl_25" VPOS="1010" WIDTH="1066"/>
</TextLine>
<TextLine BASELINE="162 1074 1257 1074" HEIGHT="32" HPOS="162" ID="tl_26" VPOS="1042" WIDTH="1095">
<String CONTENT="1825. « C’est avec de bien tendres regrets que je quitterai cette terre" HEIGHT="32" HPOS="162" ID="string_tl_26" VPOS="1042" WIDTH="1095"/>
</TextLine>
<TextLine BASELINE="160 1105 1257 1105" HEIGHT="32" HPOS="160" ID="tl_27" VPOS="1073" WIDTH="1097">
<String CONTENT="américaine, le bon, grand et heureux peuple des Etats-Unis auquel je" HEIGHT="32" HPOS="160" ID="string_tl_27" VPOS="1073" WIDTH="1097"/>
</TextLine>
<TextLine BASELINE="161 1136 1257 1136" HEIGHT="31" HPOS="161" ID="tl_28" VPOS="1105" WIDTH="1096">
<String CONTENT="suis amalgamé depuis près d’un demi-siècle, et qui vient encore de me" HEIGHT="31" HPOS="161" ID="string_tl_28" VPOS="1105" WIDTH="1096"/>
</TextLine>
<TextLine BASELINE="161 1167 1257 1167" HEIGHT="31" HPOS="161" ID="tl_29" VPOS="1136" WIDTH="1096">
<String CONTENT="combler de ses bontés. J'y ai vu les miracles de l'indépendance, de la" HEIGHT="31" HPOS="161" ID="string_tl_29" VPOS="1136" WIDTH="1096"/>
</TextLine>
<TextLine BASELINE="160 1199 1256 1199" HEIGHT="32" HPOS="160" ID="tl_30" VPOS="1167" WIDTH="1096">
<String CONTENT="liberté, égalité et self government; le problème des institutions répu¬" HEIGHT="32" HPOS="160" ID="string_tl_30" VPOS="1167" WIDTH="1096"/>
</TextLine>
<TextLine BASELINE="161 1230 1256 1230" HEIGHT="32" HPOS="161" ID="tl_31" VPOS="1198" WIDTH="1095">
<String CONTENT="caines a été résolu ici sur une grande échelle et jamais expérience" HEIGHT="32" HPOS="161" ID="string_tl_31" VPOS="1198" WIDTH="1095"/>
</TextLine>
<TextLine BASELINE="160 1261 1256 1261" HEIGHT="32" HPOS="160" ID="tl_32" VPOS="1229" WIDTH="1096">
<String CONTENT="n'a si bien réussi. » Il comptait retourner comme il était venu, sur un" HEIGHT="32" HPOS="160" ID="string_tl_32" VPOS="1229" WIDTH="1096"/>
</TextLine>
<TextLine BASELINE="161 1293 1257 1293" HEIGHT="32" HPOS="161" ID="tl_33" VPOS="1261" WIDTH="1096">
<String CONTENT="paquebot-poste, mais le peuple et le gouvernement en ont disposé" HEIGHT="32" HPOS="161" ID="string_tl_33" VPOS="1261" WIDTH="1096"/>
</TextLine>
<TextLine BASELINE="160 1324 1257 1324" HEIGHT="32" HPOS="160" ID="tl_34" VPOS="1292" WIDTH="1097">
<String CONTENT="autrement. On a donné le nom de Brandywine à une superbe frégate" HEIGHT="32" HPOS="160" ID="string_tl_34" VPOS="1292" WIDTH="1097"/>
</TextLine>
<TextLine BASELINE="159 1354 801 1354" HEIGHT="31" HPOS="159" ID="tl_35" VPOS="1323" WIDTH="642">
<String CONTENT="qui est chargée de le ramener en France." HEIGHT="31" HPOS="159" ID="string_tl_35" VPOS="1323" WIDTH="642"/>
</TextLine>
