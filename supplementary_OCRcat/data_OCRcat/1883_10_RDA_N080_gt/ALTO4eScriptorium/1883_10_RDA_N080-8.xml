<TextLine BASELINE="134 538 1195 538" HEIGHT="32" HPOS="134" ID="tl_12" VPOS="506" WIDTH="1061">
<String CONTENT="Très curieuse lettre. L'armée d’Italie n’a jamais été ni plus lorte en" HEIGHT="32" HPOS="134" ID="string_tl_12" VPOS="506" WIDTH="1061"/>
</TextLine>
<TextLine BASELINE="103 570 1196 570" HEIGHT="32" HPOS="103" ID="tl_13" VPOS="538" WIDTH="1093">
<String CONTENT="hommes, ni plus misérable en toutes sortes de moyens; elle est dans" HEIGHT="32" HPOS="103" ID="string_tl_13" VPOS="538" WIDTH="1093"/>
</TextLine>
<TextLine BASELINE="102 601 1195 601" HEIGHT="33" HPOS="102" ID="tl_14" VPOS="568" WIDTH="1093">
<String CONTENT="une détresse absolue et il ne lui reste que l’espoir de ses armes. « On" HEIGHT="33" HPOS="102" ID="string_tl_14" VPOS="568" WIDTH="1093"/>
</TextLine>
<TextLine BASELINE="103 632 1194 632" HEIGHT="32" HPOS="103" ID="tl_15" VPOS="600" WIDTH="1091">
<String CONTENT="assure que Buonaparte vient la commander, je ne m'en réjouis point," HEIGHT="32" HPOS="103" ID="string_tl_15" VPOS="600" WIDTH="1091"/>
</TextLine>
<TextLine BASELINE="103 664 1196 664" HEIGHT="32" HPOS="103" ID="tl_16" VPOS="632" WIDTH="1093">
<String CONTENT="il ne trouvera de ressource que dans le génie de la République, dans" HEIGHT="32" HPOS="103" ID="string_tl_16" VPOS="632" WIDTH="1093"/>
</TextLine>
<TextLine BASELINE="102 695 1197 695" HEIGHT="32" HPOS="102" ID="tl_17" VPOS="663" WIDTH="1095">
<String CONTENT="le patriotisme des soldats et dans l’opinion de la force nationale. Au¬" HEIGHT="32" HPOS="102" ID="string_tl_17" VPOS="663" WIDTH="1095"/>
</TextLine>
<TextLine BASELINE="102 727 1196 727" HEIGHT="32" HPOS="102" ID="tl_18" VPOS="695" WIDTH="1094">
<String CONTENT="trefois on ne demandait pour vaincre que de l’argent, cette arme est" HEIGHT="32" HPOS="102" ID="string_tl_18" VPOS="695" WIDTH="1094"/>
</TextLine>
<TextLine BASELINE="104 758 386 758" HEIGHT="31" HPOS="104" ID="tl_19" VPOS="727" WIDTH="282">
