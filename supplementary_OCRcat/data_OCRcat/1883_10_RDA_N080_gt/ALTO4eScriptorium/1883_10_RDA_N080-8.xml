</TextLine>
<TextLine BASELINE="104 929 1197 929" HEIGHT="32" HPOS="104" ID="tl_24" VPOS="897" WIDTH="1093">
<String CONTENT="joints les croquis des portraits des inculpés dans cette affaire, dessinés" HEIGHT="32" HPOS="104" ID="string_tl_24" VPOS="897" WIDTH="1093"/>
</TextLine>
<TextLine BASELINE="103 959 1196 959" HEIGHT="31" HPOS="103" ID="tl_25" VPOS="928" WIDTH="1093">
<String CONTENT="à l’audience de la chambre des Pairs par le duc de Morny, plus tard" HEIGHT="31" HPOS="103" ID="string_tl_25" VPOS="928" WIDTH="1093"/>
</TextLine>
<TextLine BASELINE="102 991 1198 991" HEIGHT="34" HPOS="102" ID="tl_26" VPOS="957" WIDTH="1096">
<String CONTENT="ministre de Napoléon III. Ces croquis sont au nombre de six, dont" HEIGHT="34" HPOS="102" ID="string_tl_26" VPOS="957" WIDTH="1096"/>
</TextLine>
<TextLine BASELINE="103 1023 1197 1023" HEIGHT="32" HPOS="103" ID="tl_27" VPOS="991" WIDTH="1094">
<String CONTENT="un de Poireau, un de Pépin, deux de Morey et deux de Fieschi. Sous" HEIGHT="32" HPOS="103" ID="string_tl_27" VPOS="991" WIDTH="1094"/>
</TextLine>
<TextLine BASELINE="104 1054 1196 1054" HEIGHT="31" HPOS="104" ID="tl_28" VPOS="1023" WIDTH="1092">
<String CONTENT="l'un de ces deux derniers, Fieschi a écrit et signé de sa main ces mots :" HEIGHT="31" HPOS="104" ID="string_tl_28" VPOS="1023" WIDTH="1092"/>
</TextLine>
<TextLine BASELINE="103 1086 1197 1086" HEIGHT="32" HPOS="103" ID="tl_29" VPOS="1054" WIDTH="1094">
<String CONTENT="Je vie trouve bien lait. Fieschi. L'authenticité de ces croquis est" HEIGHT="32" HPOS="103" ID="string_tl_29" VPOS="1054" WIDTH="1094"/>
</TextLine>
<TextLine BASELINE="104 1118 710 1118" HEIGHT="32" HPOS="104" ID="tl_30" VPOS="1086" WIDTH="606">
<String CONTENT="attestée par une note de Mme de Morny." HEIGHT="32" HPOS="104" ID="string_tl_30" VPOS="1086" WIDTH="606"/>
</TextLine>
