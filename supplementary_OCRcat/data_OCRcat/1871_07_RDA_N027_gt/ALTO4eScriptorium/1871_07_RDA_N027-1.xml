<TextLine BASELINE="274 1771 1683 1771" HEIGHT="57" HPOS="274" ID="tl_14" VPOS="1714" WIDTH="1409">
<String CONTENT="Gauthier-La Chapelle (Claude-Marie-Émiliand-Adrien), avocat à" HEIGHT="57" HPOS="274" ID="string_tl_14" VPOS="1714" WIDTH="1409"/>
</TextLine>
<TextLine BASELINE="228 1822 1684 1822" HEIGHT="52" HPOS="228" ID="tl_15" VPOS="1770" WIDTH="1456">
<String CONTENT="la Cour d'appel, professeur de droit, secrétaire-adjoint de l’Institut" HEIGHT="52" HPOS="228" ID="string_tl_15" VPOS="1770" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="228 1873 1680 1873" HEIGHT="54" HPOS="228" ID="tl_16" VPOS="1819" WIDTH="1452">
<String CONTENT="historique, membre de la Société des Amateurs d’autographes, est" HEIGHT="54" HPOS="228" ID="string_tl_16" VPOS="1819" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="229 1923 1684 1923" HEIGHT="54" HPOS="229" ID="tl_17" VPOS="1869" WIDTH="1455">
<String CONTENT="mort le 20 janvier dernier, dans sa 74e année. M. Gauthier-La Chapelle" HEIGHT="54" HPOS="229" ID="string_tl_17" VPOS="1869" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="228 1971 1681 1971" HEIGHT="52" HPOS="228" ID="tl_18" VPOS="1919" WIDTH="1453">
<String CONTENT="était un des plus anciens amateurs d’autographes de Paris. Sa collec¬" HEIGHT="52" HPOS="228" ID="string_tl_18" VPOS="1919" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="231 2022 1681 2022" HEIGHT="53" HPOS="231" ID="tl_19" VPOS="1969" WIDTH="1450">
<String CONTENT="tion est déjà mentionnée dans le Manuel de M. Jules Fontaine, en 1836." HEIGHT="53" HPOS="231" ID="string_tl_19" VPOS="1969" WIDTH="1450"/>
</TextLine>
<TextLine BASELINE="230 2071 1686 2071" HEIGHT="52" HPOS="230" ID="tl_20" VPOS="2019" WIDTH="1456">
<String CONTENT="au nombre des plus intéressantes. Elle se compose de célébrités de tous" HEIGHT="52" HPOS="230" ID="string_tl_20" VPOS="2019" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="229 2121 1684 2121" HEIGHT="53" HPOS="229" ID="tl_21" VPOS="2068" WIDTH="1455">
<String CONTENT="genres, mais particulièrement de celles qui se rattachent au règne de" HEIGHT="53" HPOS="229" ID="string_tl_21" VPOS="2068" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="235 2168 1686 2168" HEIGHT="61" HPOS="235" ID="tl_22" VPOS="2107" WIDTH="1451">
<String CONTENT="Louis XIII Homme de goût, amateur délicat, M. Gauthier n’admettait" HEIGHT="61" HPOS="235" ID="string_tl_22" VPOS="2107" WIDTH="1451"/>
</TextLine>
<TextLine BASELINE="231 2219 1687 2219" HEIGHT="52" HPOS="231" ID="tl_23" VPOS="2167" WIDTH="1456">
<String CONTENT="dans ses dossiers que des pièces d’une conservation parfaite, à moins" HEIGHT="52" HPOS="231" ID="string_tl_23" VPOS="2167" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="233 2269 1684 2269" HEIGHT="52" HPOS="233" ID="tl_24" VPOS="2217" WIDTH="1451">
<String CONTENT="qu elles ne fussent d’une rareté très-grande, et, chaque fois qu’il en" HEIGHT="52" HPOS="233" ID="string_tl_24" VPOS="2217" WIDTH="1451"/>
</TextLine>
<TextLine BASELINE="236 2318 1685 2318" HEIGHT="52" HPOS="236" ID="tl_25" VPOS="2266" WIDTH="1449">
<String CONTENT="trouvait l’occasion, il en remplaçait une médiocre par une plus belle." HEIGHT="52" HPOS="236" ID="string_tl_25" VPOS="2266" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="235 2367 1686 2367" HEIGHT="51" HPOS="235" ID="tl_26" VPOS="2316" WIDTH="1451">
<String CONTENT="Il n’a cessé d’augmenter sa collection jusqu’au dernier moment. Sa" HEIGHT="51" HPOS="235" ID="string_tl_26" VPOS="2316" WIDTH="1451"/>
</TextLine>
<TextLine BASELINE="235 2417 1686 2417" HEIGHT="51" HPOS="235" ID="tl_27" VPOS="2366" WIDTH="1451">
<String CONTENT="vue s’était beaucoup affaiblie. Néanmoins, il sortait tous les jours, et il" HEIGHT="51" HPOS="235" ID="string_tl_27" VPOS="2366" WIDTH="1451"/>
</TextLine>
<TextLine BASELINE="236 2467 1686 2467" HEIGHT="53" HPOS="236" ID="tl_28" VPOS="2414" WIDTH="1450">
<String CONTENT="avait failli, il y a quelques années, être écrasé par une voiture. Il en" HEIGHT="53" HPOS="236" ID="string_tl_28" VPOS="2414" WIDTH="1450"/>
</TextLine>
<TextLine BASELINE="237 2518 1684 2518" HEIGHT="54" HPOS="237" ID="tl_29" VPOS="2464" WIDTH="1447">
<String CONTENT="fut quille pour quelques contusions. Sa santé était tout à fait rétablie," HEIGHT="54" HPOS="237" ID="string_tl_29" VPOS="2464" WIDTH="1447"/>
</TextLine>
<TextLine BASELINE="240 2567 1689 2567" HEIGHT="53" HPOS="240" ID="tl_30" VPOS="2514" WIDTH="1449">
<String CONTENT="mais il n’a pu supporter les privations que le siège de Paris a imposées" HEIGHT="53" HPOS="240" ID="string_tl_30" VPOS="2514" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="238 2616 1686 2616" HEIGHT="53" HPOS="238" ID="tl_31" VPOS="2563" WIDTH="1448">
<String CONTENT="aux personnes même les plus aisées. Il n’y avait pas d’homme au" HEIGHT="53" HPOS="238" ID="string_tl_31" VPOS="2563" WIDTH="1448"/>
</TextLine>
