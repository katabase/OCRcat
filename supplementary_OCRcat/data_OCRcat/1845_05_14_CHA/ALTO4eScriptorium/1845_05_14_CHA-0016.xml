</Shape>
<TextLine BASELINE="1475 4275 4913 4275" HEIGHT="112" HPOS="1475" ID="tl_27" VPOS="4163" WIDTH="3438">
<String CONTENT="Cette lettre, de cinq pages, en prose et en vers, est sans contredit une des" HEIGHT="112" HPOS="1475" ID="string_tl_27" VPOS="4163" WIDTH="3438"/>
</TextLine>
<TextLine BASELINE="1377 4377 4909 4377" HEIGHT="118" HPOS="1377" ID="tl_28" VPOS="4259" WIDTH="3532">
<String CONTENT="plus importantes qui aient été écrites par le grand satirique. Elle traite" HEIGHT="118" HPOS="1377" ID="string_tl_28" VPOS="4259" WIDTH="3532"/>
</TextLine>
<TextLine BASELINE="1379 4467 4913 4467" HEIGHT="112" HPOS="1379" ID="tl_29" VPOS="4355" WIDTH="3534">
<String CONTENT="d'une entrevue qu’il vient d’avoir avec le révérend père jésuite de La Chaise," HEIGHT="112" HPOS="1379" ID="string_tl_29" VPOS="4355" WIDTH="3534"/>
</TextLine>
<TextLine BASELINE="1379 4565 4917 4565" HEIGHT="108" HPOS="1379" ID="tl_30" VPOS="4457" WIDTH="3538">
<String CONTENT="auquel il a lu sa célèbre épître sur l’Amour de Dieu. Cette épître, qui cou¬" HEIGHT="108" HPOS="1379" ID="string_tl_30" VPOS="4457" WIDTH="3538"/>
</TextLine>
<TextLine BASELINE="1383 4671 4905 4671" HEIGHT="116" HPOS="1383" ID="tl_31" VPOS="4555" WIDTH="3522">
<String CONTENT="rait en manuscrit, avait soulevé la société des jésuites, qui la proclamait" HEIGHT="116" HPOS="1383" ID="string_tl_31" VPOS="4555" WIDTH="3522"/>
</TextLine>
<TextLine BASELINE="1385 4761 4909 4761" HEIGHT="106" HPOS="1385" ID="tl_32" VPOS="4655" WIDTH="3524">
<String CONTENT="écrite contre elle. On se disputait avec acharnement, à ce propos, sur" HEIGHT="106" HPOS="1385" ID="string_tl_32" VPOS="4655" WIDTH="3524"/>
</TextLine>
<TextLine BASELINE="1387 4851 4901 4851" HEIGHT="100" HPOS="1387" ID="tl_33" VPOS="4751" WIDTH="3514">
<String CONTENT="l’Amour affectif et l’Amour effectif, sur l’Attrition et la Contrition; le feu" HEIGHT="100" HPOS="1387" ID="string_tl_33" VPOS="4751" WIDTH="3514"/>
</TextLine>
<TextLine BASELINE="1381 4951 4911 4951" HEIGHT="100" HPOS="1381" ID="tl_34" VPOS="4851" WIDTH="3530">
<String CONTENT="était allumé entre la société et Messieurs de Port-Royal. Boileau a lu avec" HEIGHT="100" HPOS="1381" ID="string_tl_34" VPOS="4851" WIDTH="3530"/>
</TextLine>
<TextLine BASELINE="1379 5059 4905 5059" HEIGHT="110" HPOS="1379" ID="tl_35" VPOS="4949" WIDTH="3526">
<String CONTENT="adresse au jésuite l'épître coupable ; il s’est concilié le père de La Chaise :" HEIGHT="110" HPOS="1379" ID="string_tl_35" VPOS="4949" WIDTH="3526"/>
</TextLine>
<TextLine BASELINE="1387 5155 2255 5155" HEIGHT="98" HPOS="1387" ID="tl_36" VPOS="5057" WIDTH="868">
<String CONTENT="I'épître est sauvée." HEIGHT="98" HPOS="1387" ID="string_tl_36" VPOS="5057" WIDTH="868"/>
</TextLine>
