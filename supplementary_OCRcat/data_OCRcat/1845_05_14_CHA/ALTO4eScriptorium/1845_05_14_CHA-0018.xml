</Shape>
<TextLine BASELINE="1521 3433 4954 3433" HEIGHT="161" HPOS="1521" ID="tl_20" VPOS="3272" WIDTH="3433">
<String CONTENT="Dans la lettre du 5 octobre 1839, il lui dit qu’autant qu’il aime à donner" HEIGHT="161" HPOS="1521" ID="string_tl_20" VPOS="3272" WIDTH="3433"/>
</TextLine>
<TextLine BASELINE="1414 3520 4957 3520" HEIGHT="150" HPOS="1414" ID="tl_21" VPOS="3370" WIDTH="3543">
<String CONTENT="des éloges à M. Ch. Durand quand il se livre a son talent, autant il croit de" HEIGHT="150" HPOS="1414" ID="string_tl_21" VPOS="3370" WIDTH="3543"/>
</TextLine>
<TextLine BASELINE="1417 3620 4954 3620" HEIGHT="152" HPOS="1417" ID="tl_22" VPOS="3468" WIDTH="3537">
<String CONTENT="son devoir de lui exprimer sa désapprobation quand il lit dans un journal" HEIGHT="152" HPOS="1417" ID="string_tl_22" VPOS="3468" WIDTH="3537"/>
</TextLine>
<TextLine BASELINE="1418 3716 4961 3716" HEIGHT="146" HPOS="1418" ID="tl_23" VPOS="3570" WIDTH="3543">
<String CONTENT="auquel il s’intéresse des articles aussi inconvenants que celui qui y était" HEIGHT="146" HPOS="1418" ID="string_tl_23" VPOS="3570" WIDTH="3543"/>
</TextLine>
<TextLine BASELINE="1419 3812 4962 3812" HEIGHT="144" HPOS="1419" ID="tl_24" VPOS="3668" WIDTH="3543">
<String CONTENT="inséré dans le n° du 2 octobre.... « Comment le Capitole, qui se fait l’or¬" HEIGHT="144" HPOS="1419" ID="string_tl_24" VPOS="3668" WIDTH="3543"/>
</TextLine>
<TextLine BASELINE="1416 3918 4963 3918" HEIGHT="152" HPOS="1416" ID="tl_25" VPOS="3766" WIDTH="3547">
<String CONTENT="« gane du napoléonisme et du système impérial, va-t-il prêcher le système" HEIGHT="152" HPOS="1416" ID="string_tl_25" VPOS="3766" WIDTH="3547"/>
</TextLine>
<TextLine BASELINE="1417 4016 4964 4016" HEIGHT="152" HPOS="1417" ID="tl_26" VPOS="3864" WIDTH="3547">
<String CONTENT="« des cochons à l’engrais!.... Comment souffre-t-il un article qui traîne le" HEIGHT="152" HPOS="1417" ID="string_tl_26" VPOS="3864" WIDTH="3547"/>
</TextLine>
<TextLine BASELINE="1418 4132 4963 4132" HEIGHT="168" HPOS="1418" ID="tl_27" VPOS="3964" WIDTH="3545">
<String CONTENT="« journal dans celte ornière des partisans de la constitution anglaise, dont" HEIGHT="168" HPOS="1418" ID="string_tl_27" VPOS="3964" WIDTH="3545"/>
</TextLine>
<TextLine BASELINE="1421 4222 4960 4222" HEIGHT="156" HPOS="1421" ID="tl_28" VPOS="4066" WIDTH="3539">
<String CONTENT="«le grand principe est cette guerre de synonymes LE ROI RÈGNE ET NE" HEIGHT="156" HPOS="1421" ID="string_tl_28" VPOS="4066" WIDTH="3539"/>
</TextLine>
<TextLine BASELINE="1428 4310 4961 4310" HEIGHT="150" HPOS="1428" ID="tl_29" VPOS="4160" WIDTH="3533">
<String CONTENT="« GOUVERNE PAS. Enfin, comment un journal qui se dit napoléoniste va-t-il" HEIGHT="150" HPOS="1428" ID="string_tl_29" VPOS="4160" WIDTH="3533"/>
</TextLine>
<TextLine BASELINE="1421 4412 4964 4412" HEIGHT="150" HPOS="1421" ID="tl_30" VPOS="4262" WIDTH="3543">
<String CONTENT="« dire que l’armée sert plutôt à opprimer le peuple en temps de paix qu’à le" HEIGHT="150" HPOS="1421" ID="string_tl_30" VPOS="4262" WIDTH="3543"/>
</TextLine>
<TextLine BASELINE="1426 4510 3553 4510" HEIGHT="125" HPOS="1426" ID="tl_31" VPOS="4385" WIDTH="2127">
<String CONTENT="« défendre en temps de guerre!.... etc., etc. »" HEIGHT="125" HPOS="1426" ID="string_tl_31" VPOS="4385" WIDTH="2127"/>
</TextLine>
</TextBlock>
