<TextLine BASELINE="1563 6423 4988 6423" HEIGHT="149" HPOS="1563" ID="tl_46" VPOS="6274" WIDTH="3425">
<String CONTENT="« .... Vous ne doutés pas , mon cher frère, de la satisfaction que nous" HEIGHT="149" HPOS="1563" ID="string_tl_46" VPOS="6274" WIDTH="3425"/>
</TextLine>
<TextLine BASELINE="1450 6524 4987 6524" HEIGHT="152" HPOS="1450" ID="tl_47" VPOS="6372" WIDTH="3537">
<String CONTENT="« éprouverions d’être réunis ; il n’est pas douteux que le meilleur seroit aux" HEIGHT="152" HPOS="1450" ID="string_tl_47" VPOS="6372" WIDTH="3537"/>
</TextLine>
<TextLine BASELINE="1443 6622 4986 6622" HEIGHT="150" HPOS="1443" ID="tl_48" VPOS="6472" WIDTH="3543">
<String CONTENT="« États-Unis. Dites à ma belle-sœur que je serois heureuse de faire sa con¬" HEIGHT="150" HPOS="1443" ID="string_tl_48" VPOS="6472" WIDTH="3543"/>
</TextLine>
<TextLine BASELINE="1446 6736 4989 6736" HEIGHT="168" HPOS="1446" ID="tl_49" VPOS="6568" WIDTH="3543">
<String CONTENT="« naissance ainsi que celle de vos enfants ; quant à vous, mon cher frère," HEIGHT="168" HPOS="1446" ID="string_tl_49" VPOS="6568" WIDTH="3543"/>
</TextLine>
<TextLine BASELINE="1441 6818 4988 6818" HEIGHT="150" HPOS="1441" ID="tl_50" VPOS="6668" WIDTH="3547">
<String CONTENT="« Jérome in’entretien si souvent de l’amitié qu’il vous porte, que j’envisage" HEIGHT="150" HPOS="1441" ID="string_tl_50" VPOS="6668" WIDTH="3547"/>
</TextLine>
<TextLine BASELINE="1444 6914 3957 6914" HEIGHT="125" HPOS="1444" ID="tl_51" VPOS="6789" WIDTH="2513">
