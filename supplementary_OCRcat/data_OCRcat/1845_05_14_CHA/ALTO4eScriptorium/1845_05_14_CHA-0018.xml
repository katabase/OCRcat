<TextLine BASELINE="1536 5105 4974 5105" HEIGHT="161" HPOS="1536" ID="tl_35" VPOS="4944" WIDTH="3438">
<String CONTENT="Il lui fait part de son désir d’aller s’établir à Rome. Le prince de Metter¬" HEIGHT="161" HPOS="1536" ID="string_tl_35" VPOS="4944" WIDTH="3438"/>
</TextLine>
<TextLine BASELINE="1432 5194 4974 5194" HEIGHT="150" HPOS="1432" ID="tl_36" VPOS="5044" WIDTH="3542">
<String CONTENT="nich lui écrit que l'Empereur d’Autriche a fait demander ses passe-ports au" HEIGHT="150" HPOS="1432" ID="string_tl_36" VPOS="5044" WIDTH="3542"/>
</TextLine>
<TextLine BASELINE="1435 5298 4976 5298" HEIGHT="156" HPOS="1435" ID="tl_37" VPOS="5142" WIDTH="3541">
<String CONTENT="saint-père « Je dois croire que cela est, mais je préfère que vous les deman¬" HEIGHT="156" HPOS="1435" ID="string_tl_37" VPOS="5142" WIDTH="3541"/>
</TextLine>
<TextLine BASELINE="1424 5388 4977 5388" HEIGHT="146" HPOS="1424" ID="tl_38" VPOS="5242" WIDTH="3553">
<String CONTENT="« diez directement vous-même au pape et me les envoyiez.... J’espère, mon" HEIGHT="146" HPOS="1424" ID="string_tl_38" VPOS="5242" WIDTH="3553"/>
</TextLine>
<TextLine BASELINE="1431 5490 4980 5490" HEIGHT="152" HPOS="1431" ID="tl_39" VPOS="5338" WIDTH="3549">
<String CONTENT="« cher Lucien, que vous éprouverez quelque plaisir à me revoir après tant" HEIGHT="152" HPOS="1431" ID="string_tl_39" VPOS="5338" WIDTH="3549"/>
</TextLine>
<TextLine BASELINE="1436 5590 4981 5590" HEIGHT="152" HPOS="1436" ID="tl_40" VPOS="5438" WIDTH="3545">
<String CONTENT="« d’années de séparation ; quant à moi, celle idée me fait oublier mes mal¬" HEIGHT="152" HPOS="1436" ID="string_tl_40" VPOS="5438" WIDTH="3545"/>
</TextLine>
<TextLine BASELINE="1435 5676 2094 5676" HEIGHT="101" HPOS="1435" ID="tl_41" VPOS="5575" WIDTH="659">
<String CONTENT="« heurs, etc. »" HEIGHT="101" HPOS="1435" ID="string_tl_41" VPOS="5575" WIDTH="659"/>
</TextLine>
