<String CONTENT="Le recensement des livres propres à Jean Racine a occupé les plus" HEIGHT="30" HPOS="1226" ID="string_tl_22" VPOS="1986" WIDTH="1016"/>
</TextLine>
<TextLine BASELINE="1224 2057 2241 2057" HEIGHT="31" HPOS="1224" ID="tl_23" VPOS="2026" WIDTH="1017">
<String CONTENT="grands savants bibliographes, Paul Mesnard, le vicomte de Grouchy," HEIGHT="31" HPOS="1224" ID="string_tl_23" VPOS="2026" WIDTH="1017"/>
</TextLine>
<TextLine BASELINE="1224 2096 2243 2096" HEIGHT="31" HPOS="1224" ID="tl_24" VPOS="2065" WIDTH="1019">
<String CONTENT="Quérard, parmi d'autres, qui ont tenté de reconstituer la bibliothèque" HEIGHT="31" HPOS="1224" ID="string_tl_24" VPOS="2065" WIDTH="1019"/>
</TextLine>
<TextLine BASELINE="1224 2136 2242 2136" HEIGHT="33" HPOS="1224" ID="tl_25" VPOS="2103" WIDTH="1018">
<String CONTENT="à partir des recherches sur les minutes notariales (l’inventaire après" HEIGHT="33" HPOS="1224" ID="string_tl_25" VPOS="2103" WIDTH="1018"/>
</TextLine>
<TextLine BASELINE="1224 2176 2243 2176" HEIGHT="33" HPOS="1224" ID="tl_26" VPOS="2143" WIDTH="1019">
<String CONTENT="décès), l'examen des exemplaires déposés à la Bibliothèque nationale" HEIGHT="33" HPOS="1224" ID="string_tl_26" VPOS="2143" WIDTH="1019"/>
</TextLine>
<TextLine BASELINE="1225 2216 2242 2216" HEIGHT="31" HPOS="1225" ID="tl_27" VPOS="2185" WIDTH="1017">
<String CONTENT="par Racine lui-même puis par son fils Louis, l’étude de la collection" HEIGHT="31" HPOS="1225" ID="string_tl_27" VPOS="2185" WIDTH="1017"/>
</TextLine>
<TextLine BASELINE="1224 2257 2243 2257" HEIGHT="32" HPOS="1224" ID="tl_28" VPOS="2225" WIDTH="1019">
<String CONTENT="achetée par Le Franc de Pompignan à la vente de Louis, rachetée" HEIGHT="32" HPOS="1224" ID="string_tl_28" VPOS="2225" WIDTH="1019"/>
</TextLine>
<TextLine BASELINE="1225 2296 2243 2296" HEIGHT="31" HPOS="1225" ID="tl_29" VPOS="2265" WIDTH="1018">
<String CONTENT="par l’archevêque Loménie de Brienne et donnée à la bibliothèque" HEIGHT="31" HPOS="1225" ID="string_tl_29" VPOS="2265" WIDTH="1018"/>
</TextLine>
<TextLine BASELINE="1224 2337 2242 2337" HEIGHT="34" HPOS="1224" ID="tl_30" VPOS="2303" WIDTH="1018">
<String CONTENT="des jésuites de Toulouse (aujourd’hui collection publique), et enfin" HEIGHT="34" HPOS="1224" ID="string_tl_30" VPOS="2303" WIDTH="1018"/>
</TextLine>
<TextLine BASELINE="1225 2377 2242 2377" HEIGHT="31" HPOS="1225" ID="tl_31" VPOS="2346" WIDTH="1017">
<String CONTENT="le dépouillage de ventes publiques au fil du temps. La liste la plus" HEIGHT="31" HPOS="1225" ID="string_tl_31" VPOS="2346" WIDTH="1017"/>
</TextLine>
<TextLine BASELINE="1224 2416 2243 2416" HEIGHT="30" HPOS="1224" ID="tl_32" VPOS="2386" WIDTH="1019">
<String CONTENT="complète est certainement celle qui fut dressée par Paul Bonnefon" HEIGHT="30" HPOS="1224" ID="string_tl_32" VPOS="2386" WIDTH="1019"/>
</TextLine>
<TextLine BASELINE="1224 2456 2243 2456" HEIGHT="31" HPOS="1224" ID="tl_33" VPOS="2425" WIDTH="1019">
<String CONTENT="en 1898. C’est dans L’Intermédiaire des chercheurs et des curieux que" HEIGHT="31" HPOS="1224" ID="string_tl_33" VPOS="2425" WIDTH="1019"/>
</TextLine>
<TextLine BASELINE="1226 2496 2242 2496" HEIGHT="31" HPOS="1226" ID="tl_34" VPOS="2465" WIDTH="1016">
<String CONTENT="Bonnefon trouve mentionné pour la première fois l’exemplaire des" HEIGHT="31" HPOS="1226" ID="string_tl_34" VPOS="2465" WIDTH="1016"/>
</TextLine>
<TextLine BASELINE="1224 2537 2242 2537" HEIGHT="31" HPOS="1224" ID="tl_35" VPOS="2506" WIDTH="1018">
<String CONTENT="oeuvres de Jodelle, qui «porte la signature de Racine, sur le titre, au" HEIGHT="31" HPOS="1224" ID="string_tl_35" VPOS="2506" WIDTH="1018"/>
</TextLine>
<TextLine BASELINE="1224 2576 2134 2576" HEIGHT="30" HPOS="1224" ID="tl_36" VPOS="2546" WIDTH="910">
<String CONTENT="dessous de la marque d’imprimeur», sans autre information." HEIGHT="30" HPOS="1224" ID="string_tl_36" VPOS="2546" WIDTH="910"/>
</TextLine>
