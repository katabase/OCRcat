<TextLine BASELINE="325 583 1999 583" HEIGHT="47" HPOS="325" ID="tl_3" VPOS="536" WIDTH="1674">
<String CONTENT="Avec l’autorité qui s’attache à tout ce qu’il écrit, M. Paul Souday a soulevé, dans un" HEIGHT="47" HPOS="325" ID="string_tl_3" VPOS="536" WIDTH="1674"/>
</TextLine>
<TextLine BASELINE="227 631 2000 631" HEIGHT="47" HPOS="227" ID="tl_4" VPOS="584" WIDTH="1773">
<String CONTENT="numéro récent du Temps, une question qui intéresse au plus haut point les acheteurs et les" HEIGHT="47" HPOS="227" ID="string_tl_4" VPOS="584" WIDTH="1773"/>
</TextLine>
<TextLine BASELINE="228 679 2000 679" HEIGHT="47" HPOS="228" ID="tl_5" VPOS="632" WIDTH="1772">
<String CONTENT="vendeurs d’autographes. Un auteur contemporain trouva citées, dans un catalogue de mar¬" HEIGHT="47" HPOS="228" ID="string_tl_5" VPOS="632" WIDTH="1772"/>
</TextLine>
<TextLine BASELINE="227 729 1998 729" HEIGHT="49" HPOS="227" ID="tl_6" VPOS="680" WIDTH="1771">
<String CONTENT="chand, quelques lignes d’un billet qu’il avait jadis adressé à un ami et dont il n’avait pas" HEIGHT="49" HPOS="227" ID="string_tl_6" VPOS="680" WIDTH="1771"/>
</TextLine>
<TextLine BASELINE="227 776 2000 776" HEIGHT="49" HPOS="227" ID="tl_7" VPOS="727" WIDTH="1773">
<String CONTENT="conservé le souvenir. Il se demanda si la lettre était authentique et protesta contre la publi¬" HEIGHT="49" HPOS="227" ID="string_tl_7" VPOS="727" WIDTH="1773"/>
</TextLine>
<TextLine BASELINE="227 823 1997 823" HEIGHT="47" HPOS="227" ID="tl_8" VPOS="776" WIDTH="1770">
<String CONTENT="cation de cette lettre dans un catalogue, sans qu’il en eût été avisé, sans que l’expert lui en" HEIGHT="47" HPOS="227" ID="string_tl_8" VPOS="776" WIDTH="1770"/>
</TextLine>
<TextLine BASELINE="227 868 2001 868" HEIGHT="45" HPOS="227" ID="tl_9" VPOS="823" WIDTH="1774">
<String CONTENT="eût demandé la permission, M. Paul Souday a pris le parti de notre confrère et lui a donné" HEIGHT="45" HPOS="227" ID="string_tl_9" VPOS="823" WIDTH="1774"/>
</TextLine>
<TextLine BASELINE="228 918 2000 918" HEIGHT="47" HPOS="228" ID="tl_10" VPOS="871" WIDTH="1772">
<String CONTENT="raison. Voilà donc appelée au tribunal de l’opinion cette question intéressante au plus" HEIGHT="47" HPOS="228" ID="string_tl_10" VPOS="871" WIDTH="1772"/>
</TextLine>
<TextLine BASELINE="228 964 2002 964" HEIGHT="45" HPOS="228" ID="tl_11" VPOS="919" WIDTH="1774">
<String CONTENT="haut point : Peut-on imprimer sans autorisation préalable quelques lignes d’un auteur" HEIGHT="45" HPOS="228" ID="string_tl_11" VPOS="919" WIDTH="1774"/>
</TextLine>
<TextLine BASELINE="227 998 380 998" HEIGHT="31" HPOS="227" ID="tl_12" VPOS="967" WIDTH="153">
<String CONTENT="vivant ?" HEIGHT="31" HPOS="227" ID="string_tl_12" VPOS="967" WIDTH="153"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="285" HPOS="226" ID="r_2_2" VPOS="1013" WIDTH="1776">
<Shape>
<Polygon POINTS="226 1013 2002 1013 2002 1298 226 1298"/>
</Shape>
<TextLine BASELINE="327 1060 2000 1060" HEIGHT="46" HPOS="327" ID="tl_13" VPOS="1014" WIDTH="1673">
<String CONTENT="Tout d’abord, une observation s’impose : les auteurs vivants ne sont pas seuls en cause:" HEIGHT="46" HPOS="327" ID="string_tl_13" VPOS="1014" WIDTH="1673"/>
</TextLine>
<TextLine BASELINE="228 1106 2001 1106" HEIGHT="43" HPOS="228" ID="tl_14" VPOS="1063" WIDTH="1773">
<String CONTENT="ceux qui sont morts depuis moins de cinquante ans doivent être englobés dans la même" HEIGHT="43" HPOS="228" ID="string_tl_14" VPOS="1063" WIDTH="1773"/>
</TextLine>
<TextLine BASELINE="228 1155 2001 1155" HEIGHT="45" HPOS="228" ID="tl_15" VPOS="1110" WIDTH="1773">
<String CONTENT="catégorie, puisque leurs œuvres ne sont pas tombées dans le domaine public. Il faudrait" HEIGHT="45" HPOS="228" ID="string_tl_15" VPOS="1110" WIDTH="1773"/>
</TextLine>
<TextLine BASELINE="227 1201 2000 1201" HEIGHT="43" HPOS="227" ID="tl_16" VPOS="1158" WIDTH="1773">
<String CONTENT="donc s’en référer aux ayants droits et, s’il est relativement facile de retrouver les" HEIGHT="43" HPOS="227" ID="string_tl_16" VPOS="1158" WIDTH="1773"/>
</TextLine>
<TextLine BASELINE="228 1250 2001 1250" HEIGHT="45" HPOS="228" ID="tl_17" VPOS="1205" WIDTH="1773">
<String CONTENT="héritiers littéraires d’un Verlaine ou d'un Barrès, comment découvrir au pied levé quel est" HEIGHT="45" HPOS="228" ID="string_tl_17" VPOS="1205" WIDTH="1773"/>
</TextLine>
<TextLine BASELINE="228 1297 1824 1297" HEIGHT="43" HPOS="228" ID="tl_18" VPOS="1254" WIDTH="1596">
<String CONTENT="l’ayant-droit littéraire actuel du Général Boulanger ou de Ferdinand de Lesseps ?" HEIGHT="43" HPOS="228" ID="string_tl_18" VPOS="1254" WIDTH="1596"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="235" HPOS="226" ID="r_2_3" VPOS="1300" WIDTH="1788">
<Shape>
<Polygon POINTS="226 1300 2014 1300 2014 1535 226 1535"/>
</Shape>
<TextLine BASELINE="324 1345 2001 1345" HEIGHT="44" HPOS="324" ID="tl_19" VPOS="1301" WIDTH="1677">
<String CONTENT="On sait qu’une jurisprudence constante attribue au destinataire la propriété maté¬" HEIGHT="44" HPOS="324" ID="string_tl_19" VPOS="1301" WIDTH="1677"/>
</TextLine>
<TextLine BASELINE="228 1391 2001 1391" HEIGHT="43" HPOS="228" ID="tl_20" VPOS="1348" WIDTH="1773">
<String CONTENT="rielle de toute lettre missive. Cette propriété, comportant le droit de cession, même à titre" HEIGHT="43" HPOS="228" ID="string_tl_20" VPOS="1348" WIDTH="1773"/>
</TextLine>
<TextLine BASELINE="228 1439 2004 1439" HEIGHT="43" HPOS="228" ID="tl_21" VPOS="1396" WIDTH="1776">
<String CONTENT="onéreux, il s’ensuit que le vendeur a le droit absolu de faire connaître aux acheteurs éven¬" HEIGHT="43" HPOS="228" ID="string_tl_21" VPOS="1396" WIDTH="1776"/>
</TextLine>
<TextLine BASELINE="227 1487 2001 1487" HEIGHT="42" HPOS="227" ID="tl_22" VPOS="1445" WIDTH="1774">
<String CONTENT="tuels ce qui constituera le mérite et le prix de la chose vendue. De là l’usage, quand on vend" HEIGHT="42" HPOS="227" ID="string_tl_22" VPOS="1445" WIDTH="1774"/>
</TextLine>
<TextLine BASELINE="228 1534 1384 1534" HEIGHT="41" HPOS="228" ID="tl_23" VPOS="1493" WIDTH="1156">
<String CONTENT="un autographe, d’en publier des extraits dans le catalogue." HEIGHT="41" HPOS="228" ID="string_tl_23" VPOS="1493" WIDTH="1156"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="128" HPOS="226" ID="r_2_4" VPOS="1540" WIDTH="1774">
<Shape>
<Polygon POINTS="226 1540 2000 1540 2000 1668 226 1668"/>
</Shape>
<TextLine BASELINE="323 1582 1999 1582" HEIGHT="41" HPOS="323" ID="tl_24" VPOS="1541" WIDTH="1676">
<String CONTENT="On sait que ces catalogues sont distribués gratuitement : aussi, voit-on mal sur quoi" HEIGHT="41" HPOS="323" ID="string_tl_24" VPOS="1541" WIDTH="1676"/>
</TextLine>
<TextLine BASELINE="227 1630 1999 1630" HEIGHT="42" HPOS="227" ID="tl_25" VPOS="1588" WIDTH="1772">
<String CONTENT="se fonderait une demande éventuelle de droits d’auteur et sur quelle base il serait possible" HEIGHT="42" HPOS="227" ID="string_tl_25" VPOS="1588" WIDTH="1772"/>
</TextLine>
<TextLine BASELINE="227 1667 713 1667" HEIGHT="31" HPOS="227" ID="tl_26" VPOS="1636" WIDTH="486">
<String CONTENT="d’en calculer le montant." HEIGHT="31" HPOS="227" ID="string_tl_26" VPOS="1636" WIDTH="486"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="137" HPOS="225" ID="r_2_5" VPOS="1683" WIDTH="1773">
<Shape>
<Polygon POINTS="225 1683 1998 1683 1998 1820 225 1820"/>
</Shape>
<TextLine BASELINE="321 1726 1997 1726" HEIGHT="42" HPOS="321" ID="tl_27" VPOS="1684" WIDTH="1676">
<String CONTENT="En droit, les vendeurs d’autographes me paraissent donc parfaitement fondés à con¬" HEIGHT="42" HPOS="321" ID="string_tl_27" VPOS="1684" WIDTH="1676"/>
</TextLine>
<TextLine BASELINE="227 1773 1997 1773" HEIGHT="41" HPOS="227" ID="tl_28" VPOS="1732" WIDTH="1770">
<String CONTENT="tinuer, comme par le passé, à publier, en tout ou en partie, les autographes modernes qu’ils" HEIGHT="41" HPOS="227" ID="string_tl_28" VPOS="1732" WIDTH="1770"/>
</TextLine>
<TextLine BASELINE="226 1819 665 1819" HEIGHT="40" HPOS="226" ID="tl_29" VPOS="1779" WIDTH="439">
<String CONTENT="offrent à leur clientèle." HEIGHT="40" HPOS="226" ID="string_tl_29" VPOS="1779" WIDTH="439"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="43" HPOS="322" ID="r_2_6" VPOS="1826" WIDTH="635">
<Shape>
<Polygon POINTS="322 1826 957 1826 957 1869 322 1869"/>
</Shape>
<TextLine BASELINE="323 1868 956 1868" HEIGHT="41" HPOS="323" ID="tl_30" VPOS="1827" WIDTH="633">
<String CONTENT="Ce droit n’a d’autre limite que :" HEIGHT="41" HPOS="323" ID="string_tl_30" VPOS="1827" WIDTH="633"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="42" HPOS="321" ID="r_2_7" VPOS="1874" WIDTH="1426">
<Shape>
<Polygon POINTS="321 1874 1747 1874 1747 1916 321 1916"/>
</Shape>
<TextLine BASELINE="322 1915 1746 1915" HEIGHT="40" HPOS="322" ID="tl_31" VPOS="1875" WIDTH="1424">
<String CONTENT="1° L’ordre public, au cas où il s’agirait d’un texte immoral ou séditieux." HEIGHT="40" HPOS="322" ID="string_tl_31" VPOS="1875" WIDTH="1424"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="91" HPOS="223" ID="r_2_8" VPOS="1921" WIDTH="1773">
<Shape>
<Polygon POINTS="223 1921 1996 1921 1996 2012 223 2012"/>
</Shape>
<TextLine BASELINE="320 1963 1995 1963" HEIGHT="41" HPOS="320" ID="tl_32" VPOS="1922" WIDTH="1675">
<String CONTENT="2° Les intérêts des tiers, si la lettre était calomnieuse ou diffamatoire, ou même, sim¬" HEIGHT="41" HPOS="320" ID="string_tl_32" VPOS="1922" WIDTH="1675"/>
</TextLine>
<TextLine BASELINE="224 2011 1638 2011" HEIGHT="41" HPOS="224" ID="tl_33" VPOS="1970" WIDTH="1414">
<String CONTENT="plement, si elle révélait des faits qu’un tiers pouvait désirer tenir cachés." HEIGHT="41" HPOS="224" ID="string_tl_33" VPOS="1970" WIDTH="1414"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="139" HPOS="224" ID="r_2_9" VPOS="2016" WIDTH="1774">
<Shape>
<Polygon POINTS="224 2016 1998 2016 1998 2155 224 2155"/>
</Shape>
<TextLine BASELINE="319 2059 1996 2059" HEIGHT="42" HPOS="319" ID="tl_34" VPOS="2017" WIDTH="1677">
<String CONTENT="3° L’abus du droit de citation : on ne saurait, par exemple, sous prétexte de mettre" HEIGHT="42" HPOS="319" ID="string_tl_34" VPOS="2017" WIDTH="1677"/>
</TextLine>
<TextLine BASELINE="225 2107 1997 2107" HEIGHT="43" HPOS="225" ID="tl_35" VPOS="2064" WIDTH="1772">
<String CONTENT="en vente un manuscrit de Thaïs, réimprimer en entier le roman de France et le distribuer" HEIGHT="43" HPOS="225" ID="string_tl_35" VPOS="2064" WIDTH="1772"/>
</TextLine>
<TextLine BASELINE="226 2154 680 2154" HEIGHT="40" HPOS="226" ID="tl_36" VPOS="2114" WIDTH="454">
<String CONTENT="gratuitement au public." HEIGHT="40" HPOS="226" ID="string_tl_36" VPOS="2114" WIDTH="454"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="188" HPOS="220" ID="r_2_10" VPOS="2158" WIDTH="1777">
<Shape>
<Polygon POINTS="220 2158 1997 2158 1997 2346 220 2346"/>
</Shape>
<TextLine BASELINE="321 2202 1996 2202" HEIGHT="43" HPOS="321" ID="tl_37" VPOS="2159" WIDTH="1675">
<String CONTENT="Il me semble que cette triple restriction sauvegarde amplement les droits de l’auteur" HEIGHT="43" HPOS="321" ID="string_tl_37" VPOS="2159" WIDTH="1675"/>
</TextLine>
<TextLine BASELINE="223 2249 1993 2249" HEIGHT="43" HPOS="223" ID="tl_38" VPOS="2206" WIDTH="1770">
<String CONTENT="de l’autographe : au reste, ne sommes-nous pas en France sous le régime de la libre publi¬" HEIGHT="43" HPOS="223" ID="string_tl_38" VPOS="2206" WIDTH="1770"/>
</TextLine>
<TextLine BASELINE="223 2298 1993 2298" HEIGHT="42" HPOS="223" ID="tl_39" VPOS="2256" WIDTH="1770">
<String CONTENT="cation et non sous celui de la censure préalable ? L'expert-vendeur est un commerçant res¬" HEIGHT="42" HPOS="223" ID="string_tl_39" VPOS="2256" WIDTH="1770"/>
</TextLine>
<TextLine BASELINE="221 2345 1674 2345" HEIGHT="42" HPOS="221" ID="tl_40" VPOS="2303" WIDTH="1453">
<String CONTENT="ponsable et peut toujours être poursuivi par l’auteur qui s’estimerait lésé." HEIGHT="42" HPOS="221" ID="string_tl_40" VPOS="2303" WIDTH="1453"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="231" HPOS="218" ID="r_2_11" VPOS="2351" WIDTH="1776">
<Shape>
<Polygon POINTS="218 2351 1994 2351 1994 2582 218 2582"/>
</Shape>
<TextLine BASELINE="319 2392 1993 2392" HEIGHT="40" HPOS="319" ID="tl_41" VPOS="2352" WIDTH="1674">
<String CONTENT="Quant à la question d’authenticité, soulevée par M. Paul Souday et par son corres¬" HEIGHT="40" HPOS="319" ID="string_tl_41" VPOS="2352" WIDTH="1674"/>
</TextLine>
<TextLine BASELINE="220 2440 1991 2440" HEIGHT="45" HPOS="220" ID="tl_42" VPOS="2395" WIDTH="1771">
<String CONTENT="pondant, on me permettra d’être sceptique. Dans bien des cas, l’appréciation autorisée d’un" HEIGHT="45" HPOS="220" ID="string_tl_42" VPOS="2395" WIDTH="1771"/>
</TextLine>
<TextLine BASELINE="221 2488 1992 2488" HEIGHT="47" HPOS="221" ID="tl_43" VPOS="2441" WIDTH="1771">
<String CONTENT="expert en autographes aura plus de poids que la mémoire défaillante d’un écrivain. Là" HEIGHT="47" HPOS="221" ID="string_tl_43" VPOS="2441" WIDTH="1771"/>
</TextLine>
<TextLine BASELINE="221 2534 1992 2534" HEIGHT="43" HPOS="221" ID="tl_44" VPOS="2491" WIDTH="1771">
<String CONTENT="encore, en cas de contestation, le recours aux tribunaux est toujours possible — et même" HEIGHT="43" HPOS="221" ID="string_tl_44" VPOS="2491" WIDTH="1771"/>
</TextLine>
<TextLine BASELINE="219 2581 335 2581" HEIGHT="40" HPOS="219" ID="tl_45" VPOS="2541" WIDTH="116">
<String CONTENT="facile." HEIGHT="40" HPOS="219" ID="string_tl_45" VPOS="2541" WIDTH="116"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="381" HPOS="216" ID="r_2_12" VPOS="2580" WIDTH="1776">
<Shape>
<Polygon POINTS="216 2580 1992 2580 1992 2961 216 2961"/>
</Shape>
<TextLine BASELINE="317 2630 1990 2630" HEIGHT="49" HPOS="317" ID="tl_46" VPOS="2581" WIDTH="1673">
<String CONTENT="Qu’on me permette une dernière observation : la même question a déjà été tranchée," HEIGHT="49" HPOS="317" ID="string_tl_46" VPOS="2581" WIDTH="1673"/>
</TextLine>
<TextLine BASELINE="220 2677 1991 2677" HEIGHT="45" HPOS="220" ID="tl_47" VPOS="2632" WIDTH="1771">
<String CONTENT="pour un cas presque identique, par l’autorité législative. Il y a quelques années, lors du vote" HEIGHT="45" HPOS="220" ID="string_tl_47" VPOS="2632" WIDTH="1771"/>
</TextLine>
<TextLine BASELINE="219 2725 1991 2725" HEIGHT="47" HPOS="219" ID="tl_48" VPOS="2678" WIDTH="1772">
<String CONTENT="de la loi sur la propriété artistique, il a été nettement stipulé par les Chambres que la repro¬" HEIGHT="47" HPOS="219" ID="string_tl_48" VPOS="2678" WIDTH="1772"/>
</TextLine>
<TextLine BASELINE="220 2772 1989 2772" HEIGHT="45" HPOS="220" ID="tl_49" VPOS="2727" WIDTH="1769">
<String CONTENT="duction d’une peinture moderne, dans un catalogue de vente distribué gratuitement, ne" HEIGHT="45" HPOS="220" ID="string_tl_49" VPOS="2727" WIDTH="1769"/>
</TextLine>
<TextLine BASELINE="217 2819 1989 2819" HEIGHT="48" HPOS="217" ID="tl_50" VPOS="2771" WIDTH="1772">
<String CONTENT="tombait pas sous le coup de la loi et que le commissaire-priseur ne pouvait être tenu, ni de" HEIGHT="48" HPOS="217" ID="string_tl_50" VPOS="2771" WIDTH="1772"/>
</TextLine>
<TextLine BASELINE="219 2866 1991 2866" HEIGHT="47" HPOS="219" ID="tl_51" VPOS="2819" WIDTH="1772">
<String CONTENT="solliciter une autorisation, ni de payer un droit de reproduction. Le cas n’est-il pas abso¬" HEIGHT="47" HPOS="219" ID="string_tl_51" VPOS="2819" WIDTH="1772"/>
</TextLine>
<TextLine BASELINE="220 2914 1990 2914" HEIGHT="48" HPOS="220" ID="tl_52" VPOS="2866" WIDTH="1770">
<String CONTENT="lument parallèle, qu’il s’agisse d’une peinture de Manet ou d’une lettre de notre excellent" HEIGHT="48" HPOS="220" ID="string_tl_52" VPOS="2866" WIDTH="1770"/>
</TextLine>
<TextLine BASELINE="219 2960 542 2960" HEIGHT="40" HPOS="219" ID="tl_53" VPOS="2920" WIDTH="323">
<String CONTENT="confrère Y. Z. ?" HEIGHT="40" HPOS="219" ID="string_tl_53" VPOS="2920" WIDTH="323"/>
</TextLine>
</TextBlock>
