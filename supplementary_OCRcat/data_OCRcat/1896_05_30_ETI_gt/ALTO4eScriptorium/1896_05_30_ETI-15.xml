<String CONTENT="SUPERBE LETTRE. Il se plaint que la souscription que l’on fait en sa" HEIGHT="43" HPOS="431" ID="string_tl_23" VPOS="1428" WIDTH="1106"/>
</TextLine>
<TextLine BASELINE="399 1507 1537 1507" HEIGHT="43" HPOS="399" ID="tl_24" VPOS="1464" WIDTH="1138">
<String CONTENT="faveur n’ait pas plus d’écho au nord qu’au midi de la France. « J'ai la" HEIGHT="43" HPOS="399" ID="string_tl_24" VPOS="1464" WIDTH="1138"/>
</TextLine>
<TextLine BASELINE="400 1541 1539 1541" HEIGHT="41" HPOS="400" ID="tl_25" VPOS="1500" WIDTH="1139">
<String CONTENT="conscience que l'on me devait mieux que des outrages et des ostra¬" HEIGHT="41" HPOS="400" ID="string_tl_25" VPOS="1500" WIDTH="1139"/>
</TextLine>
<TextLine BASELINE="399 1575 1540 1575" HEIGHT="40" HPOS="399" ID="tl_26" VPOS="1535" WIDTH="1141">
<String CONTENT="cismes. Sans mon dévouement, bien ou mal réfléchi, pour elle au jour" HEIGHT="40" HPOS="399" ID="string_tl_26" VPOS="1535" WIDTH="1141"/>
</TextLine>
<TextLine BASELINE="398 1613 1541 1613" HEIGHT="43" HPOS="398" ID="tl_27" VPOS="1570" WIDTH="1143">
<String CONTENT="de ses angoisses, je ne serais pas expulsé de mon toit paternel et" HEIGHT="43" HPOS="398" ID="string_tl_27" VPOS="1570" WIDTH="1143"/>
</TextLine>
<TextLine BASELINE="398 1645 1473 1645" HEIGHT="39" HPOS="398" ID="tl_28" VPOS="1606" WIDTH="1075">
<String CONTENT="bientôt meme de ses frontières. Que Dieu ne les lui rende pas! »" HEIGHT="39" HPOS="398" ID="string_tl_28" VPOS="1606" WIDTH="1075"/>
</TextLine>
