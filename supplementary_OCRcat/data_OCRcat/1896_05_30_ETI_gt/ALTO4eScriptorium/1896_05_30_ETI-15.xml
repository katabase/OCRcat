<TextLine BASELINE="391 555 1529 555" HEIGHT="42" HPOS="391" ID="tl_2" VPOS="513" WIDTH="1138">
<String CONTENT="fils, c’est qu’elle professe pour lui la plus vive admiration. Elle" HEIGHT="42" HPOS="391" ID="string_tl_2" VPOS="513" WIDTH="1138"/>
</TextLine>
<TextLine BASELINE="395 590 1531 590" HEIGHT="41" HPOS="395" ID="tl_3" VPOS="549" WIDTH="1136">
<String CONTENT="renonce donc à son projet. « Ah ! vous le savés, ma reconnaissance" HEIGHT="41" HPOS="395" ID="string_tl_3" VPOS="549" WIDTH="1136"/>
</TextLine>
<TextLine BASELINE="395 625 1535 625" HEIGHT="40" HPOS="395" ID="tl_4" VPOS="585" WIDTH="1140">
<String CONTENT="n'est pas celle des âmes ordinaires, mais dans ce cas-ci je serais" HEIGHT="40" HPOS="395" ID="string_tl_4" VPOS="585" WIDTH="1140"/>
</TextLine>
<TextLine BASELINE="394 661 1534 661" HEIGHT="40" HPOS="394" ID="tl_5" VPOS="621" WIDTH="1140">
<String CONTENT="effraie de sa grandeur, non que je voulusse la sentir, mais de ce que" HEIGHT="40" HPOS="394" ID="string_tl_5" VPOS="621" WIDTH="1140"/>
</TextLine>
<TextLine BASELINE="393 698 1536 698" HEIGHT="43" HPOS="393" ID="tl_6" VPOS="655" WIDTH="1143">
<String CONTENT="je ne pourrai jamais l’acquitter. Je ne puis écrire davantage; dites" HEIGHT="43" HPOS="393" ID="string_tl_6" VPOS="655" WIDTH="1143"/>
</TextLine>
<TextLine BASELINE="397 732 1536 732" HEIGHT="41" HPOS="397" ID="tl_7" VPOS="691" WIDTH="1139">
<String CONTENT="tout ce qu’il manque, faites tout ce que je désire et du moins dites-" HEIGHT="41" HPOS="397" ID="string_tl_7" VPOS="691" WIDTH="1139"/>
</TextLine>
<TextLine BASELINE="396 767 1535 767" HEIGHT="40" HPOS="396" ID="tl_8" VPOS="727" WIDTH="1139">
<String CONTENT="vous que ma proposition, quelque terrible qu’elle me paraisse à" HEIGHT="40" HPOS="396" ID="string_tl_8" VPOS="727" WIDTH="1139"/>
</TextLine>
<TextLine BASELINE="396 803 1535 803" HEIGHT="41" HPOS="396" ID="tl_9" VPOS="762" WIDTH="1139">
<String CONTENT="présent, n’était pas dicté par l’égoïsme, mais par la sensibilité, et que" HEIGHT="41" HPOS="396" ID="string_tl_9" VPOS="762" WIDTH="1139"/>
</TextLine>
<TextLine BASELINE="398 839 1533 839" HEIGHT="41" HPOS="398" ID="tl_10" VPOS="798" WIDTH="1135">
<String CONTENT="la manière dont vous la reçûtes doit enrichir votre âme d'un bien beau" HEIGHT="41" HPOS="398" ID="string_tl_10" VPOS="798" WIDTH="1135"/>
</TextLine>
<TextLine BASELINE="398 874 1534 874" HEIGHT="40" HPOS="398" ID="tl_11" VPOS="834" WIDTH="1136">
<String CONTENT="souvenir. Je suis accablé de maux de nerfs. Ah ! vous savés que j’ai" HEIGHT="40" HPOS="398" ID="string_tl_11" VPOS="834" WIDTH="1136"/>
</TextLine>
<TextLine BASELINE="397 909 1534 909" HEIGHT="40" HPOS="397" ID="tl_12" VPOS="869" WIDTH="1137">
<String CONTENT="une âme qui tue mon corps. Je vous embrasse, cher et respectable" HEIGHT="40" HPOS="397" ID="string_tl_12" VPOS="869" WIDTH="1137"/>
</TextLine>
<TextLine BASELINE="397 945 955 945" HEIGHT="36" HPOS="397" ID="tl_13" VPOS="909" WIDTH="558">
<String CONTENT="ami ; je vous aimerai toujours... »" HEIGHT="36" HPOS="397" ID="string_tl_13" VPOS="909" WIDTH="558"/>
</TextLine>
