<TextLine BASELINE="208 798 1268 798" HEIGHT="31" HPOS="208" ID="tl_19" VPOS="767" WIDTH="1060">
<String CONTENT="Belle et intéressante lettre. Il désire « depputer... lieu... conve¬" HEIGHT="31" HPOS="208" ID="string_tl_19" VPOS="767" WIDTH="1060"/>
</TextLine>
<TextLine BASELINE="178 830 1266 830" HEIGHT="31" HPOS="178" ID="tl_20" VPOS="799" WIDTH="1088">
<String CONTENT="nable et permanent pour faire et tenir l'assemblée et chapitre général »" HEIGHT="31" HPOS="178" ID="string_tl_20" VPOS="799" WIDTH="1088"/>
</TextLine>
<TextLine BASELINE="176 862 1269 862" HEIGHT="32" HPOS="176" ID="tl_21" VPOS="830" WIDTH="1093">
<String CONTENT="de l’ordre de Saint-Michel. Il le convoquera donc en la ville de Blois" HEIGHT="32" HPOS="176" ID="string_tl_21" VPOS="830" WIDTH="1093"/>
</TextLine>
<TextLine BASELINE="178 893 1263 893" HEIGHT="32" HPOS="178" ID="tl_22" VPOS="861" WIDTH="1085">
