<TextLine BASELINE="209 471 1269 471" HEIGHT="31" HPOS="209" ID="tl_9" VPOS="440" WIDTH="1060">
<String CONTENT="Importante lettre historique. Il le félicite d'avoir rétabli la paix à Cènes" HEIGHT="31" HPOS="209" ID="string_tl_9" VPOS="440" WIDTH="1060"/>
</TextLine>
<TextLine BASELINE="177 502 1267 502" HEIGHT="31" HPOS="177" ID="tl_10" VPOS="471" WIDTH="1090">
<String CONTENT="et d’avoir sauvé l’Italie entière d'une guerre imminente. On aurait besoin" HEIGHT="31" HPOS="177" ID="string_tl_10" VPOS="471" WIDTH="1090"/>
</TextLine>
<TextLine BASELINE="176 534 1268 534" HEIGHT="31" HPOS="176" ID="tl_11" VPOS="503" WIDTH="1092">
<String CONTENT="d'un homme tel que Morone pour apaiser les troubles en France. Le roi" HEIGHT="31" HPOS="176" ID="string_tl_11" VPOS="503" WIDTH="1092"/>
