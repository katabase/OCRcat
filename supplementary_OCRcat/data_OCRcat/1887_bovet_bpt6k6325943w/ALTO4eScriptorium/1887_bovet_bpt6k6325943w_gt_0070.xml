<TextLine BASELINE="785 1738 2196 1738" HEIGHT="38" HPOS="785" ID="tl_14" VPOS="1700" WIDTH="1411">
<String CONTENT="Il lui envoie une lettre de M. de La Vallette et le prie de la lui retourner" HEIGHT="38" HPOS="785" ID="string_tl_14" VPOS="1700" WIDTH="1411"/>
</TextLine>
<TextLine BASELINE="751 1782 2195 1782" HEIGHT="38" HPOS="751" ID="tl_15" VPOS="1744" WIDTH="1444">
<String CONTENT="après l’avoir lue. Il écrit au roi de Navarre (Henri IV) par des Réaus. Il" HEIGHT="38" HPOS="751" ID="string_tl_15" VPOS="1744" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="751 1822 1336 1822" HEIGHT="36" HPOS="751" ID="tl_16" VPOS="1786" WIDTH="585">
