<TextLine BASELINE="695 2905 1706 2905" HEIGHT="61" HPOS="695" ID="tl_36" VPOS="2844" WIDTH="1011">
<String CONTENT="Importante lettre, écrite par le secrétaire d’État Nicolas" HEIGHT="61" HPOS="695" ID="string_tl_36" VPOS="2844" WIDTH="1011"/>
</TextLine>
<TextLine BASELINE="660 2944 1706 2944" HEIGHT="46" HPOS="660" ID="tl_37" VPOS="2898" WIDTH="1046">
<String CONTENT="de Neufville, sieur de Villeroy, au duc d’Anjou qui, ce" HEIGHT="46" HPOS="660" ID="string_tl_37" VPOS="2898" WIDTH="1046"/>
</TextLine>
<TextLine BASELINE="661 2989 1704 2989" HEIGHT="49" HPOS="661" ID="tl_38" VPOS="2940" WIDTH="1043">
<String CONTENT="même jour, était arrivé devant La Rochelle et avait pris le" HEIGHT="49" HPOS="661" ID="string_tl_38" VPOS="2940" WIDTH="1043"/>
</TextLine>
<TextLine BASELINE="660 3024 1702 3024" HEIGHT="43" HPOS="660" ID="tl_39" VPOS="2981" WIDTH="1042">
<String CONTENT="commandement de l’armée qui assiégeait cette ville. — Il" HEIGHT="43" HPOS="660" ID="string_tl_39" VPOS="2981" WIDTH="1042"/>
</TextLine>
<TextLine BASELINE="662 3075 1703 3075" HEIGHT="51" HPOS="662" ID="tl_40" VPOS="3024" WIDTH="1041">
<String CONTENT="a appris que le duc préparait une grande et forte armée" HEIGHT="51" HPOS="662" ID="string_tl_40" VPOS="3024" WIDTH="1041"/>
</TextLine>
<TextLine BASELINE="660 3116 1703 3116" HEIGHT="51" HPOS="660" ID="tl_41" VPOS="3065" WIDTH="1043">
<String CONTENT="de mer pour combattre ceux qui voudraient secourir les" HEIGHT="51" HPOS="660" ID="string_tl_41" VPOS="3065" WIDTH="1043"/>
</TextLine>
<TextLine BASELINE="661 3157 1703 3157" HEIGHT="49" HPOS="661" ID="tl_42" VPOS="3108" WIDTH="1042">
<String CONTENT="Rochelais, et qu’on proposait de donner le commandement" HEIGHT="49" HPOS="661" ID="string_tl_42" VPOS="3108" WIDTH="1042"/>
</TextLine>
<TextLine BASELINE="659 3191 1702 3191" HEIGHT="41" HPOS="659" ID="tl_43" VPOS="3150" WIDTH="1043">
<String CONTENT="de cette armée à son frère le duc d’Alençon et au roi de" HEIGHT="41" HPOS="659" ID="string_tl_43" VPOS="3150" WIDTH="1043"/>
</TextLine>
<TextLine BASELINE="659 3241 1704 3241" HEIGHT="49" HPOS="659" ID="tl_44" VPOS="3192" WIDTH="1045">
<String CONTENT="Navarre (Henri IV). Il s’oppose absolument à ce projet," HEIGHT="49" HPOS="659" ID="string_tl_44" VPOS="3192" WIDTH="1045"/>
</TextLine>
<TextLine BASELINE="661 3284 1704 3284" HEIGHT="48" HPOS="661" ID="tl_45" VPOS="3236" WIDTH="1043">
<String CONTENT="« car vous sçavez bien que leur permettant aller avecques" HEIGHT="48" HPOS="661" ID="string_tl_45" VPOS="3236" WIDTH="1043"/>
</TextLine>
<TextLine BASELINE="659 3328 1703 3328" HEIGHT="49" HPOS="659" ID="tl_46" VPOS="3279" WIDTH="1044">
<String CONTENT="vous, c’a esté à la charge qu’ils ne vous habandonneroyent" HEIGHT="49" HPOS="659" ID="string_tl_46" VPOS="3279" WIDTH="1044"/>
</TextLine>
<TextLine BASELINE="660 3368 1703 3368" HEIGHT="48" HPOS="660" ID="tl_47" VPOS="3320" WIDTH="1043">
<String CONTENT="aucunement, ains se tiendroyent ordinairement prez de" HEIGHT="48" HPOS="660" ID="string_tl_47" VPOS="3320" WIDTH="1043"/>
</TextLine>
<TextLine BASELINE="659 3414 1703 3414" HEIGHT="50" HPOS="659" ID="tl_48" VPOS="3364" WIDTH="1044">
<String CONTENT="vous et n’iroyent en aucune faction sans vous. Au moyen" HEIGHT="50" HPOS="659" ID="string_tl_48" VPOS="3364" WIDTH="1044"/>
</TextLine>
<TextLine BASELINE="659 3458 1704 3458" HEIGHT="52" HPOS="659" ID="tl_49" VPOS="3406" WIDTH="1045">
<String CONTENT="de quoy, mon frère, je vous prie faire entendre à tous" HEIGHT="52" HPOS="659" ID="string_tl_49" VPOS="3406" WIDTH="1045"/>
</TextLine>
<TextLine BASELINE="659 3500 1702 3500" HEIGHT="51" HPOS="659" ID="tl_50" VPOS="3449" WIDTH="1043">
<String CONTENT="ceulx qui donneront ce conseil et feront ceste proposition" HEIGHT="51" HPOS="659" ID="string_tl_50" VPOS="3449" WIDTH="1043"/>
</TextLine>
<TextLine BASELINE="658 3542 1704 3542" HEIGHT="49" HPOS="658" ID="tl_51" VPOS="3493" WIDTH="1046">
<String CONTENT="que non seullement je ne l’ay agréable, mais je la trouve" HEIGHT="49" HPOS="658" ID="string_tl_51" VPOS="3493" WIDTH="1046"/>
</TextLine>
<TextLine BASELINE="658 3583 1702 3583" HEIGHT="46" HPOS="658" ID="tl_52" VPOS="3537" WIDTH="1044">
<String CONTENT="très mauvaise... » — (Charles IX se méfiait de son frère," HEIGHT="46" HPOS="658" ID="string_tl_52" VPOS="3537" WIDTH="1044"/>
</TextLine>
<TextLine BASELINE="659 3628 1703 3628" HEIGHT="48" HPOS="659" ID="tl_53" VPOS="3580" WIDTH="1044">
<String CONTENT="le duc d’Anjou, ambitieux et jaloux, et de son cousin le" HEIGHT="48" HPOS="659" ID="string_tl_53" VPOS="3580" WIDTH="1044"/>
</TextLine>
<TextLine BASELINE="659 3669 1705 3669" HEIGHT="45" HPOS="659" ID="tl_54" VPOS="3624" WIDTH="1046">
<String CONTENT="roi de Navarre, converti par force lors de la Saint-Bar¬" HEIGHT="45" HPOS="659" ID="string_tl_54" VPOS="3624" WIDTH="1046"/>
</TextLine>
<TextLine BASELINE="657 3716 1704 3716" HEIGHT="46" HPOS="657" ID="tl_55" VPOS="3670" WIDTH="1047">
<String CONTENT="thélemy, et il craignait qu'ils ne fissent cause commune avec" HEIGHT="46" HPOS="657" ID="string_tl_55" VPOS="3670" WIDTH="1047"/>
</TextLine>
<TextLine BASELINE="659 3756 1702 3756" HEIGHT="45" HPOS="659" ID="tl_56" VPOS="3711" WIDTH="1043">
<String CONTENT="les protestants de La Rochelle. Ceux-ci se défendirent si" HEIGHT="45" HPOS="659" ID="string_tl_56" VPOS="3711" WIDTH="1043"/>
</TextLine>
<TextLine BASELINE="659 3800 1602 3800" HEIGHT="46" HPOS="659" ID="tl_57" VPOS="3754" WIDTH="943">
