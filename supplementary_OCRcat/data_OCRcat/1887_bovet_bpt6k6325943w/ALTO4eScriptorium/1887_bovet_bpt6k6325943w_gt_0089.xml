</Shape>
<TextLine BASELINE="690 3230 1731 3230" HEIGHT="52" HPOS="690" ID="tl_20" VPOS="3178" WIDTH="1041">
<String CONTENT="Il lui demande vingt hommes choisis pour rendre sa com¬" HEIGHT="52" HPOS="690" ID="string_tl_20" VPOS="3178" WIDTH="1041"/>
</TextLine>
<TextLine BASELINE="659 3274 1732 3274" HEIGHT="53" HPOS="659" ID="tl_21" VPOS="3221" WIDTH="1073">
<String CONTENT="pagnie des gardes du corps wallonnes complète. « Vous me" HEIGHT="53" HPOS="659" ID="string_tl_21" VPOS="3221" WIDTH="1073"/>
</TextLine>
<TextLine BASELINE="657 3316 1732 3316" HEIGHT="52" HPOS="657" ID="tl_22" VPOS="3264" WIDTH="1075">
<String CONTENT="ferez plaisir de les tirer de la cavallerie de la mesme nation" HEIGHT="52" HPOS="657" ID="string_tl_22" VPOS="3264" WIDTH="1075"/>
</TextLine>
<TextLine BASELINE="657 3358 1734 3358" HEIGHT="53" HPOS="657" ID="tl_23" VPOS="3305" WIDTH="1077">
<String CONTENT="qui est sous vos ordres. J’ay chargé le sieur Duveland, qui" HEIGHT="53" HPOS="657" ID="string_tl_23" VPOS="3305" WIDTH="1077"/>
</TextLine>
<TextLine BASELINE="658 3396 1732 3396" HEIGHT="49" HPOS="658" ID="tl_24" VPOS="3347" WIDTH="1074">
<String CONTENT="vous rendra cette lettre, de les conduire icy, et je profite en¬" HEIGHT="49" HPOS="658" ID="string_tl_24" VPOS="3347" WIDTH="1074"/>
</TextLine>
<TextLine BASELINE="660 3442 1736 3442" HEIGHT="50" HPOS="660" ID="tl_25" VPOS="3392" WIDTH="1076">
<String CONTENT="core avec plaisir de cette occasion pour vous renouveller les" HEIGHT="50" HPOS="660" ID="string_tl_25" VPOS="3392" WIDTH="1076"/>
</TextLine>
<TextLine BASELINE="661 3481 1734 3481" HEIGHT="50" HPOS="661" ID="tl_26" VPOS="3431" WIDTH="1073">
<String CONTENT="asseurances de l’estime et de l’amitié que j’ay pour vous. »" HEIGHT="50" HPOS="661" ID="string_tl_26" VPOS="3431" WIDTH="1073"/>
</TextLine>
<TextLine BASELINE="661 3530 1736 3530" HEIGHT="54" HPOS="661" ID="tl_27" VPOS="3476" WIDTH="1075">
<String CONTENT="— (Maximilien-Emmanuel, n. 1662, électeur de Bavière en" HEIGHT="54" HPOS="661" ID="string_tl_27" VPOS="3476" WIDTH="1075"/>
</TextLine>
<TextLine BASELINE="667 3574 1739 3574" HEIGHT="56" HPOS="667" ID="tl_28" VPOS="3518" WIDTH="1072">
<String CONTENT="1679, était l’allié de Louis XIV. Ayant, dans la guerre de" HEIGHT="56" HPOS="667" ID="string_tl_28" VPOS="3518" WIDTH="1072"/>
</TextLine>
<TextLine BASELINE="664 3612 1737 3612" HEIGHT="52" HPOS="664" ID="tl_29" VPOS="3560" WIDTH="1073">
<String CONTENT="la succession d’Espagne, pris parti pour le duc d’Anjou, il" HEIGHT="52" HPOS="664" ID="string_tl_29" VPOS="3560" WIDTH="1073"/>
</TextLine>
<TextLine BASELINE="664 3655 1739 3655" HEIGHT="51" HPOS="664" ID="tl_30" VPOS="3604" WIDTH="1075">
<String CONTENT="avait été chassé de ses Etats en 1704 et s’était réfugié en" HEIGHT="51" HPOS="664" ID="string_tl_30" VPOS="3604" WIDTH="1075"/>
</TextLine>
<TextLine BASELINE="664 3698 1740 3698" HEIGHT="51" HPOS="664" ID="tl_31" VPOS="3647" WIDTH="1076">
<String CONTENT="France. Il ne rentra dans son électorat qu’en 1714, après la" HEIGHT="51" HPOS="664" ID="string_tl_31" VPOS="3647" WIDTH="1076"/>
</TextLine>
<TextLine BASELINE="666 3742 1309 3742" HEIGHT="48" HPOS="666" ID="tl_32" VPOS="3694" WIDTH="643">
<String CONTENT="paix d’Utrecht, et mourut en 1726.)" HEIGHT="48" HPOS="666" ID="string_tl_32" VPOS="3694" WIDTH="643"/>
</TextLine>
