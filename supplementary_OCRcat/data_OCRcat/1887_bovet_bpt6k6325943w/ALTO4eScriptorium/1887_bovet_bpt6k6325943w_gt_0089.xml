<String CONTENT="L. A. S. à l’électeur de Bavière, son oncle;" HEIGHT="66" HPOS="702" ID="string_tl_17" VPOS="2986" WIDTH="1023"/>
</TextLine>
<TextLine BASELINE="654 3104 1728 3104" HEIGHT="64" HPOS="654" ID="tl_18" VPOS="3040" WIDTH="1074">
