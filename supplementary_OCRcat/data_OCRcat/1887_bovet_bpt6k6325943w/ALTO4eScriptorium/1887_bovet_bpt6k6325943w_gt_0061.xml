<TextLine BASELINE="787 977 3029 977" HEIGHT="63" HPOS="787" ID="tl_2" VPOS="914" WIDTH="2242">
<String CONTENT="est libéralement ouverte aux chercheurs; à M. Jouin, l’auteur du Catalogue des" HEIGHT="63" HPOS="787" ID="string_tl_2" VPOS="914" WIDTH="2242"/>
</TextLine>
<TextLine BASELINE="781 1051 3028 1051" HEIGHT="66" HPOS="781" ID="tl_3" VPOS="985" WIDTH="2247">
<String CONTENT="portraits historiques exposés au Trocadéro; et à M. Henri de L'Isle, dont l’érudi¬" HEIGHT="66" HPOS="781" ID="string_tl_3" VPOS="985" WIDTH="2247"/>
</TextLine>
<TextLine BASELINE="788 1121 3027 1121" HEIGHT="63" HPOS="788" ID="tl_4" VPOS="1058" WIDTH="2239">
<String CONTENT="tion bibliographique est si étendue. C’est à eux que cette seconde édition doit des" HEIGHT="63" HPOS="788" ID="string_tl_4" VPOS="1058" WIDTH="2239"/>
</TextLine>
<TextLine BASELINE="790 1196 3025 1196" HEIGHT="64" HPOS="790" ID="tl_5" VPOS="1132" WIDTH="2235">
<String CONTENT="améliorations sensibles qui la rendront moins indigne des suffrages du public lettré." HEIGHT="64" HPOS="790" ID="string_tl_5" VPOS="1132" WIDTH="2235"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="2387" HPOS="777" ID="r_2_2" VPOS="1204" WIDTH="2256">
<Shape>
<Polygon POINTS="777 1204 3033 1204 3033 3591 777 3591"/>
</Shape>
<TextLine BASELINE="909 1268 3028 1268" HEIGHT="63" HPOS="909" ID="tl_6" VPOS="1205" WIDTH="2119">
<String CONTENT="Maintenant ma tâche est terminée. Je ne puis pas dire, comme le poète antique," HEIGHT="63" HPOS="909" ID="string_tl_6" VPOS="1205" WIDTH="2119"/>
</TextLine>
<TextLine BASELINE="789 1340 3029 1340" HEIGHT="63" HPOS="789" ID="tl_7" VPOS="1277" WIDTH="2240">
<String CONTENT="que l’heure du repos est arrivée : mon âge, les conditions mêmes de ma vie et mes" HEIGHT="63" HPOS="789" ID="string_tl_7" VPOS="1277" WIDTH="2240"/>
</TextLine>
<TextLine BASELINE="788 1411 3029 1411" HEIGHT="62" HPOS="788" ID="tl_8" VPOS="1349" WIDTH="2241">
<String CONTENT="études m’éloignent de la douce oisiveté si chère à Horace. Mais cette œuvre mar¬" HEIGHT="62" HPOS="788" ID="string_tl_8" VPOS="1349" WIDTH="2241"/>
</TextLine>
<TextLine BASELINE="790 1485 3030 1485" HEIGHT="61" HPOS="790" ID="tl_9" VPOS="1424" WIDTH="2240">
<String CONTENT="quera dans ma carrière une étape décisive et peut-être unique, car l’occasion de" HEIGHT="61" HPOS="790" ID="string_tl_9" VPOS="1424" WIDTH="2240"/>
</TextLine>
<TextLine BASELINE="790 1556 3032 1556" HEIGHT="60" HPOS="790" ID="tl_10" VPOS="1496" WIDTH="2242">
<String CONTENT="dresser un autre inventaire de cette nature, dans des conditions aussi exception¬" HEIGHT="60" HPOS="790" ID="string_tl_10" VPOS="1496" WIDTH="2242"/>
</TextLine>
<TextLine BASELINE="790 1631 3030 1631" HEIGHT="63" HPOS="790" ID="tl_11" VPOS="1568" WIDTH="2240">
<String CONTENT="nelles, avec des collaborateurs si unis par l’amitié et par la confraternité des goûts" HEIGHT="63" HPOS="790" ID="string_tl_11" VPOS="1568" WIDTH="2240"/>
</TextLine>
<TextLine BASELINE="786 1705 3030 1705" HEIGHT="62" HPOS="786" ID="tl_12" VPOS="1643" WIDTH="2244">
<String CONTENT="et des aspirations, se représentera-t-elle jamais, dût la nature clémente me garder" HEIGHT="62" HPOS="786" ID="string_tl_12" VPOS="1643" WIDTH="2244"/>
</TextLine>
<TextLine BASELINE="788 1778 3029 1778" HEIGHT="62" HPOS="788" ID="tl_13" VPOS="1716" WIDTH="2241">
<String CONTENT="encore de longs jours? Quoi qu’il arrive, j’ai éprouvé, pendant les quatre années" HEIGHT="62" HPOS="788" ID="string_tl_13" VPOS="1716" WIDTH="2241"/>
</TextLine>
<TextLine BASELINE="787 1851 3027 1851" HEIGHT="62" HPOS="787" ID="tl_14" VPOS="1789" WIDTH="2240">
<String CONTENT="consacrées à cette œuvre, une douce jouissance à vivre dans l’intimité de person¬" HEIGHT="62" HPOS="787" ID="string_tl_14" VPOS="1789" WIDTH="2240"/>
</TextLine>
<TextLine BASELINE="789 1920 3030 1920" HEIGHT="63" HPOS="789" ID="tl_15" VPOS="1857" WIDTH="2241">
<String CONTENT="nages illustres à des titres si divers. J’ai accompli ma tâche con amore, sachant" HEIGHT="63" HPOS="789" ID="string_tl_15" VPOS="1857" WIDTH="2241"/>
</TextLine>
<TextLine BASELINE="788 1992 3030 1992" HEIGHT="63" HPOS="788" ID="tl_16" VPOS="1929" WIDTH="2242">
<String CONTENT="qu’elle n’était pas de celles qui apportent à leur auteur la fortune, les honneurs" HEIGHT="63" HPOS="788" ID="string_tl_16" VPOS="1929" WIDTH="2242"/>
</TextLine>
<TextLine BASELINE="787 2063 3031 2063" HEIGHT="63" HPOS="787" ID="tl_17" VPOS="2000" WIDTH="2244">
<String CONTENT="ou la popularité, mais convaincu qu’elle incombait au fils et à l’héritier de celui" HEIGHT="63" HPOS="787" ID="string_tl_17" VPOS="2000" WIDTH="2244"/>
</TextLine>
<TextLine BASELINE="786 2138 3030 2138" HEIGHT="65" HPOS="786" ID="tl_18" VPOS="2073" WIDTH="2244">
<String CONTENT="qui a fondé la science des autographes. Je me suis souvenu que c’est en France" HEIGHT="65" HPOS="786" ID="string_tl_18" VPOS="2073" WIDTH="2244"/>
</TextLine>
<TextLine BASELINE="785 2211 3032 2211" HEIGHT="65" HPOS="785" ID="tl_19" VPOS="2146" WIDTH="2247">
<String CONTENT="que le goût des autographes a pris naissance et a obtenu son plus complet" HEIGHT="65" HPOS="785" ID="string_tl_19" VPOS="2146" WIDTH="2247"/>
</TextLine>
<TextLine BASELINE="786 2284 3031 2284" HEIGHT="64" HPOS="786" ID="tl_20" VPOS="2220" WIDTH="2245">
<String CONTENT="développement, et j’ai essayé de ne pas me montrer indigne des précieux ensei¬" HEIGHT="64" HPOS="786" ID="string_tl_20" VPOS="2220" WIDTH="2245"/>
</TextLine>
<TextLine BASELINE="785 2361 3032 2361" HEIGHT="67" HPOS="785" ID="tl_21" VPOS="2294" WIDTH="2247">
<String CONTENT="gnements de mon père vénéré et de mes maîtres de l’Ecole des Chartes. Je" HEIGHT="67" HPOS="785" ID="string_tl_21" VPOS="2294" WIDTH="2247"/>
</TextLine>
<TextLine BASELINE="785 2433 3030 2433" HEIGHT="63" HPOS="785" ID="tl_22" VPOS="2370" WIDTH="2245">
<String CONTENT="n’ai qu’une prétention, c’est d’avoir toujours apporté, dans l’exercice de mon" HEIGHT="63" HPOS="785" ID="string_tl_22" VPOS="2370" WIDTH="2245"/>
</TextLine>
<TextLine BASELINE="784 2508 3031 2508" HEIGHT="63" HPOS="784" ID="tl_23" VPOS="2445" WIDTH="2247">
<String CONTENT="métier en général et dans la rédaction de cet inventaire en particulier, tout le" HEIGHT="63" HPOS="784" ID="string_tl_23" VPOS="2445" WIDTH="2247"/>
</TextLine>
<TextLine BASELINE="780 2581 3029 2581" HEIGHT="62" HPOS="780" ID="tl_24" VPOS="2519" WIDTH="2249">
<String CONTENT="zèle, tout l’amour professionnel, toute la conscience dont je suis capable. J’ai" HEIGHT="62" HPOS="780" ID="string_tl_24" VPOS="2519" WIDTH="2249"/>
</TextLine>
<TextLine BASELINE="781 2654 3030 2654" HEIGHT="60" HPOS="781" ID="tl_25" VPOS="2594" WIDTH="2249">
<String CONTENT="été soutenu par cette fortifiante pensée que, quelqu’imparfaite qu’elle fût, mon" HEIGHT="60" HPOS="781" ID="string_tl_25" VPOS="2594" WIDTH="2249"/>
</TextLine>
<TextLine BASELINE="782 2726 3031 2726" HEIGHT="62" HPOS="782" ID="tl_26" VPOS="2664" WIDTH="2249">
<String CONTENT="œuvre serait profitable à mes contemporains et aux générations futures. Ceux-là" HEIGHT="62" HPOS="782" ID="string_tl_26" VPOS="2664" WIDTH="2249"/>
</TextLine>
<TextLine BASELINE="781 2799 3031 2799" HEIGHT="61" HPOS="781" ID="tl_27" VPOS="2738" WIDTH="2250">
<String CONTENT="seuls qui ont l’expérience des études d’histoire et d’érudition comprendront quelle" HEIGHT="61" HPOS="781" ID="string_tl_27" VPOS="2738" WIDTH="2250"/>
</TextLine>
<TextLine BASELINE="782 2869 3031 2869" HEIGHT="60" HPOS="782" ID="tl_28" VPOS="2809" WIDTH="2249">
<String CONTENT="somme de travail, de recherches et de patience a coûté cet inventaire. Ceux-là" HEIGHT="60" HPOS="782" ID="string_tl_28" VPOS="2809" WIDTH="2249"/>
</TextLine>
<TextLine BASELINE="780 2942 3030 2942" HEIGHT="62" HPOS="780" ID="tl_29" VPOS="2880" WIDTH="2250">
<String CONTENT="seuls aussi partageront la satisfaction que j’ai ressentie à redresser des erreurs, à" HEIGHT="62" HPOS="780" ID="string_tl_29" VPOS="2880" WIDTH="2250"/>
</TextLine>
<TextLine BASELINE="779 3020 3030 3020" HEIGHT="68" HPOS="779" ID="tl_30" VPOS="2952" WIDTH="2251">
<String CONTENT="fournir des éléments nouveaux à l’histoire générale ou particulière, à apporter ma" HEIGHT="68" HPOS="779" ID="string_tl_30" VPOS="2952" WIDTH="2251"/>
</TextLine>
<TextLine BASELINE="780 3090 3029 3090" HEIGHT="64" HPOS="780" ID="tl_31" VPOS="3026" WIDTH="2249">
<String CONTENT="pierre à cette œuvre de vérité historique si difficile à réaliser. C’est à cause de" HEIGHT="64" HPOS="780" ID="string_tl_31" VPOS="3026" WIDTH="2249"/>
</TextLine>
<TextLine BASELINE="778 3166 3029 3166" HEIGHT="67" HPOS="778" ID="tl_32" VPOS="3099" WIDTH="2251">
<String CONTENT="cette mine inépuisable de renseignements fournis par les documents analysés, que" HEIGHT="67" HPOS="778" ID="string_tl_32" VPOS="3099" WIDTH="2251"/>
</TextLine>
<TextLine BASELINE="780 3239 3024 3239" HEIGHT="67" HPOS="780" ID="tl_33" VPOS="3172" WIDTH="2244">
<String CONTENT="le catalogue Bovet est assuré d’un succès durable auprès de la postérité savante." HEIGHT="67" HPOS="780" ID="string_tl_33" VPOS="3172" WIDTH="2244"/>
</TextLine>
<TextLine BASELINE="778 3313 3027 3313" HEIGHT="67" HPOS="778" ID="tl_34" VPOS="3246" WIDTH="2249">
<String CONTENT="Que de livres plus brillants et plus remarqués auront été dès longtemps oubliés," HEIGHT="67" HPOS="778" ID="string_tl_34" VPOS="3246" WIDTH="2249"/>
</TextLine>
<TextLine BASELINE="778 3385 3028 3385" HEIGHT="65" HPOS="778" ID="tl_35" VPOS="3320" WIDTH="2250">
<String CONTENT="que celui-ci sera encore consulté avec fruit par les historiens et par les érudits" HEIGHT="65" HPOS="778" ID="string_tl_35" VPOS="3320" WIDTH="2250"/>
</TextLine>
<TextLine BASELINE="780 3459 3028 3459" HEIGHT="68" HPOS="780" ID="tl_36" VPOS="3391" WIDTH="2248">
<String CONTENT="et recherché par les bibliophiles! Cette double pensée du service rendu et de la" HEIGHT="68" HPOS="780" ID="string_tl_36" VPOS="3391" WIDTH="2248"/>
</TextLine>
<TextLine BASELINE="781 3530 3028 3530" HEIGHT="64" HPOS="781" ID="tl_37" VPOS="3466" WIDTH="2247">
<String CONTENT="perpétuité de l’œuvre n’est-elle pas une légitime récompense des efforts de" HEIGHT="64" HPOS="781" ID="string_tl_37" VPOS="3466" WIDTH="2247"/>
</TextLine>
<TextLine BASELINE="781 3590 1907 3590" HEIGHT="58" HPOS="781" ID="tl_38" VPOS="3532" WIDTH="1126">
<String CONTENT="M. Alfred Bovet et de ses collaborateurs?" HEIGHT="58" HPOS="781" ID="string_tl_38" VPOS="3532" WIDTH="1126"/>
</TextLine>
</TextBlock>
