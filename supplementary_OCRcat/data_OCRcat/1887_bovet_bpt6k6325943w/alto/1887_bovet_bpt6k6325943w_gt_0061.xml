                         WIDTH="2241"
                         VPOS="1349"
                         HPOS="788">
                  <String ID="string_tl_8"
                          HEIGHT="62"
                          WIDTH="2241"
                          VPOS="1349"
                          HPOS="788"
                          CONTENT="études m’éloignent de la douce oisiveté si chère à Horace. Mais cette œuvre mar¬"/>
