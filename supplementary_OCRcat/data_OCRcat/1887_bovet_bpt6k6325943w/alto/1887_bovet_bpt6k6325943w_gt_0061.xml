                  <String ID="string_tl_6"
                          HEIGHT="63"
                          WIDTH="2119"
                          VPOS="1205"
                          HPOS="909"
                          CONTENT="Maintenant ma tâche est terminée. Je ne puis pas dire, comme le poète antique,"/>
