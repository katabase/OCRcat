                  <String ID="string_tl_7"
                          HEIGHT="63"
                          WIDTH="2240"
                          VPOS="1277"
                          HPOS="789"
                          CONTENT="que l’heure du repos est arrivée : mon âge, les conditions mêmes de ma vie et mes"/>
