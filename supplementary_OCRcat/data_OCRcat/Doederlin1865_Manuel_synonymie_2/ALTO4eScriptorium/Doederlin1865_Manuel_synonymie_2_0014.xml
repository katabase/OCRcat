<TextLine BASELINE="313 1205 2667 1205" HEIGHT="108" HPOS="313" ID="tl_7" VPOS="1097" WIDTH="2354">
<String CONTENT="2. Rusticus, ἀγροίκος, celui qui habite le village ; agres¬" HEIGHT="108" HPOS="313" ID="string_tl_7" VPOS="1097" WIDTH="2354"/>
</TextLine>
<TextLine BASELINE="217 1323 2665 1323" HEIGHT="108" HPOS="217" ID="tl_8" VPOS="1215" WIDTH="2448">
<String CONTENT="tis, ἄγριος, celui qui a grandi aux champs, en sauvage," HEIGHT="108" HPOS="217" ID="string_tl_8" VPOS="1215" WIDTH="2448"/>
</TextLine>
<TextLine BASELINE="211 1443 2675 1443" HEIGHT="110" HPOS="211" ID="tl_9" VPOS="1333" WIDTH="2464">
<String CONTENT="comme ferus : le terme est cependant plus doux, car ferus" HEIGHT="110" HPOS="211" ID="string_tl_9" VPOS="1333" WIDTH="2464"/>
</TextLine>
<TextLine BASELINE="219 1563 2671 1563" HEIGHT="104" HPOS="219" ID="tl_10" VPOS="1459" WIDTH="2452">
<String CONTENT="exprime directement la sauvagerie comme faisant le fond" HEIGHT="104" HPOS="219" ID="string_tl_10" VPOS="1459" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="219 1691 2675 1691" HEIGHT="106" HPOS="219" ID="tl_11" VPOS="1585" WIDTH="2456">
<String CONTENT="du caractère, et agrestis en rappelle seulement l’idée par" HEIGHT="106" HPOS="219" ID="string_tl_11" VPOS="1585" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="221 1811 1937 1811" HEIGHT="102" HPOS="221" ID="tl_12" VPOS="1709" WIDTH="1716">
<String CONTENT="la désignation du séjour ou de l’origine." HEIGHT="102" HPOS="221" ID="string_tl_12" VPOS="1709" WIDTH="1716"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="1206" HPOS="216" ID="r_3_3" VPOS="1832" WIDTH="2472">
<Shape>
<Polygon POINTS="216 1832 2688 1832 2688 3038 216 3038"/>
</Shape>
<TextLine BASELINE="319 1935 2675 1935" HEIGHT="102" HPOS="319" ID="tl_13" VPOS="1833" WIDTH="2356">
<String CONTENT="3. Transportant ces termes aux qualités de l’esprit," HEIGHT="102" HPOS="319" ID="string_tl_13" VPOS="1833" WIDTH="2356"/>
</TextLine>
<TextLine BASELINE="219 2057 2681 2057" HEIGHT="104" HPOS="219" ID="tl_14" VPOS="1953" WIDTH="2462">
<String CONTENT="rusticus désigne plutôt la grossièreté intellectuelle; agres¬" HEIGHT="104" HPOS="219" ID="string_tl_14" VPOS="1953" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="225 2177 2679 2177" HEIGHT="100" HPOS="225" ID="tl_15" VPOS="2077" WIDTH="2454">
<String CONTENT="tis, la grossièreté morale. Rusticus s’entend, comme" HEIGHT="100" HPOS="225" ID="string_tl_15" VPOS="2077" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="221 2301 2669 2301" HEIGHT="104" HPOS="221" ID="tl_16" VPOS="2197" WIDTH="2448">
<String CONTENT="champêtre, de la timidité et de la simplicité; il se rap¬" HEIGHT="104" HPOS="221" ID="string_tl_16" VPOS="2197" WIDTH="2448"/>
</TextLine>
<TextLine BASELINE="223 2423 2685 2423" HEIGHT="102" HPOS="223" ID="tl_17" VPOS="2321" WIDTH="2462">
<String CONTENT="proche dans le bon sens de l’innocence et dans le mau¬" HEIGHT="102" HPOS="223" ID="string_tl_17" VPOS="2321" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="217 2547 2677 2547" HEIGHT="102" HPOS="217" ID="tl_18" VPOS="2445" WIDTH="2460">
<String CONTENT="vais de la gaucherie ; agrestis marque, comme rustique," HEIGHT="102" HPOS="217" ID="string_tl_18" VPOS="2445" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="223 2667 2683 2667" HEIGHT="100" HPOS="223" ID="tl_19" VPOS="2567" WIDTH="2460">
<String CONTENT="l’effronterie et la bassesse ; il ne se prend jamais en bonne" HEIGHT="100" HPOS="223" ID="string_tl_19" VPOS="2567" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="225 2791 2681 2791" HEIGHT="102" HPOS="225" ID="tl_20" VPOS="2689" WIDTH="2456">
<String CONTENT="part, il confine à la feritas. Le rusticus (opposé urbanus)" HEIGHT="102" HPOS="225" ID="string_tl_20" VPOS="2689" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="225 2915 2687 2915" HEIGHT="104" HPOS="225" ID="tl_21" VPOS="2811" WIDTH="2462">
<String CONTENT="ne blesse que les conventions du savoir-vivre ; Vagrestis" HEIGHT="104" HPOS="225" ID="string_tl_21" VPOS="2811" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="223 3037 2681 3037" HEIGHT="104" HPOS="223" ID="tl_22" VPOS="2933" WIDTH="2458">
<String CONTENT="(opposé humanus) blesse les lois naturelles de la décence." HEIGHT="104" HPOS="223" ID="string_tl_22" VPOS="2933" WIDTH="2458"/>
</TextLine>
</TextBlock>
