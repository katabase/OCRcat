<TextLine BASELINE="329 3159 2687 3159" HEIGHT="104" HPOS="329" ID="tl_23" VPOS="3055" WIDTH="2358">
<String CONTENT="4. Quand Cicéron veut adoucir encore l’idée exprimée" HEIGHT="104" HPOS="329" ID="string_tl_23" VPOS="3055" WIDTH="2358"/>
</TextLine>
<TextLine BASELINE="229 3283 2687 3283" HEIGHT="102" HPOS="229" ID="tl_24" VPOS="3181" WIDTH="2458">
<String CONTENT="par rusticus et prévenir toute équivoque, il emploie de" HEIGHT="102" HPOS="229" ID="string_tl_24" VPOS="3181" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="231 3407 2687 3407" HEIGHT="102" HPOS="231" ID="tl_25" VPOS="3305" WIDTH="2456">
<String CONTENT="préférence rusticanus. Rusticus désigne alors le paysan" HEIGHT="102" HPOS="231" ID="string_tl_25" VPOS="3305" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="229 3529 2689 3529" HEIGHT="100" HPOS="229" ID="tl_26" VPOS="3429" WIDTH="2460">
<String CONTENT="qui naît, vit et meurt au village; rusticanus, le citadin que" HEIGHT="100" HPOS="229" ID="string_tl_26" VPOS="3429" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="233 3651 2689 3651" HEIGHT="104" HPOS="233" ID="tl_27" VPOS="3547" WIDTH="2456">
<String CONTENT="les circonstances y relèguent. On peut ranger parmi ces" HEIGHT="104" HPOS="233" ID="string_tl_27" VPOS="3547" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="235 3777 2693 3777" HEIGHT="104" HPOS="235" ID="tl_28" VPOS="3673" WIDTH="2458">
<String CONTENT="derniers leà provinciaux, municipes, en qualité de rustico¬" HEIGHT="104" HPOS="235" ID="string_tl_28" VPOS="3673" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="231 3875 739 3875" HEIGHT="80" HPOS="231" ID="tl_29" VPOS="3795" WIDTH="508">
<String CONTENT="rum similes." HEIGHT="80" HPOS="231" ID="string_tl_29" VPOS="3795" WIDTH="508"/>
</TextLine>
