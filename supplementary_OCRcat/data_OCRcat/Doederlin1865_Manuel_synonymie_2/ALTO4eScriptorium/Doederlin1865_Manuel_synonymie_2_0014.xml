<String CONTENT="lité quelconque, les champs. Cels. Med. 1. Sanum opor¬" HEIGHT="104" HPOS="217" ID="string_tl_3" VPOS="607" WIDTH="2446"/>
</TextLine>
<TextLine BASELINE="213 829 2663 829" HEIGHT="100" HPOS="213" ID="tl_4" VPOS="729" WIDTH="2450">
<String CONTENT="tet... modo ruri esse, modo in urbe, sæpiusque in agro." HEIGHT="100" HPOS="213" ID="string_tl_4" VPOS="729" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="211 957 2659 957" HEIGHT="104" HPOS="211" ID="tl_5" VPOS="853" WIDTH="2448">
<String CONTENT="Il faut, pour se porter bien, vivre tantôt à la campagne," HEIGHT="104" HPOS="211" ID="string_tl_5" VPOS="853" WIDTH="2448"/>
</TextLine>
<TextLine BASELINE="211 1075 1785 1075" HEIGHT="100" HPOS="211" ID="tl_6" VPOS="975" WIDTH="1574">
<String CONTENT="tantôt à la ville, surtout aux champs." HEIGHT="100" HPOS="211" ID="string_tl_6" VPOS="975" WIDTH="1574"/>
</TextLine>
