<String CONTENT="che le profit. Flor. II, 6. Hannibal quum victoria posset" HEIGHT="107" HPOS="226" ID="string_tl_3" VPOS="440" WIDTH="2448"/>
</TextLine>
<TextLine BASELINE="224 669 2674 669" HEIGHT="107" HPOS="224" ID="tl_4" VPOS="562" WIDTH="2450">
<String CONTENT="uti, frui maluit. Annibal pouvait user de sa victoire, il" HEIGHT="107" HPOS="224" ID="string_tl_4" VPOS="562" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="226 791 2672 791" HEIGHT="107" HPOS="226" ID="tl_5" VPOS="684" WIDTH="2446">
