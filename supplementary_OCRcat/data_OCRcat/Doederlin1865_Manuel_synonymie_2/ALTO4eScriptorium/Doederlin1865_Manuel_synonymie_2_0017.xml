<String CONTENT="Vacare. Otiari. Feriari. Cessare. Nihil agere. Va¬" HEIGHT="90" HPOS="287" ID="string_tl_23" VPOS="3374" WIDTH="2382"/>
</TextLine>
<TextLine BASELINE="195 3607 2669 3607" HEIGHT="111" HPOS="195" ID="tl_24" VPOS="3496" WIDTH="2474">
<String CONTENT="care, avoir son temps libre, par opposition à l'occupatio," HEIGHT="111" HPOS="195" ID="string_tl_24" VPOS="3496" WIDTH="2474"/>
</TextLine>
<TextLine BASELINE="195 3731 2675 3731" HEIGHT="111" HPOS="195" ID="tl_25" VPOS="3620" WIDTH="2480">
<String CONTENT="qui oblige au travail; otiari, n’avoir point d’affaires, par" HEIGHT="111" HPOS="195" ID="string_tl_25" VPOS="3620" WIDTH="2480"/>
</TextLine>
<TextLine BASELINE="193 3855 2675 3855" HEIGHT="111" HPOS="193" ID="tl_26" VPOS="3744" WIDTH="2482">
<String CONTENT="opposition aux negotia, qui font du travail un devoir; fe¬" HEIGHT="111" HPOS="193" ID="string_tl_26" VPOS="3744" WIDTH="2482"/>
</TextLine>
<TextLine BASELINE="197 3977 2676 3977" HEIGHT="109" HPOS="197" ID="tl_27" VPOS="3868" WIDTH="2479">
<String CONTENT="riari, jouir du repos des jours de fête, par opposition à la" HEIGHT="109" HPOS="197" ID="string_tl_27" VPOS="3868" WIDTH="2479"/>
</TextLine>
<TextLine BASELINE="192 4099 2672 4099" HEIGHT="111" HPOS="192" ID="tl_28" VPOS="3988" WIDTH="2480">
<String CONTENT="besogne journalière ; cessare, cesser son travail et se re¬" HEIGHT="111" HPOS="192" ID="string_tl_28" VPOS="3988" WIDTH="2480"/>
</TextLine>
<TextLine BASELINE="194 4221 2664 4221" HEIGHT="109" HPOS="194" ID="tl_29" VPOS="4112" WIDTH="2470">
<String CONTENT="poser, par opposition à la peine qu’on vient de prendre;" HEIGHT="109" HPOS="194" ID="string_tl_29" VPOS="4112" WIDTH="2470"/>
</TextLine>
<TextLine BASELINE="192 4345 2664 4345" HEIGHT="115" HPOS="192" ID="tl_30" VPOS="4230" WIDTH="2472">
