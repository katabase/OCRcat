</TextLine>
<TextLine BASELINE="222 1037 2675 1037" HEIGHT="105" HPOS="222" ID="tl_7" VPOS="932" WIDTH="2453">
<String CONTENT="ducimus, a Deo nobis dari. Les avantages dont nous pro¬" HEIGHT="105" HPOS="222" ID="string_tl_7" VPOS="932" WIDTH="2453"/>
</TextLine>
<TextLine BASELINE="223 1159 2675 1159" HEIGHT="105" HPOS="223" ID="tl_8" VPOS="1054" WIDTH="2452">
<String CONTENT="fitons tous les jours, la lumière dont nous jouissons, l’air" HEIGHT="105" HPOS="223" ID="string_tl_8" VPOS="1054" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="219 1281 2669 1281" HEIGHT="107" HPOS="219" ID="tl_9" VPOS="1174" WIDTH="2450">
<String CONTENT="que nous respirons sont des dons de Dieu. Cic. Cat. ΙII," HEIGHT="107" HPOS="219" ID="string_tl_9" VPOS="1174" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="217 1405 2671 1405" HEIGHT="105" HPOS="217" ID="tl_10" VPOS="1300" WIDTH="2454">
<String CONTENT="2, 5. Quorum opera... assidue utor. Je profite constam¬" HEIGHT="105" HPOS="217" ID="string_tl_10" VPOS="1300" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="221 1525 2665 1525" HEIGHT="105" HPOS="221" ID="tl_11" VPOS="1420" WIDTH="2444">
<String CONTENT="ment de leur activité. Comparez avec Finn. II, 35, 418." HEIGHT="105" HPOS="221" ID="string_tl_11" VPOS="1420" WIDTH="2444"/>
</TextLine>
<TextLine BASELINE="217 1651 2674 1651" HEIGHT="105" HPOS="217" ID="tl_12" VPOS="1546" WIDTH="2457">
<String CONTENT="In ea, quam sæpe usurpabas, tranquillitate degere omnem" HEIGHT="105" HPOS="217" ID="string_tl_12" VPOS="1546" WIDTH="2457"/>
</TextLine>
<TextLine BASELINE="214 1771 2674 1771" HEIGHT="105" HPOS="214" ID="tl_13" VPOS="1666" WIDTH="2460">
<String CONTENT="vitam. Laisser couler sa vie entière dans la tranquillité" HEIGHT="105" HPOS="214" ID="string_tl_13" VPOS="1666" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="216 1897 2668 1897" HEIGHT="105" HPOS="216" ID="tl_14" VPOS="1792" WIDTH="2452">
<String CONTENT="que tu as su trouver en mainte occasion. Cic. Orat. 51," HEIGHT="105" HPOS="216" ID="string_tl_14" VPOS="1792" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="212 2019 2676 2019" HEIGHT="109" HPOS="212" ID="tl_15" VPOS="1910" WIDTH="2464">
<String CONTENT="169. Post inventa conclusio est, qua credo usuros veteres" HEIGHT="109" HPOS="212" ID="string_tl_15" VPOS="1910" WIDTH="2464"/>
</TextLine>
<TextLine BASELINE="206 2145 2670 2145" HEIGHT="111" HPOS="206" ID="tl_16" VPOS="2034" WIDTH="2464">
<String CONTENT="illos fuisse, si jam nota et usurpata res esset. La période" HEIGHT="111" HPOS="206" ID="string_tl_16" VPOS="2034" WIDTH="2464"/>
</TextLine>
<TextLine BASELINE="206 2267 2673 2267" HEIGHT="109" HPOS="206" ID="tl_17" VPOS="2158" WIDTH="2467">
<String CONTENT="oratoire fut inventée plus tard ; je crois que les anciens" HEIGHT="109" HPOS="206" ID="string_tl_17" VPOS="2158" WIDTH="2467"/>
</TextLine>
<TextLine BASELINE="199 2387 2665 2387" HEIGHT="107" HPOS="199" ID="tl_18" VPOS="2280" WIDTH="2466">
