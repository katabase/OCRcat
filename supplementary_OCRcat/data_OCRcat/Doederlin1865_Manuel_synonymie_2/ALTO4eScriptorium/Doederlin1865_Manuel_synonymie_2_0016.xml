<TextLine BASELINE="309 1695 2683 1695" HEIGHT="102" HPOS="309" ID="tl_11" VPOS="1593" WIDTH="2374">
<String CONTENT="Tutus. Securus. Incuriosus. 1. Tutus se rapporte à la" HEIGHT="102" HPOS="309" ID="string_tl_11" VPOS="1593" WIDTH="2374"/>
</TextLine>
<TextLine BASELINE="213 1819 2687 1819" HEIGHT="104" HPOS="213" ID="tl_12" VPOS="1715" WIDTH="2474">
<String CONTENT="réalité de la chose et s’entend de celui qui est en sûreté," HEIGHT="104" HPOS="213" ID="string_tl_12" VPOS="1715" WIDTH="2474"/>
</TextLine>
<TextLine BASELINE="215 1947 2695 1947" HEIGHT="108" HPOS="215" ID="tl_13" VPOS="1839" WIDTH="2480">
<String CONTENT="comme ἀσφαλής; securus se rapporte à la persuasion de" HEIGHT="108" HPOS="215" ID="string_tl_13" VPOS="1839" WIDTH="2480"/>
</TextLine>
<TextLine BASELINE="217 2063 2687 2063" HEIGHT="102" HPOS="217" ID="tl_14" VPOS="1961" WIDTH="2470">
<String CONTENT="l’esprit et s’entend de celui qui se croit en sûreté. Au sens" HEIGHT="102" HPOS="217" ID="string_tl_14" VPOS="1961" WIDTH="2470"/>
</TextLine>
<TextLine BASELINE="217 2185 2697 2185" HEIGHT="104" HPOS="217" ID="tl_15" VPOS="2081" WIDTH="2480">
<String CONTENT="réfléchi, tutus arrive à exprimer l’idée de prévoyance, et" HEIGHT="104" HPOS="217" ID="string_tl_15" VPOS="2081" WIDTH="2480"/>
</TextLine>
<TextLine BASELINE="217 2305 2687 2305" HEIGHT="100" HPOS="217" ID="tl_16" VPOS="2205" WIDTH="2470">
<String CONTENT="securus celle d’imprévoyance par euphémisme. Sen." HEIGHT="100" HPOS="217" ID="string_tl_16" VPOS="2205" WIDTH="2470"/>
</TextLine>
<TextLine BASELINE="219 2427 2693 2427" HEIGHT="100" HPOS="219" ID="tl_17" VPOS="2327" WIDTH="2474">
<String CONTENT="Ep. 97. Tuta scelera esse possunt, secura non possunt." HEIGHT="100" HPOS="219" ID="string_tl_17" VPOS="2327" WIDTH="2474"/>
</TextLine>
<TextLine BASELINE="223 2551 2697 2551" HEIGHT="104" HPOS="223" ID="tl_18" VPOS="2447" WIDTH="2474">
<String CONTENT="Le crime peut être en sûreté, mais il ne possède jamais" HEIGHT="104" HPOS="223" ID="string_tl_18" VPOS="2447" WIDTH="2474"/>
</TextLine>
<TextLine BASELINE="227 2675 2699 2675" HEIGHT="104" HPOS="227" ID="tl_19" VPOS="2571" WIDTH="2472">
<String CONTENT="la sécurité. Cependant,comme il n’existe point de substan¬" HEIGHT="104" HPOS="227" ID="string_tl_19" VPOS="2571" WIDTH="2472"/>
</TextLine>
<TextLine BASELINE="229 2797 2695 2797" HEIGHT="102" HPOS="229" ID="tl_20" VPOS="2695" WIDTH="2466">
<String CONTENT="tif tiré de tutus, securitas se prend aussi par catachrèse" HEIGHT="102" HPOS="229" ID="string_tl_20" VPOS="2695" WIDTH="2466"/>
</TextLine>
<TextLine BASELINE="235 2895 1209 2895" HEIGHT="80" HPOS="235" ID="tl_21" VPOS="2815" WIDTH="974">
<String CONTENT="dans le sens de sûreté." HEIGHT="80" HPOS="235" ID="string_tl_21" VPOS="2815" WIDTH="974"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="956" HPOS="236" ID="r_3_4" VPOS="2936" WIDTH="2470">
<Shape>
<Polygon POINTS="236 2936 2706 2936 2706 3892 236 3892"/>
</Shape>
<TextLine BASELINE="337 3041 2701 3041" HEIGHT="104" HPOS="337" ID="tl_22" VPOS="2937" WIDTH="2364">
<String CONTENT="2. Securus, securitas expriment l’absence d’inquiétude" HEIGHT="104" HPOS="337" ID="string_tl_22" VPOS="2937" WIDTH="2364"/>
</TextLine>
<TextLine BASELINE="237 3157 2691 3157" HEIGHT="96" HPOS="237" ID="tl_23" VPOS="3061" WIDTH="2454">
<String CONTENT="et de soucis comme un état de l’âme, c’est la sécurité," HEIGHT="96" HPOS="237" ID="string_tl_23" VPOS="3061" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="239 3287 2691 3287" HEIGHT="104" HPOS="239" ID="tl_24" VPOS="3183" WIDTH="2452">
<String CONTENT="ἀμέριμνος, par opposition à sollicitus; incuriosus, incuria," HEIGHT="104" HPOS="239" ID="string_tl_24" VPOS="3183" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="243 3407 2699 3407" HEIGHT="102" HPOS="243" ID="tl_25" VPOS="3305" WIDTH="2456">
<String CONTENT="expriment le manque de soin et d’attention, au point de" HEIGHT="102" HPOS="243" ID="string_tl_25" VPOS="3305" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="243 3531 2701 3531" HEIGHT="106" HPOS="243" ID="tl_26" VPOS="3425" WIDTH="2458">
<String CONTENT="vue pratique, comme insouciant, ὀλίγωρος, par opposition" HEIGHT="106" HPOS="243" ID="string_tl_26" VPOS="3425" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="243 3651 2705 3651" HEIGHT="104" HPOS="243" ID="tl_27" VPOS="3547" WIDTH="2462">
<String CONTENT="à cura. Sen. Ep. 100. Fabianus non erat negligens in ora¬" HEIGHT="104" HPOS="243" ID="string_tl_27" VPOS="3547" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="243 3769 2701 3769" HEIGHT="100" HPOS="243" ID="tl_28" VPOS="3669" WIDTH="2458">
<String CONTENT="tione, sed securus. Il y avait dans les discours de Fabianus" HEIGHT="100" HPOS="243" ID="string_tl_28" VPOS="3669" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="245 3891 2497 3891" HEIGHT="100" HPOS="245" ID="tl_29" VPOS="3791" WIDTH="2252">
<String CONTENT="un air je ne dis pas de négligence, mais d’assurance." HEIGHT="100" HPOS="245" ID="string_tl_29" VPOS="3791" WIDTH="2252"/>
</TextLine>
