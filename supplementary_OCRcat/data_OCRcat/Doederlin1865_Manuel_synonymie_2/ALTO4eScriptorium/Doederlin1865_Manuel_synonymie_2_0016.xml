<TextLine BASELINE="201 645 2687 645" HEIGHT="108" HPOS="201" ID="tl_3" VPOS="537" WIDTH="2486">
<String CONTENT="comme σπαργᾶν, σφριγᾶν; tumere, contient l’idée du néant" HEIGHT="108" HPOS="201" ID="string_tl_3" VPOS="537" WIDTH="2486"/>
</TextLine>
<TextLine BASELINE="203 761 2687 761" HEIGHT="100" HPOS="203" ID="tl_4" VPOS="661" WIDTH="2484">
<String CONTENT="et du vide déguisés sous l’enflure, comme οἰδᾶν. On appelle" HEIGHT="100" HPOS="203" ID="string_tl_4" VPOS="661" WIDTH="2484"/>
</TextLine>
<TextLine BASELINE="201 883 2687 883" HEIGHT="102" HPOS="201" ID="tl_5" VPOS="781" WIDTH="2486">
<String CONTENT="les voiles turgida lorsque le vent qui les gonfle est consi¬" HEIGHT="102" HPOS="201" ID="string_tl_5" VPOS="781" WIDTH="2486"/>
</TextLine>
<TextLine BASELINE="201 1003 2691 1003" HEIGHT="100" HPOS="201" ID="tl_6" VPOS="903" WIDTH="2490">
<String CONTENT="déré comme un corps réel, capable en effet de les rem¬" HEIGHT="100" HPOS="201" ID="string_tl_6" VPOS="903" WIDTH="2490"/>
</TextLine>
<TextLine BASELINE="205 1129 2693 1129" HEIGHT="104" HPOS="205" ID="tl_7" VPOS="1025" WIDTH="2488">
<String CONTENT="plir, et tumida, lorsqu’on ne veut voir dans le môme vent" HEIGHT="104" HPOS="205" ID="string_tl_7" VPOS="1025" WIDTH="2488"/>
</TextLine>
<TextLine BASELINE="205 1253 2693 1253" HEIGHT="106" HPOS="205" ID="tl_8" VPOS="1147" WIDTH="2488">
<String CONTENT="que de l’air, un air qui n’est rien et qui paraît seulement" HEIGHT="106" HPOS="205" ID="string_tl_8" VPOS="1147" WIDTH="2488"/>
</TextLine>
<TextLine BASELINE="213 1371 1015 1371" HEIGHT="100" HPOS="213" ID="tl_9" VPOS="1271" WIDTH="802">
<String CONTENT="remplir les voiles." HEIGHT="100" HPOS="213" ID="string_tl_9" VPOS="1271" WIDTH="802"/>
</TextLine>
