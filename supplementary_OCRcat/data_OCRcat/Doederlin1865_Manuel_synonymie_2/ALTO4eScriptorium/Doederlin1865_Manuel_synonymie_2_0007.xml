</TextLine>
<TextLine BASELINE="217 1355 2677 1355" HEIGHT="102" HPOS="217" ID="tl_6" VPOS="1253" WIDTH="2460">
<String CONTENT="ne pose point sur une base solide ; titubare, par rapport" HEIGHT="102" HPOS="217" ID="string_tl_6" VPOS="1253" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="217 1477 2673 1477" HEIGHT="104" HPOS="217" ID="tl_7" VPOS="1373" WIDTH="2456">
