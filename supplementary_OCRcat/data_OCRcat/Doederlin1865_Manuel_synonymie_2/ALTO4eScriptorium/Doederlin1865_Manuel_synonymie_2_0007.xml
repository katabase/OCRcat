</TextLine>
<TextLine BASELINE="221 1603 2681 1603" HEIGHT="106" HPOS="221" ID="tl_8" VPOS="1497" WIDTH="2460">
<String CONTENT="lare, par rapport au haut du corps qui n’a point une atti¬" HEIGHT="106" HPOS="221" ID="string_tl_8" VPOS="1497" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="217 1721 2675 1721" HEIGHT="102" HPOS="217" ID="tl_9" VPOS="1619" WIDTH="2458">
<String CONTENT="tude droite, tranquille, sûre; enfin, nutare, par rapport à" HEIGHT="102" HPOS="217" ID="string_tl_9" VPOS="1619" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="221 1843 2677 1843" HEIGHT="100" HPOS="221" ID="tl_10" VPOS="1743" WIDTH="2456">
<String CONTENT="la tête qui ne se soutient plus. Le titubans menace de s'af¬" HEIGHT="100" HPOS="221" ID="string_tl_10" VPOS="1743" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="219 1967 2671 1967" HEIGHT="102" HPOS="219" ID="tl_11" VPOS="1865" WIDTH="2452">
<String CONTENT="faisser sur lui-même ; le vacillans, de tomber à la ren¬" HEIGHT="102" HPOS="219" ID="string_tl_11" VPOS="1865" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="219 2089 2665 2089" HEIGHT="100" HPOS="219" ID="tl_12" VPOS="1989" WIDTH="2446">
