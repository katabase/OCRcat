<TextLine BASELINE="369 1891 2727 1891" HEIGHT="82" HPOS="369" ID="tl_13" VPOS="1809" WIDTH="2358">
<String CONTENT="Ignavia. Inertia. Segnitia. Desidia. Socordia. Pigri¬" HEIGHT="82" HPOS="369" ID="string_tl_13" VPOS="1809" WIDTH="2358"/>
</TextLine>
<TextLine BASELINE="269 2033 2727 2033" HEIGHT="102" HPOS="269" ID="tl_14" VPOS="1931" WIDTH="2458">
<String CONTENT="tia. 1. Ignavia, opposé à industria, l’amour du désœu¬" HEIGHT="102" HPOS="269" ID="string_tl_14" VPOS="1931" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="267 2155 2731 2155" HEIGHT="100" HPOS="267" ID="tl_15" VPOS="2055" WIDTH="2464">
<String CONTENT="vrement considéré comme une dérogation à la loi du de¬" HEIGHT="100" HPOS="267" ID="string_tl_15" VPOS="2055" WIDTH="2464"/>
</TextLine>
<TextLine BASELINE="265 2281 2725 2281" HEIGHT="102" HPOS="265" ID="tl_16" VPOS="2179" WIDTH="2460">
<String CONTENT="voir, en ce sens qu’on n’est homme, qu’on ne se distingue" HEIGHT="102" HPOS="265" ID="string_tl_16" VPOS="2179" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="263 2403 2725 2403" HEIGHT="102" HPOS="263" ID="tl_17" VPOS="2301" WIDTH="2462">
<String CONTENT="du vulgaire, qu’on ne vaut par soi-même que si on est" HEIGHT="102" HPOS="263" ID="string_tl_17" VPOS="2301" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="259 2527 2723 2527" HEIGHT="106" HPOS="259" ID="tl_18" VPOS="2421" WIDTH="2464">
<String CONTENT="doué du goût de l’action; inertia, le même amour envi¬" HEIGHT="106" HPOS="259" ID="string_tl_18" VPOS="2421" WIDTH="2464"/>
</TextLine>
<TextLine BASELINE="257 2649 2717 2649" HEIGHT="106" HPOS="257" ID="tl_19" VPOS="2543" WIDTH="2460">
<String CONTENT="sagé comme une infraction à la loi du travail, en ce sens" HEIGHT="106" HPOS="257" ID="string_tl_19" VPOS="2543" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="253 2771 2715 2771" HEIGHT="100" HPOS="253" ID="tl_20" VPOS="2671" WIDTH="2462">
<String CONTENT="que l’homme ne devient un membre utile, plus ou moins" HEIGHT="100" HPOS="253" ID="string_tl_20" VPOS="2671" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="253 2899 2715 2899" HEIGHT="106" HPOS="253" ID="tl_21" VPOS="2793" WIDTH="2462">
<String CONTENT="estimable de la société, que par son activité pratique. L’oi¬" HEIGHT="106" HPOS="253" ID="string_tl_21" VPOS="2793" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="253 3017 2713 3017" HEIGHT="100" HPOS="253" ID="tl_22" VPOS="2917" WIDTH="2460">
<String CONTENT="siveté, ignavia, est entée sur le naturel; l’action lui ré¬" HEIGHT="100" HPOS="253" ID="string_tl_22" VPOS="2917" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="249 3141 2709 3141" HEIGHT="102" HPOS="249" ID="tl_23" VPOS="3039" WIDTH="2460">
<String CONTENT="pugne; la fainéantise, inertia, est affaire d’habitude et de" HEIGHT="102" HPOS="249" ID="string_tl_23" VPOS="3039" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="245 3263 2709 3263" HEIGHT="100" HPOS="245" ID="tl_24" VPOS="3163" WIDTH="2464">
<String CONTENT="caractère; elle ne se soucie point de travailler. Un mé¬" HEIGHT="100" HPOS="245" ID="string_tl_24" VPOS="3163" WIDTH="2464"/>
</TextLine>
<TextLine BASELINE="245 3389 2703 3389" HEIGHT="104" HPOS="245" ID="tl_25" VPOS="3285" WIDTH="2458">
<String CONTENT="chant esclave est un fainéant, iners; un noble qui vit sans" HEIGHT="104" HPOS="245" ID="string_tl_25" VPOS="3285" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="237 3511 1537 3511" HEIGHT="102" HPOS="237" ID="tl_26" VPOS="3409" WIDTH="1300">
<String CONTENT="rien faire est un oisif, ignavus." HEIGHT="102" HPOS="237" ID="string_tl_26" VPOS="3409" WIDTH="1300"/>
</TextLine>
