</TextLine>
<TextLine BASELINE="297 685 2759 685" HEIGHT="102" HPOS="297" ID="tl_4" VPOS="583" WIDTH="2462">
<String CONTENT="Wolf. En d’autres termes, l'idoneus est propre à un em¬" HEIGHT="102" HPOS="297" ID="string_tl_4" VPOS="583" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="293 809 2755 809" HEIGHT="106" HPOS="293" ID="tl_5" VPOS="703" WIDTH="2462">
<String CONTENT="ploi par des qualités quelconques et par le concours des" HEIGHT="106" HPOS="293" ID="string_tl_5" VPOS="703" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="293 935 2747 935" HEIGHT="108" HPOS="293" ID="tl_6" VPOS="827" WIDTH="2454">
<String CONTENT="circonstances, ἐπιτήδειος ; l'aptus, par sa valeur personnelle," HEIGHT="108" HPOS="293" ID="string_tl_6" VPOS="827" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="289 1051 2745 1051" HEIGHT="100" HPOS="289" ID="tl_7" VPOS="951" WIDTH="2456">
<String CONTENT="par sa capacité, ίχανός. L’idoneus est inactif par lui-même ;" HEIGHT="100" HPOS="289" ID="string_tl_7" VPOS="951" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="285 1175 2745 1175" HEIGHT="100" HPOS="285" ID="tl_8" VPOS="1075" WIDTH="2460">
<String CONTENT="on se sert de lui pour atteindre un but, parce qu’il est un" HEIGHT="100" HPOS="285" ID="string_tl_8" VPOS="1075" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="285 1299 2743 1299" HEIGHT="106" HPOS="285" ID="tl_9" VPOS="1193" WIDTH="2458">
<String CONTENT="instrument commode; l'aptus entre de lui-même dans" HEIGHT="106" HPOS="285" ID="string_tl_9" VPOS="1193" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="283 1427 2739 1427" HEIGHT="108" HPOS="283" ID="tl_10" VPOS="1319" WIDTH="2456">
<String CONTENT="une affaire, parce qu’il a les dispositions nécessaires pour" HEIGHT="108" HPOS="283" ID="string_tl_10" VPOS="1319" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="275 1523 597 1523" HEIGHT="78" HPOS="275" ID="tl_11" VPOS="1445" WIDTH="322">
