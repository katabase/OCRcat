<TextLine BASELINE="337 3635 2697 3635" HEIGHT="104" HPOS="337" ID="tl_27" VPOS="3531" WIDTH="2360">
<String CONTENT="2. Segnitia, desidia, socordia et pigritia, défauts divers" HEIGHT="104" HPOS="337" ID="string_tl_27" VPOS="3531" WIDTH="2360"/>
</TextLine>
<TextLine BASELINE="239 3759 2699 3759" HEIGHT="104" HPOS="239" ID="tl_28" VPOS="3655" WIDTH="2460">
<String CONTENT="d’un tempérament trop tranquille. La nonchalance, se¬" HEIGHT="104" HPOS="239" ID="string_tl_28" VPOS="3655" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="233 3881 2691 3881" HEIGHT="102" HPOS="233" ID="tl_29" VPOS="3779" WIDTH="2458">
<String CONTENT="gnitia, attend qu’on l’excite, qu’on la contraigne, qu’on" HEIGHT="102" HPOS="233" ID="string_tl_29" VPOS="3779" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="237 4001 2689 4001" HEIGHT="100" HPOS="237" ID="tl_30" VPOS="3901" WIDTH="2452">
<String CONTENT="la prenne corps à corps avant de renoncer au repos; elle" HEIGHT="100" HPOS="237" ID="string_tl_30" VPOS="3901" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="233 4127 2687 4127" HEIGHT="106" HPOS="233" ID="tl_31" VPOS="4021" WIDTH="2454">
<String CONTENT="a pour opposé promptus. L’indifférence, desidia, se croise" HEIGHT="106" HPOS="233" ID="string_tl_31" VPOS="4021" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="233 4243 2677 4243" HEIGHT="102" HPOS="233" ID="tl_32" VPOS="4141" WIDTH="2444">
<String CONTENT="les bras et attend que les choses se fassent d'elles-mêmes." HEIGHT="102" HPOS="233" ID="string_tl_32" VPOS="4141" WIDTH="2444"/>
</TextLine>
<TextLine BASELINE="231 4371 2673 4371" HEIGHT="108" HPOS="231" ID="tl_33" VPOS="4263" WIDTH="2442">
<String CONTENT="L’apathie, socordia, est incapable de prendre à quoi que" HEIGHT="108" HPOS="231" ID="string_tl_33" VPOS="4263" WIDTH="2442"/>
</TextLine>
<TextLine BASELINE="231 4493 2677 4493" HEIGHT="110" HPOS="231" ID="tl_34" VPOS="4383" WIDTH="2446">
<String CONTENT="ce soit un vif intérêt et néglige ses devoirs faute d’y son-" HEIGHT="110" HPOS="231" ID="string_tl_34" VPOS="4383" WIDTH="2446"/>
</TextLine>
