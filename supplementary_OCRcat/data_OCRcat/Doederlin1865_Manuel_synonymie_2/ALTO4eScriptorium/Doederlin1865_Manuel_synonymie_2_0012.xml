<TextLine BASELINE="365 1049 2711 1049" HEIGHT="88" HPOS="365" ID="tl_4" VPOS="961" WIDTH="2346">
<String CONTENT="Quærere. Scrutari. Rimari. Investigare. Indagare." HEIGHT="88" HPOS="365" ID="string_tl_4" VPOS="961" WIDTH="2346"/>
</TextLine>
<TextLine BASELINE="263 1181 2721 1181" HEIGHT="102" HPOS="263" ID="tl_5" VPOS="1079" WIDTH="2458">
<String CONTENT="1. Quærere, chercher, en général, on éprouve le désir ou" HEIGHT="102" HPOS="263" ID="string_tl_5" VPOS="1079" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="257 1303 2719 1303" HEIGHT="102" HPOS="257" ID="tl_6" VPOS="1201" WIDTH="2462">
<String CONTENT="le besoih de trouver ; scrutari, rimari, investigare et inda¬" HEIGHT="102" HPOS="257" ID="string_tl_6" VPOS="1201" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="259 1427 2721 1427" HEIGHT="102" HPOS="259" ID="tl_7" VPOS="1325" WIDTH="2462">
<String CONTENT="gare ajoutent à ce sens une idée accessoire de peine et" HEIGHT="102" HPOS="259" ID="string_tl_7" VPOS="1325" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="261 1529 813 1529" HEIGHT="78" HPOS="261" ID="tl_8" VPOS="1451" WIDTH="552">
<String CONTENT="de difficulté." HEIGHT="78" HPOS="261" ID="string_tl_8" VPOS="1451" WIDTH="552"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="2558" HPOS="246" ID="r_3_3" VPOS="1570" WIDTH="2476">
<Shape>
<Polygon POINTS="246 1570 2722 1570 2722 4128 246 4128"/>
</Shape>
<TextLine BASELINE="359 1667 2717 1667" HEIGHT="96" HPOS="359" ID="tl_9" VPOS="1571" WIDTH="2358">
<String CONTENT="2. Scrutari et rimari, se mettre à la recherche d’un ob¬" HEIGHT="96" HPOS="359" ID="string_tl_9" VPOS="1571" WIDTH="2358"/>
</TextLine>
<TextLine BASELINE="247 1793 2721 1793" HEIGHT="102" HPOS="247" ID="tl_10" VPOS="1691" WIDTH="2474">
<String CONTENT="jet caché : scrutari, en fouillant de tous les côtés, on s’in¬" HEIGHT="102" HPOS="247" ID="string_tl_10" VPOS="1691" WIDTH="2474"/>
</TextLine>
<TextLine BASELINE="263 1917 2721 1917" HEIGHT="102" HPOS="263" ID="tl_11" VPOS="1815" WIDTH="2458">
<String CONTENT="téresse à la découverte, on se passionne ; rimari, en creu¬" HEIGHT="102" HPOS="263" ID="string_tl_11" VPOS="1815" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="261 2039 2717 2039" HEIGHT="100" HPOS="261" ID="tl_12" VPOS="1939" WIDTH="2456">
<String CONTENT="sant pour déterrer, la découverte exige des efforts et de" HEIGHT="100" HPOS="261" ID="string_tl_12" VPOS="1939" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="257 2163 2717 2163" HEIGHT="106" HPOS="257" ID="tl_13" VPOS="2057" WIDTH="2460">
<String CONTENT="la sagacité. Investigare et indagare, se mettre à la recher¬" HEIGHT="106" HPOS="257" ID="string_tl_13" VPOS="2057" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="261 2287 2719 2287" HEIGHT="104" HPOS="261" ID="tl_14" VPOS="2183" WIDTH="2458">
<String CONTENT="che d’un objet éloigné : investigare, à la façon du chas¬" HEIGHT="104" HPOS="261" ID="string_tl_14" VPOS="2183" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="257 2409 2715 2409" HEIGHT="102" HPOS="257" ID="tl_15" VPOS="2307" WIDTH="2458">
<String CONTENT="seur qui suit en connaissance de cause la piste ou la trace" HEIGHT="102" HPOS="257" ID="string_tl_15" VPOS="2307" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="255 2529 2705 2529" HEIGHT="102" HPOS="255" ID="tl_16" VPOS="2427" WIDTH="2450">
<String CONTENT="visible du gibier; indagare, à la façon du limier qui suit" HEIGHT="102" HPOS="255" ID="string_tl_16" VPOS="2427" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="255 2653 2711 2653" HEIGHT="106" HPOS="255" ID="tl_17" VPOS="2547" WIDTH="2456">
<String CONTENT="l’odeur guidé par son instinct. Curt. IX, 40, 14. Famem" HEIGHT="106" HPOS="255" ID="string_tl_17" VPOS="2547" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="259 2777 2713 2777" HEIGHT="102" HPOS="259" ID="tl_18" VPOS="2675" WIDTH="2454">
<String CONTENT="sentire coeperunt, radices palmarum ubique rimantes. Iis" HEIGHT="102" HPOS="259" ID="string_tl_18" VPOS="2675" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="259 2897 2713 2897" HEIGHT="100" HPOS="259" ID="tl_19" VPOS="2797" WIDTH="2454">
<String CONTENT="éprouvèrent les atteintes de la faim et ils cherchaient par¬" HEIGHT="100" HPOS="259" ID="string_tl_19" VPOS="2797" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="257 3023 2715 3023" HEIGHT="102" HPOS="257" ID="tl_20" VPOS="2921" WIDTH="2458">
<String CONTENT="tout, pour les déterrer, des racines de palmiste. Compa¬" HEIGHT="102" HPOS="257" ID="string_tl_20" VPOS="2921" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="261 3145 2709 3145" HEIGHT="102" HPOS="261" ID="tl_21" VPOS="3043" WIDTH="2448">
<String CONTENT="rez avec IX, 9, 5. Scrutati omnia tuguria tandem latentes" HEIGHT="102" HPOS="261" ID="string_tl_21" VPOS="3043" WIDTH="2448"/>
</TextLine>
<TextLine BASELINE="257 3267 2711 3267" HEIGHT="106" HPOS="257" ID="tl_22" VPOS="3161" WIDTH="2454">
<String CONTENT="reperere. À force de fouiller toutes les cabanes, ils fini¬" HEIGHT="106" HPOS="257" ID="string_tl_22" VPOS="3161" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="255 3389 2701 3389" HEIGHT="98" HPOS="255" ID="tl_23" VPOS="3291" WIDTH="2446">
<String CONTENT="rent par les trouver dans leur cachette. Tac. Ann. VI, 3." HEIGHT="98" HPOS="255" ID="string_tl_23" VPOS="3291" WIDTH="2446"/>
</TextLine>
<TextLine BASELINE="263 3493 2711 3493" HEIGHT="80" HPOS="263" ID="tl_24" VPOS="3413" WIDTH="2448">
<String CONTENT="Rimans secreta omnium. Déterrant les secrets de tout le" HEIGHT="80" HPOS="263" ID="string_tl_24" VPOS="3413" WIDTH="2448"/>
</TextLine>
<TextLine BASELINE="255 3637 2707 3637" HEIGHT="102" HPOS="255" ID="tl_25" VPOS="3535" WIDTH="2452">
<String CONTENT="monde. Et XII, 52. Quasi finem principis per Chaldæos" HEIGHT="102" HPOS="255" ID="string_tl_25" VPOS="3535" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="251 3757 2707 3757" HEIGHT="98" HPOS="251" ID="tl_26" VPOS="3659" WIDTH="2456">
<String CONTENT="scrutaretur. Furius Scribonianus est exilé sous prétexte" HEIGHT="98" HPOS="251" ID="string_tl_26" VPOS="3659" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="255 3883 2713 3883" HEIGHT="102" HPOS="255" ID="tl_27" VPOS="3781" WIDTH="2458">
<String CONTENT="qu’il avait eu la curiosité de s’adresser aux Chaldéens pour" HEIGHT="102" HPOS="255" ID="string_tl_27" VPOS="3781" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="257 4007 2707 4007" HEIGHT="106" HPOS="257" ID="tl_28" VPOS="3901" WIDTH="2450">
<String CONTENT="découvrir quand et comment l’empereur mourrait. Il n’y" HEIGHT="106" HPOS="257" ID="string_tl_28" VPOS="3901" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="253 4127 1707 4127" HEIGHT="100" HPOS="253" ID="tl_29" VPOS="4027" WIDTH="1454">
<String CONTENT="avait pas d’obstacles à surmonter." HEIGHT="100" HPOS="253" ID="string_tl_29" VPOS="4027" WIDTH="1454"/>
</TextLine>
