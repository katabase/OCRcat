</TextLine>
<TextLine BASELINE="279 2871 2725 2871" HEIGHT="96" HPOS="279" ID="tl_21" VPOS="2775" WIDTH="2446">
<String CONTENT="insimulare, accuser faussement, sans reculer devant la" HEIGHT="96" HPOS="279" ID="string_tl_21" VPOS="2775" WIDTH="2446"/>
</TextLine>
<TextLine BASELINE="281 3001 2725 3001" HEIGHT="104" HPOS="281" ID="tl_22" VPOS="2897" WIDTH="2444">
