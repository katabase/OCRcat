</TextLine>
<TextLine BASELINE="279 2387 2729 2387" HEIGHT="104" HPOS="279" ID="tl_17" VPOS="2283" WIDTH="2450">
<String CONTENT="réelle par devant la justice ou ailleurs, incriminer; incu¬" HEIGHT="104" HPOS="279" ID="string_tl_17" VPOS="2283" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="275 2509 2731 2509" HEIGHT="102" HPOS="275" ID="tl_18" VPOS="2407" WIDTH="2456">
