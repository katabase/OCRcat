</TextLine>
<TextLine BASELINE="279 2633 2731 2633" HEIGHT="104" HPOS="279" ID="tl_19" VPOS="2529" WIDTH="2452">
<String CONTENT="sation extrajudiciaire ; criminari, accuser avec des sen¬" HEIGHT="104" HPOS="279" ID="string_tl_19" VPOS="2529" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="279 2755 2727 2755" HEIGHT="102" HPOS="279" ID="tl_20" VPOS="2653" WIDTH="2448">
