<TextLine BASELINE="419 3251 2771 3251" HEIGHT="102" HPOS="419" ID="tl_31" VPOS="3149" WIDTH="2352">
<String CONTENT="2. Hostilis et inimicus indiquent une disposition per¬" HEIGHT="102" HPOS="419" ID="string_tl_31" VPOS="3149" WIDTH="2352"/>
</TextLine>
<TextLine BASELINE="317 3375 2773 3375" HEIGHT="104" HPOS="317" ID="tl_32" VPOS="3271" WIDTH="2456">
<String CONTENT="manente, infestus et infensus, un état passager : infestus" HEIGHT="104" HPOS="317" ID="string_tl_32" VPOS="3271" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="319 3495 2771 3495" HEIGHT="102" HPOS="319" ID="tl_33" VPOS="3393" WIDTH="2452">
<String CONTENT="ne suppose qu’une attitude hostile, et peut se dire même" HEIGHT="102" HPOS="319" ID="string_tl_33" VPOS="3393" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="319 3617 2761 3617" HEIGHT="100" HPOS="319" ID="tl_34" VPOS="3517" WIDTH="2442">
<String CONTENT="des objets inanimés qui nous menacent d’un danger;" HEIGHT="100" HPOS="319" ID="string_tl_34" VPOS="3517" WIDTH="2442"/>
</TextLine>
<TextLine BASELINE="321 3741 2773 3741" HEIGHT="102" HPOS="321" ID="tl_35" VPOS="3639" WIDTH="2452">
<String CONTENT="infensus suppose des mouvements passionnés et ne se dit" HEIGHT="102" HPOS="321" ID="string_tl_35" VPOS="3639" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="321 3865 2765 3865" HEIGHT="102" HPOS="321" ID="tl_36" VPOS="3763" WIDTH="2444">
<String CONTENT="que des personnes. TAC. Ann. XV, 28. Non infensum," HEIGHT="102" HPOS="321" ID="string_tl_36" VPOS="3763" WIDTH="2444"/>
</TextLine>
<TextLine BASELINE="317 3963 2767 3963" HEIGHT="106" HPOS="317" ID="tl_37" VPOS="3857" WIDTH="2450">
<String CONTENT="nedum hostili odio Corbulonis nomen habebatur. Le nom" HEIGHT="106" HPOS="317" ID="string_tl_37" VPOS="3857" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="319 4107 2761 4107" HEIGHT="102" HPOS="319" ID="tl_38" VPOS="4005" WIDTH="2442">
<String CONTENT="de Corbulon n’avait jamais excité de ressentiment, loin" HEIGHT="102" HPOS="319" ID="string_tl_38" VPOS="4005" WIDTH="2442"/>
</TextLine>
<TextLine BASELINE="319 4231 2757 4231" HEIGHT="102" HPOS="319" ID="tl_39" VPOS="4129" WIDTH="2438">
<String CONTENT="d’être l’objet d’une haine nationale. SEN. N. Q. III, pr." HEIGHT="102" HPOS="319" ID="string_tl_39" VPOS="4129" WIDTH="2438"/>
</TextLine>
<TextLine BASELINE="313 4349 2761 4349" HEIGHT="100" HPOS="313" ID="tl_40" VPOS="4249" WIDTH="2448">
<String CONTENT="Animus luxuriæ non adversus tantum, sed et infestus." HEIGHT="100" HPOS="313" ID="string_tl_40" VPOS="4249" WIDTH="2448"/>
</TextLine>
<TextLine BASELINE="315 4475 2757 4475" HEIGHT="102" HPOS="315" ID="tl_41" VPOS="4373" WIDTH="2442">
<String CONTENT="Ame non-seulement contraire, mais rebelle aux plaisirs." HEIGHT="102" HPOS="315" ID="string_tl_41" VPOS="4373" WIDTH="2442"/>
</TextLine>
<TextLine BASELINE="313 4599 2761 4599" HEIGHT="120" HPOS="313" ID="tl_42" VPOS="4479" WIDTH="2448">
<String CONTENT="Liv. 11, 20. Tarquinium infesto spiculo petit; Tarquinius" HEIGHT="120" HPOS="313" ID="string_tl_42" VPOS="4479" WIDTH="2448"/>
