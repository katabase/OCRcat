<String CONTENT="ADVERSARIUS. HOSTIS. INIMICUS. 1. Adversarius, terme" HEIGHT="94" HPOS="423" ID="string_tl_18" VPOS="1563" WIDTH="2356"/>
</TextLine>
<TextLine BASELINE="323 1787 2781 1787" HEIGHT="108" HPOS="323" ID="tl_19" VPOS="1679" WIDTH="2458">
<String CONTENT="général pour tout adversaire : à la guerre, dans la politi¬" HEIGHT="108" HPOS="323" ID="string_tl_19" VPOS="1679" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="325 1901 2781 1901" HEIGHT="100" HPOS="325" ID="tl_20" VPOS="1801" WIDTH="2456">
<String CONTENT="que, en justice, comme άντιστάτης; hostis, ennemi à la" HEIGHT="100" HPOS="325" ID="string_tl_20" VPOS="1801" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="321 2033 2775 2033" HEIGHT="104" HPOS="321" ID="tl_21" VPOS="1929" WIDTH="2454">
<String CONTENT="guerre, en campagne, par opposition à pacatus, comme" HEIGHT="104" HPOS="321" ID="string_tl_21" VPOS="1929" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="319 2153 2777 2153" HEIGHT="108" HPOS="319" ID="tl_22" VPOS="2045" WIDTH="2458">
<String CONTENT="πολέμιος; inimicus, ennemi du fond du cœur, par opposi¬" HEIGHT="108" HPOS="319" ID="string_tl_22" VPOS="2045" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="323 2291 2775 2291" HEIGHT="124" HPOS="323" ID="tl_23" VPOS="2167" WIDTH="2452">
<String CONTENT="tion à amicus, comme Ιχόρος. Cic. Man. 10. Pompeius" HEIGHT="124" HPOS="323" ID="string_tl_23" VPOS="2167" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="323 2397 2779 2397" HEIGHT="108" HPOS="323" ID="tl_24" VPOS="2289" WIDTH="2456">
<String CONTENT="sæpius cum hoste conflixit quam quisquam cum inimico" HEIGHT="108" HPOS="323" ID="string_tl_24" VPOS="2289" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="321 2521 2775 2521" HEIGHT="108" HPOS="321" ID="tl_25" VPOS="2413" WIDTH="2454">
<String CONTENT="concertavit. Pompée compte plus de combats contre des" HEIGHT="108" HPOS="321" ID="string_tl_25" VPOS="2413" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="319 2641 2773 2641" HEIGHT="106" HPOS="319" ID="tl_26" VPOS="2535" WIDTH="2454">
<String CONTENT="armées ennemies, que qui que ce soit au monde ne" HEIGHT="106" HPOS="319" ID="string_tl_26" VPOS="2535" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="319 2761 2769 2761" HEIGHT="102" HPOS="319" ID="tl_27" VPOS="2659" WIDTH="2450">
<String CONTENT="compte de luttes contre un ennemi particulier. Liv. XXII," HEIGHT="102" HPOS="319" ID="string_tl_27" VPOS="2659" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="319 2887 2781 2887" HEIGHT="108" HPOS="319" ID="tl_28" VPOS="2779" WIDTH="2462">
<String CONTENT="39. Nescio an infestior hic adversarius, quam ille hostis" HEIGHT="108" HPOS="319" ID="string_tl_28" VPOS="2779" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="321 3007 2775 3007" HEIGHT="104" HPOS="321" ID="tl_29" VPOS="2903" WIDTH="2454">
<String CONTENT="maneat. J’appréhende que ton adversaire ne reste plus" HEIGHT="104" HPOS="321" ID="string_tl_29" VPOS="2903" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="321 3127 1509 3127" HEIGHT="100" HPOS="321" ID="tl_30" VPOS="3027" WIDTH="1188">
<String CONTENT="dangereux que ton ennemi." HEIGHT="100" HPOS="321" ID="string_tl_30" VPOS="3027" WIDTH="1188"/>
</TextLine>
