</TextLine>
<TextLine BASELINE="221 2189 2699 2189" HEIGHT="104" HPOS="221" ID="tl_16" VPOS="2085" WIDTH="2478">
<String CONTENT="médiaire entre la pensée et l’action, qu’elle peut être con¬" HEIGHT="104" HPOS="221" ID="string_tl_16" VPOS="2085" WIDTH="2478"/>
</TextLine>
<TextLine BASELINE="221 2307 2697 2307" HEIGHT="100" HPOS="221" ID="tl_17" VPOS="2207" WIDTH="2476">
<String CONTENT="sidérée comme la servante de la pensée, tout comme le" HEIGHT="100" HPOS="221" ID="string_tl_17" VPOS="2207" WIDTH="2476"/>
</TextLine>
<TextLine BASELINE="221 2435 2697 2435" HEIGHT="108" HPOS="221" ID="tl_18" VPOS="2327" WIDTH="2476">
<String CONTENT="corps est le serviteur de la volonté, réciproquement mens" HEIGHT="108" HPOS="221" ID="string_tl_18" VPOS="2327" WIDTH="2476"/>
</TextLine>
<TextLine BASELINE="223 2555 2695 2555" HEIGHT="110" HPOS="223" ID="tl_19" VPOS="2445" WIDTH="2472">
<String CONTENT="se trouvera avec animus dans le ràpport d’un tout à sa" HEIGHT="110" HPOS="223" ID="string_tl_19" VPOS="2445" WIDTH="2472"/>
</TextLine>
