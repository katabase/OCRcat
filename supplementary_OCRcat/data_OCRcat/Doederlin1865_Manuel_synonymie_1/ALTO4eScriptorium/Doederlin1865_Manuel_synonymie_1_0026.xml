<TextLine BASELINE="395 1451 2753 1451" HEIGHT="108" HPOS="395" ID="tl_9" VPOS="1343" WIDTH="2358">
<String CONTENT="ARCANA. SECRETA. MYSTERIA. Arcana et mysteria, les" HEIGHT="108" HPOS="395" ID="string_tl_9" VPOS="1343" WIDTH="2358"/>
</TextLine>
<TextLine BASELINE="299 1573 2761 1573" HEIGHT="104" HPOS="299" ID="tl_10" VPOS="1469" WIDTH="2462">
<String CONTENT="secrets envisagés par leur côté honorable, ceux qui tirent" HEIGHT="104" HPOS="299" ID="string_tl_10" VPOS="1469" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="295 1693 2757 1693" HEIGHT="100" HPOS="295" ID="tl_11" VPOS="1593" WIDTH="2462">
<String CONTENT="d’eux-mêmes leur raison d’être, qui tiennent à la nature" HEIGHT="100" HPOS="295" ID="string_tl_11" VPOS="1593" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="295 1817 2755 1817" HEIGHT="104" HPOS="295" ID="tl_12" VPOS="1713" WIDTH="2460">
<String CONTENT="des choses et qui méritent à ce titre d’inspirer un saint" HEIGHT="104" HPOS="295" ID="string_tl_12" VPOS="1713" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="293 1941 2757 1941" HEIGHT="104" HPOS="293" ID="tl_13" VPOS="1837" WIDTH="2464">
<String CONTENT="respect; arcana est d’ailleurs un terme populaire pour" HEIGHT="104" HPOS="293" ID="string_tl_13" VPOS="1837" WIDTH="2464"/>
</TextLine>
<TextLine BASELINE="293 2063 2753 2063" HEIGHT="102" HPOS="293" ID="tl_14" VPOS="1961" WIDTH="2460">
<String CONTENT="toute sorte de secrets; mysteria, un terme savant pour les" HEIGHT="102" HPOS="293" ID="string_tl_14" VPOS="1961" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="293 2187 2755 2187" HEIGHT="116" HPOS="293" ID="tl_15" VPOS="2071" WIDTH="2462">
<String CONTENT="secrets religieux comparables aux mystères d'Eleusis ; en¬" HEIGHT="116" HPOS="293" ID="string_tl_15" VPOS="2071" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="293 2301 2749 2301" HEIGHT="100" HPOS="293" ID="tl_16" VPOS="2201" WIDTH="2456">
<String CONTENT="fin, secreta, les secrets, au sens le plus vulgaire, ceux qui" HEIGHT="100" HPOS="293" ID="string_tl_16" VPOS="2201" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="293 2427 2749 2427" HEIGHT="102" HPOS="293" ID="tl_17" VPOS="2325" WIDTH="2456">
<String CONTENT="ont une origine purement humaine, en parlant des choses" HEIGHT="102" HPOS="293" ID="string_tl_17" VPOS="2325" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="293 2553 2753 2553" HEIGHT="108" HPOS="293" ID="tl_18" VPOS="2445" WIDTH="2460">
<String CONTENT="qu’on tient cachées par crainte. TAC. I, 6. Sallustius Cris-" HEIGHT="108" HPOS="293" ID="string_tl_18" VPOS="2445" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="293 2675 2749 2675" HEIGHT="104" HPOS="293" ID="tl_19" VPOS="2571" WIDTH="2456">
<String CONTENT="pus particeps secretorum... monuit Liviam ne arcana do¬" HEIGHT="104" HPOS="293" ID="string_tl_19" VPOS="2571" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="293 2797 2753 2797" HEIGHT="104" HPOS="293" ID="tl_20" VPOS="2693" WIDTH="2460">
<String CONTENT="mus vulgarentur. Sallustius Crispus, pour qui Tibère et" HEIGHT="104" HPOS="293" ID="string_tl_20" VPOS="2693" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="293 2919 2755 2919" HEIGHT="102" HPOS="293" ID="tl_21" VPOS="2817" WIDTH="2462">
<String CONTENT="Livie n’avaient rien de caché, engagea Livie à ne plus livrer" HEIGHT="102" HPOS="293" ID="string_tl_21" VPOS="2817" WIDTH="2462"/>
</TextLine>
<TextLine BASELINE="293 3043 2619 3043" HEIGHT="102" HPOS="293" ID="tl_22" VPOS="2941" WIDTH="2326">
<String CONTENT="au public les augustes secrets de la famille impériale." HEIGHT="102" HPOS="293" ID="string_tl_22" VPOS="2941" WIDTH="2326"/>
</TextLine>
