<String CONTENT="ARCESSERE. ACCIRE. EVOCARE. ACCERSERE. 1. Arcessere" HEIGHT="80" HPOS="389" ID="string_tl_30" VPOS="3927" WIDTH="2350"/>
</TextLine>
<TextLine BASELINE="287 4149 2737 4149" HEIGHT="100" HPOS="287" ID="tl_31" VPOS="4049" WIDTH="2450">
<String CONTENT="et accersere, termes généraux, signifient simplement faire" HEIGHT="100" HPOS="287" ID="string_tl_31" VPOS="4049" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="283 4273 2733 4273" HEIGHT="102" HPOS="283" ID="tl_32" VPOS="4171" WIDTH="2450">
<String CONTENT="venir; accire, inviter, suppose qu’on s’adresse à un égal ;" HEIGHT="102" HPOS="283" ID="string_tl_32" VPOS="4171" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="277 4397 2733 4397" HEIGHT="104" HPOS="277" ID="tl_33" VPOS="4293" WIDTH="2456">
