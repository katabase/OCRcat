<String CONTENT="ARCERE. PROHIBERE. Arcere, repousser et empêcher" HEIGHT="102" HPOS="393" ID="string_tl_23" VPOS="3065" WIDTH="2358"/>
</TextLine>
<TextLine BASELINE="295 3289 2751 3289" HEIGHT="102" HPOS="295" ID="tl_24" VPOS="3187" WIDTH="2456">
<String CONTENT="d’entrer, par opposition à admittere ; prohibere, tenir éloi¬" HEIGHT="102" HPOS="295" ID="string_tl_24" VPOS="3187" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="291 3411 2745 3411" HEIGHT="102" HPOS="291" ID="tl_25" VPOS="3309" WIDTH="2454">
<String CONTENT="gné et empêcher d’approcher, par opposition à adhibere." HEIGHT="102" HPOS="291" ID="string_tl_25" VPOS="3309" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="287 3529 2745 3529" HEIGHT="96" HPOS="287" ID="tl_26" VPOS="3433" WIDTH="2458">
<String CONTENT="L'arcens se tient sur la défensive, comme le resistens, et" HEIGHT="96" HPOS="287" ID="string_tl_26" VPOS="3433" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="289 3659 2745 3659" HEIGHT="102" HPOS="289" ID="tl_27" VPOS="3557" WIDTH="2456">
<String CONTENT="agit par sollicitude pour la personne menacée; le prohi¬" HEIGHT="102" HPOS="289" ID="string_tl_27" VPOS="3557" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="287 3783 2741 3783" HEIGHT="102" HPOS="287" ID="tl_28" VPOS="3681" WIDTH="2454">
<String CONTENT="bens prend l'offensive, comme le propulsans, et agit par" HEIGHT="102" HPOS="287" ID="string_tl_28" VPOS="3681" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="287 3905 1449 3905" HEIGHT="102" HPOS="287" ID="tl_29" VPOS="3803" WIDTH="1162">
<String CONTENT="inimitié contre l’agresseur." HEIGHT="102" HPOS="287" ID="string_tl_29" VPOS="3803" WIDTH="1162"/>
</TextLine>
