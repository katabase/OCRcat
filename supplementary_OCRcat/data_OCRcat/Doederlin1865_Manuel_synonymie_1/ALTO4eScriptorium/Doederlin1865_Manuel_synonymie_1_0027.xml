<String CONTENT="ARDERE. FLAGRARE. Ardere, brûler comme un brasier," HEIGHT="96" HPOS="319" ID="string_tl_21" VPOS="2887" WIDTH="2338"/>
</TextLine>
<TextLine BASELINE="215 3113 2665 3113" HEIGHT="108" HPOS="215" ID="tl_22" VPOS="3005" WIDTH="2450">
<String CONTENT="κἴθειν; flagrare, être en flammes, comme φλέγεσθαι. Au fi¬" HEIGHT="108" HPOS="215" ID="string_tl_22" VPOS="3005" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="215 3233 2665 3233" HEIGHT="104" HPOS="215" ID="tl_23" VPOS="3129" WIDTH="2450">
<String CONTENT="guré, ardere marque une passion qui couve; flagrare, une" HEIGHT="104" HPOS="215" ID="string_tl_23" VPOS="3129" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="221 3353 2665 3353" HEIGHT="102" HPOS="221" ID="tl_24" VPOS="3251" WIDTH="2444">
<String CONTENT="passion qui éclate. Cic. Or. III, 2, 8.. Non vidit Crassus" HEIGHT="102" HPOS="221" ID="string_tl_24" VPOS="3251" WIDTH="2444"/>
</TextLine>
<TextLine BASELINE="221 3477 2661 3477" HEIGHT="102" HPOS="221" ID="tl_25" VPOS="3375" WIDTH="2440">
<String CONTENT="flagrantem bello Italiam, non ardentem invidia senatum." HEIGHT="102" HPOS="221" ID="string_tl_25" VPOS="3375" WIDTH="2440"/>
</TextLine>
<TextLine BASELINE="225 3601 2669 3601" HEIGHT="100" HPOS="225" ID="tl_26" VPOS="3501" WIDTH="2444">
<String CONTENT="Crassus n’a vu ni l’Italie dévorée par les flammes de la" HEIGHT="100" HPOS="225" ID="string_tl_26" VPOS="3501" WIDTH="2444"/>
</TextLine>
<TextLine BASELINE="225 3725 2465 3725" HEIGHT="104" HPOS="225" ID="tl_27" VPOS="3621" WIDTH="2240">
<String CONTENT="guerre, ni le sénat consumé par le feu de la jalousie." HEIGHT="104" HPOS="225" ID="string_tl_27" VPOS="3621" WIDTH="2240"/>
</TextLine>
