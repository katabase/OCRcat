<TextLine BASELINE="197 651 2827 651" HEIGHT="128" HPOS="197" ID="tl_3" VPOS="523" WIDTH="2630">
<String CONTENT="Tu invita mulieres, ego accivero pueros. Chargeons-nous," HEIGHT="128" HPOS="197" ID="string_tl_3" VPOS="523" WIDTH="2630"/>
</TextLine>
<TextLine BASELINE="199 773 2641 773" HEIGHT="100" HPOS="199" ID="tl_4" VPOS="673" WIDTH="2442">
<String CONTENT="toi de prier les femmes, moi d’inviter les jeunes gens." HEIGHT="100" HPOS="199" ID="string_tl_4" VPOS="673" WIDTH="2442"/>
</TextLine>
<TextLine BASELINE="201 899 2645 899" HEIGHT="104" HPOS="201" ID="tl_5" VPOS="795" WIDTH="2444">
<String CONTENT="Comparez avec Dej. 5. Venit vel rogatus ut amicus, vel" HEIGHT="104" HPOS="201" ID="string_tl_5" VPOS="795" WIDTH="2444"/>
</TextLine>
<TextLine BASELINE="201 1023 2655 1023" HEIGHT="108" HPOS="201" ID="tl_6" VPOS="915" WIDTH="2454">
<String CONTENT="arcessitus ut socius, vel evocatus ut qui senatui parere didi¬" HEIGHT="108" HPOS="201" ID="string_tl_6" VPOS="915" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="203 1141 2655 1141" HEIGHT="100" HPOS="203" ID="tl_7" VPOS="1041" WIDTH="2452">
<String CONTENT="cisset. Il s’est présenté ou en ami dont on souhaitait l’ar¬" HEIGHT="100" HPOS="203" ID="string_tl_7" VPOS="1041" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="205 1267 2653 1267" HEIGHT="102" HPOS="205" ID="tl_8" VPOS="1165" WIDTH="2448">
<String CONTENT="rivée, ou en allié qu’on faisait venir, ou en sujet mandé" HEIGHT="102" HPOS="205" ID="string_tl_8" VPOS="1165" WIDTH="2448"/>
</TextLine>
<TextLine BASELINE="203 1391 2659 1391" HEIGHT="104" HPOS="203" ID="tl_9" VPOS="1287" WIDTH="2456">
<String CONTENT="par le sénat et dressé à lui obéir. LIV. X, 19. Collèges auxi¬" HEIGHT="104" HPOS="203" ID="string_tl_9" VPOS="1287" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="205 1507 2653 1507" HEIGHT="98" HPOS="205" ID="tl_10" VPOS="1409" WIDTH="2448">
<String CONTENT="lium quod acciendum ultro fuerit. Le secours de son col¬" HEIGHT="98" HPOS="205" ID="string_tl_10" VPOS="1409" WIDTH="2448"/>
</TextLine>
<TextLine BASELINE="209 1635 2657 1635" HEIGHT="102" HPOS="209" ID="tl_11" VPOS="1533" WIDTH="2448">
<String CONTENT="lègue qu’il aurait dû demander sans façon. Comparez avec" HEIGHT="102" HPOS="209" ID="string_tl_11" VPOS="1533" WIDTH="2448"/>
</TextLine>
<TextLine BASELINE="209 1757 2659 1757" HEIGHT="102" HPOS="209" ID="tl_12" VPOS="1655" WIDTH="2450">
<String CONTENT="XLIV, 31. Evocati litteris imperatoris. Mandés par un" HEIGHT="102" HPOS="209" ID="string_tl_12" VPOS="1655" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="211 1877 2657 1877" HEIGHT="102" HPOS="211" ID="tl_13" VPOS="1775" WIDTH="2446">
<String CONTENT="ordre écrit du général. Et XXXIX, 11. Æbutia accita ad" HEIGHT="102" HPOS="211" ID="string_tl_13" VPOS="1775" WIDTH="2446"/>
</TextLine>
<TextLine BASELINE="211 2003 2661 2003" HEIGHT="102" HPOS="211" ID="tl_14" VPOS="1901" WIDTH="2450">
<String CONTENT="Sulpiciam venit. Ebutia vint trouver Sulpicie comme elle" HEIGHT="102" HPOS="211" ID="string_tl_14" VPOS="1901" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="207 2125 2655 2125" HEIGHT="100" HPOS="207" ID="tl_15" VPOS="2025" WIDTH="2448">
<String CONTENT="l’en avait priée. Mais 12. Ut Hispalam libertinam arcesse¬" HEIGHT="100" HPOS="207" ID="string_tl_15" VPOS="2025" WIDTH="2448"/>
</TextLine>
<TextLine BASELINE="209 2247 2403 2247" HEIGHT="102" HPOS="209" ID="tl_16" VPOS="2145" WIDTH="2194">
