<TextLine BASELINE="221 711 2691 711" HEIGHT="102" HPOS="221" ID="tl_3" VPOS="609" WIDTH="2470">
<String CONTENT="individuellement; penus, par rapport à l’économie do¬" HEIGHT="102" HPOS="221" ID="string_tl_3" VPOS="609" WIDTH="2470"/>
</TextLine>
<TextLine BASELINE="231 833 2695 833" HEIGHT="100" HPOS="231" ID="tl_4" VPOS="733" WIDTH="2464">
<String CONTENT="mestique de toute une famille. Cibus et esca ne se disent" HEIGHT="100" HPOS="231" ID="string_tl_4" VPOS="733" WIDTH="2464"/>
</TextLine>
<TextLine BASELINE="231 961 2683 961" HEIGHT="108" HPOS="231" ID="tl_5" VPOS="853" WIDTH="2452">
<String CONTENT="que des aliments solides par opposition à potio. Cibus," HEIGHT="108" HPOS="231" ID="string_tl_5" VPOS="853" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="231 1077 2683 1077" HEIGHT="102" HPOS="231" ID="tl_6" VPOS="975" WIDTH="2452">
<String CONTENT="aliment fourni par la nature, ressource alimentaire; esca," HEIGHT="102" HPOS="231" ID="string_tl_6" VPOS="975" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="235 1203 2685 1203" HEIGHT="104" HPOS="235" ID="tl_7" VPOS="1099" WIDTH="2450">
<String CONTENT="mets qui a subi une préparation artificielle, plat apprêté." HEIGHT="104" HPOS="235" ID="string_tl_7" VPOS="1099" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="239 1323 2691 1323" HEIGHT="104" HPOS="239" ID="tl_8" VPOS="1219" WIDTH="2452">
<String CONTENT="Cibus est le seul de ces deux mots qui se dise aussi de" HEIGHT="104" HPOS="239" ID="string_tl_8" VPOS="1219" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="237 1447 2691 1447" HEIGHT="104" HPOS="237" ID="tl_9" VPOS="1343" WIDTH="2454">
<String CONTENT="la nourriture des animaux ; esca, le seul qui convienne à" HEIGHT="104" HPOS="237" ID="string_tl_9" VPOS="1343" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="237 1569 2681 1569" HEIGHT="104" HPOS="237" ID="tl_10" VPOS="1465" WIDTH="2444">
<String CONTENT="l’appât qu’on leur prépare et qu’on leur présente. CIC." HEIGHT="104" HPOS="237" ID="string_tl_10" VPOS="1465" WIDTH="2444"/>
</TextLine>
<TextLine BASELINE="237 1691 2687 1691" HEIGHT="102" HPOS="237" ID="tl_11" VPOS="1589" WIDTH="2450">
<String CONTENT="N. D. II, 47. Animalia cibum partim dentibus capessunt." HEIGHT="102" HPOS="237" ID="string_tl_11" VPOS="1589" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="241 1793 2693 1793" HEIGHT="80" HPOS="241" ID="tl_12" VPOS="1713" WIDTH="2452">
<String CONTENT="Un certain nombre d’animaux saisissent leur nourriture" HEIGHT="80" HPOS="241" ID="string_tl_12" VPOS="1713" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="235 1933 2691 1933" HEIGHT="98" HPOS="235" ID="tl_13" VPOS="1835" WIDTH="2456">
<String CONTENT="avec les dents. Comparez avec II, 23. Dii nec escis nec" HEIGHT="98" HPOS="235" ID="string_tl_13" VPOS="1835" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="237 2059 2691 2059" HEIGHT="102" HPOS="237" ID="tl_14" VPOS="1957" WIDTH="2454">
<String CONTENT="potionibus vescuntur. Les dieux se passent pour vivre de" HEIGHT="102" HPOS="237" ID="string_tl_14" VPOS="1957" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="239 2161 1035 2161" HEIGHT="80" HPOS="239" ID="tl_15" VPOS="2081" WIDTH="796">
<String CONTENT="cuisine et de cave." HEIGHT="80" HPOS="239" ID="string_tl_15" VPOS="2081" WIDTH="796"/>
</TextLine>
