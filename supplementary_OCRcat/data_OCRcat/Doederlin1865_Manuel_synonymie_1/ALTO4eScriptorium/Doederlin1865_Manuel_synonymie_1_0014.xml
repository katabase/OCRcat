</TextLine>
<TextLine BASELINE="239 2419 2685 2419" HEIGHT="98" HPOS="239" ID="tl_17" VPOS="2321" WIDTH="2446">
<String CONTENT="edulia, morceaux friands et recherchés. SUET. Tib. 46." HEIGHT="98" HPOS="239" ID="string_tl_17" VPOS="2321" WIDTH="2446"/>
</TextLine>
<TextLine BASELINE="241 2547 2691 2547" HEIGHT="104" HPOS="241" ID="tl_18" VPOS="2443" WIDTH="2450">
<String CONTENT="Comites nunquam salario, cibariis tantum sustentavit. Les" HEIGHT="104" HPOS="241" ID="string_tl_18" VPOS="2443" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="241 2675 2691 2675" HEIGHT="108" HPOS="241" ID="tl_19" VPOS="2567" WIDTH="2450">
<String CONTENT="gens de sa suite ne tiraient de lui que des vivres, jamais de" HEIGHT="108" HPOS="241" ID="string_tl_19" VPOS="2567" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="245 2791 2695 2791" HEIGHT="100" HPOS="245" ID="tl_20" VPOS="2691" WIDTH="2450">
<String CONTENT="salaire. Comparez avec Cal. 40. Pro eduliis certum statum¬" HEIGHT="100" HPOS="245" ID="string_tl_20" VPOS="2691" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="249 2919 2685 2919" HEIGHT="106" HPOS="249" ID="tl_21" VPOS="2813" WIDTH="2436">
<String CONTENT="que exigebatur. Il avait mis un droit sur les comestibles." HEIGHT="106" HPOS="249" ID="string_tl_21" VPOS="2813" WIDTH="2436"/>
</TextLine>
