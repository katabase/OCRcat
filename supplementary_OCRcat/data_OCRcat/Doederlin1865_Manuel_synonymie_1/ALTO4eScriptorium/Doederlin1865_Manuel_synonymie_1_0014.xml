<String CONTENT="3. Cibare, nourrir de sa propre main comme une mère" HEIGHT="100" HPOS="341" ID="string_tl_22" VPOS="2935" WIDTH="2354"/>
</TextLine>
<TextLine BASELINE="245 3159 2695 3159" HEIGHT="100" HPOS="245" ID="tl_23" VPOS="3059" WIDTH="2450">
<String CONTENT="ou une bonne d’enfants; pascere, fournir seulement la" HEIGHT="100" HPOS="245" ID="string_tl_23" VPOS="3059" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="245 3279 2687 3279" HEIGHT="98" HPOS="245" ID="tl_24" VPOS="3181" WIDTH="2442">
