</TextLine>
<TextLine BASELINE="247 3529 2693 3529" HEIGHT="104" HPOS="247" ID="tl_26" VPOS="3425" WIDTH="2446">
<String CONTENT="Sineret se plebeculam pascere. Un dragon auquel il allait" HEIGHT="104" HPOS="247" ID="string_tl_26" VPOS="3425" WIDTH="2446"/>
</TextLine>
<TextLine BASELINE="247 3651 2673 3651" HEIGHT="100" HPOS="247" ID="tl_27" VPOS="3551" WIDTH="2426">
