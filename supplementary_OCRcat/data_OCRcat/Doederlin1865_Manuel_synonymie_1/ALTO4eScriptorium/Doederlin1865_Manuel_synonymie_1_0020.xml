<TextLine BASELINE="335 2509 2695 2509" HEIGHT="106" HPOS="335" ID="tl_18" VPOS="2403" WIDTH="2360">
<String CONTENT="ANIMA. ANIMUS. MENS. 1. Anima, l’âme de la physio¬" HEIGHT="106" HPOS="335" ID="string_tl_18" VPOS="2403" WIDTH="2360"/>
</TextLine>
<TextLine BASELINE="234 2633 2694 2633" HEIGHT="111" HPOS="234" ID="tl_19" VPOS="2522" WIDTH="2460">
<String CONTENT="logie, le principe de la vie animale chez les hommes et les" HEIGHT="111" HPOS="234" ID="string_tl_19" VPOS="2522" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="236 2761 2688 2761" HEIGHT="111" HPOS="236" ID="tl_20" VPOS="2650" WIDTH="2452">
<String CONTENT="bétes, vie qui cesse avec la respiration, ψυχή ; animus," HEIGHT="111" HPOS="236" ID="string_tl_20" VPOS="2650" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="236 2883 2696 2883" HEIGHT="109" HPOS="236" ID="tl_21" VPOS="2774" WIDTH="2460">
<String CONTENT="l’âme de la psychologie et de la morale, le principe de la" HEIGHT="109" HPOS="236" ID="string_tl_21" VPOS="2774" WIDTH="2460"/>
</TextLine>
<TextLine BASELINE="238 3005 2692 3005" HEIGHT="107" HPOS="238" ID="tl_22" VPOS="2898" WIDTH="2454">
<String CONTENT="personnalité qui cesse avec la volonté, θυμός. Au sens my¬" HEIGHT="107" HPOS="238" ID="string_tl_22" VPOS="2898" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="233 3127 2698 3127" HEIGHT="109" HPOS="233" ID="tl_23" VPOS="3018" WIDTH="2465">
<String CONTENT="thologique les âmes des morts s’appellent animas, ce sont" HEIGHT="109" HPOS="233" ID="string_tl_23" VPOS="3018" WIDTH="2465"/>
</TextLine>
<TextLine BASELINE="243 3249 2695 3249" HEIGHT="109" HPOS="243" ID="tl_24" VPOS="3140" WIDTH="2452">
<String CONTENT="des ombres; au sens métaphysique animi, ce sont des" HEIGHT="109" HPOS="243" ID="string_tl_24" VPOS="3140" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="247 3371 2697 3371" HEIGHT="107" HPOS="247" ID="tl_25" VPOS="3264" WIDTH="2450">
<String CONTENT="esprits. L’anima est un des éléments de l’existence du" HEIGHT="107" HPOS="247" ID="string_tl_25" VPOS="3264" WIDTH="2450"/>
</TextLine>
<TextLine BASELINE="245 3495 2699 3495" HEIGHT="109" HPOS="245" ID="tl_26" VPOS="3386" WIDTH="2454">
<String CONTENT="corps ; le même corps n’a pas d’opposé plus tranché qu'a¬" HEIGHT="109" HPOS="245" ID="string_tl_26" VPOS="3386" WIDTH="2454"/>
</TextLine>
<TextLine BASELINE="243 3615 2695 3615" HEIGHT="105" HPOS="243" ID="tl_27" VPOS="3510" WIDTH="2452">
<String CONTENT="nimas. SEN. Ep. 4. Difficile est animum perducere ad con¬" HEIGHT="105" HPOS="243" ID="string_tl_27" VPOS="3510" WIDTH="2452"/>
</TextLine>
<TextLine BASELINE="244 3739 2701 3739" HEIGHT="107" HPOS="244" ID="tl_28" VPOS="3632" WIDTH="2457">
<String CONTENT="temptionem animæ. Il est difficile d’amener l’âme raison¬" HEIGHT="107" HPOS="244" ID="string_tl_28" VPOS="3632" WIDTH="2457"/>
</TextLine>
<TextLine BASELINE="242 3861 2688 3861" HEIGHT="105" HPOS="242" ID="tl_29" VPOS="3756" WIDTH="2446">
<String CONTENT="nable jusqu’au mépris de l’âme sensitive. JUVEN. XV, 148." HEIGHT="105" HPOS="242" ID="string_tl_29" VPOS="3756" WIDTH="2446"/>
</TextLine>
<TextLine BASELINE="248 3983 2692 3983" HEIGHT="107" HPOS="248" ID="tl_30" VPOS="3876" WIDTH="2444">
<String CONTENT="Principio induisit communis conditor illis tantum animas," HEIGHT="107" HPOS="248" ID="string_tl_30" VPOS="3876" WIDTH="2444"/>
</TextLine>
<TextLine BASELINE="254 4103 2698 4103" HEIGHT="105" HPOS="254" ID="tl_31" VPOS="3998" WIDTH="2444">
<String CONTENT="nobis animum quoque. Au commencement le créateur" HEIGHT="105" HPOS="254" ID="string_tl_31" VPOS="3998" WIDTH="2444"/>
</TextLine>
<TextLine BASELINE="256 4231 2692 4231" HEIGHT="109" HPOS="256" ID="tl_32" VPOS="4122" WIDTH="2436">
<String CONTENT="commun n’accorda aux animaux que des âmes sensitives," HEIGHT="109" HPOS="256" ID="string_tl_32" VPOS="4122" WIDTH="2436"/>
</TextLine>
<TextLine BASELINE="251 4334 2220 4334" HEIGHT="90" HPOS="251" ID="tl_33" VPOS="4244" WIDTH="1969">
<String CONTENT="il nous accorda en outre une âme raisonnable." HEIGHT="90" HPOS="251" ID="string_tl_33" VPOS="4244" WIDTH="1969"/>
</TextLine>
