<TextLine BASELINE="327 1149 2677 1149" HEIGHT="94" HPOS="327" ID="tl_7" VPOS="1055" WIDTH="2350">
<String CONTENT="ANIMA. AER. AURA. SPIRITUS. SUBLIME. Anima et aer," HEIGHT="94" HPOS="327" ID="string_tl_7" VPOS="1055" WIDTH="2350"/>
</TextLine>
<TextLine BASELINE="225 1287 2683 1287" HEIGHT="111" HPOS="225" ID="tl_8" VPOS="1176" WIDTH="2458">
<String CONTENT="l’air pris comme élément, ἀήρ : anima, par opposition" HEIGHT="111" HPOS="225" ID="string_tl_8" VPOS="1176" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="228 1407 2681 1407" HEIGHT="109" HPOS="228" ID="tl_9" VPOS="1298" WIDTH="2453">
<String CONTENT="aux trois autres éléments, à terra, mare, ignis; mais aer," HEIGHT="109" HPOS="228" ID="string_tl_9" VPOS="1298" WIDTH="2453"/>
</TextLine>
<TextLine BASELINE="228 1529 2686 1529" HEIGHT="107" HPOS="228" ID="tl_10" VPOS="1422" WIDTH="2458">
<String CONTENT="terme étranger et savant, par opposition à l’air épuré des" HEIGHT="107" HPOS="228" ID="string_tl_10" VPOS="1422" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="232 1655 2688 1655" HEIGHT="123" HPOS="232" ID="tl_11" VPOS="1532" WIDTH="2456">
<String CONTENT="célestes demeures, à æther; aura et spiritus, l'air en" HEIGHT="123" HPOS="232" ID="string_tl_11" VPOS="1532" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="232 1773 2690 1773" HEIGHT="105" HPOS="232" ID="tl_12" VPOS="1668" WIDTH="2458">
<String CONTENT="mouvement : aura, l’air doucement agité, le souffle léger" HEIGHT="105" HPOS="232" ID="string_tl_12" VPOS="1668" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="236 1903 2694 1903" HEIGHT="123" HPOS="236" ID="tl_13" VPOS="1780" WIDTH="2458">
<String CONTENT="qui évente, αὖρα; spiritus, l’air qui se précipite, qui en¬" HEIGHT="123" HPOS="236" ID="string_tl_13" VPOS="1780" WIDTH="2458"/>
</TextLine>
<TextLine BASELINE="233 2019 2692 2019" HEIGHT="105" HPOS="233" ID="tl_14" VPOS="1914" WIDTH="2459">
<String CONTENT="traîne, tout courant d’air analogue à une inspiration ou à" HEIGHT="105" HPOS="233" ID="string_tl_14" VPOS="1914" WIDTH="2459"/>
</TextLine>
<TextLine BASELINE="231 2147 2695 2147" HEIGHT="111" HPOS="231" ID="tl_15" VPOS="2036" WIDTH="2464">
<String CONTENT="une expiration, πνεύμα ; enfin sublime, l’air suspendu au¬" HEIGHT="111" HPOS="231" ID="string_tl_15" VPOS="2036" WIDTH="2464"/>
</TextLine>
<TextLine BASELINE="235 2267 2691 2267" HEIGHT="107" HPOS="235" ID="tl_16" VPOS="2160" WIDTH="2456">
<String CONTENT="dessus de nous : ce dernier marque un simple rapport de" HEIGHT="107" HPOS="235" ID="string_tl_16" VPOS="2160" WIDTH="2456"/>
</TextLine>
<TextLine BASELINE="235 2392 2613 2392" HEIGHT="114" HPOS="235" ID="tl_17" VPOS="2278" WIDTH="2378">
<String CONTENT="lieu par opposition à humus, comme μετάρσιον, μετέωρον." HEIGHT="114" HPOS="235" ID="string_tl_17" VPOS="2278" WIDTH="2378"/>
</TextLine>
