               <TextLine ID="tl_31"
                         BASELINE="1502"
                         HEIGHT="37"
                         WIDTH="1988"
                         VPOS="1465"
                         HPOS="273">
                  <String ID="string_tl_31"
                          HEIGHT="37"
                          WIDTH="1988"
                          VPOS="1465"
                          HPOS="273"
                          CONTENT="des pieds », il s’extasie au spectacle de la mer : « la mer, ma mer chérie, est toujours là, et tu sais si je l’aime. C’est bête comme chou,"/>
               </TextLine>
               <TextLine ID="tl_32"
                         BASELINE="1547"
                         HEIGHT="38"
                         WIDTH="1990"
                         VPOS="1509"
                         HPOS="273">
                  <String ID="string_tl_32"
                          HEIGHT="38"
                          WIDTH="1990"
                          VPOS="1509"
                          HPOS="273"
                          CONTENT="mais je reste là des heures à regarder, comme en extase. Cette immensité me fait faire mille réflexions, ces levers de soleils, ces couchers"/>
               </TextLine>
               <TextLine ID="tl_33"
                         BASELINE="1592"
                         HEIGHT="37"
                         WIDTH="1991"
                         VPOS="1555"
                         HPOS="272">
                  <String ID="string_tl_33"
                          HEIGHT="37"
                          WIDTH="1991"
                          VPOS="1555"
                          HPOS="272"
                          CONTENT="avec ces tons dorés, violets, ces petites barques au loin, ces majestueux et prudhommesques vapeurs qui viennent du Havre et vont soit"/>
               </TextLine>
               <TextLine ID="tl_34"
                         BASELINE="1636"
                         HEIGHT="36"
                         WIDTH="1990"
                         VPOS="1600"
                         HPOS="273">
                  <String ID="string_tl_34"
                          HEIGHT="36"
                          WIDTH="1990"
                          VPOS="1600"
                          HPOS="273"
                          CONTENT="à Southampton, soit dans la mer du Nord, cette mer qui a tous les tons de la palette, du plus clair au plus sombre, avec des diaphanéités"/>
               </TextLine>
               <TextLine ID="tl_35"
                         BASELINE="1681"
                         HEIGHT="37"
                         WIDTH="1989"
                         VPOS="1644"
                         HPOS="272">
                  <String ID="string_tl_35"
                          HEIGHT="37"
                          WIDTH="1989"
                          VPOS="1644"
                          HPOS="272"
                          CONTENT="inouïes, des reflets incomparables, des verts émeraude à faire envie à ta sœur, des expressions vagues de bleu à désespérer Lamartine,"/>
               </TextLine>
               <TextLine ID="tl_36"
                         BASELINE="1727"
                         HEIGHT="38"
                         WIDTH="1988"
                         VPOS="1689"
                         HPOS="273">
                  <String ID="string_tl_36"
                          HEIGHT="38"
                          WIDTH="1988"
                          VPOS="1689"
                          HPOS="273"
                          CONTENT="et des moutonnements sur les rocs à inspirer des vers d’un km. à Hugo ; avec des frissons, des spasmes, des soupirs à étouffer Musset,"/>
               </TextLine>
               <TextLine ID="tl_37"
                         BASELINE="1771"
                         HEIGHT="37"
                         WIDTH="1991"
                         VPOS="1734"
                         HPOS="272">
                  <String ID="string_tl_37"
                          HEIGHT="37"
                          WIDTH="1991"
                          VPOS="1734"
                          HPOS="272"
                          CONTENT="avec des aiguilles, des coins inconnus, des phosphorescences, des reflets métalliques à faire pâmer Monet ou à esquinter la verve voulue"/>
               </TextLine>
               <TextLine ID="tl_38"
                         BASELINE="1817"
                         HEIGHT="37"
                         WIDTH="1990"
                         VPOS="1780"
                         HPOS="273">
                  <String ID="string_tl_38"
                          HEIGHT="37"
                          WIDTH="1990"
                          VPOS="1780"
                          HPOS="273"
                          CONTENT="de Baudelaire, que te dirai-je ! Toute cette poésie, toute cette peinture, toute cette harmonie, ce bruit haletant et cadencé de la vague qui"/>
               </TextLine>
               <TextLine ID="tl_39"
                         BASELINE="1862"
                         HEIGHT="37"
                         WIDTH="1988"
                         VPOS="1825"
                         HPOS="273">
                  <String ID="string_tl_39"
                          HEIGHT="37"
                          WIDTH="1988"
                          VPOS="1825"
                          HPOS="273"
                          CONTENT="meurt et qui contient depuis Beethoven jusqu’à Shakespeare en passant par Michel-Ange, tout ça me transporte, me paralyse, m’énerve,"/>
               </TextLine>
               <TextLine ID="tl_40"
                         BASELINE="1907"
                         HEIGHT="37"
                         WIDTH="1991"
                         VPOS="1870"
                         HPOS="272">
                  <String ID="string_tl_40"
                          HEIGHT="37"
                          WIDTH="1991"
                          VPOS="1870"
                          HPOS="272"
                          CONTENT="me comble, me crible de joies ». Il a gravi une valleuse, mais renoncé à celle de Bénouville avec ses 365 marches et l’a admirée d’en"/>
               </TextLine>
               <TextLine ID="tl_41"
                         BASELINE="1952"
                         HEIGHT="37"
                         WIDTH="1991"
                         VPOS="1915"
                         HPOS="272">
                  <String ID="string_tl_41"
                          HEIGHT="37"
                          WIDTH="1991"
                          VPOS="1915"
                          HPOS="272"
                          CONTENT="bas. Après un déjeuner d’œufs dans une auberge, et l’inévitable trou normand, retour par la route d’Etretat. Le lendemain, excursion"/>
               </TextLine>
               <TextLine ID="tl_42"
                         BASELINE="1996"
                         HEIGHT="36"
                         WIDTH="1989"
                         VPOS="1960"
                         HPOS="273">
                  <String ID="string_tl_42"
                          HEIGHT="36"
                          WIDTH="1989"
                          VPOS="1960"
                          HPOS="273"
                          CONTENT="en carriole à Gonneville : « Nous prenons la route du Havre, avec du lin à droite et du colza à gauche, avec les coquelicots et bluets"/>
               </TextLine>
               <TextLine ID="tl_43"
                         BASELINE="2041"
                         HEIGHT="36"
                         WIDTH="1990"
                         VPOS="2005"
                         HPOS="273">
                  <String ID="string_tl_43"
                          HEIGHT="36"
                          WIDTH="1990"
                          VPOS="2005"
                          HPOS="273"
                          CONTENT="de rigueur, le tout couronné par les 6 fils télégraphiques de la route départementale, sur lesquels les moineaux, chardonnerets, buses"/>
               </TextLine>
               <TextLine ID="tl_44"
                         BASELINE="2086"
                         HEIGHT="36"
                         WIDTH="1988"
                         VPOS="2050"
                         HPOS="273">
                  <String ID="string_tl_44"
                          HEIGHT="36"
                          WIDTH="1988"
                          VPOS="2050"
                          HPOS="273"
                          CONTENT="et autres mouettes viennent de temps en temps danser une saltarelle ou chercher un point d’appui pour voir tomber leur petit caca."/>
               </TextLine>
               <TextLine ID="tl_45"
                         BASELINE="2131"
                         HEIGHT="36"
                         WIDTH="1991"
                         VPOS="2095"
                         HPOS="273">
                  <String ID="string_tl_45"
                          HEIGHT="36"
                          WIDTH="1991"
                          VPOS="2095"
                          HPOS="273"
                          CONTENT="Ça monte tout le temps ; il est vrai que ça descendra en revenant, mais c’est embêtant quand ça monte, d’autant que ce cheval [...] a"/>
               </TextLine>
               <TextLine ID="tl_46"
                         BASELINE="2177"
                         HEIGHT="38"
                         WIDTH="1988"
                         VPOS="2139"
                         HPOS="273">
                  <String ID="string_tl_46"
                          HEIGHT="38"
                          WIDTH="1988"
                          VPOS="2139"
                          HPOS="273"
                          CONTENT="des jambes en coton et ne prend le trot – et quel trot ! – qu’à la condition de lui enfoncer la jambe dans le derrière, pantalon compris,"/>
               </TextLine>
               <TextLine ID="tl_47"
                         BASELINE="2222"
                         HEIGHT="38"
                         WIDTH="1993"
                         VPOS="2184"
                         HPOS="269">
                  <String ID="string_tl_47"
                          HEIGHT="38"
                          WIDTH="1993"
                          VPOS="2184"
                          HPOS="269"
                          CONTENT="jusqu’à mi-cuisse ; et il s’y habitue ! si bien qu’il faut changer de temps en temps de jambe, sans quoi ça ne lui ferait plus aucun effet ;"/>
               </TextLine>
               <TextLine ID="tl_48"
                         BASELINE="2267"
                         HEIGHT="38"
                         WIDTH="1993"
                         VPOS="2229"
                         HPOS="269">
                  <String ID="string_tl_48"
                          HEIGHT="38"
                          WIDTH="1993"
                          VPOS="2229"
                          HPOS="269"
                          CONTENT="je crois même que ça lui ferait plaisir. Nous ne pouvons cependant pas déraciner un chêne séculaire pour le lui introduire où vous"/>
               </TextLine>
               <TextLine ID="tl_49"
                         BASELINE="2312"
                         HEIGHT="37"
                         WIDTH="1990"
                         VPOS="2275"
                         HPOS="273">
                  <String ID="string_tl_49"
                          HEIGHT="37"
                          WIDTH="1990"
                          VPOS="2275"
                          HPOS="273"
                          CONTENT="savez ». Déjeuner à l’auberge d’AUBOURG, « qui est une autorité dans le pays et que touristes et excursionnistes connaissent tous. Ce"/>
               </TextLine>
               <TextLine ID="tl_50"
                         BASELINE="2357"
                         HEIGHT="38"
                         WIDTH="1991"
                         VPOS="2319"
                         HPOS="272">
                  <String ID="string_tl_50"
                          HEIGHT="38"
                          WIDTH="1991"
                          VPOS="2319"
                          HPOS="272"
                          CONTENT="bonhomme, un enfant du pays, a collectionné un vrai musée de vieilles et remarquables faïences, vieux meubles, armes, etc., le tout"/>
               </TextLine>
               <TextLine ID="tl_51"
                         BASELINE="2402"
                         HEIGHT="38"
                         WIDTH="1990"
                         VPOS="2364"
                         HPOS="272">
                  <String ID="string_tl_51"
                          HEIGHT="38"
                          WIDTH="1990"
                          VPOS="2364"
                          HPOS="272"
                          CONTENT="trouvé dans le pays. Il en a pour beaucoup d’argent et, maintes fois, il en a refusé la vente aux Offenbach, Albert Wolff, de Neuville,"/>
               </TextLine>
               <TextLine ID="tl_52"
                         BASELINE="2447"
                         HEIGHT="40"
                         WIDTH="1990"
                         VPOS="2407"
                         HPOS="273">
                  <String ID="string_tl_52"
                          HEIGHT="40"
                          WIDTH="1990"
                          VPOS="2407"
                          HPOS="273"
                          CONTENT="Dupray, Détaillé etc. »... Bain de mer à Bruneval : « Édouard et moi enlevons jusqu’au dernier rempart de la pudeur – j’ai nommé le gilet"/>
               </TextLine>
               <TextLine ID="tl_53"
                         BASELINE="2492"
                         HEIGHT="38"
                         WIDTH="1990"
                         VPOS="2454"
                         HPOS="273">
                  <String ID="string_tl_53"
                          HEIGHT="38"
                          WIDTH="1990"
                          VPOS="2454"
                          HPOS="273"
                          CONTENT="de flanelle – et deux Adonis se jettent à l’eau. Bain exquis, bain rêvé ! Ce que cela me fait de bien, c’est pas dieu possible ! Nous sortons"/>
               </TextLine>
               <TextLine ID="tl_54"
                         BASELINE="2537"
                         HEIGHT="37"
                         WIDTH="1990"
                         VPOS="2500"
                         HPOS="273">
                  <String ID="string_tl_54"
                          HEIGHT="37"
                          WIDTH="1990"
                          VPOS="2500"
                          HPOS="273"
                          CONTENT="de là roses comme les joues de notre Marcel, car la mer aime à voir circuler le sang de ceux qui s’y plongent, et après nous être séchés au"/>
               </TextLine>
               <TextLine ID="tl_55"
                         BASELINE="2582"
                         HEIGHT="37"
                         WIDTH="1990"
                         VPOS="2545"
                         HPOS="273">
                  <String ID="string_tl_55"
                          HEIGHT="37"
                          WIDTH="1990"
                          VPOS="2545"
                          HPOS="273"
                          CONTENT="soleil et avoir enlevé soigneusement les petits grains de galets qui ont la manie de venir se réfugier entre les doigts de pieds du pauvre"/>
               </TextLine>
               <TextLine ID="tl_56"
                         BASELINE="2626"
                         HEIGHT="39"
                         WIDTH="1989"
                         VPOS="2587"
                         HPOS="273">
                  <String ID="string_tl_56"
                          HEIGHT="39"
                          WIDTH="1989"
                          VPOS="2587"
                          HPOS="273"
                          CONTENT="monde, nous avons revêtu depuis la chaussette jusqu’à la cravate »... Retour à Étretat, « cahin-caha ; ce pauvre cheval n’en peut plus »..."/>
               </TextLine>
               <TextLine ID="tl_57"
                         BASELINE="2672"
                         HEIGHT="37"
                         WIDTH="1990"
                         VPOS="2635"
                         HPOS="273">
                  <String ID="string_tl_57"
                          HEIGHT="37"
                          WIDTH="1990"
                          VPOS="2635"
                          HPOS="273"
                          CONTENT="Le lendemain, il ajoute le récit de la halte à Saint-Jouin, à l’hôtel de « la Belle Ernestine [...] plus belle du tout, la belle Ernestine ; elle"/>
               </TextLine>
               <TextLine ID="tl_58"
                         BASELINE="2717"
                         HEIGHT="38"
                         WIDTH="1989"
                         VPOS="2679"
                         HPOS="273">
                  <String ID="string_tl_58"
                          HEIGHT="38"
                          WIDTH="1989"
                          VPOS="2679"
                          HPOS="273"
                          CONTENT="a, comme presque toutes les femmes d’ici, un râtelier » ; digression sur les râteliers... « Cette grosse femme a eu son heure de célébrité."/>
               </TextLine>
               <TextLine ID="tl_59"
                         BASELINE="2762"
                         HEIGHT="38"
                         WIDTH="1989"
                         VPOS="2724"
                         HPOS="272">
                  <String ID="string_tl_59"
                          HEIGHT="38"
                          WIDTH="1989"
                          VPOS="2724"
                          HPOS="272"
                          CONTENT="Alph. Karr, Alex. Dumas père, Monselet, des peintres, des musiciens ont fait, il y a quelque 15 ans, une certaine renommée à sa vertu,"/>
               </TextLine>
               <TextLine ID="tl_60"
                         BASELINE="2807"
                         HEIGHT="37"
                         WIDTH="1989"
                         VPOS="2770"
                         HPOS="273">
                  <String ID="string_tl_60"
                          HEIGHT="37"
                          WIDTH="1989"
                          VPOS="2770"
                          HPOS="273"
                          CONTENT="car elle était sage à son hôtel, car elle cuisinait bien. [...] elle a collectionné une quantité d’albums fort curieux sur lesquels tout-Paris"/>
               </TextLine>
               <TextLine ID="tl_61"
                         BASELINE="2851"
                         HEIGHT="36"
                         WIDTH="1991"
                         VPOS="2815"
                         HPOS="273">
                  <String ID="string_tl_61"
                          HEIGHT="36"
                          WIDTH="1991"
                          VPOS="2815"
                          HPOS="273"
                          CONTENT="moderne a laissé un mot d’esprit ou une bêtise, un dessin, un croquis, un compliment en vers à Ernestine, enfin tout ce qui peut passer"/>
               </TextLine>
               <TextLine ID="tl_62"
                         BASELINE="2896"
                         HEIGHT="39"
                         WIDTH="1989"
                         VPOS="2857"
                         HPOS="273">
                  <String ID="string_tl_62"
                          HEIGHT="39"
                          WIDTH="1989"
                          VPOS="2857"
                          HPOS="273"
                          CONTENT="par la cervelle ! J’ai écrit un fragment de L’Etoile en mettant au-dessous : “Extrait de L’Etoile, le plus grand succès des Bouffes-Parisiens”,"/>
               </TextLine>
               <TextLine ID="tl_63"
                         BASELINE="2942"
                         HEIGHT="38"
                         WIDTH="1012"
                         VPOS="2904"
                         HPOS="273">
                  <String ID="string_tl_63"
                          HEIGHT="38"
                          WIDTH="1012"
                          VPOS="2904"
                          HPOS="273"
                          CONTENT="puis j’ai signé ; les Offenbach seront épatés quand ils verront ça ! »..."/>
               </TextLine>
