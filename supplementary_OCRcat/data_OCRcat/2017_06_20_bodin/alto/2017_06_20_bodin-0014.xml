               <TextLine ID="tl_15"
                         BASELINE="1456"
                         HEIGHT="36"
                         WIDTH="1994"
                         VPOS="1420"
                         HPOS="308">
                  <String ID="string_tl_15"
                          HEIGHT="36"
                          WIDTH="1994"
                          VPOS="1420"
                          HPOS="308"
                          CONTENT="n’en sont pas du tout crevés. Ce point-là bien établi et à seule fin de ne pas me laisser croire que tu es malheureux comme une pierre"/>
               </TextLine>
               <TextLine ID="tl_16"
                         BASELINE="1502"
                         HEIGHT="38"
                         WIDTH="1993"
                         VPOS="1464"
                         HPOS="308">
                  <String ID="string_tl_16"
                          HEIGHT="38"
                          WIDTH="1993"
                          VPOS="1464"
                          HPOS="308"
                          CONTENT="(te rappelles-tu qu’une baderne et ton père, ça fait deux ?) je suis très heureux de te dire que tu sortiras dimanche »... – 11 septembre."/>
               </TextLine>
               <TextLine ID="tl_17"
                         BASELINE="1547"
                         HEIGHT="37"
                         WIDTH="1994"
                         VPOS="1510"
                         HPOS="308">
                  <String ID="string_tl_17"
                          HEIGHT="37"
                          WIDTH="1994"
                          VPOS="1510"
                          HPOS="308"
                          CONTENT="« Continue – ce n’est pas pour moi que tu travailles, ne le sens-tu pas ? – Ce soir lere de Lobengrin ; j’ai tellement échangé de dépêches"/>
               </TextLine>
               <TextLine ID="tl_18"
                         BASELINE="1592"
                         HEIGHT="37"
                         WIDTH="1994"
                         VPOS="1555"
                         HPOS="308">
                  <String ID="string_tl_18"
                          HEIGHT="37"
                          WIDTH="1994"
                          VPOS="1555"
                          HPOS="308"
                          CONTENT="avec LAMOUREUX que je n’y ai plus rien compris [...] et j’ai raté ça, ce qui me contrarie beaucoup pour Lamoureux et pour moi. [...] Je n’ai"/>
               </TextLine>
               <TextLine ID="tl_19"
                         BASELINE="1636"
                         HEIGHT="37"
                         WIDTH="1994"
                         VPOS="1599"
                         HPOS="308">
                  <String ID="string_tl_19"
                          HEIGHT="37"
                          WIDTH="1994"
                          VPOS="1599"
                          HPOS="308"
                          CONTENT="pas besoin de te dire que ça me fait de la peine de voir que tu es aussi faible en allemand ; – ça te servira 100 fois plus que les autres"/>
               </TextLine>
               <TextLine ID="tl_20"
                         BASELINE="1681"
                         HEIGHT="37"
                         WIDTH="1993"
                         VPOS="1644"
                         HPOS="308">
                  <String ID="string_tl_20"
                          HEIGHT="37"
                          WIDTH="1993"
                          VPOS="1644"
                          HPOS="308"
                          CONTENT="affaires, dans la vie — c’est donc idiot »... – [Vers le 8 décembre], « Je suis heureux, très heureux, (ta mère aussi) qu’on te trouve bien ;"/>
               </TextLine>
               <TextLine ID="tl_21"
                         BASELINE="1727"
                         HEIGHT="37"
                         WIDTH="1995"
                         VPOS="1690"
                         HPOS="308">
                  <String ID="string_tl_21"
                          HEIGHT="37"
                          WIDTH="1995"
                          VPOS="1690"
                          HPOS="308"
                          CONTENT="– j’ai assez fulminé, dans ma carrière, pour atteindre ce but, c’est déjà qq chose, et je t’embrasse cordialement. [...] C’est peut-être par"/>
               </TextLine>
               <TextLine ID="tl_22"
                         BASELINE="1772"
                         HEIGHT="38"
                         WIDTH="1994"
                         VPOS="1734"
                         HPOS="308">
                  <String ID="string_tl_22"
                          HEIGHT="38"
                          WIDTH="1994"
                          VPOS="1734"
                          HPOS="308"
                          CONTENT="la douceur et les qualités que j’appellerai de demi-teintes (les meilleures souvent pour se tirer d’affaire et même pour être apprécié) que"/>
               </TextLine>
               <TextLine ID="tl_23"
                         BASELINE="1816"
                         HEIGHT="36"
                         WIDTH="1994"
                         VPOS="1780"
                         HPOS="308">
                  <String ID="string_tl_23"
                          HEIGHT="36"
                          WIDTH="1994"
                          VPOS="1780"
                          HPOS="308"
                          CONTENT="tu réussiras ; mais le diplôme au grand complet est nécessaire aussi, et ne lâche pas Descartes et Malebranche ! Ces 2 vieilles barbes"/>
               </TextLine>
               <TextLine ID="tl_24"
                         BASELINE="1862"
                         HEIGHT="37"
                         WIDTH="1995"
                         VPOS="1825"
                         HPOS="307">
                  <String ID="string_tl_24"
                          HEIGHT="37"
                          WIDTH="1995"
                          VPOS="1825"
                          HPOS="307"
                          CONTENT="veillent sur toi ! »... - [Décembre ?]. « Ne m’attends pas ce mois-ci, Paris est imbécile et plein de boue, on n’y pense qu’à manger, ça me"/>
               </TextLine>
               <TextLine ID="tl_25"
                         BASELINE="1907"
                         HEIGHT="37"
                         WIDTH="1993"
                         VPOS="1870"
                         HPOS="308">
                  <String ID="string_tl_25"
                          HEIGHT="37"
                          WIDTH="1993"
                          VPOS="1870"
                          HPOS="308"
                          CONTENT="dégoûte »... – 20 décembre. Recommandations pour le voyage : « Je ne veux pas que tu voyages la nuit. Comme tu n’es pas très couvert,"/>
               </TextLine>
               <TextLine ID="tl_26"
                         BASELINE="1951"
                         HEIGHT="36"
                         WIDTH="1994"
                         VPOS="1915"
                         HPOS="308">
                  <String ID="string_tl_26"
                          HEIGHT="36"
                          WIDTH="1994"
                          VPOS="1915"
                          HPOS="308"
                          CONTENT="tu prendras des secondes, c’est moins cher qu’une fluxion de poitrine et encore moins dangereux »... – 21 décembre. « M. Bideaux te"/>
               </TextLine>
               <TextLine ID="tl_27"
                         BASELINE="1996"
                         HEIGHT="37"
                         WIDTH="1994"
                         VPOS="1959"
                         HPOS="308">
                  <String ID="string_tl_27"
                          HEIGHT="37"
                          WIDTH="1994"
                          VPOS="1959"
                          HPOS="308"
                          CONTENT="remettra (...) les patards nécessaires pour venir embrasser ta famille et t’en retourner embrasser Descartes, un vilain bougre »... Nouvelles"/>
               </TextLine>
               <TextLine ID="tl_28"
                         BASELINE="2042"
                         HEIGHT="37"
                         WIDTH="633"
                         VPOS="2005"
                         HPOS="308">
                  <String ID="string_tl_28"
                          HEIGHT="37"
                          WIDTH="633"
                          VPOS="2005"
                          HPOS="308"
                          CONTENT="recommandations pour le voyage en train..."/>
               </TextLine>
