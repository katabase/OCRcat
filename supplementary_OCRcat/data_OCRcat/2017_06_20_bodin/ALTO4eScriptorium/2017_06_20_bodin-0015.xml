<String CONTENT="[La Membrolle] Jeudi 27" HEIGHT="32" HPOS="296" ID="string_tl_38" VPOS="1898" WIDTH="395"/>
</TextLine>
<TextLine BASELINE="268 1980 688 1980" HEIGHT="37" HPOS="268" ID="tl_39" VPOS="1943" WIDTH="420">
<String CONTENT="juin [18]89. « Ah ! puisqu’on" HEIGHT="37" HPOS="268" ID="string_tl_39" VPOS="1943" WIDTH="420"/>
</TextLine>
<TextLine BASELINE="272 2025 689 2025" HEIGHT="37" HPOS="272" ID="tl_40" VPOS="1988" WIDTH="417">
<String CONTENT="retape la maison, – non seu¬" HEIGHT="37" HPOS="272" ID="string_tl_40" VPOS="1988" WIDTH="417"/>
</TextLine>
<TextLine BASELINE="271 2067 689 2067" HEIGHT="51" HPOS="271" ID="line_1582973144808_1566" VPOS="2016" WIDTH="418">
<String CONTENT="lement il faut les peindre," HEIGHT="51" HPOS="271" ID="string_line_1582973144808_1566" VPOS="2016" WIDTH="418"/>
</TextLine>
<TextLine BASELINE="270 2113 688 2113" HEIGHT="50" HPOS="270" ID="line_1582973149373_1571" VPOS="2063" WIDTH="418">
<String CONTENT="ces jalousies de malheur," HEIGHT="50" HPOS="270" ID="string_line_1582973149373_1571" VPOS="2063" WIDTH="418"/>
</TextLine>
