</Shape>
<TextLine BASELINE="272 2151 688 2151" HEIGHT="28" HPOS="272" ID="tl_41" VPOS="2123" WIDTH="416">
<String CONTENT="mais il les faut faire RÉPA¬" HEIGHT="28" HPOS="272" ID="string_tl_41" VPOS="2123" WIDTH="416"/>
</TextLine>
<TextLine BASELINE="272 2200 687 2200" HEIGHT="32" HPOS="272" ID="tl_42" VPOS="2168" WIDTH="415">
<String CONTENT="RER [...] Demande à Verd¬" HEIGHT="32" HPOS="272" ID="string_tl_42" VPOS="2168" WIDTH="415"/>
</TextLine>
<TextLine BASELINE="272 2250 688 2250" HEIGHT="37" HPOS="272" ID="tl_43" VPOS="2213" WIDTH="416">
<String CONTENT="hurt si la jeune Gwendoline" HEIGHT="37" HPOS="272" ID="string_tl_43" VPOS="2213" WIDTH="416"/>
</TextLine>
<TextLine BASELINE="273 2290 686 2290" HEIGHT="32" HPOS="273" ID="tl_44" VPOS="2258" WIDTH="413">
<String CONTENT="de Rouen fait son affaire," HEIGHT="32" HPOS="273" ID="string_tl_44" VPOS="2258" WIDTH="413"/>
</TextLine>
<TextLine BASELINE="273 2335 688 2335" HEIGHT="32" HPOS="273" ID="tl_45" VPOS="2303" WIDTH="415">
<String CONTENT="et si Jehin est bel et bien" HEIGHT="32" HPOS="273" ID="string_tl_45" VPOS="2303" WIDTH="415"/>
</TextLine>
<TextLine BASELINE="273 2380 686 2380" HEIGHT="32" HPOS="273" ID="tl_46" VPOS="2348" WIDTH="413">
<String CONTENT="chef d’orch[estre], [...] Hier," HEIGHT="32" HPOS="273" ID="string_tl_46" VPOS="2348" WIDTH="413"/>
</TextLine>
<TextLine BASELINE="269 2430 688 2430" HEIGHT="37" HPOS="269" ID="tl_47" VPOS="2393" WIDTH="419">
<String CONTENT="j’ai travaillé pendant deux" HEIGHT="37" HPOS="269" ID="string_tl_47" VPOS="2393" WIDTH="419"/>
</TextLine>
<TextLine BASELINE="272 2474 688 2474" HEIGHT="36" HPOS="272" ID="tl_48" VPOS="2438" WIDTH="416">
<String CONTENT="heures avec le petit curé de" HEIGHT="36" HPOS="272" ID="string_tl_48" VPOS="2438" WIDTH="416"/>
</TextLine>
<TextLine BASELINE="273 2515 688 2515" HEIGHT="32" HPOS="273" ID="tl_49" VPOS="2483" WIDTH="415">
<String CONTENT="Fondettes ; il m’a chanté" HEIGHT="32" HPOS="273" ID="string_tl_49" VPOS="2483" WIDTH="415"/>
</TextLine>
<TextLine BASELINE="273 2564 688 2564" HEIGHT="36" HPOS="273" ID="tl_50" VPOS="2528" WIDTH="415">
<String CONTENT="des Psaumes superbes, qui" HEIGHT="36" HPOS="273" ID="string_tl_50" VPOS="2528" WIDTH="415"/>
</TextLine>
<TextLine BASELINE="272 2601 688 2601" HEIGHT="29" HPOS="272" ID="tl_51" VPOS="2572" WIDTH="416">
<String CONTENT="feront merveille au 2d acte" HEIGHT="29" HPOS="272" ID="string_tl_51" VPOS="2572" WIDTH="416"/>
</TextLine>
<TextLine BASELINE="273 2646 457 2646" HEIGHT="28" HPOS="273" ID="tl_52" VPOS="2618" WIDTH="184">
<String CONTENT="de Briséïs »..." HEIGHT="28" HPOS="273" ID="string_tl_52" VPOS="2618" WIDTH="184"/>
</TextLine>
