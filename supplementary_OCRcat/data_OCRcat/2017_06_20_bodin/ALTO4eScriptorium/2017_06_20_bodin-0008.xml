<Polygon POINTS="1352 2146 2304 2146 2304 2544 1352 2544"/>
</Shape>
<TextLine BASELINE="1377 2184 2302 2184" HEIGHT="37" HPOS="1377" ID="tl_35" VPOS="2147" WIDTH="925">
<String CONTENT="« Il y a un an que je n’entends plus de musique, souvent malade" HEIGHT="37" HPOS="1377" ID="string_tl_35" VPOS="2147" WIDTH="925"/>
</TextLine>
<TextLine BASELINE="1353 2228 2303 2228" HEIGHT="36" HPOS="1353" ID="tl_36" VPOS="2192" WIDTH="950">
<String CONTENT="et puis détourné par d’incessantes préoccupations nottament" HEIGHT="36" HPOS="1353" ID="string_tl_36" VPOS="2192" WIDTH="950"/>
</TextLine>
<TextLine BASELINE="1353 2274 2303 2274" HEIGHT="37" HPOS="1353" ID="tl_37" VPOS="2237" WIDTH="950">
<String CONTENT="celle de mon mariage dont j’ai l’honneur de vous faire part et" HEIGHT="37" HPOS="1353" ID="string_tl_37" VPOS="2237" WIDTH="950"/>
</TextLine>
<TextLine BASELINE="1353 2319 2302 2319" HEIGHT="37" HPOS="1353" ID="tl_38" VPOS="2282" WIDTH="949">
<String CONTENT="qui vient de s’accomplir sous les plus heureux auspices jeudi" HEIGHT="37" HPOS="1353" ID="string_tl_38" VPOS="2282" WIDTH="949"/>
</TextLine>
<TextLine BASELINE="1353 2364 2302 2364" HEIGHT="37" HPOS="1353" ID="tl_39" VPOS="2327" WIDTH="949">
<String CONTENT="dernier ». Il regrette « de ne pas profiter, dans ce moment de" HEIGHT="37" HPOS="1353" ID="string_tl_39" VPOS="2327" WIDTH="949"/>
</TextLine>
<TextLine BASELINE="1353 2409 2303 2409" HEIGHT="37" HPOS="1353" ID="tl_40" VPOS="2372" WIDTH="950">
<String CONTENT="l’offre obligeante de vos billets. La Musique, j’espère, aura sa" HEIGHT="37" HPOS="1353" ID="string_tl_40" VPOS="2372" WIDTH="950"/>
</TextLine>
<TextLine BASELINE="1353 2454 2302 2454" HEIGHT="37" HPOS="1353" ID="tl_41" VPOS="2417" WIDTH="949">
<String CONTENT="revanche un jour pour me dédommager des privations que je" HEIGHT="37" HPOS="1353" ID="string_tl_41" VPOS="2417" WIDTH="949"/>
</TextLine>
<TextLine BASELINE="1354 2499 2302 2499" HEIGHT="37" HPOS="1354" ID="tl_42" VPOS="2462" WIDTH="948">
<String CONTENT="m’impose aujourd’hui d’entendre et d’admirer celle que vous" HEIGHT="37" HPOS="1354" ID="string_tl_42" VPOS="2462" WIDTH="948"/>
</TextLine>
<TextLine BASELINE="1353 2543 1921 2543" HEIGHT="36" HPOS="1353" ID="tl_43" VPOS="2507" WIDTH="568">
<String CONTENT="faites avec une si haute perfection ! »..." HEIGHT="36" HPOS="1353" ID="string_tl_43" VPOS="2507" WIDTH="568"/>
</TextLine>
