<TextLine BASELINE="296 2236 1181 2236" HEIGHT="36" HPOS="296" ID="tl_35" VPOS="2200" WIDTH="885">
<String CONTENT="Il remercie ses amis de leur « précieuse intervention pour" HEIGHT="36" HPOS="296" ID="string_tl_35" VPOS="2200" WIDTH="885"/>
</TextLine>
<TextLine BASELINE="273 2282 1180 2282" HEIGHT="37" HPOS="273" ID="tl_36" VPOS="2245" WIDTH="907">
<String CONTENT="me délester de ce gros ennui. Tout s’en mêle, l’atmosphère" HEIGHT="37" HPOS="273" ID="string_tl_36" VPOS="2245" WIDTH="907"/>
</TextLine>
<TextLine BASELINE="273 2327 1180 2327" HEIGHT="37" HPOS="273" ID="tl_37" VPOS="2290" WIDTH="907">
<String CONTENT="orageuse, le climat maritime »... Iris CLERT « s’est montrée" HEIGHT="37" HPOS="273" ID="string_tl_37" VPOS="2290" WIDTH="907"/>
</TextLine>
<TextLine BASELINE="273 2371 1180 2371" HEIGHT="36" HPOS="273" ID="tl_38" VPOS="2335" WIDTH="907">
<String CONTENT="entousiaste de mes nombreuses créations de septembre datées" HEIGHT="36" HPOS="273" ID="string_tl_38" VPOS="2335" WIDTH="907"/>
</TextLine>
<TextLine BASELINE="273 2417 1180 2417" HEIGHT="37" HPOS="273" ID="tl_39" VPOS="2380" WIDTH="907">
<String CONTENT="de Vix et elle a semblé agréablement surprise de l’abondance" HEIGHT="37" HPOS="273" ID="string_tl_39" VPOS="2380" WIDTH="907"/>
</TextLine>
<TextLine BASELINE="273 2461 1181 2461" HEIGHT="36" HPOS="273" ID="tl_40" VPOS="2425" WIDTH="908">
<String CONTENT="et la qualité de mon œuvre peinte. J’ai en ce moment en" HEIGHT="36" HPOS="273" ID="string_tl_40" VPOS="2425" WIDTH="908"/>
</TextLine>
<TextLine BASELINE="273 2507 1180 2507" HEIGHT="37" HPOS="273" ID="tl_41" VPOS="2470" WIDTH="907">
<String CONTENT="chantier une grande peinture. Ayant été remplacé auprès de" HEIGHT="37" HPOS="273" ID="string_tl_41" VPOS="2470" WIDTH="907"/>
</TextLine>
<TextLine BASELINE="273 2552 1180 2552" HEIGHT="37" HPOS="273" ID="tl_42" VPOS="2515" WIDTH="907">
<String CONTENT="ma belle-mère j’ai pu peindre en conséquence mais il me faut" HEIGHT="37" HPOS="273" ID="string_tl_42" VPOS="2515" WIDTH="907"/>
</TextLine>
<TextLine BASELINE="273 2596 1180 2596" HEIGHT="36" HPOS="273" ID="tl_43" VPOS="2560" WIDTH="907">
<String CONTENT="me réocuper d’elle, au détriment de mon art »... [C’est en 1960" HEIGHT="36" HPOS="273" ID="string_tl_43" VPOS="2560" WIDTH="907"/>
</TextLine>
<TextLine BASELINE="273 2642 1181 2642" HEIGHT="37" HPOS="273" ID="tl_44" VPOS="2605" WIDTH="908">
<String CONTENT="que Chaissac prend contact avec Iris Clert, qui lui organisera" HEIGHT="37" HPOS="273" ID="string_tl_44" VPOS="2605" WIDTH="908"/>
</TextLine>
<TextLine BASELINE="273 2687 1180 2687" HEIGHT="37" HPOS="273" ID="tl_45" VPOS="2650" WIDTH="907">
<String CONTENT="en octobre 1961 sa première grande exposition personnelle" HEIGHT="37" HPOS="273" ID="string_tl_45" VPOS="2650" WIDTH="907"/>
</TextLine>
<TextLine BASELINE="273 2732 1183 2732" HEIGHT="37" HPOS="273" ID="tl_46" VPOS="2695" WIDTH="910">
<String CONTENT="dans sa galerie du Faubourg Saint-Honoré, intitulée Le monde" HEIGHT="37" HPOS="273" ID="string_tl_46" VPOS="2695" WIDTH="910"/>
</TextLine>
<TextLine BASELINE="268 2777 702 2777" HEIGHT="37" HPOS="268" ID="tl_47" VPOS="2740" WIDTH="434">
