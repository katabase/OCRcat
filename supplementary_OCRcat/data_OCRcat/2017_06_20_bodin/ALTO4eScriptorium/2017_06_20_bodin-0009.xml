<TextLine BASELINE="296 2581 1263 2581" HEIGHT="36" HPOS="296" ID="tl_10" VPOS="2545" WIDTH="967">
<String CONTENT="« Je suis à Paris dans le château du Coq venes me voir s’il vous" HEIGHT="36" HPOS="296" ID="string_tl_10" VPOS="2545" WIDTH="967"/>
</TextLine>
<TextLine BASELINE="273 2627 1263 2627" HEIGHT="37" HPOS="273" ID="tl_11" VPOS="2590" WIDTH="990">
<String CONTENT="est possible, très amable, je retournes dimanche à Lucienne, et" HEIGHT="37" HPOS="273" ID="string_tl_11" VPOS="2590" WIDTH="990"/>
</TextLine>
<TextLine BASELINE="273 2672 1262 2672" HEIGHT="37" HPOS="273" ID="tl_12" VPOS="2635" WIDTH="989">
<String CONTENT="desire avoir vos avis sur l’arrangement de mes tableaux. Je suis" HEIGHT="37" HPOS="273" ID="string_tl_12" VPOS="2635" WIDTH="989"/>
</TextLine>
<TextLine BASELINE="272 2717 1263 2717" HEIGHT="37" HPOS="272" ID="tl_13" VPOS="2680" WIDTH="991">
<String CONTENT="vraiment comme à la campagne, venes je vous prie entre une ou" HEIGHT="37" HPOS="272" ID="string_tl_13" VPOS="2680" WIDTH="991"/>
</TextLine>
<TextLine BASELINE="273 2761 1263 2761" HEIGHT="36" HPOS="273" ID="tl_14" VPOS="2725" WIDTH="990">
<String CONTENT="2 ou 3 h, car le soir vous ne pouries pas voir mon salon &amp;c et la" HEIGHT="36" HPOS="273" ID="string_tl_14" VPOS="2725" WIDTH="990"/>
</TextLine>
<TextLine BASELINE="272 2807 1263 2807" HEIGHT="38" HPOS="272" ID="tl_15" VPOS="2769" WIDTH="991">
<String CONTENT="vue de tant de jardins, c’est une vraie feerie. Je n’ose esperer votre" HEIGHT="38" HPOS="272" ID="string_tl_15" VPOS="2769" WIDTH="991"/>
</TextLine>
<TextLine BASELINE="273 2852 1263 2852" HEIGHT="37" HPOS="273" ID="tl_16" VPOS="2815" WIDTH="990">
<String CONTENT="chere Julliete, c’est si loin ! A mon retour je veux que vous venies" HEIGHT="37" HPOS="273" ID="string_tl_16" VPOS="2815" WIDTH="990"/>
</TextLine>
<TextLine BASELINE="273 2896 1263 2896" HEIGHT="36" HPOS="273" ID="tl_17" VPOS="2860" WIDTH="990">
<String CONTENT="ensemble planter la crémayère cela portera bonheur à moi, et à mon" HEIGHT="36" HPOS="273" ID="string_tl_17" VPOS="2860" WIDTH="990"/>
</TextLine>
<TextLine BASELINE="273 2932 1124 2932" HEIGHT="27" HPOS="273" ID="tl_18" VPOS="2905" WIDTH="851">
<String CONTENT="chateau »... Elle donne son adresse « rue St Lazare n° 75 »." HEIGHT="27" HPOS="273" ID="string_tl_18" VPOS="2905" WIDTH="851"/>
</TextLine>
