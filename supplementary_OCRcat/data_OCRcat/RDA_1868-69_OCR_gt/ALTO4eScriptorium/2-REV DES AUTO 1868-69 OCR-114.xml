</Shape>
<TextLine BASELINE="289 554 1703 554" HEIGHT="58" HPOS="289" ID="tl_6" VPOS="496" WIDTH="1414">
<String CONTENT="Parmi les autographes les plus rares et les plus précieux des mem¬" HEIGHT="58" HPOS="289" ID="string_tl_6" VPOS="496" WIDTH="1414"/>
</TextLine>
<TextLine BASELINE="236 598 1703 598" HEIGHT="52" HPOS="236" ID="tl_7" VPOS="546" WIDTH="1467">
<String CONTENT="bres de l’Académie française, si recherchés aujourd’hui, nous citerons" HEIGHT="52" HPOS="236" ID="string_tl_7" VPOS="546" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="235 658 1699 658" HEIGHT="62" HPOS="235" ID="tl_8" VPOS="596" WIDTH="1464">
<String CONTENT="ceux de Montesquieu, du président Hénault et de l’abbé de Louvois." HEIGHT="62" HPOS="235" ID="string_tl_8" VPOS="596" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="235 698 1703 698" HEIGHT="52" HPOS="235" ID="tl_9" VPOS="646" WIDTH="1468">
<String CONTENT="Le catalogue fournit aussi des pièces rares d’artistes, telles que celles" HEIGHT="52" HPOS="235" ID="string_tl_9" VPOS="646" WIDTH="1468"/>
</TextLine>
<TextLine BASELINE="233 748 1708 748" HEIGHT="53" HPOS="233" ID="tl_10" VPOS="695" WIDTH="1475">
<String CONTENT="de Philibert Delorme, le grand architecte ; J.-J. de Boissieu, le chef" HEIGHT="53" HPOS="233" ID="string_tl_10" VPOS="695" WIDTH="1475"/>
</TextLine>
<TextLine BASELINE="233 799 1704 799" HEIGHT="53" HPOS="233" ID="tl_11" VPOS="746" WIDTH="1471">
<String CONTENT="des acqua-fortistes français ; Borromini et les deux Gennari, célèbres" HEIGHT="53" HPOS="233" ID="string_tl_11" VPOS="746" WIDTH="1471"/>
</TextLine>
<TextLine BASELINE="235 849 1158 849" HEIGHT="51" HPOS="235" ID="tl_12" VPOS="798" WIDTH="923">
<String CONTENT="peintres italiens ; Prud’hon, Meissonier, etc." HEIGHT="51" HPOS="235" ID="string_tl_12" VPOS="798" WIDTH="923"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="506" HPOS="232" ID="r_1_3" VPOS="852" WIDTH="1475">
<Shape>
<Polygon POINTS="232 852 1707 852 1707 1358 232 1358"/>
</Shape>
<TextLine BASELINE="285 904 1698 904" HEIGHT="51" HPOS="285" ID="tl_13" VPOS="853" WIDTH="1413">
<String CONTENT="La collection est particulièrement riche eu compositeurs de musique." HEIGHT="51" HPOS="285" ID="string_tl_13" VPOS="853" WIDTH="1413"/>
</TextLine>
<TextLine BASELINE="233 953 1701 953" HEIGHT="49" HPOS="233" ID="tl_14" VPOS="904" WIDTH="1468">
<String CONTENT="Citons dans cette série de lettres intéressantes, celles de Gluck, pièce" HEIGHT="49" HPOS="233" ID="string_tl_14" VPOS="904" WIDTH="1468"/>
</TextLine>
<TextLine BASELINE="237 1006 1697 1006" HEIGHT="53" HPOS="237" ID="tl_15" VPOS="953" WIDTH="1460">
<String CONTENT="magnifique et d’une rareté extrême ; de Gossec et Grétry, de Nicolo," HEIGHT="53" HPOS="237" ID="string_tl_15" VPOS="953" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="237 1055 1698 1055" HEIGHT="51" HPOS="237" ID="tl_16" VPOS="1004" WIDTH="1461">
<String CONTENT="Bellini, Donizetti, Paisiello, Zingarelli et Rossini ; d’Hérold, de Meyer¬" HEIGHT="51" HPOS="237" ID="string_tl_16" VPOS="1004" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="237 1105 1700 1105" HEIGHT="51" HPOS="237" ID="tl_17" VPOS="1054" WIDTH="1463">
<String CONTENT="beer, Stabinger et Richard Wagner; de Mattei, Mattioli, Pintus," HEIGHT="51" HPOS="237" ID="string_tl_17" VPOS="1054" WIDTH="1463"/>
</TextLine>
<TextLine BASELINE="235 1156 1702 1156" HEIGHT="52" HPOS="235" ID="tl_18" VPOS="1104" WIDTH="1467">
<String CONTENT="Schulterius, Sabbatini, etc. Nous ne mentionnerons que quelques" HEIGHT="52" HPOS="235" ID="string_tl_18" VPOS="1104" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="235 1204 1706 1204" HEIGHT="50" HPOS="235" ID="tl_19" VPOS="1154" WIDTH="1471">
<String CONTENT="noms de femmes célèbres la maréchale d’Ancre, Mme de Krudner et" HEIGHT="50" HPOS="235" ID="string_tl_19" VPOS="1154" WIDTH="1471"/>
</TextLine>
<TextLine BASELINE="236 1266 1705 1266" HEIGHT="62" HPOS="236" ID="tl_20" VPOS="1204" WIDTH="1469">
<String CONTENT="Mlle Mars. Une curiosité autographique sur laquelle nous appelons l’at¬" HEIGHT="62" HPOS="236" ID="string_tl_20" VPOS="1204" WIDTH="1469"/>
</TextLine>
<TextLine BASELINE="236 1307 1700 1307" HEIGHT="63" HPOS="236" ID="tl_21" VPOS="1244" WIDTH="1464">
<String CONTENT="tention, ce sont les trois lettres des ancêtres de la famille Bonaparte," HEIGHT="63" HPOS="236" ID="string_tl_21" VPOS="1244" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="236 1357 1439 1357" HEIGHT="52" HPOS="236" ID="tl_22" VPOS="1305" WIDTH="1203">
<String CONTENT="des XVIe et XVIIe siècles, portés sous le n° 51 du catalogue." HEIGHT="52" HPOS="236" ID="string_tl_22" VPOS="1305" WIDTH="1203"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="304" HPOS="238" ID="r_1_4" VPOS="1360" WIDTH="1469">
<Shape>
<Polygon POINTS="238 1360 1707 1360 1707 1664 238 1664"/>
</Shape>
<TextLine BASELINE="291 1411 1704 1411" HEIGHT="50" HPOS="291" ID="tl_23" VPOS="1361" WIDTH="1413">
<String CONTENT="La collection, formée par un amateur italien, contient naturellement" HEIGHT="50" HPOS="291" ID="string_tl_23" VPOS="1361" WIDTH="1413"/>
</TextLine>
<TextLine BASELINE="239 1463 1705 1463" HEIGHT="51" HPOS="239" ID="tl_24" VPOS="1412" WIDTH="1466">
<String CONTENT="beaucoup de personnages illustres de son pays. Parmi les autographes" HEIGHT="51" HPOS="239" ID="string_tl_24" VPOS="1412" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="240 1513 1706 1513" HEIGHT="52" HPOS="240" ID="tl_25" VPOS="1461" WIDTH="1466">
<String CONTENT="remarquables de cette série, nous nous contenterons de signaler la" HEIGHT="52" HPOS="240" ID="string_tl_25" VPOS="1461" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="242 1563 1706 1563" HEIGHT="51" HPOS="242" ID="tl_26" VPOS="1512" WIDTH="1464">
<String CONTENT="lettre de Laurent-le-Magnifique. cataloguée sous le n° 226. Celle-là" HEIGHT="51" HPOS="242" ID="string_tl_26" VPOS="1512" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="241 1615 1705 1615" HEIGHT="53" HPOS="241" ID="tl_27" VPOS="1562" WIDTH="1464">
<String CONTENT="est d’une authenticité incontestable, ce qui veut dire qu’il y en a qui" HEIGHT="53" HPOS="241" ID="string_tl_27" VPOS="1562" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="241 1663 778 1663" HEIGHT="50" HPOS="241" ID="tl_28" VPOS="1613" WIDTH="537">
<String CONTENT="ne sont point dans ce cas." HEIGHT="50" HPOS="241" ID="string_tl_28" VPOS="1613" WIDTH="537"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="607" HPOS="243" ID="r_1_5" VPOS="1666" WIDTH="1476">
<Shape>
<Polygon POINTS="243 1666 1719 1666 1719 2273 243 2273"/>
</Shape>
<TextLine BASELINE="293 1720 1709 1720" HEIGHT="53" HPOS="293" ID="tl_29" VPOS="1667" WIDTH="1416">
<String CONTENT="En effet, dans la seconde moitié du XVe siècle, les princes italiens" HEIGHT="53" HPOS="293" ID="string_tl_29" VPOS="1667" WIDTH="1416"/>
</TextLine>
<TextLine BASELINE="245 1770 1709 1770" HEIGHT="52" HPOS="245" ID="tl_30" VPOS="1718" WIDTH="1464">
<String CONTENT="faisaient souvent écrire et signer leurs lettres, en leur nom , par leurs" HEIGHT="52" HPOS="245" ID="string_tl_30" VPOS="1718" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="244 1820 1712 1820" HEIGHT="52" HPOS="244" ID="tl_31" VPOS="1768" WIDTH="1468">
<String CONTENT="secrétaires. Les amateurs d’autographes ont longtemps ignoré cette" HEIGHT="52" HPOS="244" ID="string_tl_31" VPOS="1768" WIDTH="1468"/>
</TextLine>
<TextLine BASELINE="246 1870 1713 1870" HEIGHT="52" HPOS="246" ID="tl_32" VPOS="1818" WIDTH="1467">
<String CONTENT="particularité, et beaucoup de lettres des vieux Médicis et autres, écrites" HEIGHT="52" HPOS="246" ID="string_tl_32" VPOS="1818" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="247 1910 1712 1910" HEIGHT="42" HPOS="247" ID="tl_33" VPOS="1868" WIDTH="1465">
<String CONTENT="de la sorte, se trouvent dans les collections. On a reconnu le fait en" HEIGHT="42" HPOS="247" ID="string_tl_33" VPOS="1868" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="248 1970 1713 1970" HEIGHT="52" HPOS="248" ID="tl_34" VPOS="1918" WIDTH="1465">
<String CONTENT="comparant ces prétendus autographes à des lettres seulement signées" HEIGHT="52" HPOS="248" ID="string_tl_34" VPOS="1918" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="249 2021 1715 2021" HEIGHT="53" HPOS="249" ID="tl_35" VPOS="1968" WIDTH="1466">
<String CONTENT="des mêmes personnages, dont la signature différait complètement. Ce" HEIGHT="53" HPOS="249" ID="string_tl_35" VPOS="1968" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="252 2071 1715 2071" HEIGHT="52" HPOS="252" ID="tl_36" VPOS="2019" WIDTH="1463">
<String CONTENT="ne sont point là des pièces fausses, mais de vrais originaux, en portant" HEIGHT="52" HPOS="252" ID="string_tl_36" VPOS="2019" WIDTH="1463"/>
</TextLine>
<TextLine BASELINE="251 2120 1718 2120" HEIGHT="51" HPOS="251" ID="tl_37" VPOS="2069" WIDTH="1467">
<String CONTENT="tous les caractères, ayant des cachets, etc., et écrites vraisemblable¬" HEIGHT="51" HPOS="251" ID="string_tl_37" VPOS="2069" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="256 2172 1717 2172" HEIGHT="53" HPOS="256" ID="tl_38" VPOS="2119" WIDTH="1461">
<String CONTENT="ment sous la dictée et dans le cabinet du prince. Il y avait donc alors" HEIGHT="53" HPOS="256" ID="string_tl_38" VPOS="2119" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="254 2223 1716 2223" HEIGHT="54" HPOS="254" ID="tl_39" VPOS="2169" WIDTH="1462">
<String CONTENT="des secrétaires de la main pour la lettre entière, comme il y en a eu" HEIGHT="54" HPOS="254" ID="string_tl_39" VPOS="2169" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="255 2272 1383 2272" HEIGHT="53" HPOS="255" ID="tl_40" VPOS="2219" WIDTH="1128">
<String CONTENT="sous Louis XV et Louis XVI pour la simple signature." HEIGHT="53" HPOS="255" ID="string_tl_40" VPOS="2219" WIDTH="1128"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="154" HPOS="256" ID="r_1_6" VPOS="2274" WIDTH="1466">
<Shape>
<Polygon POINTS="256 2274 1722 2274 1722 2428 256 2428"/>
</Shape>
<TextLine BASELINE="308 2333 1720 2333" HEIGHT="58" HPOS="308" ID="tl_41" VPOS="2275" WIDTH="1412">
<String CONTENT="Cet usage s’était si bien répandu parmi les princes d’Italie, que" HEIGHT="58" HPOS="308" ID="string_tl_41" VPOS="2275" WIDTH="1412"/>
</TextLine>
<TextLine BASELINE="257 2383 1721 2383" HEIGHT="58" HPOS="257" ID="tl_42" VPOS="2325" WIDTH="1464">
<String CONTENT="dans certaines pièces officielles ou actes authentiques, ils ajoutaient" HEIGHT="58" HPOS="257" ID="string_tl_42" VPOS="2325" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="261 2427 1339 2427" HEIGHT="51" HPOS="261" ID="tl_43" VPOS="2376" WIDTH="1078">
<String CONTENT="souvent à leur signature, ces mots : Manu propria." HEIGHT="51" HPOS="261" ID="string_tl_43" VPOS="2376" WIDTH="1078"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="207" HPOS="260" ID="r_1_7" VPOS="2430" WIDTH="1467">
<Shape>
<Polygon POINTS="260 2430 1727 2430 1727 2637 260 2637"/>
</Shape>
<TextLine BASELINE="311 2483 1724 2483" HEIGHT="52" HPOS="311" ID="tl_44" VPOS="2431" WIDTH="1413">
<String CONTENT="On est à peu près sûr, croyons-nous, d'avoir affaire à une écriture" HEIGHT="52" HPOS="311" ID="string_tl_44" VPOS="2431" WIDTH="1413"/>
</TextLine>
<TextLine BASELINE="261 2535 1723 2535" HEIGHT="53" HPOS="261" ID="tl_45" VPOS="2482" WIDTH="1462">
<String CONTENT="de secrétaire, quand il se trouve une accolade dans la signature. C’est" HEIGHT="53" HPOS="261" ID="string_tl_45" VPOS="2482" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="262 2584 1721 2584" HEIGHT="52" HPOS="262" ID="tl_46" VPOS="2532" WIDTH="1459">
<String CONTENT="ce qui à lieu, par exemple, dans la lettre de Pierre Ier de Médicis," HEIGHT="52" HPOS="262" ID="string_tl_46" VPOS="2532" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="261 2636 1726 2636" HEIGHT="54" HPOS="261" ID="tl_47" VPOS="2582" WIDTH="1465">
<String CONTENT="cataloguée sous le n° 225, dont la signature est ainsi disposée : Peints" HEIGHT="54" HPOS="261" ID="string_tl_47" VPOS="2582" WIDTH="1465"/>
</TextLine>
