<String CONTENT="d’Helvétius ; Restif de la Bretone, le romancier graveleux, se fai¬" HEIGHT="54" HPOS="238" ID="string_tl_1" VPOS="237" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="237 340 1702 340" HEIGHT="51" HPOS="237" ID="tl_2" VPOS="289" WIDTH="1465">
<String CONTENT="sant moraliste auprès d’un débauché ; Lamennais, une lettre d'une" HEIGHT="51" HPOS="237" ID="string_tl_2" VPOS="289" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="238 392 1703 392" HEIGHT="54" HPOS="238" ID="tl_3" VPOS="338" WIDTH="1465">
<String CONTENT="actualité telle qu’on la dirait écrite d’hier ; Béranger, une épître de" HEIGHT="54" HPOS="238" ID="string_tl_3" VPOS="338" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="237 448 1703 448" HEIGHT="57" HPOS="237" ID="tl_4" VPOS="391" WIDTH="1466">
<String CONTENT="1804, la plus ancienne et la plus piquante que nous ayons vue; une" HEIGHT="57" HPOS="237" ID="string_tl_4" VPOS="391" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="238 492 1278 492" HEIGHT="51" HPOS="238" ID="tl_5" VPOS="441" WIDTH="1040">
