<TextLine BASELINE="176 1883 1577 1883" HEIGHT="53" HPOS="176" ID="tl_19" VPOS="1830" WIDTH="1401">
<String CONTENT="Le 9 avril, doit avoir lieu par le ministère de Me Daupeley, et sous" HEIGHT="53" HPOS="176" ID="string_tl_19" VPOS="1830" WIDTH="1401"/>
</TextLine>
<TextLine BASELINE="125 1933 1577 1933" HEIGHT="54" HPOS="125" ID="tl_20" VPOS="1879" WIDTH="1452">
<String CONTENT="notre direction, la vente d’une collection de lettres autographes peu" HEIGHT="54" HPOS="125" ID="string_tl_20" VPOS="1879" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="125 1980 1577 1980" HEIGHT="50" HPOS="125" ID="tl_21" VPOS="1930" WIDTH="1452">
<String CONTENT="nombreuse, mais très-intéressante. M. Gegner, qui l’avait réunie, est" HEIGHT="50" HPOS="125" ID="string_tl_21" VPOS="1930" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="128 2032 1576 2032" HEIGHT="53" HPOS="128" ID="tl_22" VPOS="1979" WIDTH="1448">
<String CONTENT="mort récemment des suites d’une affection de poitrine, dont il avait" HEIGHT="53" HPOS="128" ID="string_tl_22" VPOS="1979" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="125 2081 1575 2081" HEIGHT="52" HPOS="125" ID="tl_23" VPOS="2029" WIDTH="1450">
<String CONTENT="rapporté le germe de Crimée. Il avait fait toute cette campagne en" HEIGHT="52" HPOS="125" ID="string_tl_23" VPOS="2029" WIDTH="1450"/>
</TextLine>
<TextLine BASELINE="125 2135 1577 2135" HEIGHT="56" HPOS="125" ID="tl_24" VPOS="2079" WIDTH="1452">
<String CONTENT="qualité d’employé du Trésor C’était un homme d'une grande douceur" HEIGHT="56" HPOS="125" ID="string_tl_24" VPOS="2079" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="124 2170 1574 2170" HEIGHT="41" HPOS="124" ID="tl_25" VPOS="2129" WIDTH="1450">
<String CONTENT="et d’une timidité extrême. Son état de souffrance le rendait même un" HEIGHT="41" HPOS="124" ID="string_tl_25" VPOS="2129" WIDTH="1450"/>
</TextLine>
<TextLine BASELINE="128 2230 1576 2230" HEIGHT="51" HPOS="128" ID="tl_26" VPOS="2179" WIDTH="1448">
<String CONTENT="peu taciturne, aussi est-il peu connu de ses collègues les amateurs" HEIGHT="51" HPOS="128" ID="string_tl_26" VPOS="2179" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="123 2279 416 2279" HEIGHT="52" HPOS="123" ID="tl_27" VPOS="2227" WIDTH="293">
<String CONTENT="d’autographes." HEIGHT="52" HPOS="123" ID="string_tl_27" VPOS="2227" WIDTH="293"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="402" HPOS="124" ID="r_13_3" VPOS="2277" WIDTH="1453">
<Shape>
<Polygon POINTS="124 2277 1577 2277 1577 2679 124 2679"/>
</Shape>
<TextLine BASELINE="176 2331 1573 2331" HEIGHT="53" HPOS="176" ID="tl_28" VPOS="2278" WIDTH="1397">
<String CONTENT="M. Gegner assistait lui-même aux ventes d’autographes, quand son" HEIGHT="53" HPOS="176" ID="string_tl_28" VPOS="2278" WIDTH="1397"/>
</TextLine>
<TextLine BASELINE="125 2380 1574 2380" HEIGHT="52" HPOS="125" ID="tl_29" VPOS="2328" WIDTH="1449">
<String CONTENT="état de santé le lui permettait, mais il n’y faisait pas de folies : quand" HEIGHT="52" HPOS="125" ID="string_tl_29" VPOS="2328" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="126 2429 1571 2429" HEIGHT="51" HPOS="126" ID="tl_30" VPOS="2378" WIDTH="1445">
<String CONTENT="une pièce montait trop haut, il la lâchait, suivant un terme du métier." HEIGHT="51" HPOS="126" ID="string_tl_30" VPOS="2378" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="126 2479 1575 2479" HEIGHT="52" HPOS="126" ID="tl_31" VPOS="2427" WIDTH="1449">
<String CONTENT="Il se contentait, pour les autographes d’un grand prix, de mettre dans" HEIGHT="52" HPOS="126" ID="string_tl_31" VPOS="2427" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="127 2529 1576 2529" HEIGHT="51" HPOS="127" ID="tl_32" VPOS="2478" WIDTH="1449">
<String CONTENT="la chemise, comme pièce d’attente, un fac-similé ou un portrait. C’est" HEIGHT="51" HPOS="127" ID="string_tl_32" VPOS="2478" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="125 2579 1575 2579" HEIGHT="51" HPOS="125" ID="tl_33" VPOS="2528" WIDTH="1450">
<String CONTENT="ainsi que nous avons trouvé dans sa collection les noms les plus écla¬" HEIGHT="51" HPOS="125" ID="string_tl_33" VPOS="2528" WIDTH="1450"/>
</TextLine>
<TextLine BASELINE="127 2628 1576 2628" HEIGHT="50" HPOS="127" ID="tl_34" VPOS="2578" WIDTH="1449">
<String CONTENT="tants représentés par des feuilles de l'Isographie, au nombre de plus de" HEIGHT="50" HPOS="127" ID="string_tl_34" VPOS="2578" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="126 2678 816 2678" HEIGHT="50" HPOS="126" ID="tl_35" VPOS="2628" WIDTH="690">
<String CONTENT="trois cents, et autant de portraits." HEIGHT="50" HPOS="126" ID="string_tl_35" VPOS="2628" WIDTH="690"/>
</TextLine>
