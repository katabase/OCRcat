<TextLine BASELINE="331 284 1739 284" HEIGHT="51" HPOS="331" ID="tl_2" VPOS="233" WIDTH="1408">
<String CONTENT="L’autre lettre de Dumouriez, datée de Turville Park, le 28 juin 1822," HEIGHT="51" HPOS="331" ID="string_tl_2" VPOS="233" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="279 323 1740 323" HEIGHT="40" HPOS="279" ID="tl_3" VPOS="283" WIDTH="1461">
<String CONTENT="est adressée au maréchal Macdonald. Il lui demande la notice nécrolo¬" HEIGHT="40" HPOS="279" ID="string_tl_3" VPOS="283" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="279 384 1740 384" HEIGHT="51" HPOS="279" ID="tl_4" VPOS="333" WIDTH="1461">
<String CONTENT="gique qu’il a faite sur leur pauvre ami Beurnoville, et il ajoute ces" HEIGHT="51" HPOS="279" ID="string_tl_4" VPOS="333" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="279 434 1737 434" HEIGHT="52" HPOS="279" ID="tl_5" VPOS="382" WIDTH="1458">
<String CONTENT="curieux détails sur l’existence qu’il mène dans sa nouvelle résidence :" HEIGHT="52" HPOS="279" ID="string_tl_5" VPOS="382" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="282 484 1740 484" HEIGHT="52" HPOS="282" ID="tl_6" VPOS="432" WIDTH="1458">
<String CONTENT="« Mgr. le duc d’Orléans vous a dit la vérité sur mes occupations" HEIGHT="52" HPOS="282" ID="string_tl_6" VPOS="432" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="277 533 1741 533" HEIGHT="51" HPOS="277" ID="tl_7" VPOS="482" WIDTH="1464">
<String CONTENT="actuelles. Je suis établi dans une campagne très-sauvage , mais assez" HEIGHT="51" HPOS="277" ID="string_tl_7" VPOS="482" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="277 583 1740 583" HEIGHT="51" HPOS="277" ID="tl_8" VPOS="532" WIDTH="1463">
<String CONTENT="agréable, où je commence, un peu tard, mais avec grand plaisir, le" HEIGHT="51" HPOS="277" ID="string_tl_8" VPOS="532" WIDTH="1463"/>
</TextLine>
<TextLine BASELINE="279 634 1736 634" HEIGHT="52" HPOS="279" ID="tl_9" VPOS="582" WIDTH="1457">
<String CONTENT="métier de cultivateur. Je m'en trouve fort bien pour ma santé, et mon" HEIGHT="52" HPOS="279" ID="string_tl_9" VPOS="582" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="279 684 1739 684" HEIGHT="51" HPOS="279" ID="tl_10" VPOS="633" WIDTH="1460">
<String CONTENT="temps est partagé en travaux de campagne, en lectures de quelques" HEIGHT="51" HPOS="279" ID="string_tl_10" VPOS="633" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="277 733 1737 733" HEIGHT="52" HPOS="277" ID="tl_11" VPOS="681" WIDTH="1460">
<String CONTENT="bons livres, et ma correspondance, très restreinte, avec quatre ou cinq" HEIGHT="52" HPOS="277" ID="string_tl_11" VPOS="681" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="274 783 1739 783" HEIGHT="51" HPOS="274" ID="tl_12" VPOS="732" WIDTH="1465">
<String CONTENT="amis, emploie le reste de mon temps, et me font supporter très-patiem¬" HEIGHT="51" HPOS="274" ID="string_tl_12" VPOS="732" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="278 832 1738 832" HEIGHT="51" HPOS="278" ID="tl_13" VPOS="781" WIDTH="1460">
<String CONTENT="ment les peines attachées à une aussi longue vie. — C’est dans cette" HEIGHT="51" HPOS="278" ID="string_tl_13" VPOS="781" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="275 883 1738 883" HEIGHT="52" HPOS="275" ID="tl_14" VPOS="831" WIDTH="1463">
<String CONTENT="retraite que je pense souvent à mes anciens compagnons d’armes, que" HEIGHT="52" HPOS="275" ID="string_tl_14" VPOS="831" WIDTH="1463"/>
</TextLine>
<TextLine BASELINE="270 933 1738 933" HEIGHT="52" HPOS="270" ID="tl_15" VPOS="881" WIDTH="1468">
<String CONTENT="je m'intéresse à leur existence, plus glorieuse, mais moins tranquille" HEIGHT="52" HPOS="270" ID="string_tl_15" VPOS="881" WIDTH="1468"/>
</TextLine>
<TextLine BASELINE="275 983 1738 983" HEIGHT="52" HPOS="275" ID="tl_16" VPOS="931" WIDTH="1463">
<String CONTENT="que la mienne. Je ne sépare jamais la vôtre de celle de notre auguste" HEIGHT="52" HPOS="275" ID="string_tl_16" VPOS="931" WIDTH="1463"/>
</TextLine>
<TextLine BASELINE="274 1033 1739 1033" HEIGHT="54" HPOS="274" ID="tl_17" VPOS="979" WIDTH="1465">
<String CONTENT="ami (le duc d’Orléans), et je ne vous envie que le plaisir de converser" HEIGHT="54" HPOS="274" ID="string_tl_17" VPOS="979" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="273 1070 659 1070" HEIGHT="39" HPOS="273" ID="tl_18" VPOS="1031" WIDTH="386">
<String CONTENT="souvent avec lui »" HEIGHT="39" HPOS="273" ID="string_tl_18" VPOS="1031" WIDTH="386"/>
</TextLine>
</TextBlock>
