<TextLine BASELINE="326 1128 1737 1128" HEIGHT="49" HPOS="326" ID="tl_19" VPOS="1079" WIDTH="1411">
<String CONTENT="Dumouriez mourut, dans cette même résidence de Turville, le" HEIGHT="49" HPOS="326" ID="string_tl_19" VPOS="1079" WIDTH="1411"/>
</TextLine>
<TextLine BASELINE="274 1183 1737 1183" HEIGHT="54" HPOS="274" ID="tl_20" VPOS="1129" WIDTH="1463">
<String CONTENT="14 mars de l'année suivante. John Bowring, l’ami de ses derniers jours" HEIGHT="54" HPOS="274" ID="string_tl_20" VPOS="1129" WIDTH="1463"/>
</TextLine>
<TextLine BASELINE="275 1233 1517 1233" HEIGHT="54" HPOS="275" ID="tl_21" VPOS="1179" WIDTH="1242">
<String CONTENT="et le légataire de ses papiers, prononça son oraison funèbre." HEIGHT="54" HPOS="275" ID="string_tl_21" VPOS="1179" WIDTH="1242"/>
</TextLine>
