<TextLine BASELINE="277 326 1739 326" HEIGHT="52" HPOS="277" ID="tl_2" VPOS="274" WIDTH="1462">
<String CONTENT="Michel (de là le nom de mère Michel qu’on lui a donné dans les jour¬" HEIGHT="52" HPOS="277" ID="string_tl_2" VPOS="274" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="276 376 1740 376" HEIGHT="52" HPOS="276" ID="tl_3" VPOS="324" WIDTH="1464">
<String CONTENT="naux), et donna une adresse. Informations prises, adresse et nom se" HEIGHT="52" HPOS="276" ID="string_tl_3" VPOS="324" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="277 426 1734 426" HEIGHT="51" HPOS="277" ID="tl_4" VPOS="375" WIDTH="1457">
<String CONTENT="trouvèrent faux. Il en était malheureusement de même des autographes." HEIGHT="51" HPOS="277" ID="string_tl_4" VPOS="375" WIDTH="1457"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="697" HPOS="264" ID="r_2_2" VPOS="435" WIDTH="1476">
<Shape>
<Polygon POINTS="264 435 1740 435 1740 1132 264 1132"/>
</Shape>
<TextLine BASELINE="326 487 1738 487" HEIGHT="51" HPOS="326" ID="tl_5" VPOS="436" WIDTH="1412">
<String CONTENT="M. Muffat, qui avait conçu quelques doutes, fit présenter ces auto¬" HEIGHT="51" HPOS="326" ID="string_tl_5" VPOS="436" WIDTH="1412"/>
</TextLine>
<TextLine BASELINE="274 539 1739 539" HEIGHT="52" HPOS="274" ID="tl_6" VPOS="487" WIDTH="1465">
<String CONTENT="graphes à M Etienne Charavay, qui n’eut pas de peine à reconnaître" HEIGHT="52" HPOS="274" ID="string_tl_6" VPOS="487" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="276 589 1738 589" HEIGHT="59" HPOS="276" ID="tl_7" VPOS="530" WIDTH="1462">
<String CONTENT="la fraude, évidente pour un'expert. A quelques jours de là. la" HEIGHT="59" HPOS="276" ID="string_tl_7" VPOS="530" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="274 639 1734 639" HEIGHT="52" HPOS="274" ID="tl_8" VPOS="587" WIDTH="1460">
<String CONTENT="mère II. présentait un nouveau lot d’autographes de la même farine," HEIGHT="52" HPOS="274" ID="string_tl_8" VPOS="587" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="274 690 1738 690" HEIGHT="52" HPOS="274" ID="tl_9" VPOS="638" WIDTH="1464">
<String CONTENT="non plus à M. Muffat, qui se fût contenté de la chasser avec semonce" HEIGHT="52" HPOS="274" ID="string_tl_9" VPOS="638" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="271 740 1739 740" HEIGHT="52" HPOS="271" ID="tl_10" VPOS="688" WIDTH="1468">
<String CONTENT="et menaces, mais à M Etienne Charavay, qui la fit arrêter. En présence" HEIGHT="52" HPOS="271" ID="string_tl_10" VPOS="688" WIDTH="1468"/>
</TextLine>
<TextLine BASELINE="271 789 1738 789" HEIGHT="58" HPOS="271" ID="tl_11" VPOS="731" WIDTH="1467">
<String CONTENT="de M. Bérillon, commissaire de police au Palais-de-Justice, elle dut" HEIGHT="58" HPOS="271" ID="string_tl_11" VPOS="731" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="271 839 1738 839" HEIGHT="50" HPOS="271" ID="tl_12" VPOS="789" WIDTH="1467">
<String CONTENT="décliner ses véritables noms et adresse. Elle avoua, en larmes, que ces" HEIGHT="50" HPOS="271" ID="string_tl_12" VPOS="789" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="271 892 1737 892" HEIGHT="53" HPOS="271" ID="tl_13" VPOS="839" WIDTH="1466">
<String CONTENT="autographes avaient été fabriqués par son fils, employé à la Compagnie" HEIGHT="53" HPOS="271" ID="string_tl_13" VPOS="839" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="269 938 1734 938" HEIGHT="49" HPOS="269" ID="tl_14" VPOS="889" WIDTH="1465">
<String CONTENT="d’assurance le Soleil. On a arrêté celui-ci à son bureau, où il était en" HEIGHT="49" HPOS="269" ID="string_tl_14" VPOS="889" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="270 992 1734 992" HEIGHT="53" HPOS="270" ID="tl_15" VPOS="939" WIDTH="1464">
<String CONTENT="train de copier un autographe de Silvio Pellico, et l’on fit perquisition" HEIGHT="53" HPOS="270" ID="string_tl_15" VPOS="939" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="268 1042 1735 1042" HEIGHT="53" HPOS="268" ID="tl_16" VPOS="989" WIDTH="1467">
<String CONTENT="à son domicile, à Levallois-Perret. On y trouva des autographes faux" HEIGHT="53" HPOS="268" ID="string_tl_16" VPOS="989" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="268 1091 1737 1091" HEIGHT="51" HPOS="268" ID="tl_17" VPOS="1040" WIDTH="1469">
<String CONTENT="mêlés avec des vrais, des fac-similé d’écritures et des feuilles de garde" HEIGHT="51" HPOS="268" ID="string_tl_17" VPOS="1040" WIDTH="1469"/>
</TextLine>
<TextLine BASELINE="265 1131 894 1131" HEIGHT="40" HPOS="265" ID="tl_18" VPOS="1091" WIDTH="629">
<String CONTENT="arrachées à des livres anciens." HEIGHT="40" HPOS="265" ID="string_tl_18" VPOS="1091" WIDTH="629"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="558" HPOS="261" ID="r_2_3" VPOS="1149" WIDTH="1478">
<Shape>
<Polygon POINTS="261 1149 1739 1149 1739 1707 261 1707"/>
</Shape>
<TextLine BASELINE="315 1203 1728 1203" HEIGHT="53" HPOS="315" ID="tl_19" VPOS="1150" WIDTH="1413">
<String CONTENT="Ce jeune homme, âgé de vingt-huit ans, est marié et père de famille." HEIGHT="53" HPOS="315" ID="string_tl_19" VPOS="1150" WIDTH="1413"/>
</TextLine>
<TextLine BASELINE="268 1254 1729 1254" HEIGHT="52" HPOS="268" ID="tl_20" VPOS="1202" WIDTH="1461">
<String CONTENT="Il avait également son père; tous habitaient le même domicile. La mère," HEIGHT="52" HPOS="268" ID="string_tl_20" VPOS="1202" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="266 1304 1734 1304" HEIGHT="52" HPOS="266" ID="tl_21" VPOS="1252" WIDTH="1468">
<String CONTENT="en déclarant le nom de son fils, avait ajouté, les larmes aux veux, que" HEIGHT="52" HPOS="266" ID="string_tl_21" VPOS="1252" WIDTH="1468"/>
</TextLine>
<TextLine BASELINE="265 1354 1732 1354" HEIGHT="52" HPOS="265" ID="tl_22" VPOS="1302" WIDTH="1467">
<String CONTENT="son emploi à la Compagnie d’assurance ne lui permettant pas de vivre," HEIGHT="52" HPOS="265" ID="string_tl_22" VPOS="1302" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="267 1405 1736 1405" HEIGHT="53" HPOS="267" ID="tl_23" VPOS="1352" WIDTH="1469">
<String CONTENT="il fabriquait ces faux autographes pour subvenir à ses nombreuses" HEIGHT="53" HPOS="267" ID="string_tl_23" VPOS="1352" WIDTH="1469"/>
</TextLine>
<TextLine BASELINE="265 1454 1734 1454" HEIGHT="52" HPOS="265" ID="tl_24" VPOS="1402" WIDTH="1469">
<String CONTENT="charges. On se demande quelles ressources pouvaient apporter dans" HEIGHT="52" HPOS="265" ID="string_tl_24" VPOS="1402" WIDTH="1469"/>
</TextLine>
<TextLine BASELINE="262 1507 1733 1507" HEIGHT="54" HPOS="262" ID="tl_25" VPOS="1453" WIDTH="1471">
<String CONTENT="cette malheureuse famille les vingt francs qu’ont produit les vingt-cinq" HEIGHT="54" HPOS="262" ID="string_tl_25" VPOS="1453" WIDTH="1471"/>
</TextLine>
<TextLine BASELINE="264 1556 1733 1556" HEIGHT="53" HPOS="264" ID="tl_26" VPOS="1503" WIDTH="1469">
<String CONTENT="autographes faux vendus à M. Muffat! Est-ce donc la peine de s’appli¬" HEIGHT="53" HPOS="264" ID="string_tl_26" VPOS="1503" WIDTH="1469"/>
</TextLine>
<TextLine BASELINE="264 1606 1729 1606" HEIGHT="52" HPOS="264" ID="tl_27" VPOS="1554" WIDTH="1465">
<String CONTENT="quer, de tâtonner sur des originaux ou des fac-similé, de rassembler du" HEIGHT="52" HPOS="264" ID="string_tl_27" VPOS="1554" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="264 1656 1733 1656" HEIGHT="52" HPOS="264" ID="tl_28" VPOS="1604" WIDTH="1469">
<String CONTENT="papier ad hoc, etc., pour toucher moins d’un franc par pièce? Le" HEIGHT="52" HPOS="264" ID="string_tl_28" VPOS="1604" WIDTH="1469"/>
</TextLine>
<TextLine BASELINE="264 1706 1738 1706" HEIGHT="62" HPOS="264" ID="tl_29" VPOS="1644" WIDTH="1474">
<String CONTENT="pauvre diable n’eût-il pas plus gagné à copier des exploits d’huissier ?" HEIGHT="62" HPOS="264" ID="string_tl_29" VPOS="1644" WIDTH="1474"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="455" HPOS="262" ID="r_2_4" VPOS="1709" WIDTH="1479">
<Shape>
<Polygon POINTS="262 1709 1741 1709 1741 2164 262 2164"/>
</Shape>
<TextLine BASELINE="314 1762 1737 1762" HEIGHT="52" HPOS="314" ID="tl_30" VPOS="1710" WIDTH="1423">
<String CONTENT="C’est peut-être ce qu’il eût fini par reconnaître s’il n’eût pas été" HEIGHT="52" HPOS="314" ID="string_tl_30" VPOS="1710" WIDTH="1423"/>
</TextLine>
<TextLine BASELINE="263 1812 1735 1812" HEIGHT="52" HPOS="263" ID="tl_31" VPOS="1760" WIDTH="1472">
<String CONTENT="surpris au début de sa carrière. Du reste, il ne pouvait aller loin." HEIGHT="52" HPOS="263" ID="string_tl_31" VPOS="1760" WIDTH="1472"/>
</TextLine>
<TextLine BASELINE="268 1862 1738 1862" HEIGHT="52" HPOS="268" ID="tl_32" VPOS="1810" WIDTH="1470">
<String CONTENT="Autour de soi, on trouve peu de personnes disposées à acheter des auto¬" HEIGHT="52" HPOS="268" ID="string_tl_32" VPOS="1810" WIDTH="1470"/>
</TextLine>
<TextLine BASELINE="266 1917 1737 1917" HEIGHT="57" HPOS="266" ID="tl_33" VPOS="1860" WIDTH="1471">
<String CONTENT="graphes. Pour placer ces objets de curiosité, il faut avoir recours" HEIGHT="57" HPOS="266" ID="string_tl_33" VPOS="1860" WIDTH="1471"/>
</TextLine>
<TextLine BASELINE="266 1963 1740 1963" HEIGHT="53" HPOS="266" ID="tl_34" VPOS="1910" WIDTH="1474">
<String CONTENT="aux experts qui en font le commerce, qui seuls peuvent les" HEIGHT="53" HPOS="266" ID="string_tl_34" VPOS="1910" WIDTH="1474"/>
</TextLine>
<TextLine BASELINE="268 2012 1737 2012" HEIGHT="52" HPOS="268" ID="tl_35" VPOS="1960" WIDTH="1469">
<String CONTENT="écouler, parce qu’ils ont la confiance des amateurs, à qui ils garan¬" HEIGHT="52" HPOS="268" ID="string_tl_35" VPOS="1960" WIDTH="1469"/>
</TextLine>
<TextLine BASELINE="268 2064 1738 2064" HEIGHT="54" HPOS="268" ID="tl_36" VPOS="2010" WIDTH="1470">
<String CONTENT="tissent l’authenticité des pièces. Mais s’adresser aux experts, qui ont" HEIGHT="54" HPOS="268" ID="string_tl_36" VPOS="2010" WIDTH="1470"/>
</TextLine>
<TextLine BASELINE="269 2113 1740 2113" HEIGHT="52" HPOS="269" ID="tl_37" VPOS="2061" WIDTH="1471">
<String CONTENT="l’expérience et tous les moyens de vérification, c’est se mettre, comme" HEIGHT="52" HPOS="269" ID="string_tl_37" VPOS="2061" WIDTH="1471"/>
</TextLine>
<TextLine BASELINE="268 2163 1090 2163" HEIGHT="52" HPOS="268" ID="tl_38" VPOS="2111" WIDTH="822">
<String CONTENT="dit le proverbe, dans la gueule du loup." HEIGHT="52" HPOS="268" ID="string_tl_38" VPOS="2111" WIDTH="822"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="406" HPOS="263" ID="r_2_5" VPOS="2168" WIDTH="1481">
<Shape>
<Polygon POINTS="263 2168 1744 2168 1744 2574 263 2574"/>
</Shape>
<TextLine BASELINE="321 2222 1739 2222" HEIGHT="53" HPOS="321" ID="tl_39" VPOS="2169" WIDTH="1418">
<String CONTENT="Voilà toute celle fameuse histoire dont les journaux ont fait si grand" HEIGHT="53" HPOS="321" ID="string_tl_39" VPOS="2169" WIDTH="1418"/>
</TextLine>
<TextLine BASELINE="272 2272 1740 2272" HEIGHT="52" HPOS="272" ID="tl_40" VPOS="2220" WIDTH="1468">
<String CONTENT="bruit, sous le titre plaisant des Autographes de la mère Michel. On a" HEIGHT="52" HPOS="272" ID="string_tl_40" VPOS="2220" WIDTH="1468"/>
</TextLine>
<TextLine BASELINE="270 2322 1742 2322" HEIGHT="53" HPOS="270" ID="tl_41" VPOS="2269" WIDTH="1472">
<String CONTENT="dit que cette affaire, venant à la suite de celle de Lucas, devait porter" HEIGHT="53" HPOS="270" ID="string_tl_41" VPOS="2269" WIDTH="1472"/>
</TextLine>
<TextLine BASELINE="273 2372 1742 2372" HEIGHT="52" HPOS="273" ID="tl_42" VPOS="2320" WIDTH="1469">
<String CONTENT="un coup terrible au goût des autographes. C’est une erreur. Les vrais" HEIGHT="52" HPOS="273" ID="string_tl_42" VPOS="2320" WIDTH="1469"/>
</TextLine>
<TextLine BASELINE="271 2424 1743 2424" HEIGHT="53" HPOS="271" ID="tl_43" VPOS="2371" WIDTH="1472">
<String CONTENT="amateurs n’en sont pas le moins du monde émus : ce n’est pas auprès" HEIGHT="53" HPOS="271" ID="string_tl_43" VPOS="2371" WIDTH="1472"/>
</TextLine>
<TextLine BASELINE="270 2473 1738 2473" HEIGHT="52" HPOS="270" ID="tl_44" VPOS="2421" WIDTH="1468">
<String CONTENT="d’eux que réussiraient les ridicules pastiches de Lucas et du jeune H. :" HEIGHT="52" HPOS="270" ID="string_tl_44" VPOS="2421" WIDTH="1468"/>
</TextLine>
<TextLine BASELINE="274 2522 1741 2522" HEIGHT="51" HPOS="274" ID="tl_45" VPOS="2471" WIDTH="1467">
<String CONTENT="ils savent démêler le faux du vrai, et, quand ils achètent, c’est tou¬" HEIGHT="51" HPOS="274" ID="string_tl_45" VPOS="2471" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="264 2573 667 2573" HEIGHT="51" HPOS="264" ID="tl_46" VPOS="2522" WIDTH="403">
<String CONTENT="jours à bon escient." HEIGHT="51" HPOS="264" ID="string_tl_46" VPOS="2522" WIDTH="403"/>
</TextLine>
