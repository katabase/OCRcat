</Shape>
<TextLine BASELINE="136 475 1542 475" HEIGHT="51" HPOS="136" ID="tl_5" VPOS="424" WIDTH="1406">
<String CONTENT="— François de Nantes, membre de l'Assemblée législative, où il" HEIGHT="51" HPOS="136" ID="string_tl_5" VPOS="424" WIDTH="1406"/>
</TextLine>
<TextLine BASELINE="87 524 1544 524" HEIGHT="50" HPOS="87" ID="tl_6" VPOS="474" WIDTH="1457">
<String CONTENT="avait montré beaucoup de véhémence contre les prêtres réfractaires," HEIGHT="50" HPOS="87" ID="string_tl_6" VPOS="474" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="87 575 1548 575" HEIGHT="51" HPOS="87" ID="tl_7" VPOS="524" WIDTH="1461">
<String CONTENT="fut nommé, sous l’Empire, directeur-général des droits réunis, et" HEIGHT="51" HPOS="87" ID="string_tl_7" VPOS="524" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="87 634 1549 634" HEIGHT="61" HPOS="87" ID="tl_8" VPOS="573" WIDTH="1462">
<String CONTENT="donna asile dans ses bureaux à une foule de littérateurs, la plupart" HEIGHT="61" HPOS="87" ID="string_tl_8" VPOS="573" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="89 675 1550 675" HEIGHT="63" HPOS="89" ID="tl_9" VPOS="612" WIDTH="1461">
<String CONTENT="opposés au nouveau régime, au point que Napoléon s’était écrié dans" HEIGHT="63" HPOS="89" ID="string_tl_9" VPOS="612" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="89 726 1550 726" HEIGHT="53" HPOS="89" ID="tl_10" VPOS="673" WIDTH="1461">
<String CONTENT="un moment d’humeur : « C’est un véritable nid d’aiglons que cette" HEIGHT="53" HPOS="89" ID="string_tl_10" VPOS="673" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="91 775 1551 775" HEIGHT="52" HPOS="91" ID="tl_11" VPOS="723" WIDTH="1460">
<String CONTENT="maison de la rue Sainte-Avoye. » La collection Gegner renferme la" HEIGHT="52" HPOS="91" ID="string_tl_11" VPOS="723" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="91 825 1553 825" HEIGHT="57" HPOS="91" ID="tl_12" VPOS="768" WIDTH="1462">
<String CONTENT="lettre même que Parny, le poète érotique et hétérodoxe, écrivit à" HEIGHT="57" HPOS="91" ID="string_tl_12" VPOS="768" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="91 874 1555 874" HEIGHT="52" HPOS="91" ID="tl_13" VPOS="822" WIDTH="1464">
<String CONTENT="François de Nantes, le 30 messidor an XII (19 juillet 1804), pour" HEIGHT="52" HPOS="91" ID="string_tl_13" VPOS="822" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="92 924 1556 924" HEIGHT="52" HPOS="92" ID="tl_14" VPOS="872" WIDTH="1464">
<String CONTENT="entrer dans ses bureaux. Elle est trop curieuse pour ne pas la citer" HEIGHT="52" HPOS="92" ID="string_tl_14" VPOS="872" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="98 962 361 962" HEIGHT="40" HPOS="98" ID="tl_15" VPOS="922" WIDTH="263">
<String CONTENT="tout entière :" HEIGHT="40" HPOS="98" ID="string_tl_15" VPOS="922" WIDTH="263"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="503" HPOS="97" ID="r_2_3" VPOS="970" WIDTH="1471">
<Shape>
<Polygon POINTS="97 970 1568 970 1568 1473 97 1473"/>
</Shape>
<TextLine BASELINE="148 1023 1558 1023" HEIGHT="52" HPOS="148" ID="tl_16" VPOS="971" WIDTH="1410">
<String CONTENT="« La place de bibliothécaire en chef du Corps législatif, qui m’avait" HEIGHT="52" HPOS="148" ID="string_tl_16" VPOS="971" WIDTH="1410"/>
</TextLine>
<TextLine BASELINE="98 1074 1560 1074" HEIGHT="52" HPOS="98" ID="tl_17" VPOS="1022" WIDTH="1462">
<String CONTENT="été promise, ne sera point créée. Si l'on avait pris sur-le-champ cette" HEIGHT="52" HPOS="98" ID="string_tl_17" VPOS="1022" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="98 1123 1561 1123" HEIGHT="52" HPOS="98" ID="tl_18" VPOS="1071" WIDTH="1463">
<String CONTENT="détermination, j’aurais sollicité, au nom des Muses, qui n’ont pas le" HEIGHT="52" HPOS="98" ID="string_tl_18" VPOS="1071" WIDTH="1463"/>
</TextLine>
<TextLine BASELINE="101 1173 1561 1173" HEIGHT="52" HPOS="101" ID="tl_19" VPOS="1121" WIDTH="1460">
<String CONTENT="privilège de pouvoir vivre sans pain, un recoin obscur dans votre" HEIGHT="52" HPOS="101" ID="string_tl_19" VPOS="1121" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="104 1228 1562 1228" HEIGHT="58" HPOS="104" ID="tl_20" VPOS="1170" WIDTH="1458">
<String CONTENT="propre bureau. Il n’est sans doute plus tems. Cependant, je m’adresse" HEIGHT="58" HPOS="104" ID="string_tl_20" VPOS="1170" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="102 1272 1563 1272" HEIGHT="51" HPOS="102" ID="tl_21" VPOS="1221" WIDTH="1461">
<String CONTENT="à vous, sinon avec espoir, du moins avec confiance. Le travail des" HEIGHT="51" HPOS="102" ID="string_tl_21" VPOS="1221" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="104 1322 1562 1322" HEIGHT="52" HPOS="104" ID="tl_22" VPOS="1270" WIDTH="1458">
<String CONTENT="bureaux ne m’est point étranger. J’ai exercé pendant treize mois un" HEIGHT="52" HPOS="104" ID="string_tl_22" VPOS="1270" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="103 1373 1566 1373" HEIGHT="53" HPOS="103" ID="tl_23" VPOS="1320" WIDTH="1463">
<String CONTENT="emploi dans ceux de l’intérieur, et ne me chargeais pas des choses les" HEIGHT="53" HPOS="103" ID="string_tl_23" VPOS="1320" WIDTH="1463"/>
</TextLine>
<TextLine BASELINE="107 1423 1567 1423" HEIGHT="53" HPOS="107" ID="tl_24" VPOS="1370" WIDTH="1460">
<String CONTENT="plus faciles. Je suis toujours tout entier à ce que je fais : peut-être" HEIGHT="53" HPOS="107" ID="string_tl_24" VPOS="1370" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="109 1472 1103 1472" HEIGHT="52" HPOS="109" ID="tl_25" VPOS="1420" WIDTH="994">
<String CONTENT="même trop, car ma santé en souffre quelquefois." HEIGHT="52" HPOS="109" ID="string_tl_25" VPOS="1420" WIDTH="994"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="152" HPOS="107" ID="r_2_4" VPOS="1470" WIDTH="1463">
<Shape>
<Polygon POINTS="107 1470 1570 1470 1570 1622 107 1622"/>
</Shape>
<TextLine BASELINE="161 1523 1569 1523" HEIGHT="52" HPOS="161" ID="tl_26" VPOS="1471" WIDTH="1408">
<String CONTENT="La requête du poète eut un plein succès : il trouva dans le modeste" HEIGHT="52" HPOS="161" ID="string_tl_26" VPOS="1471" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="108 1571 1569 1571" HEIGHT="51" HPOS="108" ID="tl_27" VPOS="1520" WIDTH="1461">
<String CONTENT="emploi que lui donna François de Nantes un asile contre le besoin et" HEIGHT="51" HPOS="108" ID="string_tl_27" VPOS="1520" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="108 1621 683 1621" HEIGHT="59" HPOS="108" ID="tl_28" VPOS="1562" WIDTH="575">
<String CONTENT="une existence indépendante." HEIGHT="59" HPOS="108" ID="string_tl_28" VPOS="1562" WIDTH="575"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="153" HPOS="110" ID="r_2_5" VPOS="1635" WIDTH="1464">
<Shape>
<Polygon POINTS="110 1635 1574 1635 1574 1788 110 1788"/>
</Shape>
<TextLine BASELINE="161 1686 1571 1686" HEIGHT="50" HPOS="161" ID="tl_29" VPOS="1636" WIDTH="1410">
<String CONTENT="— La collection Gegner offre deux lettres bien curieuses de Demeu¬" HEIGHT="50" HPOS="161" ID="string_tl_29" VPOS="1636" WIDTH="1410"/>
</TextLine>
<TextLine BASELINE="112 1736 1573 1736" HEIGHT="50" HPOS="112" ID="tl_30" VPOS="1686" WIDTH="1461">
<String CONTENT="riez, écrites dans sa vieillesse, et datées d’Angleterre, où il vivait d’une" HEIGHT="50" HPOS="112" ID="string_tl_30" VPOS="1686" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="111 1787 850 1787" HEIGHT="51" HPOS="111" ID="tl_31" VPOS="1736" WIDTH="739">
<String CONTENT="pension du cabinet de Saint-James." HEIGHT="51" HPOS="111" ID="string_tl_31" VPOS="1736" WIDTH="739"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="888" HPOS="113" ID="r_2_6" VPOS="1784" WIDTH="1476">
<Shape>
<Polygon POINTS="113 1784 1589 1784 1589 2672 113 2672"/>
</Shape>
<TextLine BASELINE="164 1836 1572 1836" HEIGHT="51" HPOS="164" ID="tl_32" VPOS="1785" WIDTH="1408">
<String CONTENT="La première de ces lettres, écrite de Little Ealing, près de Londres," HEIGHT="51" HPOS="164" ID="string_tl_32" VPOS="1785" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="116 1884 1573 1884" HEIGHT="49" HPOS="116" ID="tl_33" VPOS="1835" WIDTH="1457">
<String CONTENT="le 8 avril 1819, et adressée à Malus, intendant-général de l’armée du" HEIGHT="49" HPOS="116" ID="string_tl_33" VPOS="1835" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="114 1936 1576 1936" HEIGHT="51" HPOS="114" ID="tl_34" VPOS="1885" WIDTH="1462">
<String CONTENT="Nord, pendant que lui-même en était général en chef. Leurs rapports" HEIGHT="51" HPOS="114" ID="string_tl_34" VPOS="1885" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="114 1986 1577 1986" HEIGHT="52" HPOS="114" ID="tl_35" VPOS="1934" WIDTH="1463">
<String CONTENT="avaient été rompus depuis cette époque. Dumouriez s’épanche en sou¬" HEIGHT="52" HPOS="114" ID="string_tl_35" VPOS="1934" WIDTH="1463"/>
</TextLine>
<TextLine BASELINE="115 2046 1577 2046" HEIGHT="62" HPOS="115" ID="tl_36" VPOS="1984" WIDTH="1462">
<String CONTENT="venirs, car il ne vivait plus que dans le passé. « Nous avons tous les" HEIGHT="62" HPOS="115" ID="string_tl_36" VPOS="1984" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="115 2094 1577 2094" HEIGHT="70" HPOS="115" ID="tl_37" VPOS="2024" WIDTH="1462">
<String CONTENT="deux, dit-il à Malus, une jambe traînante, mais croyons que c’est un" HEIGHT="70" HPOS="115" ID="string_tl_37" VPOS="2024" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="116 2136 1581 2136" HEIGHT="52" HPOS="116" ID="tl_38" VPOS="2084" WIDTH="1465">
<String CONTENT="avantage, parce que nous avançons plus lentement dans la carrière, et" HEIGHT="52" HPOS="116" ID="string_tl_38" VPOS="2084" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="117 2184 1579 2184" HEIGHT="51" HPOS="117" ID="tl_39" VPOS="2133" WIDTH="1462">
<String CONTENT="nous arriverons plus tard au terme du voyage. Tant que la tête est" HEIGHT="51" HPOS="117" ID="string_tl_39" VPOS="2133" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="115 2235 1580 2235" HEIGHT="52" HPOS="115" ID="tl_40" VPOS="2183" WIDTH="1465">
<String CONTENT="bonne, l’esprit calme et le cœur susceptible d’attachement, la vieillesse" HEIGHT="52" HPOS="115" ID="string_tl_40" VPOS="2183" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="116 2285 1580 2285" HEIGHT="52" HPOS="116" ID="tl_41" VPOS="2233" WIDTH="1464">
<String CONTENT="est supportable. Mon âge de 80 ans comporte aussi sa coquetterie, et" HEIGHT="52" HPOS="116" ID="string_tl_41" VPOS="2233" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="120 2333 1582 2333" HEIGHT="51" HPOS="120" ID="tl_42" VPOS="2282" WIDTH="1462">
<String CONTENT="si Mme de Lierreville ne m’aimait pas autant que je l’aime, elle mérite¬" HEIGHT="51" HPOS="120" ID="string_tl_42" VPOS="2282" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="120 2384 1578 2384" HEIGHT="53" HPOS="120" ID="tl_43" VPOS="2331" WIDTH="1458">
<String CONTENT="rait d’être étranglée... » Après cette boutade galante, il continue :" HEIGHT="53" HPOS="120" ID="string_tl_43" VPOS="2331" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="123 2432 1580 2432" HEIGHT="51" HPOS="123" ID="tl_44" VPOS="2381" WIDTH="1457">
<String CONTENT="« Je vis ici au milieu des livres et des arts, dans une jolie campagne," HEIGHT="51" HPOS="123" ID="string_tl_44" VPOS="2381" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="122 2484 1583 2484" HEIGHT="53" HPOS="122" ID="tl_45" VPOS="2431" WIDTH="1461">
<String CONTENT="ne voyant que très-peu d’amis, m’amusant de mes souvenirs, de mes" HEIGHT="53" HPOS="122" ID="string_tl_45" VPOS="2431" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="124 2532 1586 2532" HEIGHT="52" HPOS="124" ID="tl_46" VPOS="2480" WIDTH="1462">
<String CONTENT="pensées, de mes réflexions, sans regrets, sans désirs au-delà du cercle" HEIGHT="52" HPOS="124" ID="string_tl_46" VPOS="2480" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="124 2583 1583 2583" HEIGHT="52" HPOS="124" ID="tl_47" VPOS="2531" WIDTH="1459">
<String CONTENT="de la vie que je me suis tracée, et j’ai conservé ma gaieté, ma santé," HEIGHT="52" HPOS="124" ID="string_tl_47" VPOS="2531" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="126 2632 1588 2632" HEIGHT="53" HPOS="126" ID="tl_48" VPOS="2579" WIDTH="1462">
<String CONTENT="mon activité. Tout ce qui pourrait altérer cette existence douce est" HEIGHT="53" HPOS="126" ID="string_tl_48" VPOS="2579" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="126 2671 420 2671" HEIGHT="40" HPOS="126" ID="tl_49" VPOS="2631" WIDTH="294">
<String CONTENT="hors de moi. »" HEIGHT="40" HPOS="126" ID="string_tl_49" VPOS="2631" WIDTH="294"/>
</TextLine>
</TextBlock>
