<TextLine BASELINE="81 309 1540 309" HEIGHT="50" HPOS="81" ID="tl_2" VPOS="259" WIDTH="1459">
<String CONTENT="notre honneur. Ce sera pour moi le moment de rentrer en France, où" HEIGHT="50" HPOS="81" ID="string_tl_2" VPOS="259" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="82 370 1542 370" HEIGHT="61" HPOS="82" ID="tl_3" VPOS="309" WIDTH="1460">
<String CONTENT="me rappellent de tendres affections, de grands intérêts et aussi le" HEIGHT="61" HPOS="82" ID="string_tl_3" VPOS="309" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="85 410 465 410" HEIGHT="49" HPOS="85" ID="tl_4" VPOS="361" WIDTH="380">
<String CONTENT="besoin du repos. »" HEIGHT="49" HPOS="85" ID="string_tl_4" VPOS="361" WIDTH="380"/>
</TextLine>
