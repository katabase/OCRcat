<TextLine BASELINE="202 306 1668 306" HEIGHT="57" HPOS="202" ID="tl_2" VPOS="249" WIDTH="1466">
<String CONTENT="entendait si peu raison, que l’on se contentait de gémir. M. Chasles," HEIGHT="57" HPOS="202" ID="string_tl_2" VPOS="249" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="200 351 1666 351" HEIGHT="53" HPOS="200" ID="tl_3" VPOS="298" WIDTH="1466">
<String CONTENT="outre cet amas de pastiches, possède une collection d’autographes," HEIGHT="53" HPOS="200" ID="string_tl_3" VPOS="298" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="199 402 1670 402" HEIGHT="52" HPOS="199" ID="tl_4" VPOS="350" WIDTH="1471">
<String CONTENT="moins précieuse, bien entendu, mais parfaitement authentique. Il peut" HEIGHT="52" HPOS="199" ID="string_tl_4" VPOS="350" WIDTH="1471"/>
</TextLine>
<TextLine BASELINE="202 445 1671 445" HEIGHT="45" HPOS="202" ID="tl_5" VPOS="400" WIDTH="1469">
<String CONTENT="mêler le tout comme on battrait des cartes, et nous nous taisons fort" HEIGHT="45" HPOS="202" ID="string_tl_5" VPOS="400" WIDTH="1469"/>
</TextLine>
<TextLine BASELINE="200 502 1216 502" HEIGHT="52" HPOS="200" ID="tl_6" VPOS="450" WIDTH="1016">
<String CONTENT="de réparer, sans hésitation, l’ivraie du bon grain." HEIGHT="52" HPOS="200" ID="string_tl_6" VPOS="450" WIDTH="1016"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="462" HPOS="198" ID="r_2_2" VPOS="511" WIDTH="1475">
<Shape>
<Polygon POINTS="198 511 1673 511 1673 973 198 973"/>
</Shape>
<TextLine BASELINE="252 564 1667 564" HEIGHT="52" HPOS="252" ID="tl_7" VPOS="512" WIDTH="1415">
<String CONTENT="La quantité suppléait à la qualité : le vendeur étourdissait l’acheteur" HEIGHT="52" HPOS="252" ID="string_tl_7" VPOS="512" WIDTH="1415"/>
</TextLine>
<TextLine BASELINE="202 618 1668 618" HEIGHT="55" HPOS="202" ID="tl_8" VPOS="563" WIDTH="1466">
<String CONTENT="par le nombre, au point que M. Chasles n’en est pas encore revenu." HEIGHT="55" HPOS="202" ID="string_tl_8" VPOS="563" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="199 665 1671 665" HEIGHT="51" HPOS="199" ID="tl_9" VPOS="614" WIDTH="1472">
<String CONTENT="Si le faussaire dit la vérité, il restera acquis que la plupart de ces 20,000" HEIGHT="51" HPOS="199" ID="string_tl_9" VPOS="614" WIDTH="1472"/>
</TextLine>
<TextLine BASELINE="202 719 1671 719" HEIGHT="56" HPOS="202" ID="tl_10" VPOS="663" WIDTH="1469">
<String CONTENT="pièces sont des copies prises ça et là dans des bouquins ignorés, que" HEIGHT="56" HPOS="202" ID="string_tl_10" VPOS="663" WIDTH="1469"/>
</TextLine>
<TextLine BASELINE="202 767 1672 767" HEIGHT="54" HPOS="202" ID="tl_11" VPOS="713" WIDTH="1470">
<String CONTENT="tout cela est attribué de l’un à I'autre, replâtrés à la hâte, plus ou moins" HEIGHT="54" HPOS="202" ID="string_tl_11" VPOS="713" WIDTH="1470"/>
</TextLine>
<TextLine BASELINE="200 817 1658 817" HEIGHT="52" HPOS="200" ID="tl_12" VPOS="765" WIDTH="1458">
<String CONTENT="habilement. On a déjà trouvé beaucoup de plagiats dans la collection :" HEIGHT="52" HPOS="200" ID="string_tl_12" VPOS="765" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="201 868 1668 868" HEIGHT="59" HPOS="201" ID="tl_13" VPOS="809" WIDTH="1467">
<String CONTENT="on en découvrira bien d’autres en vérifiant, à la Bibliothèque impériale," HEIGHT="59" HPOS="201" ID="string_tl_13" VPOS="809" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="203 919 1667 919" HEIGHT="52" HPOS="203" ID="tl_14" VPOS="867" WIDTH="1464">
<String CONTENT="les fiches de demandes de livres signées par Lucas depuis l'année 1861" HEIGHT="52" HPOS="203" ID="string_tl_14" VPOS="867" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="203 972 598 972" HEIGHT="54" HPOS="203" ID="tl_15" VPOS="918" WIDTH="395">
<String CONTENT="qu’il fait ce métier." HEIGHT="54" HPOS="203" ID="string_tl_15" VPOS="918" WIDTH="395"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="715" HPOS="203" ID="r_2_3" VPOS="978" WIDTH="1475">
<Shape>
<Polygon POINTS="203 978 1678 978 1678 1693 203 1693"/>
</Shape>
<TextLine BASELINE="252 1038 1672 1038" HEIGHT="59" HPOS="252" ID="tl_16" VPOS="979" WIDTH="1420">
<String CONTENT="Relativement au prix qu'aurait coûté la collection, ce n’est point" HEIGHT="59" HPOS="252" ID="string_tl_16" VPOS="979" WIDTH="1420"/>
</TextLine>
<TextLine BASELINE="204 1082 1671 1082" HEIGHT="52" HPOS="204" ID="tl_17" VPOS="1030" WIDTH="1467">
<String CONTENT="M. Chasles qui a donné le chiffre de 150,000 fr. cliché dans les jour¬" HEIGHT="52" HPOS="204" ID="string_tl_17" VPOS="1030" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="205 1132 1666 1132" HEIGHT="51" HPOS="205" ID="tl_18" VPOS="1081" WIDTH="1461">
<String CONTENT="naux. Gageons que la moitié de cette somme n’a pas été déboursée." HEIGHT="51" HPOS="205" ID="string_tl_18" VPOS="1081" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="205 1183 1671 1183" HEIGHT="52" HPOS="205" ID="tl_19" VPOS="1131" WIDTH="1466">
<String CONTENT="Nous avons vu maintes fois M. Chasles acheter des autographes dans" HEIGHT="52" HPOS="205" ID="string_tl_19" VPOS="1131" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="206 1234 1672 1234" HEIGHT="52" HPOS="206" ID="tl_20" VPOS="1182" WIDTH="1466">
<String CONTENT="les ventes, et toujours à des prix minimes, de 1 fr. 25 à 3 fr. Il n’est" HEIGHT="52" HPOS="206" ID="string_tl_20" VPOS="1182" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="206 1285 1673 1285" HEIGHT="52" HPOS="206" ID="tl_21" VPOS="1233" WIDTH="1467">
<String CONTENT="donc pas supposable qu’il ait consacré une aussi forte somme à des" HEIGHT="52" HPOS="206" ID="string_tl_21" VPOS="1233" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="209 1337 1669 1337" HEIGHT="54" HPOS="209" ID="tl_22" VPOS="1283" WIDTH="1460">
<String CONTENT="papiers auxquels il n’attachait pas. d’ailleurs, une haute valeur vénale:" HEIGHT="54" HPOS="209" ID="string_tl_22" VPOS="1283" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="208 1385 1675 1385" HEIGHT="51" HPOS="208" ID="tl_23" VPOS="1334" WIDTH="1467">
<String CONTENT="il le disait lui-même. En examinant chez lui quelques dossiers, nous" HEIGHT="51" HPOS="208" ID="string_tl_23" VPOS="1334" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="208 1437 1675 1437" HEIGHT="52" HPOS="208" ID="tl_24" VPOS="1385" WIDTH="1467">
<String CONTENT="lui fîmes remarquer que les cinq ou six lettres autographes de Shaks¬" HEIGHT="52" HPOS="208" ID="string_tl_24" VPOS="1385" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="212 1489 1675 1489" HEIGHT="53" HPOS="212" ID="tl_25" VPOS="1436" WIDTH="1463">
<String CONTENT="peare qu’il nous montrait vaudraient, si elles étaient authentiques, au" HEIGHT="53" HPOS="212" ID="string_tl_25" VPOS="1436" WIDTH="1463"/>
</TextLine>
<TextLine BASELINE="211 1538 1676 1538" HEIGHT="52" HPOS="211" ID="tl_26" VPOS="1486" WIDTH="1465">
<String CONTENT="moins cent mille francs ; que le Musée britannique, qui ne possédait" HEIGHT="52" HPOS="211" ID="string_tl_26" VPOS="1486" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="211 1590 1676 1590" HEIGHT="53" HPOS="211" ID="tl_27" VPOS="1537" WIDTH="1465">
<String CONTENT="qu’une signature, déjà fort précieuse, n’hésiterait pas à donner cette" HEIGHT="53" HPOS="211" ID="string_tl_27" VPOS="1537" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="211 1641 1677 1641" HEIGHT="53" HPOS="211" ID="tl_28" VPOS="1588" WIDTH="1466">
<String CONTENT="somme. M. Chasles sourit, en nous répondant que tout cela n’avait pour" HEIGHT="53" HPOS="211" ID="string_tl_28" VPOS="1588" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="213 1692 842 1692" HEIGHT="52" HPOS="213" ID="tl_29" VPOS="1640" WIDTH="629">
<String CONTENT="lui qu’une valeur de document." HEIGHT="52" HPOS="213" ID="string_tl_29" VPOS="1640" WIDTH="629"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="259" HPOS="215" ID="r_2_4" VPOS="1699" WIDTH="1464">
<Shape>
<Polygon POINTS="215 1699 1679 1699 1679 1958 215 1958"/>
</Shape>
<TextLine BASELINE="264 1753 1677 1753" HEIGHT="53" HPOS="264" ID="tl_30" VPOS="1700" WIDTH="1413">
<String CONTENT="Bien qu’il soit très-difficile d’estimer la valeur d’une collection sem¬" HEIGHT="53" HPOS="264" ID="string_tl_30" VPOS="1700" WIDTH="1413"/>
</TextLine>
<TextLine BASELINE="217 1804 1677 1804" HEIGHT="53" HPOS="217" ID="tl_31" VPOS="1751" WIDTH="1460">
<String CONTENT="blable, en la supposant vraie, on peut dire que le chiffre d’un million" HEIGHT="53" HPOS="217" ID="string_tl_31" VPOS="1751" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="216 1855 1678 1855" HEIGHT="53" HPOS="216" ID="tl_32" VPOS="1802" WIDTH="1462">
<String CONTENT="ne serait pas exagéré. C’eût donc été non-seulement une bonne fortune" HEIGHT="53" HPOS="216" ID="string_tl_32" VPOS="1802" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="216 1907 1678 1907" HEIGHT="54" HPOS="216" ID="tl_33" VPOS="1853" WIDTH="1462">
<String CONTENT="scientifique, mais une excellente affaire, non pas pour M. Chasles, qui" HEIGHT="54" HPOS="216" ID="string_tl_33" VPOS="1853" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="218 1957 1129 1957" HEIGHT="54" HPOS="218" ID="tl_34" VPOS="1903" WIDTH="911">
<String CONTENT="n’eût jamais vendu, mais pour ses héritiers." HEIGHT="54" HPOS="218" ID="string_tl_34" VPOS="1903" WIDTH="911"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="501" HPOS="215" ID="r_2_5" VPOS="1964" WIDTH="1466">
<Shape>
<Polygon POINTS="215 1964 1681 1964 1681 2465 215 2465"/>
</Shape>
<TextLine BASELINE="269 2018 1677 2018" HEIGHT="53" HPOS="269" ID="tl_35" VPOS="1965" WIDTH="1408">
<String CONTENT="Nous n’avions jamais vu le faussaire et n’avions même pas entendu" HEIGHT="53" HPOS="269" ID="string_tl_35" VPOS="1965" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="218 2066 1679 2066" HEIGHT="50" HPOS="218" ID="tl_36" VPOS="2016" WIDTH="1461">
<String CONTENT="citer son nom; mais il était connu de longue date des libraires et des" HEIGHT="50" HPOS="218" ID="string_tl_36" VPOS="2016" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="218 2120 1677 2120" HEIGHT="53" HPOS="218" ID="tl_37" VPOS="2067" WIDTH="1459">
<String CONTENT="bouquinistes, et sous un jour peu favorable. Il leur achetait des livres" HEIGHT="53" HPOS="218" ID="string_tl_37" VPOS="2067" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="217 2170 1679 2170" HEIGHT="53" HPOS="217" ID="tl_38" VPOS="2117" WIDTH="1462">
<String CONTENT="et des manuscrits pour les placer à des clients à lui, et oubliait souvent" HEIGHT="53" HPOS="217" ID="string_tl_38" VPOS="2117" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="219 2221 1676 2221" HEIGHT="53" HPOS="219" ID="tl_39" VPOS="2168" WIDTH="1457">
<String CONTENT="sa promesse de venir les payer le lendemain. Il ne reparaissait plus :" HEIGHT="53" HPOS="219" ID="string_tl_39" VPOS="2168" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="218 2271 1680 2271" HEIGHT="53" HPOS="218" ID="tl_40" VPOS="2218" WIDTH="1462">
<String CONTENT="où le prendre? Ses allures avaient quelque chose de mystérieux et de" HEIGHT="53" HPOS="218" ID="string_tl_40" VPOS="2218" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="218 2324 1677 2324" HEIGHT="54" HPOS="218" ID="tl_41" VPOS="2270" WIDTH="1459">
<String CONTENT="suspect, et l’on a peine à s’expliquer l’entière et longue confiance que" HEIGHT="54" HPOS="218" ID="string_tl_41" VPOS="2270" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="220 2374 1680 2374" HEIGHT="54" HPOS="220" ID="tl_42" VPOS="2320" WIDTH="1460">
<String CONTENT="lui a accordée M. Chasles. Le Figaro du 1er octobre trace de ce per¬" HEIGHT="54" HPOS="220" ID="string_tl_42" VPOS="2320" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="216 2425 1680 2425" HEIGHT="54" HPOS="216" ID="tl_43" VPOS="2371" WIDTH="1464">
<String CONTENT="sonnage le portrait suivant, assez exact, au dire des personnes qui l’ont" HEIGHT="54" HPOS="216" ID="string_tl_43" VPOS="2371" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="216 2464 368 2464" HEIGHT="27" HPOS="216" ID="tl_44" VPOS="2437" WIDTH="152">
<String CONTENT="connu :" HEIGHT="27" HPOS="216" ID="string_tl_44" VPOS="2437" WIDTH="152"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="186" HPOS="211" ID="r_2_6" VPOS="2493" WIDTH="1469">
<Shape>
<Polygon POINTS="211 2493 1680 2493 1680 2679 211 2679"/>
</Shape>
<TextLine BASELINE="259 2543 1677 2543" HEIGHT="49" HPOS="259" ID="tl_45" VPOS="2494" WIDTH="1418">
<String CONTENT="Né à Lanneray en 1816, Denis-Vrain Lucas a été domestique," HEIGHT="49" HPOS="259" ID="string_tl_45" VPOS="2494" WIDTH="1418"/>
</TextLine>
<TextLine BASELINE="213 2596 1677 2596" HEIGHT="56" HPOS="213" ID="tl_46" VPOS="2540" WIDTH="1464">
<String CONTENT="puis expéditionnaire ; nous le trouvons employé au bureau des" HEIGHT="56" HPOS="213" ID="string_tl_46" VPOS="2540" WIDTH="1464"/>
</TextLine>
<TextLine BASELINE="213 2638 1676 2638" HEIGHT="54" HPOS="213" ID="tl_47" VPOS="2584" WIDTH="1463">
<String CONTENT="hypotèques de Châteaudun, où le goût des paperasses et des" HEIGHT="54" HPOS="213" ID="string_tl_47" VPOS="2584" WIDTH="1463"/>
</TextLine>
<TextLine BASELINE="212 2678 1679 2678" HEIGHT="50" HPOS="212" ID="tl_48" VPOS="2628" WIDTH="1467">
<String CONTENT="parchemins a dû lui venir. C’est à Châteaudun aussi qu’il s’est" HEIGHT="50" HPOS="212" ID="string_tl_48" VPOS="2628" WIDTH="1467"/>
</TextLine>
</TextBlock>
