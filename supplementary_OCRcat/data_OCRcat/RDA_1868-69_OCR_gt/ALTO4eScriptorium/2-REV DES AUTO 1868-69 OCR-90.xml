<TextLine BASELINE="210 286 1661 286" HEIGHT="52" HPOS="210" ID="tl_2" VPOS="234" WIDTH="1451">
<String CONTENT="daient qu'alors Galilée était entièrement aveugle, et que, conséquem¬" HEIGHT="52" HPOS="210" ID="string_tl_2" VPOS="234" WIDTH="1451"/>
</TextLine>
<TextLine BASELINE="212 336 1664 336" HEIGHT="52" HPOS="212" ID="tl_3" VPOS="284" WIDTH="1452">
<String CONTENT="ment, les lettres de sa collection, attribuées à ce grand homme, et" HEIGHT="52" HPOS="212" ID="string_tl_3" VPOS="284" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="210 387 1659 387" HEIGHT="52" HPOS="210" ID="tl_4" VPOS="335" WIDTH="1449">
<String CONTENT="portant une date de cette époque, ou postérieure, étaient apocryphes." HEIGHT="52" HPOS="210" ID="string_tl_4" VPOS="335" WIDTH="1449"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="762" HPOS="206" ID="r_2_2" VPOS="385" WIDTH="1460">
<Shape>
<Polygon POINTS="206 385 1666 385 1666 1147 206 1147"/>
</Shape>
<TextLine BASELINE="262 437 1664 437" HEIGHT="51" HPOS="262" ID="tl_5" VPOS="386" WIDTH="1402">
<String CONTENT="M. Chasles s’empressa de se prévaloir de cette mention. Il pria" HEIGHT="51" HPOS="262" ID="string_tl_5" VPOS="386" WIDTH="1402"/>
</TextLine>
<TextLine BASELINE="213 488 1665 488" HEIGHT="52" HPOS="213" ID="tl_6" VPOS="436" WIDTH="1452">
<String CONTENT="M. Govi, correspondant de l’Institut de France, en Italie, de rechercher" HEIGHT="52" HPOS="213" ID="string_tl_6" VPOS="436" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="210 539 1664 539" HEIGHT="52" HPOS="210" ID="tl_7" VPOS="487" WIDTH="1454">
<String CONTENT="à la Bibliothèque de Florence la lettre en question. Celui-ci répondit" HEIGHT="52" HPOS="210" ID="string_tl_7" VPOS="487" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="212 589 1664 589" HEIGHT="51" HPOS="212" ID="tl_8" VPOS="538" WIDTH="1452">
<String CONTENT="par une note insérée dans les Comptes-rendus de l'Académie des" HEIGHT="51" HPOS="212" ID="string_tl_8" VPOS="538" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="212 641 1662 641" HEIGHT="52" HPOS="212" ID="tl_9" VPOS="589" WIDTH="1450">
<String CONTENT="Sciences, n° 13, qu’il n’avait point trouvé cette lettre dans le recueil" HEIGHT="52" HPOS="212" ID="string_tl_9" VPOS="589" WIDTH="1450"/>
</TextLine>
<TextLine BASELINE="212 690 1660 690" HEIGHT="50" HPOS="212" ID="tl_10" VPOS="640" WIDTH="1448">
<String CONTENT="indiqué. Il ne pouvait accepter une semblable situation. M. Barbéra," HEIGHT="50" HPOS="212" ID="string_tl_10" VPOS="640" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="211 743 1664 743" HEIGHT="54" HPOS="211" ID="tl_11" VPOS="689" WIDTH="1453">
<String CONTENT="l’éminent imprimeur-éditeur de Florence, voulut bien se charger, à ma" HEIGHT="54" HPOS="211" ID="string_tl_11" VPOS="689" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="209 791 1664 791" HEIGHT="50" HPOS="209" ID="tl_12" VPOS="741" WIDTH="1455">
<String CONTENT="demande, de faire faire les démarches nécessaires pour retrouver la" HEIGHT="50" HPOS="209" ID="string_tl_12" VPOS="741" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="211 837 1663 837" HEIGHT="46" HPOS="211" ID="tl_13" VPOS="791" WIDTH="1452">
<String CONTENT="lettre introuvable. Le 14 avril, M. Guinio Carbone, conservateur de la" HEIGHT="46" HPOS="211" ID="string_tl_13" VPOS="791" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="210 894 1664 894" HEIGHT="52" HPOS="210" ID="tl_14" VPOS="842" WIDTH="1454">
<String CONTENT="Bibliothèque nationale, m’adressait un certificat authentique, signé" HEIGHT="52" HPOS="210" ID="string_tl_14" VPOS="842" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="207 938 1663 938" HEIGHT="46" HPOS="207" ID="tl_15" VPOS="892" WIDTH="1456">
<String CONTENT="de lui et de deux de ses confrères, comme témoins, établissant" HEIGHT="46" HPOS="207" ID="string_tl_15" VPOS="892" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="211 995 1664 995" HEIGHT="53" HPOS="211" ID="tl_16" VPOS="942" WIDTH="1453">
<String CONTENT="l’existence de la lettre du 8 novembre 1639, mais où il était dit qu’elle" HEIGHT="53" HPOS="211" ID="string_tl_16" VPOS="942" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="208 1045 1658 1045" HEIGHT="52" HPOS="208" ID="tl_17" VPOS="993" WIDTH="1450">
<String CONTENT="n’était point écrite de la main de Galilée, mais de celle de son neveu," HEIGHT="52" HPOS="208" ID="string_tl_17" VPOS="993" WIDTH="1450"/>
</TextLine>
<TextLine BASELINE="212 1095 1661 1095" HEIGHT="50" HPOS="212" ID="tl_18" VPOS="1045" WIDTH="1449">
<String CONTENT="Vincent Galilée, qui imitait son écriture de manière à dérouter les" HEIGHT="50" HPOS="212" ID="string_tl_18" VPOS="1045" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="208 1146 822 1146" HEIGHT="50" HPOS="208" ID="tl_19" VPOS="1096" WIDTH="614">
<String CONTENT="calligraphes les plus experts." HEIGHT="50" HPOS="208" ID="string_tl_19" VPOS="1096" WIDTH="614"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="258" HPOS="207" ID="r_2_3" VPOS="1160" WIDTH="1459">
<Shape>
<Polygon POINTS="207 1160 1666 1160 1666 1418 207 1418"/>
</Shape>
<TextLine BASELINE="257 1214 1665 1214" HEIGHT="53" HPOS="257" ID="tl_20" VPOS="1161" WIDTH="1408">
<String CONTENT="J’avais pu me tromper, en effet, dans l'examen rapide d’une pièce" HEIGHT="53" HPOS="257" ID="string_tl_20" VPOS="1161" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="208 1265 1664 1265" HEIGHT="52" HPOS="208" ID="tl_21" VPOS="1213" WIDTH="1456">
<String CONTENT="qui eût pu embarrasser les plus lins calligraphes. La pièce était" HEIGHT="52" HPOS="208" ID="string_tl_21" VPOS="1213" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="208 1322 1663 1322" HEIGHT="59" HPOS="208" ID="tl_22" VPOS="1263" WIDTH="1455">
<String CONTENT="retrouvée, c’était l’essentiel pour moi ; j’envoyai le certilicat à" HEIGHT="59" HPOS="208" ID="string_tl_22" VPOS="1263" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="210 1359 1664 1359" HEIGHT="46" HPOS="210" ID="tl_23" VPOS="1313" WIDTH="1454">
<String CONTENT="M. Chasles, lui laissant le soin des vérifications ultérieures. Mais les" HEIGHT="46" HPOS="210" ID="string_tl_23" VPOS="1313" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="208 1417 798 1417" HEIGHT="53" HPOS="208" ID="tl_24" VPOS="1364" WIDTH="590">
<String CONTENT="choses prirent un autre tour." HEIGHT="53" HPOS="208" ID="string_tl_24" VPOS="1364" WIDTH="590"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="560" HPOS="209" ID="r_2_4" VPOS="1431" WIDTH="1462">
<Shape>
<Polygon POINTS="209 1431 1671 1431 1671 1991 209 1991"/>
</Shape>
<TextLine BASELINE="263 1472 1663 1472" HEIGHT="40" HPOS="263" ID="tl_25" VPOS="1432" WIDTH="1400">
<String CONTENT="L’attestation de M. Carbone contenait un extrait de la lettre du" HEIGHT="40" HPOS="263" ID="string_tl_25" VPOS="1432" WIDTH="1400"/>
</TextLine>
<TextLine BASELINE="210 1533 1663 1533" HEIGHT="51" HPOS="210" ID="tl_26" VPOS="1482" WIDTH="1453">
<String CONTENT="5 novembre 1639; il fut reconnu ensuite qu’elle faisait partie du" HEIGHT="51" HPOS="210" ID="string_tl_26" VPOS="1482" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="211 1584 1664 1584" HEIGHT="51" HPOS="211" ID="tl_27" VPOS="1533" WIDTH="1453">
<String CONTENT="recueil publié par M. Alberi. Vite, le faussaire en fabrique trois" HEIGHT="51" HPOS="211" ID="string_tl_27" VPOS="1533" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="210 1636 1668 1636" HEIGHT="53" HPOS="210" ID="tl_28" VPOS="1583" WIDTH="1458">
<String CONTENT="exemplaires, qu’il livre à M. Chasles, l’un comme l’original, les deux" HEIGHT="53" HPOS="210" ID="string_tl_28" VPOS="1583" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="210 1684 1669 1684" HEIGHT="51" HPOS="210" ID="tl_29" VPOS="1633" WIDTH="1459">
<String CONTENT="autres comme des copies. Et voilà notre savant, dont la bonhomie" HEIGHT="51" HPOS="210" ID="string_tl_29" VPOS="1633" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="210 1737 1668 1737" HEIGHT="52" HPOS="210" ID="tl_30" VPOS="1685" WIDTH="1458">
<String CONTENT="éclate ici dans tout son jour, proclamant à l’Académie des Sciences" HEIGHT="52" HPOS="210" ID="string_tl_30" VPOS="1685" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="210 1786 1666 1786" HEIGHT="51" HPOS="210" ID="tl_31" VPOS="1735" WIDTH="1456">
<String CONTENT="qu’il possède l’original et qu’ils n’ont qu’une copie à Florence. Il" HEIGHT="51" HPOS="210" ID="string_tl_31" VPOS="1735" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="211 1837 1665 1837" HEIGHT="51" HPOS="211" ID="tl_32" VPOS="1786" WIDTH="1454">
<String CONTENT="fait faire et envoie à Florence une photographie d’une des trois pièces." HEIGHT="51" HPOS="211" ID="string_tl_32" VPOS="1786" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="211 1888 1664 1888" HEIGHT="52" HPOS="211" ID="tl_33" VPOS="1836" WIDTH="1453">
<String CONTENT="On répond que cela n’a aucun rapport avec l'écriture de Galilée." HEIGHT="52" HPOS="211" ID="string_tl_33" VPOS="1836" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="212 1939 1670 1939" HEIGHT="52" HPOS="212" ID="tl_34" VPOS="1887" WIDTH="1458">
<String CONTENT="Troublé, il déclare qu’il s’est trompé, qu’il a pris une copie pour l’origi¬" HEIGHT="52" HPOS="212" ID="string_tl_34" VPOS="1887" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="212 1990 1413 1990" HEIGHT="52" HPOS="212" ID="tl_35" VPOS="1938" WIDTH="1201">
<String CONTENT="nal, et s’empresse d’envoyer une photographie de celui-ci." HEIGHT="52" HPOS="212" ID="string_tl_35" VPOS="1938" WIDTH="1201"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="659" HPOS="212" ID="r_2_5" VPOS="1988" WIDTH="1460">
<Shape>
<Polygon POINTS="212 1988 1672 1988 1672 2647 212 2647"/>
</Shape>
<TextLine BASELINE="266 2039 1669 2039" HEIGHT="50" HPOS="266" ID="tl_36" VPOS="1989" WIDTH="1403">
<String CONTENT="Une commission fut alors nommée à Florence, mais les vacances" HEIGHT="50" HPOS="266" ID="string_tl_36" VPOS="1989" WIDTH="1403"/>
</TextLine>
<TextLine BASELINE="213 2091 1669 2091" HEIGHT="52" HPOS="213" ID="tl_37" VPOS="2039" WIDTH="1456">
<String CONTENT="ajournèrent un peu l’examen. Pendant ce temps, M. Leverrier lit à" HEIGHT="52" HPOS="213" ID="string_tl_37" VPOS="2039" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="215 2141 1670 2141" HEIGHT="52" HPOS="215" ID="tl_38" VPOS="2089" WIDTH="1455">
<String CONTENT="l’Académie des Sciences une charge à fond de train contre la fameuse" HEIGHT="52" HPOS="215" ID="string_tl_38" VPOS="2089" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="213 2193 1669 2193" HEIGHT="52" HPOS="213" ID="tl_39" VPOS="2141" WIDTH="1456">
<String CONTENT="collection, reprenant habilement les arguments de M. Faugère et y" HEIGHT="52" HPOS="213" ID="string_tl_39" VPOS="2141" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="214 2243 1671 2243" HEIGHT="52" HPOS="214" ID="tl_40" VPOS="2191" WIDTH="1457">
<String CONTENT="ajoutant des démonstrations scientifiques irrésistibles. M. Chasles com¬" HEIGHT="52" HPOS="214" ID="string_tl_40" VPOS="2191" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="216 2293 1670 2293" HEIGHT="51" HPOS="216" ID="tl_41" VPOS="2242" WIDTH="1454">
<String CONTENT="mençait à être inquiet, et sa foi s’ébranlait. Il conçut des doutes sur les" HEIGHT="51" HPOS="216" ID="string_tl_41" VPOS="2242" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="214 2355 1671 2355" HEIGHT="72" HPOS="214" ID="tl_42" VPOS="2283" WIDTH="1457">
<String CONTENT="contés que lui faisait son vendeur sur la provenance de toutes ces" HEIGHT="72" HPOS="214" ID="string_tl_42" VPOS="2283" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="216 2396 1671 2396" HEIGHT="53" HPOS="216" ID="tl_43" VPOS="2343" WIDTH="1455">
<String CONTENT="pièces, Quinze jours avant sa déclaration à rAcadémie, il prit, auprès" HEIGHT="53" HPOS="216" ID="string_tl_43" VPOS="2343" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="214 2446 1671 2446" HEIGHT="53" HPOS="214" ID="tl_44" VPOS="2393" WIDTH="1457">
<String CONTENT="d'une personne très au courant des anciennes collections de manuscrits" HEIGHT="53" HPOS="214" ID="string_tl_44" VPOS="2393" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="215 2497 1668 2497" HEIGHT="53" HPOS="215" ID="tl_45" VPOS="2444" WIDTH="1453">
<String CONTENT="et d’autographes, des renseignements sur les amateurs qui auraient pu," HEIGHT="53" HPOS="215" ID="string_tl_45" VPOS="2444" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="215 2547 1670 2547" HEIGHT="52" HPOS="215" ID="tl_46" VPOS="2495" WIDTH="1455">
<String CONTENT="au siècle dernier, posséder des documents dans le genre des siens. On" HEIGHT="52" HPOS="215" ID="string_tl_46" VPOS="2495" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="215 2606 1671 2606" HEIGHT="60" HPOS="215" ID="tl_47" VPOS="2546" WIDTH="1456">
<String CONTENT="ne put, naturellement, lui donner aucune lumière à ce sujet. Dans la" HEIGHT="60" HPOS="215" ID="string_tl_47" VPOS="2546" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="213 2646 1670 2646" HEIGHT="50" HPOS="213" ID="tl_48" VPOS="2596" WIDTH="1457">
<String CONTENT="conversation il cita le nom de Lucas, parfaitement connu de son inter¬" HEIGHT="50" HPOS="213" ID="string_tl_48" VPOS="2596" WIDTH="1457"/>
</TextLine>
</TextBlock>
