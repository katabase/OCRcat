</Shape>
<TextLine BASELINE="168 1178 1522 1178" HEIGHT="40" HPOS="168" ID="tl_23" VPOS="1138" WIDTH="1354">
<String CONTENT="« à tout risque, et avec l’autorité que leur aurait donné leur qualité d'hommes" HEIGHT="40" HPOS="168" ID="string_tl_23" VPOS="1138" WIDTH="1354"/>
</TextLine>
<TextLine BASELINE="165 1217 1522 1217" HEIGHT="40" HPOS="165" ID="tl_24" VPOS="1177" WIDTH="1357">
<String CONTENT="élus par le suffrage universel; » mais le gouvernement impérial ayant paré le" HEIGHT="40" HPOS="165" ID="string_tl_24" VPOS="1177" WIDTH="1357"/>
</TextLine>
<TextLine BASELINE="168 1257 1522 1257" HEIGHT="41" HPOS="168" ID="tl_25" VPOS="1216" WIDTH="1354">
<String CONTENT="coup en soumettant les candidats à l’obligation préalable du serment, il ne reste" HEIGHT="41" HPOS="168" ID="string_tl_25" VPOS="1216" WIDTH="1354"/>
</TextLine>
<TextLine BASELINE="170 1295 1522 1295" HEIGHT="39" HPOS="170" ID="tl_26" VPOS="1256" WIDTH="1352">
<String CONTENT="plus qu’une seule chose à faire à ceux qui, comme lui, ne veulent pas prêter ser¬" HEIGHT="39" HPOS="170" ID="string_tl_26" VPOS="1256" WIDTH="1352"/>
</TextLine>
<TextLine BASELINE="170 1334 1524 1334" HEIGHT="40" HPOS="170" ID="tl_27" VPOS="1294" WIDTH="1354">
<String CONTENT="ment: refuser la candidature. « Je le répète, dit-il en terminant, ils doivent" HEIGHT="40" HPOS="170" ID="string_tl_27" VPOS="1294" WIDTH="1354"/>
</TextLine>
<TextLine BASELINE="170 1375 1522 1375" HEIGHT="41" HPOS="170" ID="tl_28" VPOS="1334" WIDTH="1352">
<String CONTENT="désirer qu’on leur fournisse l’occasion d’un refus public, dont le résultat moral" HEIGHT="41" HPOS="170" ID="string_tl_28" VPOS="1334" WIDTH="1352"/>
</TextLine>
<TextLine BASELINE="171 1411 719 1411" HEIGHT="38" HPOS="171" ID="tl_29" VPOS="1373" WIDTH="548">
