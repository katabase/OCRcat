<TextLine BASELINE="205 1023 1519 1023" HEIGHT="40" HPOS="205" ID="tl_19" VPOS="983" WIDTH="1314">
<String CONTENT="Original d'une importante lettre politique qui a été publiée dans les journaux" HEIGHT="40" HPOS="205" ID="string_tl_19" VPOS="983" WIDTH="1314"/>
</TextLine>
<TextLine BASELINE="168 1061 1519 1061" HEIGHT="41" HPOS="168" ID="tl_20" VPOS="1020" WIDTH="1351">
<String CONTENT="et a produit une grande sensation. Il n’adopte point le procédé de l'Union libé¬" HEIGHT="41" HPOS="168" ID="string_tl_20" VPOS="1020" WIDTH="1351"/>
</TextLine>
<TextLine BASELINE="164 1100 1520 1100" HEIGHT="41" HPOS="164" ID="tl_21" VPOS="1059" WIDTH="1356">
<String CONTENT="rale. Si, en 1837, il engageait les adversaires de l’Empire à se faire nommer," HEIGHT="41" HPOS="164" ID="string_tl_21" VPOS="1059" WIDTH="1356"/>
</TextLine>
<TextLine BASELINE="167 1139 1519 1139" HEIGHT="39" HPOS="167" ID="tl_22" VPOS="1100" WIDTH="1352">
<String CONTENT="ce n’était point pour prêter serment, mais pour le refuser en pleine chambre," HEIGHT="39" HPOS="167" ID="string_tl_22" VPOS="1100" WIDTH="1352"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="275" HPOS="164" ID="r_2_9" VPOS="1137" WIDTH="1361">
