<TextLine BASELINE="237 2055 1571 2055" HEIGHT="37" HPOS="237" ID="tl_39" VPOS="2018" WIDTH="1334">
<String CONTENT="Très-intéressante lettre relative à ses travaux sur la panification. Il a trouvé" HEIGHT="37" HPOS="237" ID="string_tl_39" VPOS="2018" WIDTH="1334"/>
</TextLine>
<TextLine BASELINE="200 2096 1570 2096" HEIGHT="39" HPOS="200" ID="tl_40" VPOS="2057" WIDTH="1370">
