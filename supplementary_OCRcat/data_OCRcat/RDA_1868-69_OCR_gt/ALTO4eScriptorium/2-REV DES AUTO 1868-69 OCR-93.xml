<Polygon POINTS="90 522 1561 522 1561 884 90 884"/>
</Shape>
<TextLine BASELINE="133 569 1555 569" HEIGHT="46" HPOS="133" ID="tl_7" VPOS="523" WIDTH="1422">
<String CONTENT="Il trompe en effet, car non-seulement il est convenu, contraire¬" HEIGHT="46" HPOS="133" ID="string_tl_7" VPOS="523" WIDTH="1422"/>
</TextLine>
<TextLine BASELINE="91 617 1553 617" HEIGHT="49" HPOS="91" ID="tl_8" VPOS="568" WIDTH="1462">
<String CONTENT="ment à sa première déclaration, qu’il avait reçu des documents du" HEIGHT="49" HPOS="91" ID="string_tl_8" VPOS="568" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="91 658 1557 658" HEIGHT="45" HPOS="91" ID="tl_9" VPOS="613" WIDTH="1466">
<String CONTENT="comte de Menou, mort en 1862; mais une note de lui, trouvée" HEIGHT="45" HPOS="91" ID="string_tl_9" VPOS="613" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="91 706 1557 706" HEIGHT="49" HPOS="91" ID="tl_10" VPOS="657" WIDTH="1466">
<String CONTENT="dans ses papiers, porte qu’il a reçu en 1861, de ce même comte de" HEIGHT="49" HPOS="91" ID="string_tl_10" VPOS="657" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="91 749 1558 749" HEIGHT="52" HPOS="91" ID="tl_11" VPOS="697" WIDTH="1467">
<String CONTENT="Menou, des documents précieux, au nombre d’une vingtaine de" HEIGHT="52" HPOS="91" ID="string_tl_11" VPOS="697" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="92 800 1557 800" HEIGHT="53" HPOS="92" ID="tl_12" VPOS="747" WIDTH="1465">
<String CONTENT="mille, que le possesseur n’avait pas encore bien explorés, et qu’il" HEIGHT="53" HPOS="92" ID="string_tl_12" VPOS="747" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="94 843 1560 843" HEIGHT="51" HPOS="94" ID="tl_13" VPOS="792" WIDTH="1466">
<String CONTENT="lui cédait en échangé de quelques titres généalogiques et des tra¬" HEIGHT="51" HPOS="94" ID="string_tl_13" VPOS="792" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="92 883 777 883" HEIGHT="46" HPOS="92" ID="tl_14" VPOS="837" WIDTH="685">
<String CONTENT="vaux qu’il avait faits pour lui." HEIGHT="46" HPOS="92" ID="string_tl_14" VPOS="837" WIDTH="685"/>
</TextLine>
</TextBlock>
