<TextLine BASELINE="87 291 1548 291" HEIGHT="49" HPOS="87" ID="tl_1" VPOS="242" WIDTH="1461">
<String CONTENT="de M. Lotellier (mais ce cabinet ne renfermait que des pièces" HEIGHT="49" HPOS="87" ID="string_tl_1" VPOS="242" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="87 334 1316 334" HEIGHT="47" HPOS="87" ID="tl_2" VPOS="287" WIDTH="1229">
