</Shape>
<TextLine BASELINE="137 1118 1564 1118" HEIGHT="45" HPOS="137" ID="tl_19" VPOS="1073" WIDTH="1427">
<String CONTENT="Les documents dont j’ai eu à entretenir l’Académie ne sont" HEIGHT="45" HPOS="137" ID="string_tl_19" VPOS="1073" WIDTH="1427"/>
</TextLine>
<TextLine BASELINE="93 1174 1558 1174" HEIGHT="56" HPOS="93" ID="tl_20" VPOS="1118" WIDTH="1465">
<String CONTENT="qu’une partie de ceux qu’il m’a livrés; il en est beaucoup d’autres" HEIGHT="56" HPOS="93" ID="string_tl_20" VPOS="1118" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="95 1209 1562 1209" HEIGHT="55" HPOS="95" ID="tl_21" VPOS="1154" WIDTH="1467">
<String CONTENT="dont je n’ai point eu à parler, puisque je devais me renfermer dans" HEIGHT="55" HPOS="95" ID="string_tl_21" VPOS="1154" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="96 1255 929 1255" HEIGHT="47" HPOS="96" ID="tl_22" VPOS="1208" WIDTH="833">
<String CONTENT="une question scientifique déterminée." HEIGHT="47" HPOS="96" ID="string_tl_22" VPOS="1208" WIDTH="833"/>
</TextLine>
</TextBlock>
