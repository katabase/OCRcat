</Shape>
<TextLine BASELINE="132 385 1550 385" HEIGHT="47" HPOS="132" ID="tl_3" VPOS="338" WIDTH="1418">
<String CONTENT="Il déclare donc qu’il a fabriqué depuis 1861 les pièces, plus de" HEIGHT="47" HPOS="132" ID="string_tl_3" VPOS="338" WIDTH="1418"/>
</TextLine>
<TextLine BASELINE="87 430 1554 430" HEIGHT="47" HPOS="87" ID="tl_4" VPOS="383" WIDTH="1467">
<String CONTENT="20,000 certainement, qu’il m’a vendues : il déclare aussi qu’il m’a" HEIGHT="47" HPOS="87" ID="string_tl_4" VPOS="383" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="87 475 1554 475" HEIGHT="47" HPOS="87" ID="tl_5" VPOS="428" WIDTH="1467">
<String CONTENT="trompé depuis ce temps; dès lors on doit croire qu’il peut tromper" HEIGHT="47" HPOS="87" ID="string_tl_5" VPOS="428" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="89 508 246 508" HEIGHT="25" HPOS="89" ID="tl_6" VPOS="483" WIDTH="157">
<String CONTENT="encore." HEIGHT="25" HPOS="89" ID="string_tl_6" VPOS="483" WIDTH="157"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="362" HPOS="90" ID="r_1_3" VPOS="522" WIDTH="1471">
