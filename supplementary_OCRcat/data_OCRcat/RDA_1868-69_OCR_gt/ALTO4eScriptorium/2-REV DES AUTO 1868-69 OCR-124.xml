<TextLine BASELINE="338 1134 1675 1134" HEIGHT="38" HPOS="338" ID="tl_21" VPOS="1096" WIDTH="1337">
<String CONTENT="Précieuse lettre relative à sa mission dans l'Inde. Il raconte les efforts qu'il fait" HEIGHT="38" HPOS="338" ID="string_tl_21" VPOS="1096" WIDTH="1337"/>
</TextLine>
<TextLine BASELINE="297 1177 1673 1177" HEIGHT="41" HPOS="297" ID="tl_22" VPOS="1136" WIDTH="1376">
<String CONTENT="pour tirer les établissements français de l’Inde de l'avilissement où ils sont tombés," HEIGHT="41" HPOS="297" ID="string_tl_22" VPOS="1136" WIDTH="1376"/>
</TextLine>
<TextLine BASELINE="297 1217 1675 1217" HEIGHT="42" HPOS="297" ID="tl_23" VPOS="1175" WIDTH="1378">
<String CONTENT="par le fait de cette dégoûtante marine anglaise. Ses propres compatriotes sont" HEIGHT="42" HPOS="297" ID="string_tl_23" VPOS="1175" WIDTH="1378"/>
</TextLine>
<TextLine BASELINE="299 1255 1675 1255" HEIGHT="40" HPOS="299" ID="tl_24" VPOS="1215" WIDTH="1376">
<String CONTENT="les premiers à se déclarer contre lui. Il s’est trouvé obligé d’aller chercher des" HEIGHT="40" HPOS="299" ID="string_tl_24" VPOS="1215" WIDTH="1376"/>
</TextLine>
<TextLine BASELINE="299 1293 1676 1293" HEIGHT="38" HPOS="299" ID="tl_25" VPOS="1255" WIDTH="1377">
<String CONTENT="amis au Thibet, où, malheureusement, la maladie lui a fait perdre le fruit de ses" HEIGHT="38" HPOS="299" ID="string_tl_25" VPOS="1255" WIDTH="1377"/>
</TextLine>
<TextLine BASELINE="301 1332 1677 1332" HEIGHT="38" HPOS="301" ID="tl_26" VPOS="1294" WIDTH="1376">
<String CONTENT="recherches d'histoire naturelle. Il se plaint fort de sir Raffles et de ses Anglais, et" HEIGHT="38" HPOS="301" ID="string_tl_26" VPOS="1294" WIDTH="1376"/>
</TextLine>
<TextLine BASELINE="300 1373 1676 1373" HEIGHT="40" HPOS="300" ID="tl_27" VPOS="1333" WIDTH="1376">
<String CONTENT="commence à comprendre que le métier de naturaliste est un fichu métier ; mais il" HEIGHT="40" HPOS="300" ID="string_tl_27" VPOS="1333" WIDTH="1376"/>
</TextLine>
<TextLine BASELINE="301 1412 1676 1412" HEIGHT="39" HPOS="301" ID="tl_28" VPOS="1373" WIDTH="1375">
<String CONTENT="restera à son poste jusqu'à ce que le travail l’ai tué, ou qu’il ait acquis assez de" HEIGHT="39" HPOS="301" ID="string_tl_28" VPOS="1373" WIDTH="1375"/>
</TextLine>
<TextLine BASELINE="302 1451 929 1451" HEIGHT="37" HPOS="302" ID="tl_29" VPOS="1414" WIDTH="627">
<String CONTENT="considération pour revenir en France." HEIGHT="37" HPOS="302" ID="string_tl_29" VPOS="1414" WIDTH="627"/>
</TextLine>
