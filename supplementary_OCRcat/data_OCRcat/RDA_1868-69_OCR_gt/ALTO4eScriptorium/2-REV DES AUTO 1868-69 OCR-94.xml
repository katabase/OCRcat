<TextLine BASELINE="223 313 1679 313" HEIGHT="52" HPOS="223" ID="tl_2" VPOS="261" WIDTH="1456">
<String CONTENT="sa détention, par la jeune fille, sa compagne de lit à Orléans, qui" HEIGHT="52" HPOS="223" ID="string_tl_2" VPOS="261" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="224 343 1682 343" HEIGHT="37" HPOS="224" ID="tl_3" VPOS="306" WIDTH="1458">
<String CONTENT="était admise à la visiter. Comment le même individu aurait-il" HEIGHT="37" HPOS="224" ID="string_tl_3" VPOS="306" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="224 397 1684 397" HEIGHT="46" HPOS="224" ID="tl_4" VPOS="351" WIDTH="1460">
<String CONTENT="composé, indépendamment de toutes les pièces scientifiques et" HEIGHT="46" HPOS="224" ID="string_tl_4" VPOS="351" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="224 443 1682 443" HEIGHT="49" HPOS="224" ID="tl_5" VPOS="394" WIDTH="1458">
<String CONTENT="autres que l’Académie connaît, toutes les lettres et poésies" HEIGHT="49" HPOS="224" ID="string_tl_5" VPOS="394" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="226 489 1684 489" HEIGHT="48" HPOS="226" ID="tl_6" VPOS="441" WIDTH="1458">
<String CONTENT="françaises de Dante et de Pétrarque particulièrement? On ne peut" HEIGHT="48" HPOS="226" ID="string_tl_6" VPOS="441" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="228 533 1684 533" HEIGHT="47" HPOS="228" ID="tl_7" VPOS="486" WIDTH="1456">
<String CONTENT="dire qu’il les a empruntées des ouvrages imprimés, qui ne enfer¬" HEIGHT="47" HPOS="228" ID="string_tl_7" VPOS="486" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="226 577 1684 577" HEIGHT="46" HPOS="226" ID="tl_8" VPOS="531" WIDTH="1458">
<String CONTENT="ment que des pièces en italien. Si l’on en croit certaines lettres, les" HEIGHT="46" HPOS="226" ID="string_tl_8" VPOS="531" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="227 622 1683 622" HEIGHT="47" HPOS="227" ID="tl_9" VPOS="575" WIDTH="1456">
<String CONTENT="pièces de Pétrarque, de Laure et de Clémence Isaure auraient été" HEIGHT="47" HPOS="227" ID="string_tl_9" VPOS="575" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="226 668 1684 668" HEIGHT="49" HPOS="226" ID="tl_10" VPOS="619" WIDTH="1458">
<String CONTENT="envoyées à Rabelais par son ami Nostradamus, qui les avait re¬" HEIGHT="49" HPOS="226" ID="string_tl_10" VPOS="619" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="226 712 666 712" HEIGHT="56" HPOS="226" ID="tl_11" VPOS="656" WIDTH="440">
<String CONTENT="cueillies à Avignon." HEIGHT="56" HPOS="226" ID="string_tl_11" VPOS="656" WIDTH="440"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="274" HPOS="225" ID="r_2_2" VPOS="720" WIDTH="1462">
<Shape>
<Polygon POINTS="225 720 1687 720 1687 994 225 994"/>
</Shape>
<TextLine BASELINE="267 768 1685 768" HEIGHT="47" HPOS="267" ID="tl_12" VPOS="721" WIDTH="1418">
<String CONTENT="La collection s’étend aux premiers temps de l’ère chrétienne, et" HEIGHT="47" HPOS="267" ID="string_tl_12" VPOS="721" WIDTH="1418"/>
</TextLine>
<TextLine BASELINE="226 813 1686 813" HEIGHT="47" HPOS="226" ID="tl_13" VPOS="766" WIDTH="1460">
<String CONTENT="meme au delà; car il s’y trouve quelques lettres et de nombreuses" HEIGHT="47" HPOS="226" ID="string_tl_13" VPOS="766" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="226 858 1685 858" HEIGHT="46" HPOS="226" ID="tl_14" VPOS="812" WIDTH="1459">
<String CONTENT="notes de Jules César et des empereurs romains; des apôtres, prin¬" HEIGHT="46" HPOS="226" ID="string_tl_14" VPOS="812" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="226 902 1686 902" HEIGHT="45" HPOS="226" ID="tl_15" VPOS="857" WIDTH="1460">
<String CONTENT="cipalement de saint Jérôme; de Boèce, de Cassiodore, de Grégoire" HEIGHT="45" HPOS="226" ID="string_tl_15" VPOS="857" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="228 948 1685 948" HEIGHT="47" HPOS="228" ID="tl_16" VPOS="901" WIDTH="1457">
<String CONTENT="de Tours, de saint Augustin, de plusieurs rois mérovingiens; un" HEIGHT="47" HPOS="228" ID="string_tl_16" VPOS="901" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="229 993 1359 993" HEIGHT="46" HPOS="229" ID="tl_17" VPOS="947" WIDTH="1130">
<String CONTENT="grand nombre de Charlemagne, ainsi que d’Alcuin." HEIGHT="46" HPOS="229" ID="string_tl_17" VPOS="947" WIDTH="1130"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="184" HPOS="226" ID="r_2_3" VPOS="996" WIDTH="1462">
<Shape>
<Polygon POINTS="226 996 1688 996 1688 1180 226 1180"/>
</Shape>
<TextLine BASELINE="270 1043 1684 1043" HEIGHT="46" HPOS="270" ID="tl_18" VPOS="997" WIDTH="1414">
<String CONTENT="Voici, à en croire ces documents, l’origine de ce trésor. L’abbaye" HEIGHT="46" HPOS="270" ID="string_tl_18" VPOS="997" WIDTH="1414"/>
</TextLine>
<TextLine BASELINE="227 1089 1686 1089" HEIGHT="47" HPOS="227" ID="tl_19" VPOS="1042" WIDTH="1459">
<String CONTENT="de Tours était très-riche en documents anciens. Alcuin, qui en fut" HEIGHT="47" HPOS="227" ID="string_tl_19" VPOS="1042" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="229 1131 1687 1131" HEIGHT="44" HPOS="229" ID="tl_20" VPOS="1087" WIDTH="1458">
<String CONTENT="abbé, l’enrichit encore, en faisant rechercher en Italie et dans les" HEIGHT="44" HPOS="229" ID="string_tl_20" VPOS="1087" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="234 1179 1336 1179" HEIGHT="47" HPOS="234" ID="tl_21" VPOS="1132" WIDTH="1102">
<String CONTENT="pays étrangers tout ce qui pouvait s’y rencontrer." HEIGHT="47" HPOS="234" ID="string_tl_21" VPOS="1132" WIDTH="1102"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="364" HPOS="230" ID="r_2_4" VPOS="1181" WIDTH="1464">
<Shape>
<Polygon POINTS="230 1181 1694 1181 1694 1545 230 1545"/>
</Shape>
<TextLine BASELINE="273 1230 1688 1230" HEIGHT="48" HPOS="273" ID="tl_22" VPOS="1182" WIDTH="1415">
<String CONTENT="Rabelais, qui était grand amateur de pièces de ce genre, et qui" HEIGHT="48" HPOS="273" ID="string_tl_22" VPOS="1182" WIDTH="1415"/>
</TextLine>
<TextLine BASELINE="231 1277 1691 1277" HEIGHT="49" HPOS="231" ID="tl_23" VPOS="1228" WIDTH="1460">
<String CONTENT="était môme stimulé dans ses recherches par François Ier et Mar¬" HEIGHT="49" HPOS="231" ID="string_tl_23" VPOS="1228" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="231 1320 1689 1320" HEIGHT="48" HPOS="231" ID="tl_24" VPOS="1272" WIDTH="1458">
<String CONTENT="guerite d’Angoulême, connaissait ces archives de l’abbaye de Tours," HEIGHT="48" HPOS="231" ID="string_tl_24" VPOS="1272" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="231 1365 1689 1365" HEIGHT="47" HPOS="231" ID="tl_25" VPOS="1318" WIDTH="1458">
<String CONTENT="et en fit faire des copies et des traductions en nombre considérable." HEIGHT="47" HPOS="231" ID="string_tl_25" VPOS="1318" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="231 1409 1693 1409" HEIGHT="46" HPOS="231" ID="tl_26" VPOS="1363" WIDTH="1462">
<String CONTENT="Tout cela se trouvait à son ermitage de Langey, dépendant de la" HEIGHT="46" HPOS="231" ID="string_tl_26" VPOS="1363" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="233 1454 1693 1454" HEIGHT="47" HPOS="233" ID="tl_27" VPOS="1407" WIDTH="1460">
<String CONTENT="propriété des Du Belley, et aurait passé dans la collection de l’in¬" HEIGHT="47" HPOS="233" ID="string_tl_27" VPOS="1407" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="234 1499 1690 1499" HEIGHT="47" HPOS="234" ID="tl_28" VPOS="1452" WIDTH="1456">
<String CONTENT="tendant Foucault, mort dans les premiers temps du siècle dernier," HEIGHT="47" HPOS="234" ID="string_tl_28" VPOS="1452" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="235 1544 1116 1544" HEIGHT="46" HPOS="235" ID="tl_29" VPOS="1498" WIDTH="881">
<String CONTENT="membre de l’Académie des Inscriptions." HEIGHT="46" HPOS="235" ID="string_tl_29" VPOS="1498" WIDTH="881"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="410" HPOS="234" ID="r_2_5" VPOS="1547" WIDTH="1462">
<Shape>
<Polygon POINTS="234 1547 1696 1547 1696 1957 234 1957"/>
</Shape>
<TextLine BASELINE="279 1597 1693 1597" HEIGHT="49" HPOS="279" ID="tl_30" VPOS="1548" WIDTH="1414">
<String CONTENT="Je ne me porte point garant de ces pièces. Quelles qu’elles" HEIGHT="49" HPOS="279" ID="string_tl_30" VPOS="1548" WIDTH="1414"/>
</TextLine>
<TextLine BASELINE="236 1640 1693 1640" HEIGHT="46" HPOS="236" ID="tl_31" VPOS="1594" WIDTH="1457">
<String CONTENT="soient, il est certain que leur composition, si elles ne sont pas" HEIGHT="46" HPOS="236" ID="string_tl_31" VPOS="1594" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="235 1691 1692 1691" HEIGHT="53" HPOS="235" ID="tl_32" VPOS="1638" WIDTH="1457">
<String CONTENT="originales, a dû exiger un long travail, de nombreux matériaux;" HEIGHT="53" HPOS="235" ID="string_tl_32" VPOS="1638" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="235 1731 1694 1731" HEIGHT="48" HPOS="235" ID="tl_33" VPOS="1683" WIDTH="1459">
<String CONTENT="et si l'on considère qu’elles s’ajoutent à tant d’autres, de tous les" HEIGHT="48" HPOS="235" ID="string_tl_33" VPOS="1683" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="237 1782 1695 1782" HEIGHT="61" HPOS="237" ID="tl_34" VPOS="1721" WIDTH="1458">
<String CONTENT="temps jusqu’au siècle dernier, et traitant de tant de matières" HEIGHT="61" HPOS="237" ID="string_tl_34" VPOS="1721" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="238 1821 1693 1821" HEIGHT="48" HPOS="238" ID="tl_35" VPOS="1773" WIDTH="1455">
<String CONTENT="différentes, on ne peut croire qu’elles soient l’œuvre d’un seul" HEIGHT="48" HPOS="238" ID="string_tl_35" VPOS="1773" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="237 1865 1693 1865" HEIGHT="46" HPOS="237" ID="tl_36" VPOS="1819" WIDTH="1456">
<String CONTENT="individu, d’un seul fabricateur, qui du reste ne sait ni le latin, ni" HEIGHT="46" HPOS="237" ID="string_tl_36" VPOS="1819" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="238 1917 1695 1917" HEIGHT="53" HPOS="238" ID="tl_37" VPOS="1864" WIDTH="1457">
<String CONTENT="l’italien, ni aucune partie des mathématiques ou des autres scien¬" HEIGHT="53" HPOS="238" ID="string_tl_37" VPOS="1864" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="239 1956 1649 1956" HEIGHT="47" HPOS="239" ID="tl_38" VPOS="1909" WIDTH="1410">
<String CONTENT="ces sur lesquelles roule une partie considérable des documents." HEIGHT="47" HPOS="239" ID="string_tl_38" VPOS="1909" WIDTH="1410"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="83" HPOS="239" ID="r_2_6" VPOS="1959" WIDTH="1460">
<Shape>
<Polygon POINTS="239 1959 1699 1959 1699 2042 239 2042"/>
</Shape>
<TextLine BASELINE="281 2006 1698 2006" HEIGHT="46" HPOS="281" ID="tl_39" VPOS="1960" WIDTH="1417">
<String CONTENT="Il y a donc un mystère à pénétrer, et jusque-là il n’y a rien à" HEIGHT="46" HPOS="281" ID="string_tl_39" VPOS="1960" WIDTH="1417"/>
</TextLine>
<TextLine BASELINE="240 2041 773 2041" HEIGHT="45" HPOS="240" ID="tl_40" VPOS="1996" WIDTH="533">
<String CONTENT="conclure avec certitude." HEIGHT="45" HPOS="240" ID="string_tl_40" VPOS="1996" WIDTH="533"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="356" HPOS="240" ID="r_2_7" VPOS="2072" WIDTH="1459">
<Shape>
<Polygon POINTS="240 2072 1699 2072 1699 2428 240 2428"/>
</Shape>
<TextLine BASELINE="289 2124 1697 2124" HEIGHT="51" HPOS="289" ID="tl_41" VPOS="2073" WIDTH="1408">
<String CONTENT="La dernière phase de cette communication a fortement indisposé" HEIGHT="51" HPOS="289" ID="string_tl_41" VPOS="2073" WIDTH="1408"/>
</TextLine>
<TextLine BASELINE="241 2175 1696 2175" HEIGHT="52" HPOS="241" ID="tl_42" VPOS="2123" WIDTH="1455">
<String CONTENT="l’Académie, qui, au lieu de tous ces détails, eût préféré une déclara¬" HEIGHT="52" HPOS="241" ID="string_tl_42" VPOS="2123" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="242 2226 1694 2226" HEIGHT="52" HPOS="242" ID="tl_43" VPOS="2174" WIDTH="1452">
<String CONTENT="tion pure et simple, mais catégorique. Dès le lendemain, M. Dumas," HEIGHT="52" HPOS="242" ID="string_tl_43" VPOS="2174" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="244 2276 1696 2276" HEIGHT="52" HPOS="244" ID="tl_44" VPOS="2224" WIDTH="1452">
<String CONTENT="secrétaire perpétuel, en pria M. Chasles, par lettre, l’avertissant qu’il" HEIGHT="52" HPOS="244" ID="string_tl_44" VPOS="2224" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="241 2326 1698 2326" HEIGHT="51" HPOS="241" ID="tl_45" VPOS="2275" WIDTH="1457">
<String CONTENT="y aurait une protestation s’il ne se conformait pas à ce désir. M. Chasles" HEIGHT="51" HPOS="241" ID="string_tl_45" VPOS="2275" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="242 2378 1697 2378" HEIGHT="52" HPOS="242" ID="tl_46" VPOS="2326" WIDTH="1455">
<String CONTENT="insista pour l’insertion intégrale de sa note au Comtep-rendu, et la" HEIGHT="52" HPOS="242" ID="string_tl_46" VPOS="2326" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="245 2427 1298 2427" HEIGHT="49" HPOS="245" ID="tl_47" VPOS="2378" WIDTH="1053">
<String CONTENT="protestation eut lieu dans la séance suivante, le 20." HEIGHT="49" HPOS="245" ID="string_tl_47" VPOS="2378" WIDTH="1053"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="256" HPOS="240" ID="r_2_8" VPOS="2427" WIDTH="1460">
<Shape>
<Polygon POINTS="240 2427 1700 2427 1700 2683 240 2683"/>
</Shape>
<TextLine BASELINE="292 2479 1698 2479" HEIGHT="51" HPOS="292" ID="tl_48" VPOS="2428" WIDTH="1406">
<String CONTENT="M. Dumas, dans un discours énergique, a voulu dégager la respon¬" HEIGHT="51" HPOS="292" ID="string_tl_48" VPOS="2428" WIDTH="1406"/>
</TextLine>
<TextLine BASELINE="241 2530 1696 2530" HEIGHT="51" HPOS="241" ID="tl_49" VPOS="2479" WIDTH="1455">
<String CONTENT="sabilité de l’Académie, qui, par déférence pour le caractère de M. Chasles," HEIGHT="51" HPOS="241" ID="string_tl_49" VPOS="2479" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="243 2581 1699 2581" HEIGHT="53" HPOS="243" ID="tl_50" VPOS="2528" WIDTH="1456">
<String CONTENT="a laissé se prolonger devant elle pendant plus de deux ans, « une" HEIGHT="53" HPOS="243" ID="string_tl_50" VPOS="2528" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="242 2632 1699 2632" HEIGHT="52" HPOS="242" ID="tl_51" VPOS="2580" WIDTH="1457">
<String CONTENT="discussion qu’elle ne supportait qu’avec impatience et douleur. » Il a" HEIGHT="52" HPOS="242" ID="string_tl_51" VPOS="2580" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="243 2682 1698 2682" HEIGHT="51" HPOS="243" ID="tl_52" VPOS="2631" WIDTH="1455">
<String CONTENT="réparé, au nom de ce corps savant, l’outrage fait à deux grands hommes" HEIGHT="51" HPOS="243" ID="string_tl_52" VPOS="2631" WIDTH="1455"/>
</TextLine>
