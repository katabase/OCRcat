<TextLine BASELINE="185 1037 1586 1037" HEIGHT="51" HPOS="185" ID="tl_15" VPOS="986" WIDTH="1401">
<String CONTENT="M. Addington est un curieux universel : on lui a vu, successivement," HEIGHT="51" HPOS="185" ID="string_tl_15" VPOS="986" WIDTH="1401"/>
</TextLine>
<TextLine BASELINE="136 1087 1591 1087" HEIGHT="51" HPOS="136" ID="tl_16" VPOS="1036" WIDTH="1455">
<String CONTENT="former des collections de tableaux, d’estampes, de médailles, de pote¬" HEIGHT="51" HPOS="136" ID="string_tl_16" VPOS="1036" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="136 1137 1592 1137" HEIGHT="51" HPOS="136" ID="tl_17" VPOS="1086" WIDTH="1456">
<String CONTENT="ries, etc. Quand il est fatigué d’un genre de curiosités, il les revend et" HEIGHT="51" HPOS="136" ID="string_tl_17" VPOS="1086" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="139 1186 1588 1186" HEIGHT="51" HPOS="139" ID="tl_18" VPOS="1135" WIDTH="1449">
<String CONTENT="passe à d’autres amours. Sa collection d’autographes est toute récente." HEIGHT="51" HPOS="139" ID="string_tl_18" VPOS="1135" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="140 1237 1588 1237" HEIGHT="53" HPOS="140" ID="tl_19" VPOS="1184" WIDTH="1448">
<String CONTENT="Il l’a commencée, sous nos yeux, le 26 avril dernier, à la vente Young." HEIGHT="53" HPOS="140" ID="string_tl_19" VPOS="1184" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="136 1287 1590 1287" HEIGHT="52" HPOS="136" ID="tl_20" VPOS="1235" WIDTH="1454">
<String CONTENT="Ses débuts promettent. Il n’achète que des pièces rares, et surtout" HEIGHT="52" HPOS="136" ID="string_tl_20" VPOS="1235" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="139 1337 1595 1337" HEIGHT="52" HPOS="139" ID="tl_21" VPOS="1285" WIDTH="1456">
<String CONTENT="celles qui ont de l’intérêt : quand elles lui conviennent, le prix n’y fait" HEIGHT="52" HPOS="139" ID="string_tl_21" VPOS="1285" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="141 1387 1595 1387" HEIGHT="52" HPOS="141" ID="tl_22" VPOS="1335" WIDTH="1454">
<String CONTENT="rien. Il lâche rarement, quelle que soit l’altitude des enchères. Nous" HEIGHT="52" HPOS="141" ID="string_tl_22" VPOS="1335" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="141 1437 1589 1437" HEIGHT="52" HPOS="141" ID="tl_23" VPOS="1385" WIDTH="1448">
<String CONTENT="lui avons pourtant quelquefois entendu prononcer le no more (assez)," HEIGHT="52" HPOS="141" ID="string_tl_23" VPOS="1385" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="141 1485 1592 1485" HEIGHT="50" HPOS="141" ID="tl_24" VPOS="1435" WIDTH="1451">
<String CONTENT="en riant aux éclats d’avoir, comme on dit dans ces sortes de tournois," HEIGHT="50" HPOS="141" ID="string_tl_24" VPOS="1435" WIDTH="1451"/>
</TextLine>
<TextLine BASELINE="142 1536 1489 1536" HEIGHT="52" HPOS="142" ID="tl_25" VPOS="1484" WIDTH="1347">
<String CONTENT="fait avaler un bouillon à son adversaire, qui le lui rendait bien." HEIGHT="52" HPOS="142" ID="string_tl_25" VPOS="1484" WIDTH="1347"/>
</TextLine>
