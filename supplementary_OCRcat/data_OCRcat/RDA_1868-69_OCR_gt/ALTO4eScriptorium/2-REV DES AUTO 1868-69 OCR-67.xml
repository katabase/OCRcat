<TextLine BASELINE="123 325 1573 325" HEIGHT="52" HPOS="123" ID="tl_1" VPOS="273" WIDTH="1450">
<String CONTENT="commissions sans limite ; l’autre, qui assistait lui-même aux enchères," HEIGHT="52" HPOS="123" ID="string_tl_1" VPOS="273" WIDTH="1450"/>
</TextLine>
<TextLine BASELINE="123 374 1576 374" HEIGHT="52" HPOS="123" ID="tl_2" VPOS="322" WIDTH="1453">
<String CONTENT="est un type curieux qui mérite un coup de crayon. C’est un très-riche" HEIGHT="52" HPOS="123" ID="string_tl_2" VPOS="322" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="125 424 1574 424" HEIGHT="52" HPOS="125" ID="tl_3" VPOS="372" WIDTH="1449">
<String CONTENT="marchand de drap de la cité, nommé Addington. Agé d’environ 65 ans," HEIGHT="52" HPOS="125" ID="string_tl_3" VPOS="372" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="124 484 1578 484" HEIGHT="63" HPOS="124" ID="tl_4" VPOS="421" WIDTH="1454">
<String CONTENT="grand et bien proportionné, il porte toute la barbe. Son vêlement est" HEIGHT="63" HPOS="124" ID="string_tl_4" VPOS="421" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="124 524 1579 524" HEIGHT="62" HPOS="124" ID="tl_5" VPOS="462" WIDTH="1455">
<String CONTENT="d’une simplicité presque sordide. Son nez est surmonté de lunettes, sur" HEIGHT="62" HPOS="124" ID="string_tl_5" VPOS="462" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="128 574 1577 574" HEIGHT="54" HPOS="128" ID="tl_6" VPOS="520" WIDTH="1449">
<String CONTENT="lesquelles il regarde quand il porte les yeux devant lui, et son chef," HEIGHT="54" HPOS="128" ID="string_tl_6" VPOS="520" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="127 624 1581 624" HEIGHT="55" HPOS="127" ID="tl_7" VPOS="569" WIDTH="1454">
<String CONTENT="affublé d’un chapeau souple, noir, à larges bords, comme en portent" HEIGHT="55" HPOS="127" ID="string_tl_7" VPOS="569" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="129 672 1583 672" HEIGHT="52" HPOS="129" ID="tl_8" VPOS="620" WIDTH="1454">
<String CONTENT="les Auvergnats, dont il a d’ailleurs toute la physionomie, car on ne re¬" HEIGHT="52" HPOS="129" ID="string_tl_8" VPOS="620" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="129 723 1584 723" HEIGHT="52" HPOS="129" ID="tl_9" VPOS="671" WIDTH="1455">
<String CONTENT="connaît rien en lui du type anglais. Il fait du reste l’effet d’un excel¬" HEIGHT="52" HPOS="129" ID="string_tl_9" VPOS="671" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="130 773 1582 773" HEIGHT="53" HPOS="130" ID="tl_10" VPOS="720" WIDTH="1452">
<String CONTENT="lent homme, très-jovial. Rien de plus réjouissant que le rire homérique" HEIGHT="53" HPOS="130" ID="string_tl_10" VPOS="720" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="130 822 1584 822" HEIGHT="52" HPOS="130" ID="tl_11" VPOS="770" WIDTH="1454">
<String CONTENT="dont il éclatait chaque fois qu’il enlevait à son adversaire, à grand" HEIGHT="52" HPOS="130" ID="string_tl_11" VPOS="770" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="131 872 1584 872" HEIGHT="52" HPOS="131" ID="tl_12" VPOS="820" WIDTH="1453">
<String CONTENT="coup de guinées, une pièce sur laquelle il avait jeté son dévolu. Il" HEIGHT="52" HPOS="131" ID="string_tl_12" VPOS="820" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="131 921 1589 921" HEIGHT="51" HPOS="131" ID="tl_13" VPOS="870" WIDTH="1458">
<String CONTENT="communiquait, naturellement, son rire à tout le monde, et il était heu¬" HEIGHT="51" HPOS="131" ID="string_tl_13" VPOS="870" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="136 960 779 960" HEIGHT="40" HPOS="136" ID="tl_14" VPOS="920" WIDTH="643">
<String CONTENT="reux d’être le héros de la salle." HEIGHT="40" HPOS="136" ID="string_tl_14" VPOS="920" WIDTH="643"/>
</TextLine>
</TextBlock>
