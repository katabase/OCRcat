<TextLine BASELINE="275 1767 1691 1767" HEIGHT="51" HPOS="275" ID="tl_30" VPOS="1716" WIDTH="1416">
<String CONTENT="Il vient de se faire à Londres, coup sur coup, deux ventes d’autogra¬" HEIGHT="51" HPOS="275" ID="string_tl_30" VPOS="1716" WIDTH="1416"/>
</TextLine>
<TextLine BASELINE="227 1817 1692 1817" HEIGHT="51" HPOS="227" ID="tl_31" VPOS="1766" WIDTH="1465">
<String CONTENT="phes importantes, comme il n’y en avait pas eu depuis plusieurs an¬" HEIGHT="51" HPOS="227" ID="string_tl_31" VPOS="1766" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="229 1866 1690 1866" HEIGHT="50" HPOS="229" ID="tl_32" VPOS="1816" WIDTH="1461">
<String CONTENT="nées. La première, celle de M. Young, qui a eu lieu du 26 avril au" HEIGHT="50" HPOS="229" ID="string_tl_32" VPOS="1816" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="227 1917 1688 1917" HEIGHT="52" HPOS="227" ID="tl_33" VPOS="1865" WIDTH="1461">
<String CONTENT="1er mai, a produit, en six jours, 2,318 liv , 16 shel., 6 pence (57,970 f." HEIGHT="52" HPOS="227" ID="string_tl_33" VPOS="1865" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="231 1967 1693 1967" HEIGHT="52" HPOS="231" ID="tl_34" VPOS="1915" WIDTH="1462">
<String CONTENT="60 c.); la seconde, celle de feu M. John Dillon, a produit, en cinq" HEIGHT="52" HPOS="231" ID="string_tl_34" VPOS="1915" WIDTH="1462"/>
</TextLine>
<TextLine BASELINE="228 2017 1692 2017" HEIGHT="52" HPOS="228" ID="tl_35" VPOS="1965" WIDTH="1464">
<String CONTENT="jours, du 10 au 15 juin, la somme de 3,080 liv., 6 pence (77,000 fr. 60)." HEIGHT="52" HPOS="228" ID="string_tl_35" VPOS="1965" WIDTH="1464"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="540" HPOS="234" ID="r_1_5" VPOS="2024" WIDTH="1473">
<Shape>
<Polygon POINTS="234 2024 1707 2024 1707 2564 234 2564"/>
</Shape>
<TextLine BASELINE="286 2077 1693 2077" HEIGHT="52" HPOS="286" ID="tl_36" VPOS="2025" WIDTH="1407">
<String CONTENT="Ce sont surtout les autographes anglais qui ont atteint des prix" HEIGHT="52" HPOS="286" ID="string_tl_36" VPOS="2025" WIDTH="1407"/>
</TextLine>
<TextLine BASELINE="235 2132 1696 2132" HEIGHT="57" HPOS="235" ID="tl_37" VPOS="2075" WIDTH="1461">
<String CONTENT="élevés. Nous avions déjà assisté, à Londres, à un certain nombre de" HEIGHT="57" HPOS="235" ID="string_tl_37" VPOS="2075" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="237 2176 1698 2176" HEIGHT="51" HPOS="237" ID="tl_38" VPOS="2125" WIDTH="1461">
<String CONTENT="ventes, mais nous n’avions jamais vu ces mêmes autographes arriver à" HEIGHT="51" HPOS="237" ID="string_tl_38" VPOS="2125" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="238 2226 1696 2226" HEIGHT="51" HPOS="238" ID="tl_39" VPOS="2175" WIDTH="1458">
<String CONTENT="des prix aussi exorbitants. A quoi attribuer ce résultat splendide," HEIGHT="51" HPOS="238" ID="string_tl_39" VPOS="2175" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="239 2276 1700 2276" HEIGHT="51" HPOS="239" ID="tl_40" VPOS="2225" WIDTH="1461">
<String CONTENT="vraiment extraordinaire? Sans doute, on doit compter pour quelque" HEIGHT="51" HPOS="239" ID="string_tl_40" VPOS="2225" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="241 2325 1699 2325" HEIGHT="51" HPOS="241" ID="tl_41" VPOS="2274" WIDTH="1458">
<String CONTENT="chose les noms des deux collectionneurs, très connus en Angleterre ;" HEIGHT="51" HPOS="241" ID="string_tl_41" VPOS="2274" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="244 2376 1702 2376" HEIGHT="51" HPOS="244" ID="tl_42" VPOS="2325" WIDTH="1458">
<String CONTENT="mais cela ne suffit point pour expliquer une hausse hors de toute pro¬" HEIGHT="51" HPOS="244" ID="string_tl_42" VPOS="2325" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="243 2425 1701 2425" HEIGHT="51" HPOS="243" ID="tl_43" VPOS="2374" WIDTH="1458">
<String CONTENT="portion. La cause en est dans ce seul fait : deux amateurs étaient en" HEIGHT="51" HPOS="243" ID="string_tl_43" VPOS="2374" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="246 2475 1704 2475" HEIGHT="51" HPOS="246" ID="tl_44" VPOS="2424" WIDTH="1458">
<String CONTENT="lutte, voulant à tout prix les pièces qui leur convenaient, sans tenir" HEIGHT="51" HPOS="246" ID="string_tl_44" VPOS="2424" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="245 2525 1706 2525" HEIGHT="52" HPOS="245" ID="tl_45" VPOS="2473" WIDTH="1461">
<String CONTENT="compte des guinées. Ils ont ainsi, sans exagérer, fait gagner trente" HEIGHT="52" HPOS="245" ID="string_tl_45" VPOS="2473" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="244 2563 1114 2563" HEIGHT="40" HPOS="244" ID="tl_46" VPOS="2523" WIDTH="870">
<String CONTENT="mille francs à chacune de ces deux ventes" HEIGHT="40" HPOS="244" ID="string_tl_46" VPOS="2523" WIDTH="870"/>
</TextLine>
</TextBlock>
