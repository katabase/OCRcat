</Shape>
<TextLine BASELINE="162 2242 1558 2242" HEIGHT="52" HPOS="162" ID="tl_42" VPOS="2190" WIDTH="1396">
<String CONTENT="Le 29 avril dernier, les amateurs d’autographes perdaient un de" HEIGHT="52" HPOS="162" ID="string_tl_42" VPOS="2190" WIDTH="1396"/>
</TextLine>
<TextLine BASELINE="111 2296 1557 2296" HEIGHT="56" HPOS="111" ID="tl_43" VPOS="2240" WIDTH="1446">
<String CONTENT="leurs plus aimables confrères, M. Auguste-Pierre-François Guérin, qui" HEIGHT="56" HPOS="111" ID="string_tl_43" VPOS="2240" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="108 2345 1558 2345" HEIGHT="55" HPOS="108" ID="tl_44" VPOS="2290" WIDTH="1450">
<String CONTENT="était né à Paris, en septembre 1801. Après avoir fait de bonnes études" HEIGHT="55" HPOS="108" ID="string_tl_44" VPOS="2290" WIDTH="1450"/>
</TextLine>
<TextLine BASELINE="109 2397 1557 2397" HEIGHT="55" HPOS="109" ID="tl_45" VPOS="2342" WIDTH="1448">
<String CONTENT="au collège Henri IV, il passa plusieurs années dans le notariat, puis" HEIGHT="55" HPOS="109" ID="string_tl_45" VPOS="2342" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="108 2446 1557 2446" HEIGHT="51" HPOS="108" ID="tl_46" VPOS="2395" WIDTH="1449">
<String CONTENT="entra dans la librairie. Il dirigea successivement, et avec une rare" HEIGHT="51" HPOS="108" ID="string_tl_46" VPOS="2395" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="110 2497 1554 2497" HEIGHT="54" HPOS="110" ID="tl_47" VPOS="2443" WIDTH="1444">
<String CONTENT="intelligence, les librairies Pichard, Fume, Bixio, et, en dernier lieu," HEIGHT="54" HPOS="110" ID="string_tl_47" VPOS="2443" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="109 2545 1555 2545" HEIGHT="51" HPOS="109" ID="tl_48" VPOS="2494" WIDTH="1446">
<String CONTENT="celle de MM. Cosse et Marchal, où sa mort a laissé les plus vifs regrets." HEIGHT="51" HPOS="109" ID="string_tl_48" VPOS="2494" WIDTH="1446"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="158" HPOS="108" ID="r_3_3" VPOS="2543" WIDTH="1451">
<Shape>
<Polygon POINTS="108 2543 1559 2543 1559 2701 108 2701"/>
</Shape>
<TextLine BASELINE="161 2598 1558 2598" HEIGHT="54" HPOS="161" ID="tl_49" VPOS="2544" WIDTH="1397">
<String CONTENT="M. Guérin joignait à une douceur communicative, à une, grande" HEIGHT="54" HPOS="161" ID="string_tl_49" VPOS="2544" WIDTH="1397"/>
</TextLine>
<TextLine BASELINE="109 2648 1556 2648" HEIGHT="54" HPOS="109" ID="tl_50" VPOS="2594" WIDTH="1447">
<String CONTENT="bienveillance, un charmant talent d’artiste. Les portraits qu'il ne pouvait" HEIGHT="54" HPOS="109" ID="string_tl_50" VPOS="2594" WIDTH="1447"/>
</TextLine>
<TextLine BASELINE="110 2700 1557 2700" HEIGHT="56" HPOS="110" ID="tl_51" VPOS="2644" WIDTH="1447">
<String CONTENT="se procurer pour illustrer sa collection d’autographes, il les dessinait" HEIGHT="56" HPOS="110" ID="string_tl_51" VPOS="2644" WIDTH="1447"/>
</TextLine>
