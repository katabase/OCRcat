<TextLine BASELINE="98 330 1544 330" HEIGHT="49" HPOS="98" ID="tl_2" VPOS="281" WIDTH="1446">
<String CONTENT="marié, sa femme est morte ; mais sa tille, qui doit avoir dix-sept" HEIGHT="49" HPOS="98" ID="string_tl_2" VPOS="281" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="97 374 1544 374" HEIGHT="48" HPOS="97" ID="tl_3" VPOS="326" WIDTH="1447">
<String CONTENT="ou dix-huit ans, travaille, si je suis bien renseigné, chez une" HEIGHT="48" HPOS="97" ID="string_tl_3" VPOS="326" WIDTH="1447"/>
</TextLine>
<TextLine BASELINE="98 407 291 407" HEIGHT="36" HPOS="98" ID="tl_4" VPOS="371" WIDTH="193">
<String CONTENT="fleuriste." HEIGHT="36" HPOS="98" ID="string_tl_4" VPOS="371" WIDTH="193"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="272" HPOS="97" ID="r_2_2" VPOS="415" WIDTH="1451">
