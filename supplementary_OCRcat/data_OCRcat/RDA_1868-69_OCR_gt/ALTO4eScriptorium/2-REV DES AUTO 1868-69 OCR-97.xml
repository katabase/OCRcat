</Shape>
<TextLine BASELINE="141 464 1543 464" HEIGHT="48" HPOS="141" ID="tl_5" VPOS="416" WIDTH="1402">
<String CONTENT="Lucas est un petit homme olivâtre, crépu, marchant la tête" HEIGHT="48" HPOS="141" ID="string_tl_5" VPOS="416" WIDTH="1402"/>
</TextLine>
<TextLine BASELINE="98 509 1544 509" HEIGHT="49" HPOS="98" ID="tl_6" VPOS="460" WIDTH="1446">
<String CONTENT="courbée et le pas rapide. Ses allures étaient de tout temps assez" HEIGHT="49" HPOS="98" ID="string_tl_6" VPOS="460" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="98 554 1547 554" HEIGHT="48" HPOS="98" ID="tl_7" VPOS="506" WIDTH="1449">
<String CONTENT="étranges pour que depuis plusieurs années déjà il ait été signalé par" HEIGHT="48" HPOS="98" ID="string_tl_7" VPOS="506" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="99 603 1544 603" HEIGHT="52" HPOS="99" ID="tl_8" VPOS="551" WIDTH="1445">
<String CONTENT="les employés des bibliothèques comme homme à surveiller; mais" HEIGHT="52" HPOS="99" ID="string_tl_8" VPOS="551" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="99 643 1547 643" HEIGHT="47" HPOS="99" ID="tl_9" VPOS="596" WIDTH="1448">
<String CONTENT="rien ne pouvait faire prévoir à quelles hauteurs il saurait élever" HEIGHT="47" HPOS="99" ID="string_tl_9" VPOS="596" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="100 686 453 686" HEIGHT="45" HPOS="100" ID="tl_10" VPOS="641" WIDTH="353">
<String CONTENT="la mystification." HEIGHT="45" HPOS="100" ID="string_tl_10" VPOS="641" WIDTH="353"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="236" HPOS="96" ID="r_2_3" VPOS="684" WIDTH="1453">
<Shape>
<Polygon POINTS="96 684 1549 684 1549 920 96 920"/>
</Shape>
<TextLine BASELINE="143 733 1546 733" HEIGHT="48" HPOS="143" ID="tl_11" VPOS="685" WIDTH="1403">
<String CONTENT="On le signale d’abord à Paris assez malheureux, s’occupant de" HEIGHT="48" HPOS="143" ID="string_tl_11" VPOS="685" WIDTH="1403"/>
</TextLine>
<TextLine BASELINE="97 783 1545 783" HEIGHT="53" HPOS="97" ID="tl_12" VPOS="730" WIDTH="1448">
<String CONTENT="recherches généalogiques. Il se trouvait alors en rapport avec" HEIGHT="53" HPOS="97" ID="string_tl_12" VPOS="730" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="102 823 1547 823" HEIGHT="48" HPOS="102" ID="tl_13" VPOS="775" WIDTH="1445">
<String CONTENT="beaucoup de familles. Il recevait des sommes fort modiques pour" HEIGHT="48" HPOS="102" ID="string_tl_13" VPOS="775" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="100 866 1548 866" HEIGHT="45" HPOS="100" ID="tl_14" VPOS="821" WIDTH="1448">
<String CONTENT="prix de ses travaux et de ses découvertes. Vraisemblablement" HEIGHT="45" HPOS="100" ID="string_tl_14" VPOS="821" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="100 919 932 919" HEIGHT="53" HPOS="100" ID="tl_15" VPOS="866" WIDTH="832">
<String CONTENT="alors il copiait et ne pastichait point." HEIGHT="53" HPOS="100" ID="string_tl_15" VPOS="866" WIDTH="832"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="318" HPOS="99" ID="r_2_4" VPOS="909" WIDTH="1453">
<Shape>
<Polygon POINTS="99 909 1552 909 1552 1227 99 1227"/>
</Shape>
<TextLine BASELINE="145 965 1550 965" HEIGHT="55" HPOS="145" ID="tl_16" VPOS="910" WIDTH="1405">
<String CONTENT="Quand lui vint l’idée d’élargir le cercle de ses spéculations ?" HEIGHT="55" HPOS="145" ID="string_tl_16" VPOS="910" WIDTH="1405"/>
</TextLine>
<TextLine BASELINE="100 1002 1549 1002" HEIGHT="47" HPOS="100" ID="tl_17" VPOS="955" WIDTH="1449">
<String CONTENT="Les renseignements me font defaut ici, mais on peut, sans trop" HEIGHT="47" HPOS="100" ID="string_tl_17" VPOS="955" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="100 1047 1549 1047" HEIGHT="47" HPOS="100" ID="tl_18" VPOS="1000" WIDTH="1449">
<String CONTENT="d’invraisemblance, supposer qu’il est allé trouver M. Michel Chasles" HEIGHT="47" HPOS="100" ID="string_tl_18" VPOS="1000" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="100 1092 1551 1092" HEIGHT="46" HPOS="100" ID="tl_19" VPOS="1046" WIDTH="1451">
<String CONTENT="comme par hasard — comme compatriote peut-être, les Chasles" HEIGHT="46" HPOS="100" ID="string_tl_19" VPOS="1046" WIDTH="1451"/>
</TextLine>
<TextLine BASELINE="100 1137 1551 1137" HEIGHT="46" HPOS="100" ID="tl_20" VPOS="1091" WIDTH="1451">
<String CONTENT="sont originaires du pays chartrain — pour lui proposer quelques" HEIGHT="46" HPOS="100" ID="string_tl_20" VPOS="1091" WIDTH="1451"/>
</TextLine>
<TextLine BASELINE="103 1191 1550 1191" HEIGHT="55" HPOS="103" ID="tl_21" VPOS="1136" WIDTH="1447">
<String CONTENT="autographes authentiques, qu’on a causé, que Lucas a vu une proie" HEIGHT="55" HPOS="103" ID="string_tl_21" VPOS="1136" WIDTH="1447"/>
</TextLine>
<TextLine BASELINE="103 1228 1330 1228" HEIGHT="48" HPOS="103" ID="tl_22" VPOS="1180" WIDTH="1227">
<String CONTENT="facile et fructueuse à exploiter, et qu’il n’a plus hésité." HEIGHT="48" HPOS="103" ID="string_tl_22" VPOS="1180" WIDTH="1227"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="139" HPOS="101" ID="r_2_5" VPOS="1224" WIDTH="1451">
<Shape>
<Polygon POINTS="101 1224 1552 1224 1552 1363 101 1363"/>
</Shape>
<TextLine BASELINE="150 1271 1551 1271" HEIGHT="46" HPOS="150" ID="tl_23" VPOS="1225" WIDTH="1401">
<String CONTENT="Dés lors, la vie de l’archiviste paléographe (il se donnait, sans y" HEIGHT="46" HPOS="150" ID="string_tl_23" VPOS="1225" WIDTH="1401"/>
</TextLine>
<TextLine BASELINE="102 1317 1551 1317" HEIGHT="47" HPOS="102" ID="tl_24" VPOS="1270" WIDTH="1449">
<String CONTENT="avoir aucun droit, ce titre qui ne peut être porté que par diplôme" HEIGHT="47" HPOS="102" ID="string_tl_24" VPOS="1270" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="102 1362 1185 1362" HEIGHT="48" HPOS="102" ID="tl_25" VPOS="1314" WIDTH="1083">
<String CONTENT="de l’Ecole des chartes), prend un aspect nouveau." HEIGHT="48" HPOS="102" ID="string_tl_25" VPOS="1314" WIDTH="1083"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="275" HPOS="103" ID="r_2_6" VPOS="1358" WIDTH="1456">
<Shape>
<Polygon POINTS="103 1358 1559 1358 1559 1633 103 1633"/>
</Shape>
<TextLine BASELINE="146 1408 1553 1408" HEIGHT="49" HPOS="146" ID="tl_26" VPOS="1359" WIDTH="1407">
<String CONTENT="Il s’installe dans un quartier élégant, va déjeuner au café Riche," HEIGHT="49" HPOS="146" ID="string_tl_26" VPOS="1359" WIDTH="1407"/>
</TextLine>
<TextLine BASELINE="105 1458 1554 1458" HEIGHT="53" HPOS="105" ID="tl_27" VPOS="1405" WIDTH="1449">
<String CONTENT="prend des voitures, passe des après-midi à la Bibliothèque impé¬" HEIGHT="53" HPOS="105" ID="string_tl_27" VPOS="1405" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="104 1496 1558 1496" HEIGHT="46" HPOS="104" ID="tl_28" VPOS="1450" WIDTH="1454">
<String CONTENT="riale, au milieu des in-folio. Il demandait presque constamment" HEIGHT="46" HPOS="104" ID="string_tl_28" VPOS="1450" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="105 1542 1556 1542" HEIGHT="47" HPOS="105" ID="tl_29" VPOS="1495" WIDTH="1451">
<String CONTENT="les quinze volumes des œuvres de Galilée; puis le soir, si nos ren¬" HEIGHT="47" HPOS="105" ID="string_tl_29" VPOS="1495" WIDTH="1451"/>
</TextLine>
<TextLine BASELINE="104 1586 1556 1586" HEIGHT="46" HPOS="104" ID="tl_30" VPOS="1540" WIDTH="1452">
<String CONTENT="seignements sont exacts, il allait travailler chez une petite dame" HEIGHT="46" HPOS="104" ID="string_tl_30" VPOS="1540" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="105 1632 703 1632" HEIGHT="48" HPOS="105" ID="tl_31" VPOS="1584" WIDTH="598">
<String CONTENT="du quartier Saint-Georges." HEIGHT="48" HPOS="105" ID="string_tl_31" VPOS="1584" WIDTH="598"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="257" HPOS="101" ID="r_2_7" VPOS="1645" WIDTH="1461">
<Shape>
<Polygon POINTS="101 1645 1562 1645 1562 1902 101 1902"/>
</Shape>
<TextLine BASELINE="155 1698 1558 1698" HEIGHT="52" HPOS="155" ID="tl_32" VPOS="1646" WIDTH="1403">
<String CONTENT="Le Figaro intitule son article : M. Libri est innocent. Telle a tou¬" HEIGHT="52" HPOS="155" ID="string_tl_32" VPOS="1646" WIDTH="1403"/>
</TextLine>
<TextLine BASELINE="102 1748 1561 1748" HEIGHT="51" HPOS="102" ID="tl_33" VPOS="1697" WIDTH="1459">
<String CONTENT="jours été notre conviction. Les débats l’établiront d’une manière cer¬" HEIGHT="51" HPOS="102" ID="string_tl_33" VPOS="1697" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="108 1800 1559 1800" HEIGHT="54" HPOS="108" ID="tl_34" VPOS="1746" WIDTH="1451">
<String CONTENT="taine, et comme il sera reconnu que la somme déboursée a été relative¬" HEIGHT="54" HPOS="108" ID="string_tl_34" VPOS="1746" WIDTH="1451"/>
</TextLine>
<TextLine BASELINE="109 1851 1561 1851" HEIGHT="53" HPOS="109" ID="tl_35" VPOS="1798" WIDTH="1452">
<String CONTENT="ment minime, on en conclura que M. Libri n’aurait pas voulu se livrer" HEIGHT="53" HPOS="109" ID="string_tl_35" VPOS="1798" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="107 1901 998 1901" HEIGHT="51" HPOS="107" ID="tl_36" VPOS="1850" WIDTH="891">
<String CONTENT="à cet immense travail pour si peu de chose." HEIGHT="51" HPOS="107" ID="string_tl_36" VPOS="1850" WIDTH="891"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="164" HPOS="106" ID="r_2_8" VPOS="1914" WIDTH="1453">
<Shape>
<Polygon POINTS="106 1914 1559 1914 1559 2078 106 2078"/>
</Shape>
<TextLine BASELINE="148 1957 1558 1957" HEIGHT="42" HPOS="148" ID="tl_37" VPOS="1915" WIDTH="1410">
<String CONTENT="P.-S. — M. Libri vient de mourir ii Florence. — Le Fiqaro du 6 octobre l’accuse," HEIGHT="42" HPOS="148" ID="string_tl_37" VPOS="1915" WIDTH="1410"/>
</TextLine>
<TextLine BASELINE="107 1998 1557 1998" HEIGHT="43" HPOS="107" ID="tl_38" VPOS="1955" WIDTH="1450">
<String CONTENT="comme pendant à l'affaire Chasles, d’avoir fabriqué les documents publiés par lui," HEIGHT="43" HPOS="107" ID="string_tl_38" VPOS="1955" WIDTH="1450"/>
</TextLine>
<TextLine BASELINE="107 2037 1557 2037" HEIGHT="41" HPOS="107" ID="tl_39" VPOS="1996" WIDTH="1450">
<String CONTENT="en 1812, sur la jeunesse de Napoléon Ier. Dans le prochain numéro, nous ferons con¬" HEIGHT="41" HPOS="107" ID="string_tl_39" VPOS="1996" WIDTH="1450"/>
</TextLine>
<TextLine BASELINE="107 2077 1370 2077" HEIGHT="42" HPOS="107" ID="tl_40" VPOS="2035" WIDTH="1263">
<String CONTENT="naître l’origine de ces documents, ignorée sans doute de l’auteur de l’article." HEIGHT="42" HPOS="107" ID="string_tl_40" VPOS="2035" WIDTH="1263"/>
</TextLine>
</TextBlock>
