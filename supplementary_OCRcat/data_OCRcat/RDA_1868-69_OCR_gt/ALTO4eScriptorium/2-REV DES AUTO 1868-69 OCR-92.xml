<TextLine BASELINE="237 300 1691 300" HEIGHT="48" HPOS="237" ID="tl_2" VPOS="252" WIDTH="1454">
<String CONTENT="ainsi, était connue de notre confrère M. Balard, qui l’avait remar¬" HEIGHT="48" HPOS="237" ID="string_tl_2" VPOS="252" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="236 345 1691 345" HEIGHT="48" HPOS="236" ID="tl_3" VPOS="297" WIDTH="1455">
<String CONTENT="quée plusieurs mois avant que j’en fisse usage, en explorant, un" HEIGHT="48" HPOS="236" ID="string_tl_3" VPOS="297" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="236 389 1694 389" HEIGHT="46" HPOS="236" ID="tl_4" VPOS="343" WIDTH="1458">
<String CONTENT="soir chez moi, avec deux savants anglais, le dossier renfermant" HEIGHT="46" HPOS="236" ID="string_tl_4" VPOS="343" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="235 434 1693 434" HEIGHT="46" HPOS="235" ID="tl_5" VPOS="388" WIDTH="1458">
<String CONTENT="les lettres de Jacques II et des deux reines Marie et Anne, ses" HEIGHT="46" HPOS="235" ID="string_tl_5" VPOS="388" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="236 469 352 469" HEIGHT="36" HPOS="236" ID="tl_6" VPOS="433" WIDTH="116">
<String CONTENT="filles." HEIGHT="36" HPOS="236" ID="string_tl_6" VPOS="433" WIDTH="116"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="678" HPOS="219" ID="r_2_2" VPOS="488" WIDTH="1479">
<Shape>
<Polygon POINTS="219 488 1698 488 1698 1166 219 1166"/>
</Shape>
<TextLine BASELINE="276 536 1692 536" HEIGHT="47" HPOS="276" ID="tl_7" VPOS="489" WIDTH="1416">
<String CONTENT="Mais, indépendamment de ce simple fait, je puis dire que tout ce" HEIGHT="47" HPOS="276" ID="string_tl_7" VPOS="489" WIDTH="1416"/>
</TextLine>
<TextLine BASELINE="231 580 1694 580" HEIGHT="46" HPOS="231" ID="tl_8" VPOS="534" WIDTH="1463">
<String CONTENT="qui s’est passé continuellement à nos séances aurait dû écarter" HEIGHT="46" HPOS="231" ID="string_tl_8" VPOS="534" WIDTH="1463"/>
</TextLine>
<TextLine BASELINE="232 625 1693 625" HEIGHT="46" HPOS="232" ID="tl_9" VPOS="579" WIDTH="1461">
<String CONTENT="ridée que j’eusse recours à des documents qui n’auraient pas été" HEIGHT="46" HPOS="232" ID="string_tl_9" VPOS="579" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="228 671 1694 671" HEIGHT="47" HPOS="228" ID="tl_10" VPOS="624" WIDTH="1466">
<String CONTENT="entre mes mains, par la raison que je répondais toujours séance te¬" HEIGHT="47" HPOS="228" ID="string_tl_10" VPOS="624" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="228 716 1695 716" HEIGHT="47" HPOS="228" ID="tl_11" VPOS="669" WIDTH="1467">
<String CONTENT="nante, s’il n’y avait pas quelque empêchement indépendant de ma vo¬" HEIGHT="47" HPOS="228" ID="string_tl_11" VPOS="669" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="226 760 1693 760" HEIGHT="46" HPOS="226" ID="tl_12" VPOS="714" WIDTH="1467">
<String CONTENT="lonté, à toutes les communications qui arrivaient du dehors. Si j’en" HEIGHT="46" HPOS="226" ID="string_tl_12" VPOS="714" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="225 805 1692 805" HEIGHT="47" HPOS="225" ID="tl_13" VPOS="758" WIDTH="1467">
<String CONTENT="étais prévenu par M. le Secrétaire perpétuel, au commencement de" HEIGHT="47" HPOS="225" ID="string_tl_13" VPOS="758" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="227 851 1696 851" HEIGHT="47" HPOS="227" ID="tl_14" VPOS="804" WIDTH="1469">
<String CONTENT="la séance, je courais chercher les pièces qui pouvaient servir à ma" HEIGHT="47" HPOS="227" ID="string_tl_14" VPOS="804" WIDTH="1469"/>
</TextLine>
<TextLine BASELINE="226 896 1693 896" HEIGHT="47" HPOS="226" ID="tl_15" VPOS="849" WIDTH="1467">
<String CONTENT="réponse; et si je ne connaissais une communication qu’à la lec¬" HEIGHT="47" HPOS="226" ID="string_tl_15" VPOS="849" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="225 941 1694 941" HEIGHT="47" HPOS="225" ID="tl_16" VPOS="894" WIDTH="1469">
<String CONTENT="ture qui en était faite dans le dépouillement de la correspondance," HEIGHT="47" HPOS="225" ID="string_tl_16" VPOS="894" WIDTH="1469"/>
</TextLine>
<TextLine BASELINE="220 985 1694 985" HEIGHT="46" HPOS="220" ID="tl_17" VPOS="939" WIDTH="1474">
<String CONTENT="je répondais néanmoins en invoquant, de souvenir, les documents" HEIGHT="46" HPOS="220" ID="string_tl_17" VPOS="939" WIDTH="1474"/>
</TextLine>
<TextLine BASELINE="224 1037 1694 1037" HEIGHT="54" HPOS="224" ID="tl_18" VPOS="983" WIDTH="1470">
<String CONTENT="qui se rapportaient au sujet; et ces documents, je ne les faisais" HEIGHT="54" HPOS="224" ID="string_tl_18" VPOS="983" WIDTH="1470"/>
</TextLine>
<TextLine BASELINE="226 1075 1697 1075" HEIGHT="46" HPOS="226" ID="tl_19" VPOS="1029" WIDTH="1471">
<String CONTENT="point attendre le lendemain, c’était toujours dés le matin, de 7 à" HEIGHT="46" HPOS="226" ID="string_tl_19" VPOS="1029" WIDTH="1471"/>
</TextLine>
<TextLine BASELINE="222 1120 1696 1120" HEIGHT="46" HPOS="222" ID="tl_20" VPOS="1074" WIDTH="1474">
<String CONTENT="8 heures, que je les envoyais à l’imprimerie, avec le texte de ma" HEIGHT="46" HPOS="222" ID="string_tl_20" VPOS="1074" WIDTH="1474"/>
</TextLine>
<TextLine BASELINE="226 1165 1268 1165" HEIGHT="45" HPOS="226" ID="tl_21" VPOS="1120" WIDTH="1042">
<String CONTENT="réponse. Les Comptes-vendus constatent ces faits." HEIGHT="45" HPOS="226" ID="string_tl_21" VPOS="1120" WIDTH="1042"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="218" HPOS="220" ID="r_2_3" VPOS="1174" WIDTH="1477">
<Shape>
<Polygon POINTS="220 1174 1697 1174 1697 1392 220 1392"/>
</Shape>
<TextLine BASELINE="269 1221 1694 1221" HEIGHT="46" HPOS="269" ID="tl_22" VPOS="1175" WIDTH="1425">
<String CONTENT="Je déclare, en outre, que le vendeur qui m’apportait les docu¬" HEIGHT="46" HPOS="269" ID="string_tl_22" VPOS="1175" WIDTH="1425"/>
</TextLine>
<TextLine BASELINE="227 1265 1695 1265" HEIGHT="45" HPOS="227" ID="tl_23" VPOS="1220" WIDTH="1468">
<String CONTENT="ments, venait toujours chez moi entre 11 heures et midi, ou de" HEIGHT="45" HPOS="227" ID="string_tl_23" VPOS="1220" WIDTH="1468"/>
</TextLine>
<TextLine BASELINE="221 1311 1696 1311" HEIGHT="54" HPOS="221" ID="tl_24" VPOS="1257" WIDTH="1475">
<String CONTENT="5 heures et demie à 6 heures; et, en outre, que jamais je n’ai été chez" HEIGHT="54" HPOS="221" ID="string_tl_24" VPOS="1257" WIDTH="1475"/>
</TextLine>
<TextLine BASELINE="226 1357 1695 1357" HEIGHT="47" HPOS="226" ID="tl_25" VPOS="1310" WIDTH="1469">
<String CONTENT="lui, et que jamais je n’ai envoyé personne lui demander aucun" HEIGHT="47" HPOS="226" ID="string_tl_25" VPOS="1310" WIDTH="1469"/>
</TextLine>
<TextLine BASELINE="228 1391 458 1391" HEIGHT="35" HPOS="228" ID="tl_26" VPOS="1356" WIDTH="230">
<String CONTENT="document." HEIGHT="35" HPOS="228" ID="string_tl_26" VPOS="1356" WIDTH="230"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="1263" HPOS="227" ID="r_2_4" VPOS="1409" WIDTH="1481">
<Shape>
<Polygon POINTS="227 1409 1708 1409 1708 2672 227 2672"/>
</Shape>
<TextLine BASELINE="272 1462 1695 1462" HEIGHT="52" HPOS="272" ID="tl_27" VPOS="1410" WIDTH="1423">
<String CONTENT="L’insistance que j’ai mise, dans ces derniers temps pour que" HEIGHT="52" HPOS="272" ID="string_tl_27" VPOS="1410" WIDTH="1423"/>
</TextLine>
<TextLine BASELINE="228 1507 1698 1507" HEIGHT="52" HPOS="228" ID="tl_28" VPOS="1455" WIDTH="1470">
<String CONTENT="M. Leverrier traitât la question astronomique qu’il avait annoncée" HEIGHT="52" HPOS="228" ID="string_tl_28" VPOS="1455" WIDTH="1470"/>
</TextLine>
<TextLine BASELINE="233 1548 1696 1548" HEIGHT="48" HPOS="233" ID="tl_29" VPOS="1500" WIDTH="1463">
<String CONTENT="le 19 avril, et que beaucoup de Membres voulaient ajourner après" HEIGHT="48" HPOS="233" ID="string_tl_29" VPOS="1500" WIDTH="1463"/>
</TextLine>
<TextLine BASELINE="231 1592 1698 1592" HEIGHT="46" HPOS="231" ID="tl_30" VPOS="1546" WIDTH="1467">
<String CONTENT="la publication complète des Mss. de Pascal, prouve que j’avais" HEIGHT="46" HPOS="231" ID="string_tl_30" VPOS="1546" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="231 1637 1697 1637" HEIGHT="46" HPOS="231" ID="tl_31" VPOS="1591" WIDTH="1466">
<String CONTENT="encore à ce moment une pleine confiance dans mes documents." HEIGHT="46" HPOS="231" ID="string_tl_31" VPOS="1591" WIDTH="1466"/>
</TextLine>
<TextLine BASELINE="230 1682 1699 1682" HEIGHT="47" HPOS="230" ID="tl_32" VPOS="1635" WIDTH="1469">
<String CONTENT="Mais bientôt après, les observations qui ont été faites à Florence" HEIGHT="47" HPOS="230" ID="string_tl_32" VPOS="1635" WIDTH="1469"/>
</TextLine>
<TextLine BASELINE="232 1728 1700 1728" HEIGHT="47" HPOS="232" ID="tl_33" VPOS="1681" WIDTH="1468">
<String CONTENT="sur la lettre de Galilée, du 5 novembre 1639, dont j’avais envoyé" HEIGHT="47" HPOS="232" ID="string_tl_33" VPOS="1681" WIDTH="1468"/>
</TextLine>
<TextLine BASELINE="234 1772 1699 1772" HEIGHT="47" HPOS="234" ID="tl_34" VPOS="1725" WIDTH="1465">
<String CONTENT="une photographie, ont éveillé mon attention, et ont commencé" HEIGHT="47" HPOS="234" ID="string_tl_34" VPOS="1725" WIDTH="1465"/>
</TextLine>
<TextLine BASELINE="234 1816 1701 1816" HEIGHT="46" HPOS="234" ID="tl_35" VPOS="1770" WIDTH="1467">
<String CONTENT="à m’inspirer des inquiétudes, qui m’ont porté à certaines recherches" HEIGHT="46" HPOS="234" ID="string_tl_35" VPOS="1770" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="235 1858 1702 1858" HEIGHT="43" HPOS="235" ID="tl_36" VPOS="1815" WIDTH="1467">
<String CONTENT="et à des mesures d’information, et même à solliciter de M. le Pré¬" HEIGHT="43" HPOS="235" ID="string_tl_36" VPOS="1815" WIDTH="1467"/>
</TextLine>
<TextLine BASELINE="236 1906 1697 1906" HEIGHT="48" HPOS="236" ID="tl_37" VPOS="1858" WIDTH="1461">
<String CONTENT="fet de police une surveillance, à l’effet de connaître enfin le véri¬" HEIGHT="48" HPOS="236" ID="string_tl_37" VPOS="1858" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="237 1952 1700 1952" HEIGHT="47" HPOS="237" ID="tl_38" VPOS="1905" WIDTH="1463">
<String CONTENT="table dépôt des pièces qui m’étaient vendues. L’examen que" HEIGHT="47" HPOS="237" ID="string_tl_38" VPOS="1905" WIDTH="1463"/>
</TextLine>
<TextLine BASELINE="239 1996 1700 1996" HEIGHT="47" HPOS="239" ID="tl_39" VPOS="1949" WIDTH="1461">
<String CONTENT="M. Volpicelli a bien voulu faire faire par ses amis, MM. Corridi" HEIGHT="47" HPOS="239" ID="string_tl_39" VPOS="1949" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="239 2040 1700 2040" HEIGHT="45" HPOS="239" ID="tl_40" VPOS="1995" WIDTH="1461">
<String CONTENT="et Gu asti, en l’absence de la Commission de Florence, de la der¬" HEIGHT="45" HPOS="239" ID="string_tl_40" VPOS="1995" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="242 2086 1700 2086" HEIGHT="46" HPOS="242" ID="tl_41" VPOS="2040" WIDTH="1458">
<String CONTENT="nière lettre que j’avais envoyée à M. Carbone, est venu accroître" HEIGHT="46" HPOS="242" ID="string_tl_41" VPOS="2040" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="244 2132 1701 2132" HEIGHT="56" HPOS="244" ID="tl_42" VPOS="2076" WIDTH="1457">
<String CONTENT="mes inquiétudes, et j’ai cru devoir adresser à M. le Préfet de po¬" HEIGHT="56" HPOS="244" ID="string_tl_42" VPOS="2076" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="245 2177 1701 2177" HEIGHT="47" HPOS="245" ID="tl_43" VPOS="2130" WIDTH="1456">
<String CONTENT="lice une déclaration tendant à ce que, sans continuer davantage" HEIGHT="47" HPOS="245" ID="string_tl_43" VPOS="2130" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="244 2222 1702 2222" HEIGHT="47" HPOS="244" ID="tl_44" VPOS="2175" WIDTH="1458">
<String CONTENT="les recherches et la surveillance établie, on procédât à l’arrestation" HEIGHT="47" HPOS="244" ID="string_tl_44" VPOS="2175" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="244 2267 1702 2267" HEIGHT="47" HPOS="244" ID="tl_45" VPOS="2220" WIDTH="1458">
<String CONTENT="du vendeur, ce qui a eu lieu. Mais on n’a trouvé chez lui que" HEIGHT="47" HPOS="244" ID="string_tl_45" VPOS="2220" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="246 2313 1702 2313" HEIGHT="48" HPOS="246" ID="tl_46" VPOS="2265" WIDTH="1456">
<String CONTENT="quelques paniers blancs, provenant de registres, des plumes, un" HEIGHT="48" HPOS="246" ID="string_tl_46" VPOS="2265" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="246 2357 1702 2357" HEIGHT="47" HPOS="246" ID="tl_47" VPOS="2310" WIDTH="1456">
<String CONTENT="seul flacon d’encre et quelques fac-similé de l’Isographie, quand" HEIGHT="47" HPOS="246" ID="string_tl_47" VPOS="2310" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="242 2402 1703 2402" HEIGHT="47" HPOS="242" ID="tl_48" VPOS="2355" WIDTH="1461">
<String CONTENT="j’avais espéré qu’on y trouverait la masse des documents dont il" HEIGHT="47" HPOS="242" ID="string_tl_48" VPOS="2355" WIDTH="1461"/>
</TextLine>
<TextLine BASELINE="249 2447 1703 2447" HEIGHT="55" HPOS="249" ID="tl_49" VPOS="2392" WIDTH="1454">
<String CONTENT="ne m’avait livré que des copies et dont une partie considérable" HEIGHT="55" HPOS="249" ID="string_tl_49" VPOS="2392" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="250 2492 1703 2492" HEIGHT="47" HPOS="250" ID="tl_50" VPOS="2445" WIDTH="1453">
<String CONTENT="m’était encore due. Il a refusé d’abord de faire connaître de qui" HEIGHT="47" HPOS="250" ID="string_tl_50" VPOS="2445" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="251 2537 1704 2537" HEIGHT="47" HPOS="251" ID="tl_51" VPOS="2490" WIDTH="1453">
<String CONTENT="il tenait ces documents, et il a déclaré depuis que c’était lui qui" HEIGHT="47" HPOS="251" ID="string_tl_51" VPOS="2490" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="251 2587 1706 2587" HEIGHT="52" HPOS="251" ID="tl_52" VPOS="2535" WIDTH="1455">
<String CONTENT="les fabriquait. M. le Commissaire instructeur lui a demandé s’il" HEIGHT="52" HPOS="251" ID="string_tl_52" VPOS="2535" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="252 2627 1705 2627" HEIGHT="47" HPOS="252" ID="tl_53" VPOS="2580" WIDTH="1453">
<String CONTENT="n’en avait pas eu du comte de Menou, en 1861; il a répondu une" HEIGHT="47" HPOS="252" ID="string_tl_53" VPOS="2580" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="252 2671 1707 2671" HEIGHT="46" HPOS="252" ID="tl_54" VPOS="2625" WIDTH="1455">
<String CONTENT="soixantaine, ajoutant qu’il en avait aussi eu un résidu du cabinet" HEIGHT="46" HPOS="252" ID="string_tl_54" VPOS="2625" WIDTH="1455"/>
</TextLine>
</TextBlock>
