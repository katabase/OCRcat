<TextLine BASELINE="133 1901 1533 1901" HEIGHT="51" HPOS="133" ID="tl_18" VPOS="1850" WIDTH="1400">
<String CONTENT="Ce dénouement a eu lieu de la manière la plus inattendue, dans la" HEIGHT="51" HPOS="133" ID="string_tl_18" VPOS="1850" WIDTH="1400"/>
</TextLine>
<TextLine BASELINE="82 1953 1527 1953" HEIGHT="53" HPOS="82" ID="tl_19" VPOS="1900" WIDTH="1445">
<String CONTENT="séance de l’Académie des Sciences du 13 septembre. Sans doute." HEIGHT="53" HPOS="82" ID="string_tl_19" VPOS="1900" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="82 2005 1532 2005" HEIGHT="54" HPOS="82" ID="tl_20" VPOS="1951" WIDTH="1450">
<String CONTENT="M. Chasles se trouvait dans un grand embarras, vivement pressé par" HEIGHT="54" HPOS="82" ID="string_tl_20" VPOS="1951" WIDTH="1450"/>
</TextLine>
<TextLine BASELINE="83 2054 1531 2054" HEIGHT="52" HPOS="83" ID="tl_21" VPOS="2002" WIDTH="1448">
<String CONTENT="M. Leverrier, qui portait à ses autographes un coup terrible à l’Aca¬" HEIGHT="52" HPOS="83" ID="string_tl_21" VPOS="2002" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="82 2098 1531 2098" HEIGHT="46" HPOS="82" ID="tl_22" VPOS="2052" WIDTH="1449">
<String CONTENT="démie, et menacé d’une déclaration très nette de la commission" HEIGHT="46" HPOS="82" ID="string_tl_22" VPOS="2052" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="82 2155 1531 2155" HEIGHT="52" HPOS="82" ID="tl_23" VPOS="2103" WIDTH="1449">
<String CONTENT="d’expertise de Florence; mais, à l’ardeur qu’il mettait à la défense de" HEIGHT="52" HPOS="82" ID="string_tl_23" VPOS="2103" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="83 2205 1530 2205" HEIGHT="52" HPOS="83" ID="tl_24" VPOS="2153" WIDTH="1447">
<String CONTENT="sa cause, personne ne pouvait prévoir une solution si prochaine. Nous" HEIGHT="52" HPOS="83" ID="string_tl_24" VPOS="2153" WIDTH="1447"/>
</TextLine>
<TextLine BASELINE="84 2258 1528 2258" HEIGHT="55" HPOS="84" ID="tl_25" VPOS="2203" WIDTH="1444">
<String CONTENT="allons résumer les dernières péripéties de cette étrange affaire, qui" HEIGHT="55" HPOS="84" ID="string_tl_25" VPOS="2203" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="86 2306 927 2306" HEIGHT="52" HPOS="86" ID="tl_26" VPOS="2254" WIDTH="841">
<String CONTENT="intéresse au plus haut point nos lecteurs." HEIGHT="52" HPOS="86" ID="string_tl_26" VPOS="2254" WIDTH="841"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="412" HPOS="83" ID="r_11_3" VPOS="2301" WIDTH="1448">
<Shape>
<Polygon POINTS="83 2301 1531 2301 1531 2713 83 2713"/>
</Shape>
<TextLine BASELINE="134 2356 1525 2356" HEIGHT="54" HPOS="134" ID="tl_27" VPOS="2302" WIDTH="1391">
<String CONTENT="On se rappelle l’article de notre n° 19, paru en décembre dernier." HEIGHT="54" HPOS="134" ID="string_tl_27" VPOS="2302" WIDTH="1391"/>
</TextLine>
<TextLine BASELINE="86 2409 1530 2409" HEIGHT="54" HPOS="86" ID="tl_28" VPOS="2355" WIDTH="1444">
<String CONTENT="Nous disions franchement à M. Chasles, au risque de lui déplaire, que" HEIGHT="54" HPOS="86" ID="string_tl_28" VPOS="2355" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="84 2458 1530 2458" HEIGHT="52" HPOS="84" ID="tl_29" VPOS="2406" WIDTH="1446">
<String CONTENT="sa prétendue collection d’autographes était composée, tout entière, de" HEIGHT="52" HPOS="84" ID="string_tl_29" VPOS="2406" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="86 2512 1529 2512" HEIGHT="56" HPOS="86" ID="tl_30" VPOS="2456" WIDTH="1443">
<String CONTENT="pièces fabriquées, et nous l’adjurions de consentir à, une expertise" HEIGHT="56" HPOS="86" ID="string_tl_30" VPOS="2456" WIDTH="1443"/>
</TextLine>
<TextLine BASELINE="86 2558 1530 2558" HEIGHT="51" HPOS="86" ID="tl_31" VPOS="2507" WIDTH="1444">
<String CONTENT="sérieuse. Un des faits que nous rapportions semblait être favorable à sa" HEIGHT="51" HPOS="86" ID="string_tl_31" VPOS="2507" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="87 2608 1529 2608" HEIGHT="51" HPOS="87" ID="tl_32" VPOS="2557" WIDTH="1442">
<String CONTENT="thèse. En parcourant, à la Bibliothèque nationale de Florence, le" HEIGHT="51" HPOS="87" ID="string_tl_32" VPOS="2557" WIDTH="1442"/>
</TextLine>
<TextLine BASELINE="87 2659 1528 2659" HEIGHT="50" HPOS="87" ID="tl_33" VPOS="2609" WIDTH="1441">
<String CONTENT="recueil des lettres autographes de Galilée, nous en avions trouvé une" HEIGHT="50" HPOS="87" ID="string_tl_33" VPOS="2609" WIDTH="1441"/>
</TextLine>
<TextLine BASELINE="88 2712 1526 2712" HEIGHT="54" HPOS="88" ID="tl_34" VPOS="2658" WIDTH="1438">
<String CONTENT="datée du 5 novembre 1639. Or, les adversaires de M. Chasles préton¬" HEIGHT="54" HPOS="88" ID="string_tl_34" VPOS="2658" WIDTH="1438"/>
</TextLine>
