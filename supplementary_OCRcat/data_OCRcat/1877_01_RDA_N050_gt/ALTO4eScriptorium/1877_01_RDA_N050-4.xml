</Shape>
<TextLine BASELINE="333 1311 1730 1311" HEIGHT="51" HPOS="333" ID="tl_21" VPOS="1260" WIDTH="1397">
<String CONTENT="On remarquera les lettres de Mmes de Lanssac et de Sènecé," HEIGHT="51" HPOS="333" ID="string_tl_21" VPOS="1260" WIDTH="1397"/>
</TextLine>
<TextLine BASELINE="284 1361 1732 1361" HEIGHT="50" HPOS="284" ID="tl_22" VPOS="1311" WIDTH="1448">
<String CONTENT="qui se sont succédées à la cour au gré de Richelieu, suivant" HEIGHT="50" HPOS="284" ID="string_tl_22" VPOS="1311" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="283 1411 1731 1411" HEIGHT="50" HPOS="283" ID="tl_23" VPOS="1361" WIDTH="1448">
<String CONTENT="les besoins de sa politique ; de Mme de Motteville, qui nous a" HEIGHT="50" HPOS="283" ID="string_tl_23" VPOS="1361" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="284 1461 1731 1461" HEIGHT="49" HPOS="284" ID="tl_24" VPOS="1412" WIDTH="1447">
<String CONTENT="laissé les plus curieux mémoires sur Anne d’Autriche et sur" HEIGHT="49" HPOS="284" ID="string_tl_24" VPOS="1412" WIDTH="1447"/>
</TextLine>
<TextLine BASELINE="285 1512 1733 1512" HEIGHT="50" HPOS="285" ID="tl_25" VPOS="1462" WIDTH="1448">
<String CONTENT="la foule dorée qui s’agitait autour de sa maîtresse ; de Mlle d’Au¬" HEIGHT="50" HPOS="285" ID="string_tl_25" VPOS="1462" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="285 1561 1732 1561" HEIGHT="49" HPOS="285" ID="tl_26" VPOS="1512" WIDTH="1447">
<String CONTENT="male, la confidente de Mme de Maintenon. Les pièces sur" HEIGHT="49" HPOS="285" ID="string_tl_26" VPOS="1512" WIDTH="1447"/>
</TextLine>
<TextLine BASELINE="283 1612 1728 1612" HEIGHT="49" HPOS="283" ID="tl_27" VPOS="1563" WIDTH="1445">
<String CONTENT="l’évasion du cardinal de Retz ne passeront pas inaperçues, non" HEIGHT="49" HPOS="283" ID="string_tl_27" VPOS="1563" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="285 1663 1731 1663" HEIGHT="50" HPOS="285" ID="tl_28" VPOS="1613" WIDTH="1446">
<String CONTENT="plus que la lettre d’Anne Poussart de Vigean, duchesse de" HEIGHT="50" HPOS="285" ID="string_tl_28" VPOS="1613" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="286 1713 1521 1713" HEIGHT="50" HPOS="286" ID="tl_29" VPOS="1663" WIDTH="1235">
<String CONTENT="Richelieu, lettre qui vaut à elle seule tout un poème." HEIGHT="50" HPOS="286" ID="string_tl_29" VPOS="1663" WIDTH="1235"/>
</TextLine>
