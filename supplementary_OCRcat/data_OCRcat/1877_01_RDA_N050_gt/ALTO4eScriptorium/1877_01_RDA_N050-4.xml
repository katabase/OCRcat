</Shape>
<TextLine BASELINE="335 1781 1729 1781" HEIGHT="51" HPOS="335" ID="tl_30" VPOS="1730" WIDTH="1394">
<String CONTENT="Nous avons oublié de dire dans le catalogue que la corres¬" HEIGHT="51" HPOS="335" ID="string_tl_30" VPOS="1730" WIDTH="1394"/>
</TextLine>
<TextLine BASELINE="285 1831 1728 1831" HEIGHT="51" HPOS="285" ID="tl_31" VPOS="1780" WIDTH="1443">
<String CONTENT="pondance de Mme de Pompadour sera vendue en un seul bloc." HEIGHT="51" HPOS="285" ID="string_tl_31" VPOS="1780" WIDTH="1443"/>
</TextLine>
<TextLine BASELINE="289 1881 1733 1881" HEIGHT="50" HPOS="289" ID="tl_32" VPOS="1831" WIDTH="1444">
<String CONTENT="Il serait en effet fâcheux de diviser des pièces qui forment" HEIGHT="50" HPOS="289" ID="string_tl_32" VPOS="1831" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="288 1932 1732 1932" HEIGHT="49" HPOS="288" ID="tl_33" VPOS="1883" WIDTH="1444">
<String CONTENT="corps et dont la valeur consiste surtout dans l’ensemble. Il en" HEIGHT="49" HPOS="288" ID="string_tl_33" VPOS="1883" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="287 1984 1731 1984" HEIGHT="51" HPOS="287" ID="tl_34" VPOS="1933" WIDTH="1444">
<String CONTENT="sera de même des articles concernant Mme d’Hautefort, et de" HEIGHT="51" HPOS="287" ID="string_tl_34" VPOS="1933" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="288 2033 1732 2033" HEIGHT="51" HPOS="288" ID="tl_35" VPOS="1982" WIDTH="1444">
<String CONTENT="ceux relatifs à Mlle de Lafayette. Ce sont autant de dossiers" HEIGHT="51" HPOS="288" ID="string_tl_35" VPOS="1982" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="289 2083 1158 2083" HEIGHT="50" HPOS="289" ID="tl_36" VPOS="2033" WIDTH="869">
<String CONTENT="historiques qui doivent rester intacts." HEIGHT="50" HPOS="289" ID="string_tl_36" VPOS="2033" WIDTH="869"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="354" HPOS="290" ID="r_2_5" VPOS="2100" WIDTH="1446">
