<TextLine BASELINE="335 689 1729 689" HEIGHT="51" HPOS="335" ID="tl_9" VPOS="638" WIDTH="1394">
<String CONTENT="Le catalogue nous offre deux femmes qui ont joué un grand" HEIGHT="51" HPOS="335" ID="string_tl_9" VPOS="638" WIDTH="1394"/>
</TextLine>
<TextLine BASELINE="285 739 1729 739" HEIGHT="51" HPOS="285" ID="tl_10" VPOS="688" WIDTH="1444">
<String CONTENT="rôle dans notre diplomatie secrète, la princesse des Ursins," HEIGHT="51" HPOS="285" ID="string_tl_10" VPOS="688" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="285 790 1730 790" HEIGHT="51" HPOS="285" ID="tl_11" VPOS="739" WIDTH="1445">
<String CONTENT="qui valait plus qu’un ambassadeur de France à la cour de" HEIGHT="51" HPOS="285" ID="string_tl_11" VPOS="739" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="285 839 1730 839" HEIGHT="49" HPOS="285" ID="tl_12" VPOS="790" WIDTH="1445">
<String CONTENT="Madrid, et la duchesse de Portsmouth, par qui Louis XIV" HEIGHT="49" HPOS="285" ID="string_tl_12" VPOS="790" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="285 889 1726 889" HEIGHT="48" HPOS="285" ID="tl_13" VPOS="841" WIDTH="1441">
<String CONTENT="tenait dans sa main la cour de Saint-James. On peut en ajouter" HEIGHT="48" HPOS="285" ID="string_tl_13" VPOS="841" WIDTH="1441"/>
</TextLine>
<TextLine BASELINE="283 941 1724 941" HEIGHT="50" HPOS="283" ID="tl_14" VPOS="891" WIDTH="1441">
<String CONTENT="une troisième, la gracieuse comtesse d'Albany, qui, mariée au" HEIGHT="50" HPOS="283" ID="string_tl_14" VPOS="891" WIDTH="1441"/>
</TextLine>
<TextLine BASELINE="283 991 1729 991" HEIGHT="50" HPOS="283" ID="tl_15" VPOS="941" WIDTH="1446">
<String CONTENT="prétendant Charles-Edouard, représentait les intérêts dynas¬" HEIGHT="50" HPOS="283" ID="string_tl_15" VPOS="941" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="285 1043 1732 1043" HEIGHT="52" HPOS="285" ID="tl_16" VPOS="991" WIDTH="1447">
<String CONTENT="tiques des Bourbons. Elle nous dit elle-même qu’elle recevait" HEIGHT="52" HPOS="285" ID="string_tl_16" VPOS="991" WIDTH="1447"/>
</TextLine>
<TextLine BASELINE="284 1092 1732 1092" HEIGHT="51" HPOS="284" ID="tl_17" VPOS="1041" WIDTH="1448">
<String CONTENT="pour ce service une rente annuelle de 60,000 francs, et elle" HEIGHT="51" HPOS="284" ID="string_tl_17" VPOS="1041" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="283 1143 1731 1143" HEIGHT="51" HPOS="283" ID="tl_18" VPOS="1092" WIDTH="1448">
<String CONTENT="constate qu'elle l’a touchée jusqu’au mois d’août 1792, époque" HEIGHT="51" HPOS="283" ID="string_tl_18" VPOS="1092" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="284 1193 1731 1193" HEIGHT="51" HPOS="284" ID="tl_19" VPOS="1142" WIDTH="1447">
<String CONTENT="de la chute de Louis XVI, qui la lui avait gracieusement" HEIGHT="51" HPOS="284" ID="string_tl_19" VPOS="1142" WIDTH="1447"/>
</TextLine>
<TextLine BASELINE="285 1234 460 1234" HEIGHT="39" HPOS="285" ID="tl_20" VPOS="1195" WIDTH="175">
<String CONTENT="donnée." HEIGHT="39" HPOS="285" ID="string_tl_20" VPOS="1195" WIDTH="175"/>
</TextLine>
</TextBlock>
