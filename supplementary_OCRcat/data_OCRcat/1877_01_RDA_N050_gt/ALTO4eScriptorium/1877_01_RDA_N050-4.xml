<Polygon POINTS="290 2100 1736 2100 1736 2454 290 2454"/>
</Shape>
<TextLine BASELINE="340 2151 1734 2151" HEIGHT="50" HPOS="340" ID="tl_37" VPOS="2101" WIDTH="1394">
<String CONTENT="La vente de cette curieuse collection aura lieu, par nos" HEIGHT="50" HPOS="340" ID="string_tl_37" VPOS="2101" WIDTH="1394"/>
</TextLine>
<TextLine BASELINE="291 2201 1732 2201" HEIGHT="50" HPOS="291" ID="tl_38" VPOS="2151" WIDTH="1441">
<String CONTENT="soins, à l’hôtel Drouot, salle n° 5, le samedi 10 février prochain," HEIGHT="50" HPOS="291" ID="string_tl_38" VPOS="2151" WIDTH="1441"/>
</TextLine>
<TextLine BASELINE="291 2251 1735 2251" HEIGHT="50" HPOS="291" ID="tl_39" VPOS="2201" WIDTH="1444">
<String CONTENT="à deux heures de l’après-midi. Le catalogue sera adressé à tous" HEIGHT="50" HPOS="291" ID="string_tl_39" VPOS="2201" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="291 2302 1733 2302" HEIGHT="49" HPOS="291" ID="tl_40" VPOS="2253" WIDTH="1442">
<String CONTENT="nos lecteurs. Les personnes qui ne l’auraient pas reçu et qui" HEIGHT="49" HPOS="291" ID="string_tl_40" VPOS="2253" WIDTH="1442"/>
</TextLine>
<TextLine BASELINE="292 2352 1735 2352" HEIGHT="50" HPOS="292" ID="tl_41" VPOS="2302" WIDTH="1443">
<String CONTENT="désireraient l’avoir, sont priées de nous le mander par lettres" HEIGHT="50" HPOS="292" ID="string_tl_41" VPOS="2302" WIDTH="1443"/>
</TextLine>
<TextLine BASELINE="291 2403 1735 2403" HEIGHT="50" HPOS="291" ID="tl_42" VPOS="2353" WIDTH="1444">
<String CONTENT="affranchies. Il en sera tiré cinquante exemplaires sur papier de" HEIGHT="50" HPOS="291" ID="string_tl_42" VPOS="2353" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="294 2453 1072 2453" HEIGHT="50" HPOS="294" ID="tl_43" VPOS="2403" WIDTH="778">
<String CONTENT="Hollande, au prix de deux francs." HEIGHT="50" HPOS="294" ID="string_tl_43" VPOS="2403" WIDTH="778"/>
</TextLine>
