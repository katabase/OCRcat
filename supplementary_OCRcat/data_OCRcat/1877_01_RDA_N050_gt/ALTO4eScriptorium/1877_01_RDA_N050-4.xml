<TextLine BASELINE="284 320 1729 320" HEIGHT="51" HPOS="284" ID="tl_2" VPOS="269" WIDTH="1445">
<String CONTENT="Maure et de Mlle de Vandy, regardées comme les premières," HEIGHT="51" HPOS="284" ID="string_tl_2" VPOS="269" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="287 369 1730 369" HEIGHT="50" HPOS="287" ID="tl_3" VPOS="319" WIDTH="1443">
<String CONTENT="avec Mme de Sablé, leur amie, qui aient donné à ce style une" HEIGHT="50" HPOS="287" ID="string_tl_3" VPOS="319" WIDTH="1443"/>
</TextLine>
<TextLine BASELINE="285 419 1730 419" HEIGHT="49" HPOS="285" ID="tl_4" VPOS="370" WIDTH="1445">
<String CONTENT="tournure originale. Elles ont commencé la révolution épisto¬" HEIGHT="49" HPOS="285" ID="string_tl_4" VPOS="370" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="285 472 1728 472" HEIGHT="51" HPOS="285" ID="tl_5" VPOS="421" WIDTH="1443">
<String CONTENT="laire qui a été close par l’illustre marquise. Leurs lettres" HEIGHT="51" HPOS="285" ID="string_tl_5" VPOS="421" WIDTH="1443"/>
</TextLine>
<TextLine BASELINE="283 520 1727 520" HEIGHT="49" HPOS="283" ID="tl_6" VPOS="471" WIDTH="1444">
<String CONTENT="faisaient les délices des délicats de la première moitié du" HEIGHT="49" HPOS="283" ID="string_tl_6" VPOS="471" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="284 572 1727 572" HEIGHT="51" HPOS="284" ID="tl_7" VPOS="521" WIDTH="1443">
<String CONTENT="XVIIe siècle : on s’en arrachait les copies, qui circulaient de" HEIGHT="51" HPOS="284" ID="string_tl_7" VPOS="521" WIDTH="1443"/>
</TextLine>
<TextLine BASELINE="284 611 624 611" HEIGHT="38" HPOS="284" ID="tl_8" VPOS="573" WIDTH="340">
