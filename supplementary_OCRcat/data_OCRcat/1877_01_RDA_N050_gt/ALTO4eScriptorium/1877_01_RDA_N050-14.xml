<TextLine BASELINE="450 832 1776 832" HEIGHT="40" HPOS="450" ID="tl_15" VPOS="792" WIDTH="1326">
<String CONTENT="Très-curieuse lettre politique, écrite un an avant sa mort. Alors" HEIGHT="40" HPOS="450" ID="string_tl_15" VPOS="792" WIDTH="1326"/>
</TextLine>
<TextLine BASELINE="411 871 1774 871" HEIGHT="39" HPOS="411" ID="tl_16" VPOS="832" WIDTH="1363">
<String CONTENT="dans le parti républicain et en pleine opposition avec le gouvernement," HEIGHT="39" HPOS="411" ID="string_tl_16" VPOS="832" WIDTH="1363"/>
</TextLine>
<TextLine BASELINE="412 911 1776 911" HEIGHT="40" HPOS="412" ID="tl_17" VPOS="871" WIDTH="1364">
<String CONTENT="il a été obligé de faire valoir auprès de M. de Broglie, ministre, la" HEIGHT="40" HPOS="412" ID="string_tl_17" VPOS="871" WIDTH="1364"/>
</TextLine>
<TextLine BASELINE="410 950 1774 950" HEIGHT="41" HPOS="410" ID="tl_18" VPOS="909" WIDTH="1364">
<String CONTENT="dissidence d’opinions qui existe entre lui et M. Masclet, son protégé." HEIGHT="41" HPOS="410" ID="string_tl_18" VPOS="909" WIDTH="1364"/>
</TextLine>
<TextLine BASELINE="413 989 1774 989" HEIGHT="40" HPOS="413" ID="tl_19" VPOS="949" WIDTH="1361">
<String CONTENT="« J’ai même été obligé d’ajouter en riant, dit-il, que pour mettre à" HEIGHT="40" HPOS="413" ID="string_tl_19" VPOS="949" WIDTH="1361"/>
</TextLine>
<TextLine BASELINE="411 1029 1775 1029" HEIGHT="41" HPOS="411" ID="tl_20" VPOS="988" WIDTH="1364">
<String CONTENT="l'aise sa politique de juste milieu, je devais en conscience lui dire que" HEIGHT="41" HPOS="411" ID="string_tl_20" VPOS="988" WIDTH="1364"/>
</TextLine>
<TextLine BASELINE="412 1067 1775 1067" HEIGHT="39" HPOS="412" ID="tl_21" VPOS="1028" WIDTH="1363">
<String CONTENT="vous étiez beaucoup plus de sa nuance que de la mienne. » Il a" HEIGHT="39" HPOS="412" ID="string_tl_21" VPOS="1028" WIDTH="1363"/>
</TextLine>
<TextLine BASELINE="412 1106 1776 1106" HEIGHT="39" HPOS="412" ID="tl_22" VPOS="1067" WIDTH="1364">
<String CONTENT="rappelé aussi l’amitié que la belle-mère de M. de Broglie, Mme de" HEIGHT="39" HPOS="412" ID="string_tl_22" VPOS="1067" WIDTH="1364"/>
</TextLine>
<TextLine BASELINE="412 1145 1776 1145" HEIGHT="39" HPOS="412" ID="tl_23" VPOS="1106" WIDTH="1364">
<String CONTENT="Staël, a pour M. Masclet, dont on veut donner la place à un autre, etc." HEIGHT="39" HPOS="412" ID="string_tl_23" VPOS="1106" WIDTH="1364"/>
</TextLine>
