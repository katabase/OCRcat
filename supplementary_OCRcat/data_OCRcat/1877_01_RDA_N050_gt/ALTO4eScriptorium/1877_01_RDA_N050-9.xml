<TextLine BASELINE="294 1615 1612 1615" HEIGHT="42" HPOS="294" ID="tl_32" VPOS="1573" WIDTH="1318">
<String CONTENT="Très-curieuse lettre sur les événements qui ont suivi la bataille de" HEIGHT="42" HPOS="294" ID="string_tl_32" VPOS="1573" WIDTH="1318"/>
</TextLine>
<TextLine BASELINE="256 1654 1611 1654" HEIGHT="39" HPOS="256" ID="tl_33" VPOS="1615" WIDTH="1355">
<String CONTENT="Waterloo. Ces événements se succèdent avec une telle rapidité, qu’on" HEIGHT="39" HPOS="256" ID="string_tl_33" VPOS="1615" WIDTH="1355"/>
</TextLine>
<TextLine BASELINE="259 1695 1611 1695" HEIGHT="41" HPOS="259" ID="tl_34" VPOS="1654" WIDTH="1352">
<String CONTENT="ne peut ni les prévenir ni les utiliser. Il est essentiel que tous les" HEIGHT="41" HPOS="259" ID="string_tl_34" VPOS="1654" WIDTH="1352"/>
</TextLine>
<TextLine BASELINE="261 1733 1612 1733" HEIGHT="39" HPOS="261" ID="tl_35" VPOS="1694" WIDTH="1351">
<String CONTENT="Français se réunissent et que toutes les nuances cessent. Les étrangers" HEIGHT="39" HPOS="261" ID="string_tl_35" VPOS="1694" WIDTH="1351"/>
</TextLine>
<TextLine BASELINE="258 1772 1612 1772" HEIGHT="41" HPOS="258" ID="tl_36" VPOS="1731" WIDTH="1354">
<String CONTENT="n’ont pas conquis la France : elle s’est soumise, et cependant la" HEIGHT="41" HPOS="258" ID="string_tl_36" VPOS="1731" WIDTH="1354"/>
</TextLine>
<TextLine BASELINE="257 1811 1613 1811" HEIGHT="39" HPOS="257" ID="tl_37" VPOS="1772" WIDTH="1356">
<String CONTENT="situation est affreuse. Quant à lui, il attend le retour du calme pour" HEIGHT="39" HPOS="257" ID="string_tl_37" VPOS="1772" WIDTH="1356"/>
</TextLine>
<TextLine BASELINE="256 1850 723 1850" HEIGHT="38" HPOS="256" ID="tl_38" VPOS="1812" WIDTH="467">
<String CONTENT="aller aux Aigalades, etc." HEIGHT="38" HPOS="256" ID="string_tl_38" VPOS="1812" WIDTH="467"/>
</TextLine>
