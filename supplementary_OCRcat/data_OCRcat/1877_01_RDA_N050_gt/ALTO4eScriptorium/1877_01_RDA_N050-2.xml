<TextLine BASELINE="321 292 1763 292" HEIGHT="50" HPOS="321" ID="tl_1" VPOS="242" WIDTH="1442">
<String CONTENT="prématurément, il est vrai, l’enlever au milieu de son œuvre." HEIGHT="50" HPOS="321" ID="string_tl_1" VPOS="242" WIDTH="1442"/>
</TextLine>
<TextLine BASELINE="324 344 1764 344" HEIGHT="51" HPOS="324" ID="tl_2" VPOS="293" WIDTH="1440">
<String CONTENT="Tout incomplète qu’elle est restée, elle n’aura pas été stérile," HEIGHT="51" HPOS="324" ID="string_tl_2" VPOS="293" WIDTH="1440"/>
</TextLine>
<TextLine BASELINE="323 393 1768 393" HEIGHT="49" HPOS="323" ID="tl_3" VPOS="344" WIDTH="1445">
<String CONTENT="car elle a fourni des documents précieux pour l’histoire d’une" HEIGHT="49" HPOS="323" ID="string_tl_3" VPOS="344" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="323 444 1766 444" HEIGHT="50" HPOS="323" ID="tl_4" VPOS="394" WIDTH="1443">
<String CONTENT="époque importante, qui est devenue, depuis quarante ans," HEIGHT="50" HPOS="323" ID="string_tl_4" VPOS="394" WIDTH="1443"/>
</TextLine>
<TextLine BASELINE="323 495 1435 495" HEIGHT="50" HPOS="323" ID="tl_5" VPOS="445" WIDTH="1112">
<String CONTENT="l’objet des plus sérieuses études de nos érudits." HEIGHT="50" HPOS="323" ID="string_tl_5" VPOS="445" WIDTH="1112"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="857" HPOS="318" ID="r_1_2" VPOS="493" WIDTH="1454">
