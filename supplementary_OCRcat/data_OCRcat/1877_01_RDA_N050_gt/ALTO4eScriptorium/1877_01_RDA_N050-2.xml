</Shape>
<TextLine BASELINE="373 1399 1770 1399" HEIGHT="51" HPOS="373" ID="tl_23" VPOS="1348" WIDTH="1397">
<String CONTENT="On comprend alors que cette collection d’autographes de" HEIGHT="51" HPOS="373" ID="string_tl_23" VPOS="1348" WIDTH="1397"/>
</TextLine>
<TextLine BASELINE="326 1448 1772 1448" HEIGHT="50" HPOS="326" ID="tl_24" VPOS="1398" WIDTH="1446">
<String CONTENT="femmes célèbres, dont la plupart appartiennent au règne de" HEIGHT="50" HPOS="326" ID="string_tl_24" VPOS="1398" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="327 1499 1775 1499" HEIGHT="48" HPOS="327" ID="tl_25" VPOS="1451" WIDTH="1448">
<String CONTENT="Louis XIII, tout en nous présentant ce règne sous un aspect" HEIGHT="48" HPOS="327" ID="string_tl_25" VPOS="1451" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="326 1551 1774 1551" HEIGHT="51" HPOS="326" ID="tl_26" VPOS="1500" WIDTH="1448">
<String CONTENT="attrayant, ne laisse pas que d’avoir une haute importance" HEIGHT="51" HPOS="326" ID="string_tl_26" VPOS="1500" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="326 1600 568 1600" HEIGHT="47" HPOS="326" ID="tl_27" VPOS="1553" WIDTH="242">
<String CONTENT="historique." HEIGHT="47" HPOS="326" ID="string_tl_27" VPOS="1553" WIDTH="242"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="204" HPOS="328" ID="r_1_4" VPOS="1600" WIDTH="1449">
<Shape>
<Polygon POINTS="328 1600 1777 1600 1777 1804 328 1804"/>
</Shape>
<TextLine BASELINE="376 1650 1773 1650" HEIGHT="49" HPOS="376" ID="tl_28" VPOS="1601" WIDTH="1397">
<String CONTENT="Le soin tout particulier avec lequel a été rédigé le catalogue," HEIGHT="49" HPOS="376" ID="string_tl_28" VPOS="1601" WIDTH="1397"/>
</TextLine>
<TextLine BASELINE="330 1701 1775 1701" HEIGHT="49" HPOS="330" ID="tl_29" VPOS="1652" WIDTH="1445">
<String CONTENT="la table que nous y avons ajoutée, nous dispensent d’entrer ici" HEIGHT="49" HPOS="330" ID="string_tl_29" VPOS="1652" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="329 1751 1776 1751" HEIGHT="49" HPOS="329" ID="tl_30" VPOS="1702" WIDTH="1447">
<String CONTENT="dans de grands détails. Nous nous bornerons à quelques" HEIGHT="49" HPOS="329" ID="string_tl_30" VPOS="1702" WIDTH="1447"/>
</TextLine>
<TextLine BASELINE="329 1803 584 1803" HEIGHT="39" HPOS="329" ID="tl_31" VPOS="1764" WIDTH="255">
<String CONTENT="remarques." HEIGHT="39" HPOS="329" ID="string_tl_31" VPOS="1764" WIDTH="255"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="657" HPOS="333" ID="r_1_5" VPOS="1807" WIDTH="1454">
<Shape>
<Polygon POINTS="333 1807 1787 1807 1787 2464 333 2464"/>
</Shape>
<TextLine BASELINE="381 1858 1775 1858" HEIGHT="50" HPOS="381" ID="tl_32" VPOS="1808" WIDTH="1394">
<String CONTENT="L’idée primitive avait été de former une galerie d’auto¬" HEIGHT="50" HPOS="381" ID="string_tl_32" VPOS="1808" WIDTH="1394"/>
</TextLine>
<TextLine BASELINE="335 1909 1776 1909" HEIGHT="50" HPOS="335" ID="tl_33" VPOS="1859" WIDTH="1441">
<String CONTENT="graphes des femmes célèbres citées dans Mme, de Sévigné. On en" HEIGHT="50" HPOS="335" ID="string_tl_33" VPOS="1859" WIDTH="1441"/>
</TextLine>
<TextLine BASELINE="334 1958 1776 1958" HEIGHT="49" HPOS="334" ID="tl_34" VPOS="1909" WIDTH="1442">
<String CONTENT="avait déjà réuni un bon nombre, et des plus difficiles à trouver." HEIGHT="49" HPOS="334" ID="string_tl_34" VPOS="1909" WIDTH="1442"/>
</TextLine>
<TextLine BASELINE="336 2008 1781 2008" HEIGHT="49" HPOS="336" ID="tl_35" VPOS="1959" WIDTH="1445">
<String CONTENT="A. ces autographes sont venus se joindre ceux des grandes dames" HEIGHT="49" HPOS="336" ID="string_tl_35" VPOS="1959" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="334 2057 1780 2057" HEIGHT="48" HPOS="334" ID="tl_36" VPOS="2009" WIDTH="1446">
<String CONTENT="citées dans Tallemant des Réaux, dans Mme de Motteville, dans" HEIGHT="48" HPOS="334" ID="string_tl_36" VPOS="2009" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="335 2109 1781 2109" HEIGHT="49" HPOS="335" ID="tl_37" VPOS="2060" WIDTH="1446">
<String CONTENT="Saint-Simon, Barbier, etc. On se proposait d’illustrer des" HEIGHT="49" HPOS="335" ID="string_tl_37" VPOS="2060" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="336 2160 1782 2160" HEIGHT="50" HPOS="336" ID="tl_38" VPOS="2110" WIDTH="1446">
<String CONTENT="exemplaires exceptionnels de ces célèbres chroniqueurs, soit" HEIGHT="50" HPOS="336" ID="string_tl_38" VPOS="2110" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="337 2208 1783 2208" HEIGHT="48" HPOS="337" ID="tl_39" VPOS="2160" WIDTH="1446">
<String CONTENT="en les intercalant dans les volumes, soit en en formant des" HEIGHT="48" HPOS="337" ID="string_tl_39" VPOS="2160" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="340 2260 1781 2260" HEIGHT="49" HPOS="340" ID="tl_40" VPOS="2211" WIDTH="1441">
<String CONTENT="albums annexes ; mais la collection, commencée de longue date," HEIGHT="49" HPOS="340" ID="string_tl_40" VPOS="2211" WIDTH="1441"/>
</TextLine>
<TextLine BASELINE="339 2311 1784 2311" HEIGHT="50" HPOS="339" ID="tl_41" VPOS="2261" WIDTH="1445">
<String CONTENT="s’est trouvée interrompue depuis longtemps déjà et est restée" HEIGHT="50" HPOS="339" ID="string_tl_41" VPOS="2261" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="340 2361 1786 2361" HEIGHT="49" HPOS="340" ID="tl_42" VPOS="2312" WIDTH="1446">
<String CONTENT="incomplète. Maintenant elle va être dispersée; ruais ses débris" HEIGHT="49" HPOS="340" ID="string_tl_42" VPOS="2312" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="340 2411 1785 2411" HEIGHT="49" HPOS="340" ID="tl_43" VPOS="2362" WIDTH="1445">
<String CONTENT="serviront à composer d’autres collections de ce genre si origi¬" HEIGHT="49" HPOS="340" ID="string_tl_43" VPOS="2362" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="340 2463 729 2463" HEIGHT="49" HPOS="340" ID="tl_44" VPOS="2414" WIDTH="389">
<String CONTENT="nal et si piquant." HEIGHT="49" HPOS="340" ID="string_tl_44" VPOS="2414" WIDTH="389"/>
</TextLine>
</TextBlock>
