</Shape>
<TextLine BASELINE="371 544 1766 544" HEIGHT="50" HPOS="371" ID="tl_6" VPOS="494" WIDTH="1395">
<String CONTENT="Ce qui avait passionné M. Pécard, c’était la grande figure de" HEIGHT="50" HPOS="371" ID="string_tl_6" VPOS="494" WIDTH="1395"/>
</TextLine>
<TextLine BASELINE="319 594 1767 594" HEIGHT="50" HPOS="319" ID="tl_7" VPOS="544" WIDTH="1448">
<String CONTENT="Richelieu, dominant tout le règne. Tout autre a été le point de" HEIGHT="50" HPOS="319" ID="string_tl_7" VPOS="544" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="320 644 1769 644" HEIGHT="49" HPOS="320" ID="tl_8" VPOS="595" WIDTH="1449">
<String CONTENT="vue de l’amateur qui a formé la collection dont nous annonçons" HEIGHT="49" HPOS="320" ID="string_tl_8" VPOS="595" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="322 695 1768 695" HEIGHT="50" HPOS="322" ID="tl_9" VPOS="645" WIDTH="1446">
<String CONTENT="la vente. S'attachant à la même époque, il en a écarté la partie" HEIGHT="50" HPOS="322" ID="string_tl_9" VPOS="645" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="320 747 1768 747" HEIGHT="52" HPOS="320" ID="tl_10" VPOS="695" WIDTH="1448">
<String CONTENT="grave, sombre, tragique, et n’a voulu voir que le côté gracieux" HEIGHT="52" HPOS="320" ID="string_tl_10" VPOS="695" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="319 794 1768 794" HEIGHT="48" HPOS="319" ID="tl_11" VPOS="746" WIDTH="1449">
<String CONTENT="du tableau : les femmes. Il faut avouer que jamais, en aucun" HEIGHT="48" HPOS="319" ID="string_tl_11" VPOS="746" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="320 846 1769 846" HEIGHT="51" HPOS="320" ID="tl_12" VPOS="795" WIDTH="1449">
<String CONTENT="temps, elles n’ont joué un si grand rôle. L’ancienne chevalerie" HEIGHT="51" HPOS="320" ID="string_tl_12" VPOS="795" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="320 895 1770 895" HEIGHT="49" HPOS="320" ID="tl_13" VPOS="846" WIDTH="1450">
<String CONTENT="a refleuri à la Cour de Louis XIII, pour s'éteindre ensuite tout" HEIGHT="49" HPOS="320" ID="string_tl_13" VPOS="846" WIDTH="1450"/>
</TextLine>
<TextLine BASELINE="320 946 1765 946" HEIGHT="50" HPOS="320" ID="tl_14" VPOS="896" WIDTH="1445">
<String CONTENT="à fait. Ce prince, pourtant, y était peu porté de lui-même ;" HEIGHT="50" HPOS="320" ID="string_tl_14" VPOS="896" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="322 997 1768 997" HEIGHT="51" HPOS="322" ID="tl_15" VPOS="946" WIDTH="1446">
<String CONTENT="mais cela le distrayait sans doute du profond ennui qui n’a" HEIGHT="51" HPOS="322" ID="string_tl_15" VPOS="946" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="319 1047 1770 1047" HEIGHT="51" HPOS="319" ID="tl_16" VPOS="996" WIDTH="1451">
<String CONTENT="cessé de l’accabler. Ce qu’il y a de certain, c’est que la galan¬" HEIGHT="51" HPOS="319" ID="string_tl_16" VPOS="996" WIDTH="1451"/>
</TextLine>
<TextLine BASELINE="322 1096 1769 1096" HEIGHT="49" HPOS="322" ID="tl_17" VPOS="1047" WIDTH="1447">
<String CONTENT="terie a été un des plus puissants ressorts politiques de" HEIGHT="49" HPOS="322" ID="string_tl_17" VPOS="1047" WIDTH="1447"/>
</TextLine>
<TextLine BASELINE="321 1146 1771 1146" HEIGHT="49" HPOS="321" ID="tl_18" VPOS="1097" WIDTH="1450">
<String CONTENT="Richelieu. Par les femmes, il tenait les hommes, et les intrigues" HEIGHT="49" HPOS="321" ID="string_tl_18" VPOS="1097" WIDTH="1450"/>
</TextLine>
<TextLine BASELINE="322 1198 1768 1198" HEIGHT="50" HPOS="322" ID="tl_19" VPOS="1148" WIDTH="1446">
<String CONTENT="galantes étaient pour lui les auxiliaires des affaires de l’Etat :" HEIGHT="50" HPOS="322" ID="string_tl_19" VPOS="1148" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="322 1246 1771 1246" HEIGHT="50" HPOS="322" ID="tl_20" VPOS="1196" WIDTH="1449">
<String CONTENT="il les menait de front, avec la même habileté, on pourrait dire" HEIGHT="50" HPOS="322" ID="string_tl_20" VPOS="1196" WIDTH="1449"/>
</TextLine>
<TextLine BASELINE="323 1298 1769 1298" HEIGHT="50" HPOS="323" ID="tl_21" VPOS="1248" WIDTH="1446">
<String CONTENT="avec la même ruse, mais la noblesse du but justifiait tout à" HEIGHT="50" HPOS="323" ID="string_tl_21" VPOS="1248" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="322 1349 528 1349" HEIGHT="38" HPOS="322" ID="tl_22" VPOS="1311" WIDTH="206">
<String CONTENT="ses yeux." HEIGHT="38" HPOS="322" ID="string_tl_22" VPOS="1311" WIDTH="206"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="254" HPOS="325" ID="r_1_3" VPOS="1347" WIDTH="1451">
