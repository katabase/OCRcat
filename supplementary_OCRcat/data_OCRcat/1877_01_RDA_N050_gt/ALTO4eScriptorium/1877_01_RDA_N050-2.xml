</Shape>
<TextLine BASELINE="392 2517 1786 2517" HEIGHT="49" HPOS="392" ID="tl_45" VPOS="2468" WIDTH="1394">
<String CONTENT="Parmi les femmes les plus célèbres nous citerons : les reines" HEIGHT="49" HPOS="392" ID="string_tl_45" VPOS="2468" WIDTH="1394"/>
</TextLine>
<TextLine BASELINE="343 2570 1787 2570" HEIGHT="52" HPOS="343" ID="tl_46" VPOS="2518" WIDTH="1444">
<String CONTENT="de France Marie d’Anjou et Jeanne de France, autographes" HEIGHT="52" HPOS="343" ID="string_tl_46" VPOS="2518" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="343 2618 1785 2618" HEIGHT="50" HPOS="343" ID="tl_47" VPOS="2568" WIDTH="1442">
<String CONTENT="de toute rareté; Marie de Médicis, Anne d’Autriche, Marie-" HEIGHT="50" HPOS="343" ID="string_tl_47" VPOS="2568" WIDTH="1442"/>
</TextLine>
<TextLine BASELINE="353 2669 1787 2669" HEIGHT="49" HPOS="353" ID="tl_48" VPOS="2620" WIDTH="1434">
<String CONTENT="Thérèse d’Autriche et Marie Leczinska, femmes de quatre" HEIGHT="49" HPOS="353" ID="string_tl_48" VPOS="2620" WIDTH="1434"/>
</TextLine>
