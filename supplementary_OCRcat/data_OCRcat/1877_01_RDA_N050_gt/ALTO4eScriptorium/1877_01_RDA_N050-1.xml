<TextLine BASELINE="230 2457 1624 2457" HEIGHT="52" HPOS="230" ID="tl_37" VPOS="2405" WIDTH="1394">
<String CONTENT="On se rappelle la collection Pécard, qui s’est vendue en" HEIGHT="52" HPOS="230" ID="string_tl_37" VPOS="2405" WIDTH="1394"/>
</TextLine>
<TextLine BASELINE="180 2508 1624 2508" HEIGHT="52" HPOS="180" ID="tl_38" VPOS="2456" WIDTH="1444">
<String CONTENT="1873, et qui a produit quelque sensation. Il s’était épris du" HEIGHT="52" HPOS="180" ID="string_tl_38" VPOS="2456" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="180 2557 1626 2557" HEIGHT="51" HPOS="180" ID="tl_39" VPOS="2506" WIDTH="1446">
<String CONTENT="règne de Louis XIII, et ce règne d’une trentaine d’années avait" HEIGHT="51" HPOS="180" ID="string_tl_39" VPOS="2506" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="182 2608 1624 2608" HEIGHT="52" HPOS="182" ID="tl_40" VPOS="2556" WIDTH="1442">
<String CONTENT="suffi à remplir sa carrière de collectionneur ; que dis-je ? il" HEIGHT="52" HPOS="182" ID="string_tl_40" VPOS="2556" WIDTH="1442"/>
</TextLine>
<TextLine BASELINE="182 2660 1625 2660" HEIGHT="51" HPOS="182" ID="tl_41" VPOS="2609" WIDTH="1443">
<String CONTENT="s’était épuisé en recherches opiniâtres, et la mort est venue," HEIGHT="51" HPOS="182" ID="string_tl_41" VPOS="2609" WIDTH="1443"/>
</TextLine>
