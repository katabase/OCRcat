<TextLine BASELINE="227 1599 1618 1599" HEIGHT="49" HPOS="227" ID="tl_20" VPOS="1550" WIDTH="1391">
<String CONTENT="Les goûts, si on voulait les examiner au microscope, sont" HEIGHT="49" HPOS="227" ID="string_tl_20" VPOS="1550" WIDTH="1391"/>
</TextLine>
<TextLine BASELINE="173 1651 1616 1651" HEIGHT="50" HPOS="173" ID="tl_21" VPOS="1601" WIDTH="1443">
<String CONTENT="peut-être aussi variés que les exemplaires du genre humain." HEIGHT="50" HPOS="173" ID="string_tl_21" VPOS="1601" WIDTH="1443"/>
</TextLine>
<TextLine BASELINE="173 1701 1617 1701" HEIGHT="51" HPOS="173" ID="tl_22" VPOS="1650" WIDTH="1444">
<String CONTENT="Mais, sans se livrer à une analyse minutieuse, qui est du" HEIGHT="51" HPOS="173" ID="string_tl_22" VPOS="1650" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="177 1751 1621 1751" HEIGHT="50" HPOS="177" ID="tl_23" VPOS="1701" WIDTH="1444">
<String CONTENT="domaine de la psychologie pure, en regardant les choses de" HEIGHT="50" HPOS="177" ID="string_tl_23" VPOS="1701" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="176 1802 1622 1802" HEIGHT="51" HPOS="176" ID="tl_24" VPOS="1751" WIDTH="1446">
<String CONTENT="haut, on arrive à un classement par groupes qui satisfait la" HEIGHT="51" HPOS="176" ID="string_tl_24" VPOS="1751" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="178 1853 1622 1853" HEIGHT="51" HPOS="178" ID="tl_25" VPOS="1802" WIDTH="1444">
<String CONTENT="raison. Il y en a déjà un nombre respectable, rien que dans le" HEIGHT="51" HPOS="178" ID="string_tl_25" VPOS="1802" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="177 1902 1621 1902" HEIGHT="51" HPOS="177" ID="tl_26" VPOS="1851" WIDTH="1444">
<String CONTENT="domaine de la fantaisie. Il suffirait, pour s’en convaincre, d’é¬" HEIGHT="51" HPOS="177" ID="string_tl_26" VPOS="1851" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="177 1953 1622 1953" HEIGHT="50" HPOS="177" ID="tl_27" VPOS="1903" WIDTH="1445">
<String CONTENT="numérer les divers genres de collections qui ont été faites, et qui" HEIGHT="50" HPOS="177" ID="string_tl_27" VPOS="1903" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="179 2003 1621 2003" HEIGHT="49" HPOS="179" ID="tl_28" VPOS="1954" WIDTH="1442">
<String CONTENT="toutes répondent à un besoin de l’esprit. Ces goûts, qui ont un" HEIGHT="49" HPOS="179" ID="string_tl_28" VPOS="1954" WIDTH="1442"/>
</TextLine>
<TextLine BASELINE="178 2054 1624 2054" HEIGHT="51" HPOS="178" ID="tl_29" VPOS="2003" WIDTH="1446">
<String CONTENT="nom générique, se subdivisent encore à l’infini, suivant le point" HEIGHT="51" HPOS="178" ID="string_tl_29" VPOS="2003" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="179 2105 1623 2105" HEIGHT="52" HPOS="179" ID="tl_30" VPOS="2053" WIDTH="1444">
<String CONTENT="de vue particulier. Ainsi, pour les autographes, bien peu de" HEIGHT="52" HPOS="179" ID="string_tl_30" VPOS="2053" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="178 2154 1624 2154" HEIGHT="51" HPOS="178" ID="tl_31" VPOS="2103" WIDTH="1446">
<String CONTENT="personnes embrassent l’ensemble des personnages célèbres" HEIGHT="51" HPOS="178" ID="string_tl_31" VPOS="2103" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="180 2206 1620 2206" HEIGHT="52" HPOS="180" ID="tl_32" VPOS="2154" WIDTH="1440">
<String CONTENT="dont on peut se procurer l’écriture : chacun à ses prédilections," HEIGHT="52" HPOS="180" ID="string_tl_32" VPOS="2154" WIDTH="1440"/>
</TextLine>
<TextLine BASELINE="179 2255 1625 2255" HEIGHT="51" HPOS="179" ID="tl_33" VPOS="2204" WIDTH="1446">
<String CONTENT="suivant son éducation, le milieu où il vit, la tendance de ses" HEIGHT="51" HPOS="179" ID="string_tl_33" VPOS="2204" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="180 2306 1623 2306" HEIGHT="51" HPOS="180" ID="tl_34" VPOS="2255" WIDTH="1443">
<String CONTENT="études, ou son simple caprice, qui est aussi souverain que les" HEIGHT="51" HPOS="180" ID="string_tl_34" VPOS="2255" WIDTH="1443"/>
</TextLine>
<TextLine BASELINE="180 2355 1625 2355" HEIGHT="50" HPOS="180" ID="tl_35" VPOS="2305" WIDTH="1445">
<String CONTENT="autres mobiles. De là les spécialités dans le genre, ou les" HEIGHT="50" HPOS="180" ID="string_tl_35" VPOS="2305" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="180 2396 324 2396" HEIGHT="38" HPOS="180" ID="tl_36" VPOS="2358" WIDTH="144">
<String CONTENT="séries." HEIGHT="38" HPOS="180" ID="string_tl_36" VPOS="2358" WIDTH="144"/>
</TextLine>
