<TextLine BASELINE="225 422 1525 422" HEIGHT="67" HPOS="225" ID="tl_1" VPOS="355" WIDTH="1300">
<String CONTENT="merce maritime un peu malgré le gouvernement, qui ne se sent pas la force" HEIGHT="67" HPOS="225" ID="string_tl_1" VPOS="355" WIDTH="1300"/>
</TextLine>
<TextLine BASELINE="224 467 1522 467" HEIGHT="67" HPOS="224" ID="tl_2" VPOS="400" WIDTH="1298">
<String CONTENT="de soutenir efficacement leurs entreprises ; cette attitude attira au prési¬" HEIGHT="67" HPOS="224" ID="string_tl_2" VPOS="400" WIDTH="1298"/>
</TextLine>
<TextLine BASELINE="225 511 1520 511" HEIGHT="67" HPOS="225" ID="tl_3" VPOS="444" WIDTH="1295">
<String CONTENT="dent Jefferson l’animadversion de la classe commerçante et de la population" HEIGHT="67" HPOS="225" ID="string_tl_3" VPOS="444" WIDTH="1295"/>
</TextLine>
<TextLine BASELINE="226 553 1521 553" HEIGHT="65" HPOS="226" ID="tl_4" VPOS="488" WIDTH="1295">
<String CONTENT="du littoral. Il signale le jeu de l’Angleterre qui excite les états contre le" HEIGHT="65" HPOS="226" ID="string_tl_4" VPOS="488" WIDTH="1295"/>
</TextLine>
<TextLine BASELINE="227 599 1525 599" HEIGHT="67" HPOS="227" ID="tl_5" VPOS="532" WIDTH="1298">
<String CONTENT="gouvernement suivant les circonstances et ses intérêts et empêche ensuite" HEIGHT="67" HPOS="227" ID="string_tl_5" VPOS="532" WIDTH="1298"/>
</TextLine>
<TextLine BASELINE="228 644 1527 644" HEIGHT="67" HPOS="228" ID="tl_6" VPOS="577" WIDTH="1299">
<String CONTENT="la formation d’un plan et d’une ligne de conduite fixe, mais ces faits, si" HEIGHT="67" HPOS="228" ID="string_tl_6" VPOS="577" WIDTH="1299"/>
</TextLine>
<TextLine BASELINE="228 689 1525 689" HEIGHT="68" HPOS="228" ID="tl_7" VPOS="621" WIDTH="1297">
<String CONTENT="regrettables à certains égards, peuvent avoir quelques aspects avantageux." HEIGHT="68" HPOS="228" ID="string_tl_7" VPOS="621" WIDTH="1297"/>
</TextLine>
<TextLine BASELINE="230 733 1526 733" HEIGHT="67" HPOS="230" ID="tl_8" VPOS="666" WIDTH="1296">
<String CONTENT="La richesse de l’Angleterre et celle de l’Amérique sont basées sur des prin¬" HEIGHT="67" HPOS="230" ID="string_tl_8" VPOS="666" WIDTH="1296"/>
</TextLine>
<TextLine BASELINE="229 775 1529 775" HEIGHT="65" HPOS="229" ID="tl_9" VPOS="710" WIDTH="1300">
<String CONTENT="cipes opposés ; l’Angleterre ne peut se maintenir que par le domaine exclusif" HEIGHT="65" HPOS="229" ID="string_tl_9" VPOS="710" WIDTH="1300"/>
</TextLine>
<TextLine BASELINE="230 822 1529 822" HEIGHT="68" HPOS="230" ID="tl_10" VPOS="754" WIDTH="1299">
<String CONTENT="de la mer ; l’Amérique ne peut conserver ses biens et les accroître que par" HEIGHT="68" HPOS="230" ID="string_tl_10" VPOS="754" WIDTH="1299"/>
</TextLine>
<TextLine BASELINE="230 866 1529 866" HEIGHT="67" HPOS="230" ID="tl_11" VPOS="799" WIDTH="1299">
<String CONTENT="une navigation indépendante. L’intérêt, mobile de la vie des états, rompra" HEIGHT="67" HPOS="230" ID="string_tl_11" VPOS="799" WIDTH="1299"/>
</TextLine>
<TextLine BASELINE="232 907 1529 907" HEIGHT="63" HPOS="232" ID="tl_12" VPOS="844" WIDTH="1297">
<String CONTENT="tous les arrangements que l’on pourra prendre, « et la discorde, et une dis¬" HEIGHT="63" HPOS="232" ID="string_tl_12" VPOS="844" WIDTH="1297"/>
</TextLine>
<TextLine BASELINE="232 953 1529 953" HEIGHT="66" HPOS="232" ID="tl_13" VPOS="887" WIDTH="1297">
<String CONTENT="corde éternelle, éclatera enfin entre des peuples qu’il est impossible d’asso¬" HEIGHT="66" HPOS="232" ID="string_tl_13" VPOS="887" WIDTH="1297"/>
</TextLine>
<TextLine BASELINE="232 997 1530 997" HEIGHT="65" HPOS="232" ID="tl_14" VPOS="932" WIDTH="1298">
<String CONTENT="cier solidement et avec durée, à moins que l’un n’établisse pour toujours" HEIGHT="65" HPOS="232" ID="string_tl_14" VPOS="932" WIDTH="1298"/>
</TextLine>
<TextLine BASELINE="235 1040 1534 1040" HEIGHT="69" HPOS="235" ID="tl_15" VPOS="971" WIDTH="1299">
<String CONTENT="les droits de sa supériorité et que l’autre ne consente à les reconnaître. » Le" HEIGHT="69" HPOS="235" ID="string_tl_15" VPOS="971" WIDTH="1299"/>
</TextLine>
<TextLine BASELINE="236 1086 1532 1086" HEIGHT="65" HPOS="236" ID="tl_16" VPOS="1021" WIDTH="1296">
<String CONTENT="gouvernement actuel des Etats-Unis a des vues supérieures à l’esprit et à" HEIGHT="65" HPOS="236" ID="string_tl_16" VPOS="1021" WIDTH="1296"/>
</TextLine>
<TextLine BASELINE="235 1131 1533 1131" HEIGHT="66" HPOS="235" ID="tl_17" VPOS="1065" WIDTH="1298">
<String CONTENT="l’instinct national des peuples qu’il gouverne. Si le gouvernement français" HEIGHT="66" HPOS="235" ID="string_tl_17" VPOS="1065" WIDTH="1298"/>
</TextLine>
<TextLine BASELINE="236 1174 1536 1174" HEIGHT="64" HPOS="236" ID="tl_18" VPOS="1110" WIDTH="1300">
<String CONTENT="l’avait voulu il est certain que l’Amérique se serait rangée à côté de la" HEIGHT="64" HPOS="236" ID="string_tl_18" VPOS="1110" WIDTH="1300"/>
</TextLine>
<TextLine BASELINE="238 1218 1537 1218" HEIGHT="64" HPOS="238" ID="tl_19" VPOS="1154" WIDTH="1299">
<String CONTENT="France depuis longtemps. M. d’Hauterive préconise la bienveillance en ce" HEIGHT="64" HPOS="238" ID="string_tl_19" VPOS="1154" WIDTH="1299"/>
</TextLine>
<TextLine BASELINE="239 1263 1537 1263" HEIGHT="64" HPOS="239" ID="tl_20" VPOS="1199" WIDTH="1298">
<String CONTENT="qui concerne l’application du blocus à l’égard des Américains ; il faut main¬" HEIGHT="64" HPOS="239" ID="string_tl_20" VPOS="1199" WIDTH="1298"/>
</TextLine>
<TextLine BASELINE="239 1307 1540 1307" HEIGHT="64" HPOS="239" ID="tl_21" VPOS="1243" WIDTH="1301">
<String CONTENT="tenir ce blocus, puisqu’il n’est qu’une représaille du système anglais, mais" HEIGHT="64" HPOS="239" ID="string_tl_21" VPOS="1243" WIDTH="1301"/>
</TextLine>
<TextLine BASELINE="241 1351 1541 1351" HEIGHT="64" HPOS="241" ID="tl_22" VPOS="1287" WIDTH="1300">
<String CONTENT="il en faut en adoucir les conséquences vis-à-vis des Américains, de façon que" HEIGHT="64" HPOS="241" ID="string_tl_22" VPOS="1287" WIDTH="1300"/>
</TextLine>
<TextLine BASELINE="240 1396 1541 1396" HEIGHT="67" HPOS="240" ID="tl_23" VPOS="1329" WIDTH="1301">
<String CONTENT="tout l’odieux de cette guerre retombe sur l’Angleterre et que les Américains" HEIGHT="67" HPOS="240" ID="string_tl_23" VPOS="1329" WIDTH="1301"/>
</TextLine>
<TextLine BASELINE="238 1441 643 1441" HEIGHT="44" HPOS="238" ID="tl_24" VPOS="1397" WIDTH="405">
<String CONTENT="se rangent à nos côtés." HEIGHT="44" HPOS="238" ID="string_tl_24" VPOS="1397" WIDTH="405"/>
</TextLine>
