<Polygon POINTS="35 754 1222 754 1222 1118 35 1118"/>
</Shape>
<TextLine BASELINE="75 794 1221 794" HEIGHT="39" HPOS="75" ID="tl_15" VPOS="755" WIDTH="1146">
<String CONTENT="Indépendamment de mémoires, de brochures, d’articles" HEIGHT="39" HPOS="75" ID="string_tl_15" VPOS="755" WIDTH="1146"/>
</TextLine>
<TextLine BASELINE="36 835 1217 835" HEIGHT="39" HPOS="36" ID="tl_16" VPOS="796" WIDTH="1181">
<String CONTENT="insérés dans les Comptes-rendus de l'Académie des Sciences," HEIGHT="39" HPOS="36" ID="string_tl_16" VPOS="796" WIDTH="1181"/>
</TextLine>
<TextLine BASELINE="37 874 1219 874" HEIGHT="39" HPOS="37" ID="tl_17" VPOS="835" WIDTH="1182">
<String CONTENT="dans le Dictionnaire du Commerce, etc., M. Dubrunfaut a" HEIGHT="39" HPOS="37" ID="string_tl_17" VPOS="835" WIDTH="1182"/>
</TextLine>
<TextLine BASELINE="36 916 1218 916" HEIGHT="40" HPOS="36" ID="tl_18" VPOS="876" WIDTH="1182">
<String CONTENT="publié : De l'art de la Distillation (1824); De la Fabrication du" HEIGHT="40" HPOS="36" ID="string_tl_18" VPOS="876" WIDTH="1182"/>
</TextLine>
<TextLine BASELINE="36 956 1218 956" HEIGHT="41" HPOS="36" ID="tl_19" VPOS="915" WIDTH="1182">
<String CONTENT="Sucre de betteraves (1822); Sucrage des vendanges avec les" HEIGHT="41" HPOS="36" ID="string_tl_19" VPOS="915" WIDTH="1182"/>
</TextLine>
<TextLine BASELINE="37 996 1220 996" HEIGHT="40" HPOS="37" ID="tl_20" VPOS="956" WIDTH="1183">
<String CONTENT="sucres raffinés de canne, de betterave, etc. (1854); Notice histo¬" HEIGHT="40" HPOS="37" ID="string_tl_20" VPOS="956" WIDTH="1183"/>
</TextLine>
<TextLine BASELINE="37 1037 1219 1037" HEIGHT="40" HPOS="37" ID="tl_21" VPOS="997" WIDTH="1182">
<String CONTENT="rique sur la distillation des betteraves (1856); L’Osmose et ses" HEIGHT="40" HPOS="37" ID="string_tl_21" VPOS="997" WIDTH="1182"/>
</TextLine>
<TextLine BASELINE="38 1077 1219 1077" HEIGHT="40" HPOS="38" ID="tl_22" VPOS="1037" WIDTH="1181">
<String CONTENT="applications industrielles (1873); Le Sucre dans ses rapports" HEIGHT="40" HPOS="38" ID="string_tl_22" VPOS="1037" WIDTH="1181"/>
</TextLine>
<TextLine BASELINE="38 1117 938 1117" HEIGHT="40" HPOS="38" ID="tl_23" VPOS="1077" WIDTH="900">
<String CONTENT="avec la science, l’agriculture, le commerce, etc." HEIGHT="40" HPOS="38" ID="string_tl_23" VPOS="1077" WIDTH="900"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="723" HPOS="39" ID="r_2_4" VPOS="1125" WIDTH="1186">
<Shape>
<Polygon POINTS="39 1125 1225 1125 1225 1848 39 1848"/>
</Shape>
<TextLine BASELINE="80 1165 1219 1165" HEIGHT="39" HPOS="80" ID="tl_24" VPOS="1126" WIDTH="1139">
<String CONTENT="C’est, croyons-nous, vers 1830, — il y a plus d’un demi-" HEIGHT="39" HPOS="80" ID="string_tl_24" VPOS="1126" WIDTH="1139"/>
</TextLine>
<TextLine BASELINE="40 1206 1221 1206" HEIGHT="40" HPOS="40" ID="tl_25" VPOS="1166" WIDTH="1181">
<String CONTENT="siècle,— que M. Dubrunfaut s’occupa d’autographes. Il se" HEIGHT="40" HPOS="40" ID="string_tl_25" VPOS="1166" WIDTH="1181"/>
</TextLine>
<TextLine BASELINE="41 1245 1221 1245" HEIGHT="39" HPOS="41" ID="tl_26" VPOS="1206" WIDTH="1180">
<String CONTENT="contenta, pour commencer, des pièces que lui donnaient ses" HEIGHT="39" HPOS="41" ID="string_tl_26" VPOS="1206" WIDTH="1180"/>
</TextLine>
<TextLine BASELINE="42 1278 1220 1278" HEIGHT="32" HPOS="42" ID="tl_27" VPOS="1246" WIDTH="1178">
<String CONTENT="nombreux amis du monde savant. De ce nombre était Jullien" HEIGHT="32" HPOS="42" ID="string_tl_27" VPOS="1246" WIDTH="1178"/>
</TextLine>
<TextLine BASELINE="43 1325 1222 1325" HEIGHT="38" HPOS="43" ID="tl_28" VPOS="1287" WIDTH="1179">
<String CONTENT="de Paris, qui lui laissait prendre dans sa volumineuse corres¬" HEIGHT="38" HPOS="43" ID="string_tl_28" VPOS="1287" WIDTH="1179"/>
</TextLine>
<TextLine BASELINE="43 1366 1217 1366" HEIGHT="40" HPOS="43" ID="tl_29" VPOS="1326" WIDTH="1174">
<String CONTENT="pondance les lettres de contemporains pouvant lui faire plaisir." HEIGHT="40" HPOS="43" ID="string_tl_29" VPOS="1326" WIDTH="1174"/>
</TextLine>
<TextLine BASELINE="44 1406 1219 1406" HEIGHT="39" HPOS="44" ID="tl_30" VPOS="1367" WIDTH="1175">
<String CONTENT="Il fut à cette époque un des premiers clients du père Danquin," HEIGHT="39" HPOS="44" ID="string_tl_30" VPOS="1367" WIDTH="1175"/>
</TextLine>
<TextLine BASELINE="45 1446 1222 1446" HEIGHT="40" HPOS="45" ID="tl_31" VPOS="1406" WIDTH="1177">
<String CONTENT="de Lefèvre et de Charon; sa collection s’augmenta rapidement" HEIGHT="40" HPOS="45" ID="string_tl_31" VPOS="1406" WIDTH="1177"/>
</TextLine>
<TextLine BASELINE="45 1486 1218 1486" HEIGHT="39" HPOS="45" ID="tl_32" VPOS="1447" WIDTH="1173">
<String CONTENT="des nombreuses pièces qu’il achetait dans les ventes, où il" HEIGHT="39" HPOS="45" ID="string_tl_32" VPOS="1447" WIDTH="1173"/>
</TextLine>
<TextLine BASELINE="45 1527 1221 1527" HEIGHT="40" HPOS="45" ID="tl_33" VPOS="1487" WIDTH="1176">
<String CONTENT="assistait toujours lui-même, et devint par la suite ce qu’elle est" HEIGHT="40" HPOS="45" ID="string_tl_33" VPOS="1487" WIDTH="1176"/>
</TextLine>
<TextLine BASELINE="46 1568 1222 1568" HEIGHT="41" HPOS="46" ID="tl_34" VPOS="1527" WIDTH="1176">
<String CONTENT="aujourd'hui, c’est-à-dire la plus importante de Paris. Il collec¬" HEIGHT="41" HPOS="46" ID="string_tl_34" VPOS="1527" WIDTH="1176"/>
</TextLine>
<TextLine BASELINE="47 1606 1221 1606" HEIGHT="38" HPOS="47" ID="tl_35" VPOS="1568" WIDTH="1174">
<String CONTENT="tionnait à peu près tous les genres : les savants en première ligne," HEIGHT="38" HPOS="47" ID="string_tl_35" VPOS="1568" WIDTH="1174"/>
</TextLine>
<TextLine BASELINE="47 1647 1224 1647" HEIGHT="40" HPOS="47" ID="tl_36" VPOS="1607" WIDTH="1177">
<String CONTENT="ce qui est tout naturel; les grands écrivains, les artistes et les" HEIGHT="40" HPOS="47" ID="string_tl_36" VPOS="1607" WIDTH="1177"/>
</TextLine>
<TextLine BASELINE="48 1686 1222 1686" HEIGHT="39" HPOS="48" ID="tl_37" VPOS="1647" WIDTH="1174">
<String CONTENT="personnages de la Révolution. Il y a une série cependant à" HEIGHT="39" HPOS="48" ID="string_tl_37" VPOS="1647" WIDTH="1174"/>
</TextLine>
<TextLine BASELINE="46 1726 1223 1726" HEIGHT="38" HPOS="46" ID="tl_38" VPOS="1688" WIDTH="1177">
<String CONTENT="laquelle il n’attachait pas autant d’importance : ce sont les" HEIGHT="38" HPOS="46" ID="string_tl_38" VPOS="1688" WIDTH="1177"/>
</TextLine>
<TextLine BASELINE="46 1766 1223 1766" HEIGHT="40" HPOS="46" ID="tl_39" VPOS="1726" WIDTH="1177">
<String CONTENT="souverains; il fit néanmoins une exception pour la fameuse" HEIGHT="40" HPOS="46" ID="string_tl_39" VPOS="1726" WIDTH="1177"/>
</TextLine>
<TextLine BASELINE="47 1806 1222 1806" HEIGHT="39" HPOS="47" ID="tl_40" VPOS="1767" WIDTH="1175">
<String CONTENT="lettre de Napoléon à Marie-Louise, qu’il paya 1,200 fr. à la vente" HEIGHT="39" HPOS="47" ID="string_tl_40" VPOS="1767" WIDTH="1175"/>
</TextLine>
<TextLine BASELINE="47 1847 693 1847" HEIGHT="39" HPOS="47" ID="tl_41" VPOS="1808" WIDTH="646">
<String CONTENT="Lajarriette. C’est un de ses joyaux." HEIGHT="39" HPOS="47" ID="string_tl_41" VPOS="1808" WIDTH="646"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="282" HPOS="43" ID="r_2_5" VPOS="1854" WIDTH="1182">
<Shape>
<Polygon POINTS="43 1854 1225 1854 1225 2136 43 2136"/>
</Shape>
<TextLine BASELINE="87 1895 1223 1895" HEIGHT="40" HPOS="87" ID="tl_42" VPOS="1855" WIDTH="1136">
<String CONTENT="M. Dubrunfaut, qui était chevalier de la Légion d’honneur" HEIGHT="40" HPOS="87" ID="string_tl_42" VPOS="1855" WIDTH="1136"/>
</TextLine>
<TextLine BASELINE="47 1935 1223 1935" HEIGHT="40" HPOS="47" ID="tl_43" VPOS="1895" WIDTH="1176">
<String CONTENT="depuis 1861, a été promu au grade d’officier en 1878; mais il" HEIGHT="40" HPOS="47" ID="string_tl_43" VPOS="1895" WIDTH="1176"/>
</TextLine>
<TextLine BASELINE="47 1975 1223 1975" HEIGHT="40" HPOS="47" ID="tl_44" VPOS="1935" WIDTH="1176">
<String CONTENT="avait déjà reçu de nombreuses distinctions honorifiques. Une" HEIGHT="40" HPOS="47" ID="string_tl_44" VPOS="1935" WIDTH="1176"/>
</TextLine>
<TextLine BASELINE="47 2015 1224 2015" HEIGHT="40" HPOS="47" ID="tl_45" VPOS="1975" WIDTH="1177">
<String CONTENT="anecdote à ce propos : On sait que c’est à son insu que plusieurs" HEIGHT="40" HPOS="47" ID="string_tl_45" VPOS="1975" WIDTH="1177"/>
</TextLine>
<TextLine BASELINE="47 2055 1223 2055" HEIGHT="39" HPOS="47" ID="tl_46" VPOS="2016" WIDTH="1176">
<String CONTENT="de ses amis et compatriotes firent les démarches nécessaires" HEIGHT="39" HPOS="47" ID="string_tl_46" VPOS="2016" WIDTH="1176"/>
</TextLine>
<TextLine BASELINE="46 2095 1224 2095" HEIGHT="39" HPOS="46" ID="tl_47" VPOS="2056" WIDTH="1178">
<String CONTENT="pour lui obtenir le ruban rouge, M. Dumas, de l’Académie, fut" HEIGHT="39" HPOS="46" ID="string_tl_47" VPOS="2056" WIDTH="1178"/>
</TextLine>
<TextLine BASELINE="44 2135 1221 2135" HEIGHT="40" HPOS="44" ID="tl_48" VPOS="2095" WIDTH="1177">
<String CONTENT="chargé de lui annoncer la nouvelle. Il alla le voir et l’interpella" HEIGHT="40" HPOS="44" ID="string_tl_48" VPOS="2095" WIDTH="1177"/>
</TextLine>
</TextBlock>
