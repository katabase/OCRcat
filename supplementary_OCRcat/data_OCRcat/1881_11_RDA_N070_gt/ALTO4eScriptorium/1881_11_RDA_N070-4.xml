<String CONTENT="voyage qu’il fit en Belgique et en Hollande, il s'adonna défini¬" HEIGHT="39" HPOS="34" ID="string_tl_2" VPOS="206" WIDTH="1183"/>
</TextLine>
<TextLine BASELINE="38 286 1216 286" HEIGHT="39" HPOS="38" ID="tl_3" VPOS="247" WIDTH="1178">
<String CONTENT="tivement à la carrière industrielle et scientifique. Plus tard il" HEIGHT="39" HPOS="38" ID="string_tl_3" VPOS="247" WIDTH="1178"/>
</TextLine>
<TextLine BASELINE="35 326 1216 326" HEIGHT="40" HPOS="35" ID="tl_4" VPOS="286" WIDTH="1181">
<String CONTENT="professa la chimie a l’Ecole de Commerce de Paris, et s’attacha" HEIGHT="40" HPOS="35" ID="string_tl_4" VPOS="286" WIDTH="1181"/>
</TextLine>
<TextLine BASELINE="34 367 1212 367" HEIGHT="40" HPOS="34" ID="tl_5" VPOS="327" WIDTH="1178">
<String CONTENT="à propager la connaissance de la chimie appliquée à l’industrie." HEIGHT="40" HPOS="34" ID="string_tl_5" VPOS="327" WIDTH="1178"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="362" HPOS="32" ID="r_2_2" VPOS="375" WIDTH="1189">
<Shape>
<Polygon POINTS="32 375 1221 375 1221 737 32 737"/>
</Shape>
<TextLine BASELINE="74 416 1217 416" HEIGHT="40" HPOS="74" ID="tl_6" VPOS="376" WIDTH="1143">
<String CONTENT="Le peu de place dont nous disposons ne nous permet pas" HEIGHT="40" HPOS="74" ID="string_tl_6" VPOS="376" WIDTH="1143"/>
</TextLine>
<TextLine BASELINE="34 448 1217 448" HEIGHT="31" HPOS="34" ID="tl_7" VPOS="417" WIDTH="1183">
<String CONTENT="malheureusement de citer tous les travaux et toutes les inven¬" HEIGHT="31" HPOS="34" ID="string_tl_7" VPOS="417" WIDTH="1183"/>
</TextLine>
<TextLine BASELINE="35 488 1218 488" HEIGHT="31" HPOS="35" ID="tl_8" VPOS="457" WIDTH="1183">
<String CONTENT="tions dont la science est redevable à M. Dubrunfaut. La" HEIGHT="31" HPOS="35" ID="string_tl_8" VPOS="457" WIDTH="1183"/>
</TextLine>
<TextLine BASELINE="35 528 1218 528" HEIGHT="31" HPOS="35" ID="tl_9" VPOS="497" WIDTH="1183">
<String CONTENT="distillation et la fabrication des eaux-de-vie furent tour à" HEIGHT="31" HPOS="35" ID="string_tl_9" VPOS="497" WIDTH="1183"/>
</TextLine>
<TextLine BASELINE="35 575 1216 575" HEIGHT="38" HPOS="35" ID="tl_10" VPOS="537" WIDTH="1181">
<String CONTENT="tour l’objet de ses recherches et de ses découvertes. Il s’oc¬" HEIGHT="38" HPOS="35" ID="string_tl_10" VPOS="537" WIDTH="1181"/>
</TextLine>
<TextLine BASELINE="33 615 1220 615" HEIGHT="38" HPOS="33" ID="tl_11" VPOS="577" WIDTH="1187">
<String CONTENT="cupa d’une façon toute particulière de la fabrication du sucre" HEIGHT="38" HPOS="33" ID="string_tl_11" VPOS="577" WIDTH="1187"/>
</TextLine>
<TextLine BASELINE="34 656 1220 656" HEIGHT="39" HPOS="34" ID="tl_12" VPOS="617" WIDTH="1186">
<String CONTENT="de betterave ; les mémoires qu’il écrivit sur cette question" HEIGHT="39" HPOS="34" ID="string_tl_12" VPOS="617" WIDTH="1186"/>
</TextLine>
<TextLine BASELINE="36 697 1218 697" HEIGHT="40" HPOS="36" ID="tl_13" VPOS="657" WIDTH="1182">
<String CONTENT="ont été justement remarqués. En 1833 il abandonna l’ensei¬" HEIGHT="40" HPOS="36" ID="string_tl_13" VPOS="657" WIDTH="1182"/>
</TextLine>
<TextLine BASELINE="34 736 898 736" HEIGHT="38" HPOS="34" ID="tl_14" VPOS="698" WIDTH="864">
<String CONTENT="gnement pour mettre ses théories en pratique." HEIGHT="38" HPOS="34" ID="string_tl_14" VPOS="698" WIDTH="864"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="364" HPOS="35" ID="r_2_3" VPOS="754" WIDTH="1187">
