<TextLine BASELINE="467 2010 2966 2010" HEIGHT="74" HPOS="467" ID="tl_10" VPOS="1936" WIDTH="2499">
<String CONTENT="CURIEUSE CORRESPONDANCE AMOUREUSE. Dans la première lettre, datée du" HEIGHT="74" HPOS="467" ID="string_tl_10" VPOS="1936" WIDTH="2499"/>
</TextLine>
<TextLine BASELINE="401 2074 2968 2074" HEIGHT="73" HPOS="401" ID="tl_11" VPOS="2001" WIDTH="2567">
<String CONTENT="24 juin 1840, il lui déclare son amour et lui demande un rendez-vous. Il" HEIGHT="73" HPOS="401" ID="string_tl_11" VPOS="2001" WIDTH="2567"/>
</TextLine>
<TextLine BASELINE="399 2139 2967 2139" HEIGHT="74" HPOS="399" ID="tl_12" VPOS="2065" WIDTH="2568">
<String CONTENT="n'ose pas encore se nommer et signe F. G. La première lettre étant restée" HEIGHT="74" HPOS="399" ID="string_tl_12" VPOS="2065" WIDTH="2568"/>
</TextLine>
<TextLine BASELINE="397 2206 2968 2206" HEIGHT="77" HPOS="397" ID="tl_13" VPOS="2129" WIDTH="2571">
<String CONTENT="sans réponse, il l'avise qu'il se rendra au théâtre; il se fera reconnaître par" HEIGHT="77" HPOS="397" ID="string_tl_13" VPOS="2129" WIDTH="2571"/>
</TextLine>
<TextLine BASELINE="395 2277 2967 2277" HEIGHT="83" HPOS="395" ID="tl_14" VPOS="2194" WIDTH="2572">
<String CONTENT="un signe fait avec son mouchoir. Un peu plus tard il se plaint de n’être plus" HEIGHT="83" HPOS="395" ID="string_tl_14" VPOS="2194" WIDTH="2572"/>
</TextLine>
<TextLine BASELINE="393 2329 2968 2329" HEIGHT="71" HPOS="393" ID="tl_15" VPOS="2258" WIDTH="2575">
<String CONTENT="aimé et s'écrie : « Adieu, mes douces illusions! vous m'avez bercé quelques" HEIGHT="71" HPOS="393" ID="string_tl_15" VPOS="2258" WIDTH="2575"/>
</TextLine>
<TextLine BASELINE="394 2405 2966 2405" HEIGHT="82" HPOS="394" ID="tl_16" VPOS="2323" WIDTH="2572">
<String CONTENT="instants dans un rêve qu'un soudain réveil a pour toujours dissipé. » Le" HEIGHT="82" HPOS="394" ID="string_tl_16" VPOS="2323" WIDTH="2572"/>
</TextLine>
<TextLine BASELINE="390 2458 2970 2458" HEIGHT="72" HPOS="390" ID="tl_17" VPOS="2386" WIDTH="2580">
<String CONTENT="21 octobre 1840, il lui écrit : « Vous êtes bien injuste de me dire que d'ici à" HEIGHT="72" HPOS="390" ID="string_tl_17" VPOS="2386" WIDTH="2580"/>
</TextLine>
<TextLine BASELINE="390 2528 2971 2528" HEIGHT="77" HPOS="390" ID="tl_18" VPOS="2451" WIDTH="2581">
<String CONTENT="mon retour je pourrai vous oublier. Croyez-vous donc que tout ce que je" HEIGHT="77" HPOS="390" ID="string_tl_18" VPOS="2451" WIDTH="2581"/>
</TextLine>
<TextLine BASELINE="392 2591 2968 2591" HEIGHT="78" HPOS="392" ID="tl_19" VPOS="2513" WIDTH="2576">
<String CONTENT="vous ai dit soit de vaines paroles?» Le 20 mai 1841 il se plaint vivement de" HEIGHT="78" HPOS="392" ID="string_tl_19" VPOS="2513" WIDTH="2576"/>
</TextLine>
<TextLine BASELINE="394 2651 2971 2651" HEIGHT="70" HPOS="394" ID="tl_20" VPOS="2581" WIDTH="2577">
<String CONTENT="sa froideur. «Je suis bien malheureux d'être traité ainsi, quand de mon" HEIGHT="70" HPOS="394" ID="string_tl_20" VPOS="2581" WIDTH="2577"/>
</TextLine>
<TextLine BASELINE="391 2721 2974 2721" HEIGHT="77" HPOS="391" ID="tl_21" VPOS="2644" WIDTH="2583">
<String CONTENT="côté je fois tout pour te prouver combien je t'aime... Mon Dieu! il vaudrait" HEIGHT="77" HPOS="391" ID="string_tl_21" VPOS="2644" WIDTH="2583"/>
</TextLine>
<TextLine BASELINE="391 2786 2971 2786" HEIGHT="75" HPOS="391" ID="tl_22" VPOS="2711" WIDTH="2580">
<String CONTENT="mieux dire aux personnes que l’on n apprécie pas, laissez-moi, je ne vous" HEIGHT="75" HPOS="391" ID="string_tl_22" VPOS="2711" WIDTH="2580"/>
</TextLine>
<TextLine BASELINE="392 2850 2974 2850" HEIGHT="76" HPOS="392" ID="tl_23" VPOS="2774" WIDTH="2582">
<String CONTENT="aime pas!» La lettre la plus importante est la dernière datée du 6 dé¬" HEIGHT="76" HPOS="392" ID="string_tl_23" VPOS="2774" WIDTH="2582"/>
</TextLine>
<TextLine BASELINE="390 2910 2976 2910" HEIGHT="70" HPOS="390" ID="tl_24" VPOS="2840" WIDTH="2586">
<String CONTENT="cembre 1842. Il s'excuse de l'avoir blessée en employant des expressions" HEIGHT="70" HPOS="390" ID="string_tl_24" VPOS="2840" WIDTH="2586"/>
</TextLine>
<TextLine BASELINE="387 2982 2978 2982" HEIGHT="80" HPOS="387" ID="tl_25" VPOS="2902" WIDTH="2591">
<String CONTENT="énergiques, mais justes, puisqu'il pourrait prouver tout ce qu'il a avancé." HEIGHT="80" HPOS="387" ID="string_tl_25" VPOS="2902" WIDTH="2591"/>
</TextLine>
<TextLine BASELINE="391 3044 2980 3044" HEIGHT="74" HPOS="391" ID="tl_26" VPOS="2970" WIDTH="2589">
<String CONTENT="«C'est bien à tort que vous employez le mot de Prince. Vous croyez donc" HEIGHT="74" HPOS="391" ID="string_tl_26" VPOS="2970" WIDTH="2589"/>
</TextLine>
<TextLine BASELINE="387 3110 2979 3110" HEIGHT="75" HPOS="387" ID="tl_27" VPOS="3035" WIDTH="2592">
<String CONTENT="qu'en se fondant sur ce titre on ait le droit de se permettre des choses" HEIGHT="75" HPOS="387" ID="string_tl_27" VPOS="3035" WIDTH="2592"/>
</TextLine>
<TextLine BASELINE="385 3176 2980 3176" HEIGHT="74" HPOS="385" ID="tl_28" VPOS="3102" WIDTH="2595">
<String CONTENT="qu'une autre personne ne se permettrait pas?» IL ne craint pas ses menaces" HEIGHT="74" HPOS="385" ID="string_tl_28" VPOS="3102" WIDTH="2595"/>
</TextLine>
<TextLine BASELINE="385 3224 1724 3224" HEIGHT="58" HPOS="385" ID="tl_29" VPOS="3166" WIDTH="1339">
<String CONTENT="et la somme de les mettre à exécution." HEIGHT="58" HPOS="385" ID="string_tl_29" VPOS="3166" WIDTH="1339"/>
</TextLine>
