<TextLine BASELINE="304 2013 1698 2013" HEIGHT="51" HPOS="304" ID="tl_36" VPOS="1962" WIDTH="1394">
<String CONTENT="M. l’avocat impérial a rapporté ce fait significatif : «Il a été expulsé" HEIGHT="51" HPOS="304" ID="string_tl_36" VPOS="1962" WIDTH="1394"/>
</TextLine>
<TextLine BASELINE="254 2063 1697 2063" HEIGHT="51" HPOS="254" ID="tl_37" VPOS="2012" WIDTH="1443">
<String CONTENT="de la bibliothèque Sainte-Geneviève dans des circonstances graves. Un" HEIGHT="51" HPOS="254" ID="string_tl_37" VPOS="2012" WIDTH="1443"/>
</TextLine>
<TextLine BASELINE="254 2122 1698 2122" HEIGHT="60" HPOS="254" ID="tl_38" VPOS="2062" WIDTH="1444">
<String CONTENT="surveillant l’avait surpris dans les rayons des archives ; il était porteur" HEIGHT="60" HPOS="254" ID="string_tl_38" VPOS="2062" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="256 2162 1697 2162" HEIGHT="62" HPOS="256" ID="tl_39" VPOS="2100" WIDTH="1441">
<String CONTENT="d’un instrument très-tranchant, qui pouvait servir à détacher des" HEIGHT="62" HPOS="256" ID="string_tl_39" VPOS="2100" WIDTH="1441"/>
</TextLine>
<TextLine BASELINE="258 2212 1697 2212" HEIGHT="51" HPOS="258" ID="tl_40" VPOS="2161" WIDTH="1439">
<String CONTENT="feuillets. On suppose qu’il cherchait à détacher les gardes des volumes," HEIGHT="51" HPOS="258" ID="string_tl_40" VPOS="2161" WIDTH="1439"/>
</TextLine>
<TextLine BASELINE="259 2262 1700 2262" HEIGHT="52" HPOS="259" ID="tl_41" VPOS="2210" WIDTH="1441">
<String CONTENT="pour se procurer du vieux papier. » On a saisi, d’ailleurs, à l’un de ses" HEIGHT="52" HPOS="259" ID="string_tl_41" VPOS="2210" WIDTH="1441"/>
</TextLine>
<TextLine BASELINE="257 2312 1700 2312" HEIGHT="51" HPOS="257" ID="tl_42" VPOS="2261" WIDTH="1443">
<String CONTENT="domiciles :1° un grand nombre de lettres qui lui avaient été adressées" HEIGHT="51" HPOS="257" ID="string_tl_42" VPOS="2261" WIDTH="1443"/>
</TextLine>
<TextLine BASELINE="260 2362 1698 2362" HEIGHT="51" HPOS="260" ID="tl_43" VPOS="2311" WIDTH="1438">
<String CONTENT="par des personnes auxquelles il faisait des offres de pièces généalo¬" HEIGHT="51" HPOS="260" ID="string_tl_43" VPOS="2311" WIDTH="1438"/>
</TextLine>
<TextLine BASELINE="259 2411 1701 2411" HEIGHT="51" HPOS="259" ID="tl_44" VPOS="2360" WIDTH="1442">
<String CONTENT="giques ; 2° des papiers concernant Galilée, Bann, Copernick ; 3° une" HEIGHT="51" HPOS="259" ID="string_tl_44" VPOS="2360" WIDTH="1442"/>
</TextLine>
<TextLine BASELINE="260 2461 1701 2461" HEIGHT="51" HPOS="260" ID="tl_45" VPOS="2410" WIDTH="1441">
<String CONTENT="grande quantité de pièces autographiées provenant de visographie, et" HEIGHT="51" HPOS="260" ID="string_tl_45" VPOS="2410" WIDTH="1441"/>
</TextLine>
<TextLine BASELINE="262 2511 1698 2511" HEIGHT="51" HPOS="262" ID="tl_46" VPOS="2460" WIDTH="1436">
<String CONTENT="un certain nombre de signatures fac-similé ; 4° des papiers anciens ;" HEIGHT="51" HPOS="262" ID="string_tl_46" VPOS="2460" WIDTH="1436"/>
</TextLine>
<TextLine BASELINE="263 2559 1701 2559" HEIGHT="50" HPOS="263" ID="tl_47" VPOS="2509" WIDTH="1438">
<String CONTENT="5° des volumes de Moréri et du Mercure de France, dont les gardes" HEIGHT="50" HPOS="263" ID="string_tl_47" VPOS="2509" WIDTH="1438"/>
</TextLine>
<TextLine BASELINE="262 2599 566 2599" HEIGHT="38" HPOS="262" ID="tl_48" VPOS="2561" WIDTH="304">
<String CONTENT="sont arrachées." HEIGHT="38" HPOS="262" ID="string_tl_48" VPOS="2561" WIDTH="304"/>
</TextLine>
