<TextLine BASELINE="246 300 1688 300" HEIGHT="51" HPOS="246" ID="tl_2" VPOS="249" WIDTH="1442">
<String CONTENT="la dernière heure, c’est-à-dire l’arrestation de Lucas, pour parler des" HEIGHT="51" HPOS="246" ID="string_tl_2" VPOS="249" WIDTH="1442"/>
</TextLine>
<TextLine BASELINE="246 351 1688 351" HEIGHT="51" HPOS="246" ID="tl_3" VPOS="300" WIDTH="1442">
<String CONTENT="pièces stupéfiantes dont nous venons de donner la liste? Eu vain" HEIGHT="51" HPOS="246" ID="string_tl_3" VPOS="300" WIDTH="1442"/>
</TextLine>
<TextLine BASELINE="244 399 1688 399" HEIGHT="50" HPOS="244" ID="tl_4" VPOS="349" WIDTH="1444">
<String CONTENT="dira-t-il que ces pièces étant étrangères à ses travaux, il n’avait aucun" HEIGHT="50" HPOS="244" ID="string_tl_4" VPOS="349" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="246 448 1691 448" HEIGHT="50" HPOS="246" ID="tl_5" VPOS="398" WIDTH="1445">
<String CONTENT="motif de les faire connaître à l’Académie et au public, il restera" HEIGHT="50" HPOS="246" ID="string_tl_5" VPOS="398" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="245 498 1691 498" HEIGHT="50" HPOS="245" ID="tl_6" VPOS="448" WIDTH="1446">
<String CONTENT="évident que rien n’était, plus propre que celle communication à jeter la" HEIGHT="50" HPOS="245" ID="string_tl_6" VPOS="448" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="248 548 1687 548" HEIGHT="51" HPOS="248" ID="tl_7" VPOS="497" WIDTH="1439">
<String CONTENT="lumière sur I ensemble de la collection, qui, dès-lors, eût été jugée." HEIGHT="51" HPOS="248" ID="string_tl_7" VPOS="497" WIDTH="1439"/>
</TextLine>
<TextLine BASELINE="251 598 1692 598" HEIGHT="52" HPOS="251" ID="tl_8" VPOS="546" WIDTH="1441">
<String CONTENT="Une seule série, celle des lettres de Molière, eût suffit pour tout faire" HEIGHT="52" HPOS="251" ID="string_tl_8" VPOS="546" WIDTH="1441"/>
</TextLine>
<TextLine BASELINE="247 647 1693 647" HEIGHT="51" HPOS="247" ID="tl_9" VPOS="596" WIDTH="1446">
<String CONTENT="découvrir, si M. Chasles en eût parlé plus tôt. On sait qu’il n’existe" HEIGHT="51" HPOS="247" ID="string_tl_9" VPOS="596" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="247 696 1694 696" HEIGHT="51" HPOS="247" ID="tl_10" VPOS="645" WIDTH="1447">
<String CONTENT="dans les collections que des signatures du grand comique, et pas une" HEIGHT="51" HPOS="247" ID="string_tl_10" VPOS="645" WIDTH="1447"/>
</TextLine>
<TextLine BASELINE="250 746 1694 746" HEIGHT="52" HPOS="250" ID="tl_11" VPOS="694" WIDTH="1444">
<String CONTENT="lettre, pas le plus petit billet. C’est un désidératum poursuivi avec" HEIGHT="52" HPOS="250" ID="string_tl_11" VPOS="694" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="248 795 1694 795" HEIGHT="51" HPOS="248" ID="tl_12" VPOS="744" WIDTH="1446">
<String CONTENT="ardeur et qui serait payé au poids de l’or. M. Chasles déclare que la" HEIGHT="51" HPOS="248" ID="string_tl_12" VPOS="744" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="252 844 1692 844" HEIGHT="58" HPOS="252" ID="tl_13" VPOS="786" WIDTH="1440">
<String CONTENT="première pièce que lui à vendue Lucas est une lettre de Molière, qu’il" HEIGHT="58" HPOS="252" ID="string_tl_13" VPOS="786" WIDTH="1440"/>
</TextLine>
<TextLine BASELINE="249 895 1691 895" HEIGHT="52" HPOS="249" ID="tl_14" VPOS="843" WIDTH="1442">
<String CONTENT="a payé 500 fr., et il trouve que c’est assez cher. C’était, au contraire," HEIGHT="52" HPOS="249" ID="string_tl_14" VPOS="843" WIDTH="1442"/>
</TextLine>
<TextLine BASELINE="251 944 1694 944" HEIGHT="52" HPOS="251" ID="tl_15" VPOS="892" WIDTH="1443">
<String CONTENT="pour rien ; car il s’est vendu de simples signatures jusqu'à 950 fr., et" HEIGHT="52" HPOS="251" ID="string_tl_15" VPOS="892" WIDTH="1443"/>
</TextLine>
<TextLine BASELINE="248 994 1689 994" HEIGHT="52" HPOS="248" ID="tl_16" VPOS="942" WIDTH="1441">
<String CONTENT="une lettre vaudrait on ne sait quoi, peut-être 5,000 fr., peut être plus." HEIGHT="52" HPOS="248" ID="string_tl_16" VPOS="942" WIDTH="1441"/>
</TextLine>
<TextLine BASELINE="251 1042 1695 1042" HEIGHT="50" HPOS="251" ID="tl_17" VPOS="992" WIDTH="1444">
<String CONTENT="Si donc M. Chasles eût fait part de suite de celte trouvaille à ses con¬" HEIGHT="50" HPOS="251" ID="string_tl_17" VPOS="992" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="254 1091 1696 1091" HEIGHT="50" HPOS="254" ID="tl_18" VPOS="1041" WIDTH="1442">
<String CONTENT="frères les amateurs d’autographes, à qui il montrait tant d’autres" HEIGHT="50" HPOS="254" ID="string_tl_18" VPOS="1041" WIDTH="1442"/>
</TextLine>
<TextLine BASELINE="252 1143 1695 1143" HEIGHT="52" HPOS="252" ID="tl_19" VPOS="1091" WIDTH="1443">
<String CONTENT="choses, l’attention se fût concentrée sur ce point, et le pot aux roses" HEIGHT="52" HPOS="252" ID="string_tl_19" VPOS="1091" WIDTH="1443"/>
</TextLine>
<TextLine BASELINE="247 1180 874 1180" HEIGHT="39" HPOS="247" ID="tl_20" VPOS="1141" WIDTH="627">
<String CONTENT="eût été découvert dès le début." HEIGHT="39" HPOS="247" ID="string_tl_20" VPOS="1141" WIDTH="627"/>
</TextLine>
</TextBlock>
