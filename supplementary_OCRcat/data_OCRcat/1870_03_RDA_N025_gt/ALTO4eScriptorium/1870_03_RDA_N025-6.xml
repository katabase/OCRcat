<TextLine BASELINE="303 1268 1697 1268" HEIGHT="52" HPOS="303" ID="tl_21" VPOS="1216" WIDTH="1394">
<String CONTENT="PROCÉDÉS DE LUCAS. — «Il se procurait, dit le réquisitoire, du papier" HEIGHT="52" HPOS="303" ID="string_tl_21" VPOS="1216" WIDTH="1394"/>
</TextLine>
<TextLine BASELINE="251 1317 1698 1317" HEIGHT="51" HPOS="251" ID="tl_22" VPOS="1266" WIDTH="1447">
<String CONTENT="de fabrication ancienne et usait de divers procédés pour lui donner" HEIGHT="51" HPOS="251" ID="string_tl_22" VPOS="1266" WIDTH="1447"/>
</TextLine>
<TextLine BASELINE="251 1367 1696 1367" HEIGHT="51" HPOS="251" ID="tl_23" VPOS="1316" WIDTH="1445">
<String CONTENT="l’apparence qu’il devait avoir. Tantôt il le noircissait à la lampe ou à la" HEIGHT="51" HPOS="251" ID="string_tl_23" VPOS="1316" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="251 1418 1695 1418" HEIGHT="53" HPOS="251" ID="tl_24" VPOS="1365" WIDTH="1444">
<String CONTENT="chandelle, tantôt, il le faisait chauffer et roussir, tantôt il le trempait" HEIGHT="53" HPOS="251" ID="string_tl_24" VPOS="1365" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="250 1467 1697 1467" HEIGHT="51" HPOS="250" ID="tl_25" VPOS="1416" WIDTH="1447">
<String CONTENT="dans une eau sale après l’avoir écrit. A l'aide d’un procédé qu’il a" HEIGHT="51" HPOS="250" ID="string_tl_25" VPOS="1416" WIDTH="1447"/>
</TextLine>
<TextLine BASELINE="250 1517 1692 1517" HEIGHT="51" HPOS="250" ID="tl_26" VPOS="1466" WIDTH="1442">
<String CONTENT="refusé de faire connaître, il modifiait la couleur de l’encre qu’il" HEIGHT="51" HPOS="250" ID="string_tl_26" VPOS="1466" WIDTH="1442"/>
</TextLine>
<TextLine BASELINE="249 1567 1696 1567" HEIGHT="53" HPOS="249" ID="tl_27" VPOS="1514" WIDTH="1447">
<String CONTENT="employait. Sa fabrication s’était perfectionnée, et les chimistes les plus" HEIGHT="53" HPOS="249" ID="string_tl_27" VPOS="1514" WIDTH="1447"/>
</TextLine>
<TextLine BASELINE="252 1615 1698 1615" HEIGHT="50" HPOS="252" ID="tl_28" VPOS="1565" WIDTH="1446">
<String CONTENT="habiles de l’Académie avaient été impuissants à reconnaître la produc¬" HEIGHT="50" HPOS="252" ID="string_tl_28" VPOS="1565" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="252 1665 1697 1665" HEIGHT="51" HPOS="252" ID="tl_29" VPOS="1614" WIDTH="1445">
<String CONTENT="tion récente des autographes » M Bordier, de son côté, après avoir" HEIGHT="51" HPOS="252" ID="string_tl_29" VPOS="1614" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="250 1715 1696 1715" HEIGHT="51" HPOS="250" ID="tl_30" VPOS="1664" WIDTH="1446">
<String CONTENT="exposé ces faits dans les mêmes termes, ajoutait : « Nous avons essayé" HEIGHT="51" HPOS="250" ID="string_tl_30" VPOS="1664" WIDTH="1446"/>
</TextLine>
<TextLine BASELINE="251 1765 1695 1765" HEIGHT="51" HPOS="251" ID="tl_31" VPOS="1714" WIDTH="1444">
<String CONTENT="ce procédé, mais nous sommes obligés d’avouer que nous n’avons pas" HEIGHT="51" HPOS="251" ID="string_tl_31" VPOS="1714" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="250 1815 1693 1815" HEIGHT="51" HPOS="250" ID="tl_32" VPOS="1764" WIDTH="1443">
<String CONTENT="réussi aussi bien que lui. » Le prévenu, interpellé sur ce procédé," HEIGHT="51" HPOS="250" ID="string_tl_32" VPOS="1764" WIDTH="1443"/>
</TextLine>
<TextLine BASELINE="251 1863 1695 1863" HEIGHT="50" HPOS="251" ID="tl_33" VPOS="1813" WIDTH="1444">
<String CONTENT="répond avec un certain air de satisfaction, que messieurs les experts ont" HEIGHT="50" HPOS="251" ID="string_tl_33" VPOS="1813" WIDTH="1444"/>
</TextLine>
<TextLine BASELINE="253 1913 1698 1913" HEIGHT="50" HPOS="253" ID="tl_34" VPOS="1863" WIDTH="1445">
<String CONTENT="apprécié assez bien sa manière de faire, mais ajoute qu’on ne réussit" HEIGHT="50" HPOS="253" ID="string_tl_34" VPOS="1863" WIDTH="1445"/>
</TextLine>
<TextLine BASELINE="255 1963 1569 1963" HEIGHT="50" HPOS="255" ID="tl_35" VPOS="1913" WIDTH="1314">
<String CONTENT="pas toujours, et qu’il faut souvent s’y reprendre à plusieurs fois." HEIGHT="50" HPOS="255" ID="string_tl_35" VPOS="1913" WIDTH="1314"/>
</TextLine>
</TextBlock>
