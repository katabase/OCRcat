</Shape>
<TextLine BASELINE="628 1713 1449 1713" HEIGHT="43" HPOS="628" ID="tl_28" VPOS="1670" WIDTH="821">
<String CONTENT="COLLECTION JOSEPH AJAZZI." HEIGHT="43" HPOS="628" ID="string_tl_28" VPOS="1670" WIDTH="821"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="452" HPOS="294" ID="r_3_2" VPOS="1758" WIDTH="1463">
<Shape>
<Polygon POINTS="294 1758 1757 1758 1757 2210 294 2210"/>
</Shape>
<TextLine BASELINE="349 1811 1756 1811" HEIGHT="52" HPOS="349" ID="tl_29" VPOS="1759" WIDTH="1407">
<String CONTENT="La vente de cette collection d’autographes, peu nombreuse, mais" HEIGHT="52" HPOS="349" ID="string_tl_29" VPOS="1759" WIDTH="1407"/>
</TextLine>
<TextLine BASELINE="297 1858 1756 1858" HEIGHT="50" HPOS="297" ID="tl_30" VPOS="1808" WIDTH="1459">
<String CONTENT="choisie, aura lieu le 29 mars courant, sous notre direction, à la salle" HEIGHT="50" HPOS="297" ID="string_tl_30" VPOS="1808" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="298 1910 1753 1910" HEIGHT="52" HPOS="298" ID="tl_31" VPOS="1858" WIDTH="1455">
<String CONTENT="Silvestre. L’amateur qui l’a formée était un savant, auteur d’un grand" HEIGHT="52" HPOS="298" ID="string_tl_31" VPOS="1858" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="299 1961 1756 1961" HEIGHT="52" HPOS="299" ID="tl_32" VPOS="1909" WIDTH="1457">
<String CONTENT="nombre d’ouvrages d’érudition fort estimés en Italie. A sa propre" HEIGHT="52" HPOS="299" ID="string_tl_32" VPOS="1909" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="295 2010 1755 2010" HEIGHT="50" HPOS="295" ID="tl_33" VPOS="1960" WIDTH="1460">
<String CONTENT="correspondance, qui était très-étendue, et dont nous n’avons catalogué" HEIGHT="50" HPOS="295" ID="string_tl_33" VPOS="1960" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="296 2069 1755 2069" HEIGHT="60" HPOS="296" ID="tl_34" VPOS="2009" WIDTH="1459">
<String CONTENT="qu’une faible partie, il avait ajouté quelques pièces intéressantes par" HEIGHT="60" HPOS="296" ID="string_tl_34" VPOS="2009" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="297 2110 1752 2110" HEIGHT="51" HPOS="297" ID="tl_35" VPOS="2059" WIDTH="1455">
<String CONTENT="I'éclat des noms, et souvent par le contenu. Tous nos lecteurs ayant dû" HEIGHT="51" HPOS="297" ID="string_tl_35" VPOS="2059" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="295 2156 1754 2156" HEIGHT="46" HPOS="295" ID="tl_36" VPOS="2110" WIDTH="1459">
<String CONTENT="recevoir le catalogue, nous nous contenterons d’attirer l’attention sur" HEIGHT="46" HPOS="295" ID="string_tl_36" VPOS="2110" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="298 2209 920 2209" HEIGHT="51" HPOS="298" ID="tl_37" VPOS="2158" WIDTH="622">
<String CONTENT="les quelques articles suivants :" HEIGHT="51" HPOS="298" ID="string_tl_37" VPOS="2158" WIDTH="622"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="453" HPOS="293" ID="r_3_3" VPOS="2207" WIDTH="1461">
<Shape>
<Polygon POINTS="293 2207 1754 2207 1754 2660 293 2660"/>
</Shape>
<TextLine BASELINE="344 2260 1753 2260" HEIGHT="52" HPOS="344" ID="tl_38" VPOS="2208" WIDTH="1409">
<String CONTENT="Le Trissin, une lettre splendide et fort rare, une seule ayant encore" HEIGHT="52" HPOS="344" ID="string_tl_38" VPOS="2208" WIDTH="1409"/>
</TextLine>
<TextLine BASELINE="297 2308 1752 2308" HEIGHT="49" HPOS="297" ID="tl_39" VPOS="2259" WIDTH="1455">
<String CONTENT="passé dans le commerce, et en mauvais étal ; le Maire de Belges, auto¬" HEIGHT="49" HPOS="297" ID="string_tl_39" VPOS="2259" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="294 2359 1750 2359" HEIGHT="51" HPOS="294" ID="tl_40" VPOS="2308" WIDTH="1456">
<String CONTENT="graphe précieux ; Théodore de Bèze, une lettre en français, d’un grand" HEIGHT="51" HPOS="294" ID="string_tl_40" VPOS="2308" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="297 2408 1753 2408" HEIGHT="50" HPOS="297" ID="tl_41" VPOS="2358" WIDTH="1456">
<String CONTENT="intérêt pour l’histoire de la Réforme ; Du Plessis-Mornay, un mémoire" HEIGHT="50" HPOS="297" ID="string_tl_41" VPOS="2358" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="294 2459 1748 2459" HEIGHT="50" HPOS="294" ID="tl_42" VPOS="2409" WIDTH="1454">
<String CONTENT="autographe sur le divorce de Henri IV, provoqué par lui ; Subtermans," HEIGHT="50" HPOS="294" ID="string_tl_42" VPOS="2409" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="294 2509 1752 2509" HEIGHT="52" HPOS="294" ID="tl_43" VPOS="2457" WIDTH="1458">
<String CONTENT="grand peintre flamand, lettre rarissime ; Tartini, une lettre au célèbre" HEIGHT="52" HPOS="294" ID="string_tl_43" VPOS="2457" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="296 2569 1751 2569" HEIGHT="60" HPOS="296" ID="tl_44" VPOS="2509" WIDTH="1455">
<String CONTENT="père Martini ; Rossini, deux lettres sur la statue de Rameau ; Tallien" HEIGHT="60" HPOS="296" ID="string_tl_44" VPOS="2509" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="296 2606 1751 2606" HEIGHT="59" HPOS="296" ID="tl_45" VPOS="2547" WIDTH="1455">
<String CONTENT="lettre fort intéressante adressée à Hoche ; etc. Citons enfin une suite" HEIGHT="59" HPOS="296" ID="string_tl_45" VPOS="2547" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="294 2659 1507 2659" HEIGHT="51" HPOS="294" ID="tl_46" VPOS="2608" WIDTH="1213">
<String CONTENT="de Chartes historiques anglo-françaises, de 1375 à 1475." HEIGHT="51" HPOS="294" ID="string_tl_46" VPOS="2608" WIDTH="1213"/>
</TextLine>
