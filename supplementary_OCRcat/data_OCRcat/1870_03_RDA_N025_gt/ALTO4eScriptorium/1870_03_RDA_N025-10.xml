<TextLine BASELINE="352 335 1753 335" HEIGHT="53" HPOS="352" ID="tl_2" VPOS="282" WIDTH="1401">
<String CONTENT="QUELQUES RÉFLEXIONS. — Une chose qui aurait dû frapper M. Chasles," HEIGHT="53" HPOS="352" ID="string_tl_2" VPOS="282" WIDTH="1401"/>
</TextLine>
<TextLine BASELINE="300 385 1757 385" HEIGHT="51" HPOS="300" ID="tl_3" VPOS="334" WIDTH="1457">
<String CONTENT="c’est que, de toutes ces lettres de Lucas pas une ne portait une" HEIGHT="51" HPOS="300" ID="string_tl_3" VPOS="334" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="299 431 1757 431" HEIGHT="49" HPOS="299" ID="tl_4" VPOS="382" WIDTH="1458">
<String CONTENT="adresse ou un cachet, ni de feuillets blancs. Quand on lui faisait" HEIGHT="49" HPOS="299" ID="string_tl_4" VPOS="382" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="300 484 1757 484" HEIGHT="52" HPOS="300" ID="tl_5" VPOS="432" WIDTH="1457">
<String CONTENT="cette objection, il répondait que beaucoup d’amateurs étaient dans" HEIGHT="52" HPOS="300" ID="string_tl_5" VPOS="432" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="302 534 1754 534" HEIGHT="53" HPOS="302" ID="tl_6" VPOS="481" WIDTH="1452">
<String CONTENT="l’usage d’enlever les feuillets de suscriptions pour alléger leurs dossiers," HEIGHT="53" HPOS="302" ID="string_tl_6" VPOS="481" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="300 582 1756 582" HEIGHT="49" HPOS="300" ID="tl_7" VPOS="533" WIDTH="1456">
<String CONTENT="ce qui avait dû arriver pour celte collection. Non, les amateurs ne" HEIGHT="49" HPOS="300" ID="string_tl_7" VPOS="533" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="302 633 1755 633" HEIGHT="52" HPOS="302" ID="tl_8" VPOS="581" WIDTH="1453">
<String CONTENT="lacèrent pas leurs autographes, qui empruntent une grande partie de" HEIGHT="52" HPOS="302" ID="string_tl_8" VPOS="581" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="303 683 1753 683" HEIGHT="51" HPOS="303" ID="tl_9" VPOS="632" WIDTH="1450">
<String CONTENT="leur valeur à leur intégrité. C’est tout au plus si quelques gens sans" HEIGHT="51" HPOS="303" ID="string_tl_9" VPOS="632" WIDTH="1450"/>
</TextLine>
<TextLine BASELINE="300 731 1756 731" HEIGHT="50" HPOS="300" ID="tl_10" VPOS="681" WIDTH="1456">
<String CONTENT="goût se sont livrés à ce vandalisme, et encore sur des pièces sans" HEIGHT="50" HPOS="300" ID="string_tl_10" VPOS="681" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="301 782 1757 782" HEIGHT="52" HPOS="301" ID="tl_11" VPOS="730" WIDTH="1456">
<String CONTENT="valeur. D’un autre côté, il est bien vrai qu’il se rencontre souvent des" HEIGHT="52" HPOS="301" ID="string_tl_11" VPOS="730" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="302 832 1756 832" HEIGHT="51" HPOS="302" ID="tl_12" VPOS="781" WIDTH="1454">
<String CONTENT="lettres sans adresse ni cachet et qui n'en sont pas moins parfaitement" HEIGHT="51" HPOS="302" ID="string_tl_12" VPOS="781" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="300 882 1756 882" HEIGHT="52" HPOS="300" ID="tl_13" VPOS="830" WIDTH="1456">
<String CONTENT="authentiques : c’est qu’elles ont été sous enveloppe ; mais on ne peut" HEIGHT="52" HPOS="300" ID="string_tl_13" VPOS="830" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="303 930 1751 930" HEIGHT="49" HPOS="303" ID="tl_14" VPOS="881" WIDTH="1448">
<String CONTENT="pas admettre que toutes les pièces d’une collection soient dans ce cas." HEIGHT="49" HPOS="303" ID="string_tl_14" VPOS="881" WIDTH="1448"/>
</TextLine>
<TextLine BASELINE="302 982 1756 982" HEIGHT="51" HPOS="302" ID="tl_15" VPOS="931" WIDTH="1454">
<String CONTENT="M. Chasles ne pouvait pas l’ignorer, lui qui possédait une bonne" HEIGHT="51" HPOS="302" ID="string_tl_15" VPOS="931" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="300 1031 1755 1031" HEIGHT="51" HPOS="300" ID="tl_16" VPOS="980" WIDTH="1455">
<String CONTENT="collection, c’est-à-dire composée de pièces authentiques: elle devait" HEIGHT="51" HPOS="300" ID="string_tl_16" VPOS="980" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="304 1080 1270 1080" HEIGHT="50" HPOS="304" ID="tl_17" VPOS="1030" WIDTH="966">
<String CONTENT="lui dire autre chose que les pastiches de Lucas." HEIGHT="50" HPOS="304" ID="string_tl_17" VPOS="1030" WIDTH="966"/>
</TextLine>
</TextBlock>
<TextBlock HEIGHT="490" HPOS="295" ID="r_2_2" VPOS="1078" WIDTH="1463">
<Shape>
<Polygon POINTS="295 1078 1758 1078 1758 1568 295 1568"/>
</Shape>
<TextLine BASELINE="350 1131 1756 1131" HEIGHT="52" HPOS="350" ID="tl_18" VPOS="1079" WIDTH="1406">
<String CONTENT="Quelqu’un nous disait dernièrement que si l'on mettait quelque" HEIGHT="52" HPOS="350" ID="string_tl_18" VPOS="1079" WIDTH="1406"/>
</TextLine>
<TextLine BASELINE="296 1181 1755 1181" HEIGHT="52" HPOS="296" ID="tl_19" VPOS="1129" WIDTH="1459">
<String CONTENT="jour en vente la collection de VI. Chasles (la vraie), elle serait frappée" HEIGHT="52" HPOS="296" ID="string_tl_19" VPOS="1129" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="300 1229 1756 1229" HEIGHT="51" HPOS="300" ID="tl_20" VPOS="1178" WIDTH="1456">
<String CONTENT="d’une grande défaveur. Ce serait à tort, car M. Chasles, jusqu’à" HEIGHT="51" HPOS="300" ID="string_tl_20" VPOS="1178" WIDTH="1456"/>
</TextLine>
<TextLine BASELINE="303 1278 1757 1278" HEIGHT="49" HPOS="303" ID="tl_21" VPOS="1229" WIDTH="1454">
<String CONTENT="l’arrivée de ce mauvais génie de Lucas, avait su choisir en connais¬" HEIGHT="49" HPOS="303" ID="string_tl_21" VPOS="1229" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="300 1329 1754 1329" HEIGHT="52" HPOS="300" ID="tl_22" VPOS="1277" WIDTH="1454">
<String CONTENT="seur, et l’on doit avoir pleine confiance dans ses anciennes acquisitions." HEIGHT="52" HPOS="300" ID="string_tl_22" VPOS="1277" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="301 1380 1755 1380" HEIGHT="52" HPOS="301" ID="tl_23" VPOS="1328" WIDTH="1454">
<String CONTENT="Du reste, les amateurs sérieux ne s’y trompent pas : ce ne sont pas" HEIGHT="52" HPOS="301" ID="string_tl_23" VPOS="1328" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="301 1429 1756 1429" HEIGHT="51" HPOS="301" ID="tl_24" VPOS="1378" WIDTH="1455">
<String CONTENT="eux qui prendront la panique. Ils sont si peu alarmés, si peu dégoûtés" HEIGHT="51" HPOS="301" ID="string_tl_24" VPOS="1378" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="303 1478 1755 1478" HEIGHT="50" HPOS="303" ID="tl_25" VPOS="1428" WIDTH="1452">
<String CONTENT="par cette affaire, que pas un ne songe à se défaire de sa collection ce" HEIGHT="50" HPOS="303" ID="string_tl_25" VPOS="1428" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="300 1529 1755 1529" HEIGHT="52" HPOS="300" ID="tl_26" VPOS="1477" WIDTH="1455">
<String CONTENT="qui fait que les beaux et bons autographes deviennent de plus en plus" HEIGHT="52" HPOS="300" ID="string_tl_26" VPOS="1477" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="301 1567 414 1567" HEIGHT="28" HPOS="301" ID="tl_27" VPOS="1539" WIDTH="113">
<String CONTENT="rares." HEIGHT="28" HPOS="301" ID="string_tl_27" VPOS="1539" WIDTH="113"/>
</TextLine>
</TextBlock>
