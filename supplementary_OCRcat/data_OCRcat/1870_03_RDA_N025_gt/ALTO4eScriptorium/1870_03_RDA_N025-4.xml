<TextLine BASELINE="301 1302 1719 1302" HEIGHT="37" HPOS="301" ID="tl_29" VPOS="1265" WIDTH="1418">
<String CONTENT="Lucas venait habituellement chez M. Chasles le lundi après les séances de l’Aacadémie." HEIGHT="37" HPOS="301" ID="string_tl_29" VPOS="1265" WIDTH="1418"/>
</TextLine>
<TextLine BASELINE="264 1341 1718 1341" HEIGHT="38" HPOS="264" ID="tl_30" VPOS="1303" WIDTH="1454">
<String CONTENT="Il attendait M. Chasles dans son cabinet et le voyait revenir tout ému de la discussion." HEIGHT="38" HPOS="264" ID="string_tl_30" VPOS="1303" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="262 1381 1721 1381" HEIGHT="39" HPOS="262" ID="tl_31" VPOS="1342" WIDTH="1459">
<String CONTENT="M. Chasles lui communiquait les objections qui lui avaient été adressées, lui montrait le" HEIGHT="39" HPOS="262" ID="string_tl_31" VPOS="1342" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="261 1420 1720 1420" HEIGHT="39" HPOS="261" ID="tl_32" VPOS="1381" WIDTH="1459">
<String CONTENT="côté faible des pièces qu’il avait à sa disposition et l’engageait à obtenir à force d'instances," HEIGHT="39" HPOS="261" ID="string_tl_32" VPOS="1381" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="262 1458 1722 1458" HEIGHT="38" HPOS="262" ID="tl_33" VPOS="1420" WIDTH="1460">
<String CONTENT="du vieux M. de Boisjourdain, de nouvelles communications. Il lui donnait les désigna¬" HEIGHT="38" HPOS="262" ID="string_tl_33" VPOS="1420" WIDTH="1460"/>
</TextLine>
<TextLine BASELINE="263 1498 1721 1498" HEIGHT="39" HPOS="263" ID="tl_34" VPOS="1459" WIDTH="1458">
<String CONTENT="tions nécessaires et se trouvait ainsi, à son insu, le coopérateur de l’escroc qui l’exploi¬" HEIGHT="39" HPOS="263" ID="string_tl_34" VPOS="1459" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="262 1527 324 1527" HEIGHT="29" HPOS="262" ID="tl_35" VPOS="1498" WIDTH="62">
<String CONTENT="tait." HEIGHT="29" HPOS="262" ID="string_tl_35" VPOS="1498" WIDTH="62"/>
</TextLine>
</TextBlock>
