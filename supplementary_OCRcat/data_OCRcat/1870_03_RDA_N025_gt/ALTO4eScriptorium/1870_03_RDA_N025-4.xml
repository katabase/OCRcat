</Shape>
<TextLine BASELINE="303 526 1715 526" HEIGHT="39" HPOS="303" ID="tl_9" VPOS="487" WIDTH="1412">
<String CONTENT="« Je m'accuse d’être coupable, et pourtant, entre nous soit dit, beaucoup de ces" HEIGHT="39" HPOS="303" ID="string_tl_9" VPOS="487" WIDTH="1412"/>
</TextLine>
<TextLine BASELINE="260 565 1713 565" HEIGHT="40" HPOS="260" ID="tl_10" VPOS="525" WIDTH="1453">
<String CONTENT="lettres simulées ne l’ont été que d'après vos indications, car enfin, pour celles de Newton," HEIGHT="40" HPOS="260" ID="string_tl_10" VPOS="525" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="262 605 1714 605" HEIGHT="40" HPOS="262" ID="tl_11" VPOS="565" WIDTH="1452">
<String CONTENT="par exemple, n'est-ce pas vous qui m’avez fait connaître son écriture? qui m’avez indi¬" HEIGHT="40" HPOS="262" ID="string_tl_11" VPOS="565" WIDTH="1452"/>
</TextLine>
<TextLine BASELINE="262 642 1715 642" HEIGHT="38" HPOS="262" ID="tl_12" VPOS="604" WIDTH="1453">
<String CONTENT="qué et fait voir comment il faisait ses e, ses h, ses l, ses t, et comment surtout il faisait" HEIGHT="38" HPOS="262" ID="string_tl_12" VPOS="604" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="262 682 1715 682" HEIGHT="39" HPOS="262" ID="tl_13" VPOS="643" WIDTH="1453">
<String CONTENT="sa signature? Pour Galilée, n’en est-il pas de même? n’est-ce pas vous qui m’avez" HEIGHT="39" HPOS="262" ID="string_tl_13" VPOS="643" WIDTH="1453"/>
</TextLine>
<TextLine BASELINE="262 720 1716 720" HEIGHT="38" HPOS="262" ID="tl_14" VPOS="682" WIDTH="1454">
<String CONTENT="observé comment il faisait toujours le G de son nom, comment il n’oubliait jamais les" HEIGHT="38" HPOS="262" ID="string_tl_14" VPOS="682" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="264 759 1715 759" HEIGHT="38" HPOS="264" ID="tl_15" VPOS="721" WIDTH="1451">
<String CONTENT="points sur les i, et plusieurs autres remarques ? Et cette lettre du 5 novembre 1639," HEIGHT="38" HPOS="264" ID="string_tl_15" VPOS="721" WIDTH="1451"/>
</TextLine>
<TextLine BASELINE="262 798 1717 798" HEIGHT="38" HPOS="262" ID="tl_16" VPOS="760" WIDTH="1455">
<String CONTENT="rectifiée plusieurs fois, ne l’a t-elle pas été d’après vos indications ? N’est-ce pas vous" HEIGHT="38" HPOS="262" ID="string_tl_16" VPOS="760" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="262 837 1719 837" HEIGHT="39" HPOS="262" ID="tl_17" VPOS="798" WIDTH="1457">
<String CONTENT="qui m’avez signalé que le premier mot de celle lettre, qui d’abord fut écrit aurei, devait" HEIGHT="39" HPOS="262" ID="string_tl_17" VPOS="798" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="262 876 1716 876" HEIGHT="39" HPOS="262" ID="tl_18" VPOS="837" WIDTH="1454">
<String CONTENT="s’écrire haurei, et ensuite ne mavez-vous pas dit, d’après des observations que vous" HEIGHT="39" HPOS="262" ID="string_tl_18" VPOS="837" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="261 914 1715 914" HEIGHT="38" HPOS="261" ID="tl_19" VPOS="876" WIDTH="1454">
<String CONTENT="avez reçues d'Italie, que ce mot devait s’écrire hauerei ? N'est-ce pas vous aussi qui" HEIGHT="38" HPOS="261" ID="string_tl_19" VPOS="876" WIDTH="1454"/>
</TextLine>
<TextLine BASELINE="261 953 1718 953" HEIGHT="38" HPOS="261" ID="tl_20" VPOS="915" WIDTH="1457">
<String CONTENT="m'avez fait remarquer comment se faisait la signature du père Boulliau, comment il" HEIGHT="38" HPOS="261" ID="string_tl_20" VPOS="915" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="262 994 1717 994" HEIGHT="40" HPOS="262" ID="tl_21" VPOS="954" WIDTH="1455">
<String CONTENT="faisait ses g, ses p, ses q, ? N’est-ce pas vous aussi qui m’avez fait remarquer que Mau¬" HEIGHT="40" HPOS="262" ID="string_tl_21" VPOS="954" WIDTH="1455"/>
</TextLine>
<TextLine BASELINE="261 1032 1718 1032" HEIGHT="39" HPOS="261" ID="tl_22" VPOS="993" WIDTH="1457">
<String CONTENT="pertuis ne barrait jamais ses l ? N'est-ce pas vous encore, qui m’avez fait connaître une" HEIGHT="39" HPOS="261" ID="string_tl_22" VPOS="993" WIDTH="1457"/>
</TextLine>
<TextLine BASELINE="261 1069 1719 1069" HEIGHT="38" HPOS="261" ID="tl_23" VPOS="1031" WIDTH="1458">
<String CONTENT="lettre du cardinal Gentil et qui m’avez même communiqué un fac-similé de son écriture ?" HEIGHT="38" HPOS="261" ID="string_tl_23" VPOS="1031" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="262 1108 1721 1108" HEIGHT="38" HPOS="262" ID="tl_24" VPOS="1070" WIDTH="1459">
<String CONTENT="Et enfin une infinité d’autres indications et remarques que vous m’avez faites. C’est" HEIGHT="38" HPOS="262" ID="string_tl_24" VPOS="1070" WIDTH="1459"/>
</TextLine>
<TextLine BASELINE="261 1148 1719 1148" HEIGHT="39" HPOS="261" ID="tl_25" VPOS="1109" WIDTH="1458">
<String CONTENT="donc d'après vos observations et indications que beaucoup de ces lettres ont été simulées" HEIGHT="39" HPOS="261" ID="string_tl_25" VPOS="1109" WIDTH="1458"/>
</TextLine>
<TextLine BASELINE="262 1186 433 1186" HEIGHT="36" HPOS="262" ID="tl_26" VPOS="1150" WIDTH="171">
<String CONTENT="par moi. «" HEIGHT="36" HPOS="262" ID="string_tl_26" VPOS="1150" WIDTH="171"/>
</TextLine>
</TextBlock>
